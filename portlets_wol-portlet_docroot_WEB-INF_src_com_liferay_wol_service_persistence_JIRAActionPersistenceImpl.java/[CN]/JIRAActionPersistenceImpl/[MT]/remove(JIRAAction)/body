{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(jiraAction);
    }
  }
  jiraAction=removeImpl(jiraAction);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(jiraAction);
    }
  }
  return jiraAction;
}
