{
  List<SampleLARBooking> sampleLARBookings=SampleLARBookingLocalServiceUtil.getSampleLARBookingsByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<SampleLARBooking>());
  if (ListUtil.isEmpty(sampleLARBookings)) {
    return null;
  }
  return sampleLARBookings.get(0);
}
