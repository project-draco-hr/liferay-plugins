{
  List<SampleLARBooking> bookings=SampleLARBookingLocalServiceUtil.getSampleLARBookingsByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<SampleLARBooking>());
  if (ListUtil.isEmpty(bookings)) {
    return null;
  }
  return bookings.get(0);
}
