{
  calendarResourcePersistence.remove(calendarResource);
  expandoValueLocalService.deleteValues(CalendarResource.class.getName(),calendarResource.getCalendarResourceId());
  resourceLocalService.deleteResource(calendarResource.getCompanyId(),CalendarResource.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,calendarResource.getCalendarResourceId());
  deleteCalendarBookings(calendarResource);
}
