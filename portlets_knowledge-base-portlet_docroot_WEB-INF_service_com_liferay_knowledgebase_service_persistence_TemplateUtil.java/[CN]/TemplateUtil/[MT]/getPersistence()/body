{
  if (_persistence == null) {
    _persistence=(TemplatePersistence)PortletBeanLocatorUtil.locate(com.liferay.knowledgebase.service.ClpSerializer.getServletContextName(),TemplatePersistence.class.getName());
    ReferenceRegistry.registerReference(TemplateUtil.class,"_persistence");
  }
  return _persistence;
}
