{
  if (obj == null) {
    return false;
  }
  SVNRevisionImpl svnRevision=null;
  try {
    svnRevision=(SVNRevisionImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=svnRevision.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
