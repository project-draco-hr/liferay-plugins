{
  if (obj == null) {
    return false;
  }
  HRTerminationTypeClp hrTerminationType=null;
  try {
    hrTerminationType=(HRTerminationTypeClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrTerminationType.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
