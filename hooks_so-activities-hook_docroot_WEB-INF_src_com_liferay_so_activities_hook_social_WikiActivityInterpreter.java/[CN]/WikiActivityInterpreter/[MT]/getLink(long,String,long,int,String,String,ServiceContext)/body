{
  StringBundler sb=new StringBundler(3);
  sb.append("<span>");
  sb.append(wrapLink(getLinkURL(className,classPK,serviceContext),serviceContext.translate("view-wiki")));
  sb.append("</span>");
  if (type == _ACTIVITY_KEY_UPDATE_PAGE) {
    sb.append("<span>");
    sb.append(wrapLink(getDiffsURL(classPK,groupId,sourceVersion,targetVersion,serviceContext),serviceContext.translate("view-changes")));
    sb.append("</span>");
  }
  return sb.toString();
}
