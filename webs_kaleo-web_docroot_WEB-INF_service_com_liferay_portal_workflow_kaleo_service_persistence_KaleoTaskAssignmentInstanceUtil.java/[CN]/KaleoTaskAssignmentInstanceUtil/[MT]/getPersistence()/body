{
  if (_persistence == null) {
    _persistence=(KaleoTaskAssignmentInstancePersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoTaskAssignmentInstancePersistence.class.getName());
