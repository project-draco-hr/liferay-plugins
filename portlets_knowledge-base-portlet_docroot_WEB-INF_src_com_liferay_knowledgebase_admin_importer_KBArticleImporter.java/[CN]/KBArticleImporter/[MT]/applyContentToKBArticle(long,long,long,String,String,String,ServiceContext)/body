{
  KBArticle article=KBArticleLocalServiceUtil.fetchKBArticleByUrlTitle(groupId,urlTitle);
  if (article != null) {
    article=KBArticleLocalServiceUtil.updateKBArticle(userId,article.getResourcePrimKey(),title,html,article.getDescription(),null,null,serviceContext);
  }
 else {
    article=KBArticleLocalServiceUtil.addKBArticle(userId,parentResourcePrimaryKey,title,urlTitle,html,null,null,null,serviceContext);
  }
  return article;
}
