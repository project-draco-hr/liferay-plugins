{
  KBArticle kbArticle=KBArticleLocalServiceUtil.fetchKBArticleByUrlTitle(groupId,urlTitle);
  if (kbArticle != null) {
    kbArticle=KBArticleLocalServiceUtil.updateKBArticle(userId,kbArticle.getResourcePrimKey(),title,html,kbArticle.getDescription(),null,null,serviceContext);
  }
 else {
    kbArticle=KBArticleLocalServiceUtil.addKBArticle(userId,parentResourcePrimaryKey,title,urlTitle,html,null,null,null,serviceContext);
  }
  return kbArticle;
}
