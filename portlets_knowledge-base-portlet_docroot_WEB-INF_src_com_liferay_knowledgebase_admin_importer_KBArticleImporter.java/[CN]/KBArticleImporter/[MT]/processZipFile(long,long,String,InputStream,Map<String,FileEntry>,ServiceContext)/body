{
  if (inputStream == null) {
    throw new KBArticleImportException("Null import file");
  }
  try {
    ZipReader zipReader=ZipReaderFactoryUtil.getZipReader(inputStream);
    KBArticleImporterUtil.processImageFiles(groupId,fileName,zipReader,fileEntriesMap,serviceContext);
    processArticleFiles(userId,groupId,zipReader,fileEntriesMap,serviceContext);
  }
 catch (  IOException ioe) {
    throw new KBArticleImportException(ioe);
  }
}
