{
  DB db=DBManagerUtil.getDB();
  DBType dbType=db.getDBType();
  try {
    if (dbType == DBType.MYSQL) {
      int stringIndexMaxLength=GetterUtil.getInteger(PropsUtil.get(PropsKeys.DATABASE_STRING_INDEX_MAX_LENGTH,new Filter(dbType.getName())));
      runSQL("create index IX_32B691FF on SyncDLObject (treePath(" + stringIndexMaxLength + "))");
      runSQL("create index IX_EE41CBEB on SyncDLObject (treePath(" + stringIndexMaxLength + "), event_)");
    }
 else {
      runSQL("create index IX_32B691FF on SyncDLObject (treePath))");
      runSQL("create index IX_EE41CBEB on SyncDLObject (treePath, " + "event_)");
    }
  }
 catch (  Exception e) {
  }
}
