{
  PollsQuestionPermission.check(getPermissionChecker(),pollsQuestionId,ActionKeys.UPDATE);
  return pollsQuestionLocalService.updatePollsQuestion(getUserId(),pollsQuestionId,titleMap,descriptionMap,expirationDateMonth,expirationDateDay,expirationDateYear,expirationDateHour,expirationDateMinute,neverExpire,pollsChoices,serviceContext);
}
