{
  KaleoTimer kaleoTimer=null;
  try {
    kaleoTimer=kaleoTimerLocalService.getDefaultKaleoTimer(kaleoTask.getKaleoTaskId());
  }
 catch (  NoSuchTimerException nste) {
  }
  if (kaleoTimer == null) {
    return null;
  }
  DelayDuration delayDuration=new DelayDuration(kaleoTimer.getDuration(),DurationScale.parse(kaleoTimer.getScale()));
  return _dueDateCalculator.getDueDate(new Date(),delayDuration);
}
