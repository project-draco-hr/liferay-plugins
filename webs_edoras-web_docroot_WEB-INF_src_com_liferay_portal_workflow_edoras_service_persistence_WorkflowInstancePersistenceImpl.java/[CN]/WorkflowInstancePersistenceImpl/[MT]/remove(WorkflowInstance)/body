{
  for (  ModelListener<WorkflowInstance> listener : listeners) {
    listener.onBeforeRemove(workflowInstance);
  }
  workflowInstance=removeImpl(workflowInstance);
  for (  ModelListener<WorkflowInstance> listener : listeners) {
    listener.onAfterRemove(workflowInstance);
  }
  return workflowInstance;
}
