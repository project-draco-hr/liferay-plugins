{
  Calendar jCalendar=JCalendarUtil.getJCalendar(_calendarBooking.getStartTime());
  Calendar startTimeJCalendar=JCalendarUtil.getJCalendar(dateValue.year(),dateValue.month() - 1,dateValue.day(),jCalendar.get(Calendar.HOUR_OF_DAY),jCalendar.get(Calendar.MINUTE),jCalendar.get(Calendar.SECOND),jCalendar.get(Calendar.MILLISECOND),TimeZone.getTimeZone(StringPool.UTC));
  TimeZone timeZone=_getTimeZone(_calendarBooking);
  int shift=JCalendarUtil.getDSTShift(jCalendar,startTimeJCalendar,timeZone);
  startTimeJCalendar.add(Calendar.MILLISECOND,shift);
  return startTimeJCalendar;
}
