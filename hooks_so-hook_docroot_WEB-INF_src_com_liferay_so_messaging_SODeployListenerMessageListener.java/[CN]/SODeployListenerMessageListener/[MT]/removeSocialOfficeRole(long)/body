{
  Role role=RoleLocalServiceUtil.fetchRole(companyId,"Social Office User");
  if (role != null) {
    try {
      RoleLocalServiceUtil.deleteRole(role);
    }
 catch (    Exception e) {
      if (_log.isInfoEnabled()) {
        _log.info("Unable to delete role Social Office User");
      }
    }
  }
}
