{
  ExpandoTable table=ExpandoTableLocalServiceUtil.getTable(companyId,className,tableName);
  ExpandoColumn column=ExpandoColumnLocalServiceUtil.getColumn(table.getTableId(),columnName);
  ExpandoValue value=new MongoExpandoValue();
  value.setCompanyId(table.getCompanyId());
  value.setColumnId(column.getColumnId());
  value.setDate(data);
  return addValue(table.getClassNameId(),table.getTableId(),column.getColumnId(),classPK,value.getData());
}
