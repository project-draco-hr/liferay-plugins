{
  EntityCacheUtil.putResult(KBArticleModelImpl.ENTITY_CACHE_ENABLED,KBArticleImpl.class,kbArticle.getPrimaryKey(),kbArticle);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{kbArticle.getUuid(),kbArticle.getGroupId()},kbArticle);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_R_V,new Object[]{kbArticle.getResourcePrimKey(),kbArticle.getVersion()},kbArticle);
  kbArticle.resetOriginalValues();
}
