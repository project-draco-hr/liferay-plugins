{
  int count=countByG_P_S_S(groupId,parentResourcePrimKey,sections,status);
  if (count == 0) {
    return null;
  }
  List<KBArticle> list=findByG_P_S_S(groupId,parentResourcePrimKey,sections,status,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
