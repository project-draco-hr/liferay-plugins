{
  int count=countByG_P_S_M(groupId,parentResourcePrimKey,sections,main);
  if (count == 0) {
    return null;
  }
  List<KBArticle> list=findByG_P_S_M(groupId,parentResourcePrimKey,sections,main,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
