{
  String language=renderRequest.getPreferences().getValue("language",ConfigurationActionImpl.DEFAULT_LANGUAGE);
  String script=renderRequest.getPreferences().getValue("script",ConfigurationActionImpl.DEFAULT_SCRIPT);
  renderResponse.setContentType(ContentTypes.TEXT_HTML);
  if (Validator.isNotNull(script)) {
    include(script,language,renderRequest,renderResponse);
  }
}
