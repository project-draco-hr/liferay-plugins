{
  Session session=null;
  try {
    session=openSession();
    KaleoNode kaleoNode=(KaleoNode)session.get(KaleoNodeImpl.class,new Long(kaleoNodeId));
    if (kaleoNode == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoNodeId);
      }
      throw new NoSuchNodeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoNodeId);
    }
    return kaleoNodePersistence.remove(kaleoNode);
  }
 catch (  NoSuchNodeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
