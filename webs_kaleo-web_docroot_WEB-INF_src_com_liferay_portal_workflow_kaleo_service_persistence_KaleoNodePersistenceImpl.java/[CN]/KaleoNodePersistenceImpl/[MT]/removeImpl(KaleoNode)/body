{
  kaleoNode=toUnwrappedModel(kaleoNode);
  Session session=null;
  try {
    session=openSession();
    if (kaleoNode.isCachedModel()) {
      kaleoNode=(KaleoNode)session.get(KaleoNodeImpl.class,kaleoNode.getPrimaryKeyObj());
    }
    session.delete(kaleoNode);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(kaleoNode);
  return kaleoNode;
}
