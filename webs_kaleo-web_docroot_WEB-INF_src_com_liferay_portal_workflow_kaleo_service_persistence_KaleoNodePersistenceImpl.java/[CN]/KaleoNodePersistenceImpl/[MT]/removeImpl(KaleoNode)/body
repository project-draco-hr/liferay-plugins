{
  kaleoNode=toUnwrappedModel(kaleoNode);
  Session session=null;
  try {
    session=openSession();
    if (kaleoNode.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(KaleoNodeImpl.class,kaleoNode.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(kaleoNode);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(KaleoNodeModelImpl.ENTITY_CACHE_ENABLED,KaleoNodeImpl.class,kaleoNode.getPrimaryKey());
  return kaleoNode;
}
