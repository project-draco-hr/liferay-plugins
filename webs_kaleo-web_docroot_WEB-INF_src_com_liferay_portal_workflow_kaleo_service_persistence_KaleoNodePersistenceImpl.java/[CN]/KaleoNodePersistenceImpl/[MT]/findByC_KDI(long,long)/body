{
  Object[] finderArgs=new Object[]{new Long(companyId),new Long(kaleoDefinitionId)};
  List<KaleoNode> list=(List<KaleoNode>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_C_KDI,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler(4);
      query.append(_SQL_SELECT_KALEONODE_WHERE);
      query.append(_FINDER_COLUMN_C_KDI_COMPANYID_2);
      query.append(_FINDER_COLUMN_C_KDI_KALEODEFINITIONID_2);
      query.append(KaleoNodeModelImpl.ORDER_BY_JPQL);
      String sql=query.toString();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(companyId);
      qPos.add(kaleoDefinitionId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<KaleoNode>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_C_KDI,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
