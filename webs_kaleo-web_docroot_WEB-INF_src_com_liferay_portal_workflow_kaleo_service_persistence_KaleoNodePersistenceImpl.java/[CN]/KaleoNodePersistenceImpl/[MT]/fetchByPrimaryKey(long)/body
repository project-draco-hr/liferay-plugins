{
  KaleoNode kaleoNode=(KaleoNode)EntityCacheUtil.getResult(KaleoNodeModelImpl.ENTITY_CACHE_ENABLED,KaleoNodeImpl.class,kaleoNodeId);
  if (kaleoNode == _nullKaleoNode) {
    return null;
  }
  if (kaleoNode == null) {
    Session session=null;
    try {
      session=openSession();
      kaleoNode=(KaleoNode)session.get(KaleoNodeImpl.class,Long.valueOf(kaleoNodeId));
      if (kaleoNode != null) {
        cacheResult(kaleoNode);
      }
 else {
        EntityCacheUtil.putResult(KaleoNodeModelImpl.ENTITY_CACHE_ENABLED,KaleoNodeImpl.class,kaleoNodeId,_nullKaleoNode);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(KaleoNodeModelImpl.ENTITY_CACHE_ENABLED,KaleoNodeImpl.class,kaleoNodeId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return kaleoNode;
}
