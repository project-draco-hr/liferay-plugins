{
  if (!context.addPrimaryKey(AdminPortletDataHandlerImpl.class,"deleteData")) {
    ArticleLocalServiceUtil.deleteGroupArticles(context.getScopeGroupId());
    CommentUtil.removeByGroupId(context.getScopeGroupId());
    TemplateLocalServiceUtil.deleteGroupTemplates(context.getScopeGroupId());
  }
  return null;
}
