{
  if (!portletDataContext.addPrimaryKey(AdminPortletDataHandlerImpl.class,"deleteData")) {
    ArticleLocalServiceUtil.deleteGroupArticles(portletDataContext.getScopeGroupId());
    CommentUtil.removeByGroupId(portletDataContext.getScopeGroupId());
    TemplateLocalServiceUtil.deleteGroupTemplates(portletDataContext.getScopeGroupId());
  }
  return null;
}
