{
  try {
    context.importPermissions("com.liferay.knowledgebase.admin",context.getSourceGroupId(),context.getGroupId());
    Document doc=SAXReaderUtil.read(data);
    Element root=doc.getRootElement();
    importArticles(context,root);
    return null;
  }
 catch (  Exception e) {
    throw new PortletDataException(e);
  }
}
