{
  Session session=null;
  try {
    session=openSession();
    CalendarBooking calendarBooking=(CalendarBooking)session.get(CalendarBookingImpl.class,Long.valueOf(calendarBookingId));
    if (calendarBooking == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + calendarBookingId);
      }
      throw new NoSuchBookingException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + calendarBookingId);
    }
    return calendarBookingPersistence.remove(calendarBooking);
  }
 catch (  NoSuchBookingException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
