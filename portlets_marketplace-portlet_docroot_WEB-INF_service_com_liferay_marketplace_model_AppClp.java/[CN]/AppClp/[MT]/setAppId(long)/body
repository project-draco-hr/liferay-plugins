{
  _appId=appId;
  if (_appRemoteModel != null) {
    try {
      Class<?> clazz=_appRemoteModel.getClass();
      Method method=clazz.getMethod("setAppId",long.class);
      method.invoke(_appRemoteModel,appId);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
