{
  _version=version;
  if (_appRemoteModel != null) {
    try {
      Class<?> clazz=_appRemoteModel.getClass();
      Method method=clazz.getMethod("setVersion",String.class);
      method.invoke(_appRemoteModel,version);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
