{
  _title=title;
  if (_appRemoteModel != null) {
    try {
      Class<?> clazz=_appRemoteModel.getClass();
      Method method=clazz.getMethod("setTitle",String.class);
      method.invoke(_appRemoteModel,title);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
