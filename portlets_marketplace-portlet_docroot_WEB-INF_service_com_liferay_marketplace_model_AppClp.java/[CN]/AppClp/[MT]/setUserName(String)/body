{
  _userName=userName;
  if (_appRemoteModel != null) {
    try {
      Class<?> clazz=_appRemoteModel.getClass();
      Method method=clazz.getMethod("setUserName",String.class);
      method.invoke(_appRemoteModel,userName);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
