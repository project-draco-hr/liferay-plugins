{
  _uuid=uuid;
  if (_appRemoteModel != null) {
    try {
      Class<?> clazz=_appRemoteModel.getClass();
      Method method=clazz.getMethod("setUuid",String.class);
      method.invoke(_appRemoteModel,uuid);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
