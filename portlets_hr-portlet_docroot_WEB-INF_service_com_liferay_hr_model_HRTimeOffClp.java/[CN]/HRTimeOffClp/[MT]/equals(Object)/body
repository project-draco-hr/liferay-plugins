{
  if (obj == null) {
    return false;
  }
  HRTimeOffClp hrTimeOff=null;
  try {
    hrTimeOff=(HRTimeOffClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrTimeOff.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
