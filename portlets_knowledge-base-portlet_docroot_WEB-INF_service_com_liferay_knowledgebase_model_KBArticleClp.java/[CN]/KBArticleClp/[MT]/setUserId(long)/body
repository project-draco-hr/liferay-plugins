{
  _userId=userId;
  if (_kbArticleRemoteModel != null) {
    try {
      Class<?> clazz=_kbArticleRemoteModel.getClass();
      Method method=clazz.getMethod("setUserId",long.class);
      method.invoke(_kbArticleRemoteModel,userId);
    }
