{
  if (_syncPropsPersistence != null) {
    return _syncPropsPersistence;
  }
  try {
    _syncPropsPersistence=new SyncPropsPersistence();
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
  }
  return _syncPropsPersistence;
}
