{
  ExtRepositoryModel extRepositoryModel=extRepositoryModelAdapter.getExtRepositoryModel();
  String extRepositoryModelKey=extRepositoryModel.getExtRepositoryModelKey();
  Map<String,ExtRepositoryModelAdapter<?>> extRepositoryAdaptersCache=_getExtRepositoryAdaptersCache();
  extRepositoryAdaptersCache.put(extRepositoryModelKey,extRepositoryModelAdapter);
  if (_log.isInfoEnabled()) {
    _log.info("Cache put: " + extRepositoryModelKey);
  }
}
