{
  type.setNew(false);
  type=typePersistence.update(type,merge);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.reindex(type);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
  return type;
}
