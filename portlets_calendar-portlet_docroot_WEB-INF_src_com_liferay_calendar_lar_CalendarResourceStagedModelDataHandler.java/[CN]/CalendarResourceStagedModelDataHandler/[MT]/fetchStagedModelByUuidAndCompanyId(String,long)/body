{
  List<CalendarResource> calendarResources=CalendarResourceLocalServiceUtil.getCalendarResourcesByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<CalendarResource>());
  if (ListUtil.isEmpty(calendarResources)) {
    return null;
  }
  return calendarResources.get(0);
}
