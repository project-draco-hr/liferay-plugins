{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),PollsChoiceService.class.getName());
    if (invokableService instanceof PollsChoiceService) {
      _service=(PollsChoiceService)invokableService;
    }
 else {
      _service=new PollsChoiceServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(PollsChoiceServiceUtil.class,"_service");
  }
  return _service;
}
