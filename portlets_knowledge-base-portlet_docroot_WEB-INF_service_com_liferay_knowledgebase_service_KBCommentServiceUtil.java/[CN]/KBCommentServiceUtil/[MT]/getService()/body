{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KBCommentService.class.getName());
    if (invokableService instanceof KBCommentService) {
      _service=(KBCommentService)invokableService;
    }
 else {
      _service=new KBCommentServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(KBCommentServiceUtil.class,"_service");
  }
  return _service;
}
