{
  if (permissionChecker.hasOwnerPermission(kbStructure.getCompanyId(),KBStructure.class.getName(),kbStructure.getKbStructureId(),kbStructure.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(kbStructure.getGroupId(),KBStructure.class.getName(),kbStructure.getKbStructureId(),actionId);
}
