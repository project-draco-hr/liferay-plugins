{
  Session session=null;
  try {
    session=openSession();
    SemanticsFile semanticsFile=(SemanticsFile)session.get(SemanticsFileImpl.class,semanticsURI);
    if (semanticsFile == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No SemanticsFile exists with the primary key " + semanticsURI);
      }
      throw new NoSuchSemanticsFileException("No SemanticsFile exists with the primary key " + semanticsURI);
    }
    return remove(semanticsFile);
  }
 catch (  NoSuchSemanticsFileException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
