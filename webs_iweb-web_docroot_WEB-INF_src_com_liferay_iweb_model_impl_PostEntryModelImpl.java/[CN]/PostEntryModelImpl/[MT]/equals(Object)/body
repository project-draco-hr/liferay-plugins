{
  if (obj == null) {
    return false;
  }
  PostEntryImpl postEntry=null;
  try {
    postEntry=(PostEntryImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=postEntry.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
