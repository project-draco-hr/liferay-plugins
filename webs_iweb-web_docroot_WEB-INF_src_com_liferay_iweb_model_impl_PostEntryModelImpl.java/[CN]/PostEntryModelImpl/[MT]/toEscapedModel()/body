{
  if (isEscapedModel()) {
    return (PostEntry)this;
  }
 else {
    PostEntry model=new PostEntryImpl();
    model.setEscapedModel(true);
    model.setUid(getUid());
    model.setInterestGroupId(getInterestGroupId());
    model.setPid(getPid());
    model.setType(HtmlUtil.escape(getType()));
    model=(PostEntry)Proxy.newProxyInstance(PostEntry.class.getClassLoader(),new Class[]{PostEntry.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
