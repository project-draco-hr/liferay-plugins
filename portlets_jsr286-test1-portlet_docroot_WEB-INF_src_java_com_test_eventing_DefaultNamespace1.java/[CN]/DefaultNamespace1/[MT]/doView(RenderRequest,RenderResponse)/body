{
  response.setContentType("text/html");
  PrintWriter writer=response.getWriter();
  PortletURL aurl1=response.createActionURL();
  aurl1.setParameter("op","eventDefaultNS");
  writer.println("<br><a href=\"" + aurl1 + "\">SendDefaultNamespaceEvent</a>");
  PortletURL aurl2=response.createActionURL();
  aurl2.setParameter("op","renderDefaultNS");
  writer.println("<br><a href=\"" + aurl2 + "\">SetDefaultNameSpaceRender</a>");
}
