{
  String creatorUserName=getUserName(request.getUserId(),themeDisplay);
  int requestType=request.getType();
  String title=StringPool.BLANK;
  if (requestType == SocialRelationConstants.TYPE_BI_COWORKER) {
    title=themeDisplay.translate("request-social-networking-summary-add-coworker",new Object[]{creatorUserName});
  }
 else   if (requestType == SocialRelationConstants.TYPE_BI_FRIEND) {
    title=themeDisplay.translate("request-social-networking-summary-add-friend",new Object[]{creatorUserName});
  }
  String body=StringPool.BLANK;
  return new SocialRequestFeedEntry(title,body);
}
