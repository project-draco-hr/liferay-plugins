{
  int requestType=request.getType();
  String title=StringPool.BLANK;
  if (requestType == SocialRelationConstants.TYPE_BI_CONNECTION) {
    String creatorUserName=getUserNameLink(request.getUserId(),themeDisplay);
    title=themeDisplay.translate("request-social-networking-summary-add-connection",new Object[]{creatorUserName});
  }
  String body=StringPool.BLANK;
  return new SocialRequestFeedEntry(title,body);
}
