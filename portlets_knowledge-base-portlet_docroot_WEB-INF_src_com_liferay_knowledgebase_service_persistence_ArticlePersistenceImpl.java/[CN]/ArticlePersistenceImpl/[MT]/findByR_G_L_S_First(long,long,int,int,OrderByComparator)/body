{
  List<Article> list=findByR_G_L_S(resourcePrimKey,groupId,latest,status,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(10);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("resourcePrimKey=");
    msg.append(resourcePrimKey);
    msg.append(", groupId=");
    msg.append(groupId);
    msg.append(", latest=");
    msg.append(latest);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchArticleException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
