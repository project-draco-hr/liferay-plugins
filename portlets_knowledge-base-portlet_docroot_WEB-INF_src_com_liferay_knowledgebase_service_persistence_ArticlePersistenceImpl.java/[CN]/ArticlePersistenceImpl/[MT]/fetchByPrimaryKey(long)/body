{
  Article article=(Article)EntityCacheUtil.getResult(ArticleModelImpl.ENTITY_CACHE_ENABLED,ArticleImpl.class,articleId,this);
  if (article == null) {
    Session session=null;
    try {
      session=openSession();
      article=(Article)session.get(ArticleImpl.class,new Long(articleId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (article != null) {
        cacheResult(article);
      }
      closeSession(session);
    }
  }
  return article;
}
