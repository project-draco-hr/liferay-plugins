{
  Article article=findByPrimaryKey(articleId);
  Session session=null;
  try {
    session=openSession();
    Article[] array=new ArticleImpl[3];
    array[0]=getByR_G_M_PrevAndNext(session,article,resourcePrimKey,groupId,main,orderByComparator,true);
    array[1]=article;
    array[2]=getByR_G_M_PrevAndNext(session,article,resourcePrimKey,groupId,main,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
