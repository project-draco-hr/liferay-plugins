{
  int count=countByNotR_G_P_P_L(resourcePrimKey,groupId,parentResourcePrimKey,priority,latest);
  List<Article> list=findByNotR_G_P_P_L(resourcePrimKey,groupId,parentResourcePrimKey,priority,latest,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(12);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("resourcePrimKey=");
    msg.append(resourcePrimKey);
    msg.append(", groupId=");
    msg.append(groupId);
    msg.append(", parentResourcePrimKey=");
    msg.append(parentResourcePrimKey);
    msg.append(", priority=");
    msg.append(priority);
    msg.append(", latest=");
    msg.append(latest);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchArticleException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
