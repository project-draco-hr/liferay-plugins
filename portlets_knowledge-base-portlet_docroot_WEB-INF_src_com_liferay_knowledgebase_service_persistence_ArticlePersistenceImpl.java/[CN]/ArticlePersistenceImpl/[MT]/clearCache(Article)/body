{
  EntityCacheUtil.removeResult(ArticleModelImpl.ENTITY_CACHE_ENABLED,ArticleImpl.class,article.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{article.getUuid(),new Long(article.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_R_V,new Object[]{new Long(article.getResourcePrimKey()),new Double(article.getVersion())});
}
