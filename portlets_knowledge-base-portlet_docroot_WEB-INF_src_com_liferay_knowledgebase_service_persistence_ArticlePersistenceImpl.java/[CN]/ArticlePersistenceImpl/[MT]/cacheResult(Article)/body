{
  EntityCacheUtil.putResult(ArticleModelImpl.ENTITY_CACHE_ENABLED,ArticleImpl.class,article.getPrimaryKey(),article);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{article.getUuid(),Long.valueOf(article.getGroupId())},article);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_R_V,new Object[]{Long.valueOf(article.getResourcePrimKey()),Integer.valueOf(article.getVersion())},article);
}
