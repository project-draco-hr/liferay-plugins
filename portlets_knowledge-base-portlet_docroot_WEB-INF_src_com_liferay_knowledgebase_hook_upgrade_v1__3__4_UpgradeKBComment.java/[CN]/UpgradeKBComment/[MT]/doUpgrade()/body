{
  if (!tableHasColumn("KBComment","helpful")) {
    return;
  }
  runSQL("update KBComment set userRating = " + KBCommentConstants.RATING_LIKE + " where helpful = TRUE");
  runSQL("update KBComment set userRating = " + KBCommentConstants.RATING_DISLIKE + " where helpful = FALSE");
  runSQL("alter table KBComment drop column helpful");
}
