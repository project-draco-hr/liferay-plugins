{
  ExpandoTable table=null;
  try {
    table=ExpandoTableLocalServiceUtil.addTable(entityName,ExpandoTableConstants.DEFAULT_TABLE_NAME);
  }
 catch (  Exception dtne) {
    try {
      table=ExpandoTableLocalServiceUtil.getTable(entityName,ExpandoTableConstants.DEFAULT_TABLE_NAME);
    }
 catch (    Exception e) {
    }
  }
  try {
    ExpandoColumnLocalServiceUtil.addColumn(table.getTableId(),newFieldName,newFieldType);
  }
 catch (  Exception dcne) {
  }
}
