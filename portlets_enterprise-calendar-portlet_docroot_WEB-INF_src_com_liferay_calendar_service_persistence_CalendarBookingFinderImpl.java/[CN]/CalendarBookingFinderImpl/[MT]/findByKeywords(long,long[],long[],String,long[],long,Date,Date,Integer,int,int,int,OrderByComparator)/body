    andOperator=true;
  }
  return findByC_G_C_C_P_T_D_L_T_S_E_P_S(companyId,groupIds,calendarIds,calendarResourceIds,parentCalendarBookingId,titles,descriptions,locations,types,startDate,endDate,priority,status,andOperator,start,end,orderByComparator);
