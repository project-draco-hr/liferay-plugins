{
  if (calendarIds == null || calendarIds.length == 0) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler(calendarIds.length * 2 + 1);
  sb.append(" (");
  for (int i=0; i < calendarIds.length; i++) {
    sb.append("calendarId = ? ");
    if ((i + 1) != calendarIds.length) {
      sb.append("OR ");
    }
  }
  sb.append(") AND");
  return sb.toString();
}
