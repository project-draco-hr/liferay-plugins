{
  Session session=null;
  try {
    session=openSession();
    session.delete(producer);
    session.flush();
    return producer;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Producer.class.getName());
  }
}
