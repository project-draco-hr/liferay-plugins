{
  Session session=null;
  try {
    session=openSession();
    Producer producer=(Producer)session.get(ProducerImpl.class,new Long(producerId));
    if (producer == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No Producer exists with the primary key " + producerId);
      }
      throw new NoSuchProducerException("No Producer exists with the primary key " + producerId);
    }
    return remove(producer);
  }
 catch (  NoSuchProducerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
