{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(notepadEntry);
    }
  }
  notepadEntry=removeImpl(notepadEntry);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(notepadEntry);
    }
  }
  return notepadEntry;
}
