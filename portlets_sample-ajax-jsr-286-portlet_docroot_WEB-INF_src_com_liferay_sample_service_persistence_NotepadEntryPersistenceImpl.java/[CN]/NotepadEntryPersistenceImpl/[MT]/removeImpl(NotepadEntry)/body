{
  Session session=null;
  try {
    session=openSession();
    session.delete(notepadEntry);
    session.flush();
    return notepadEntry;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(NotepadEntry.class.getName());
  }
}
