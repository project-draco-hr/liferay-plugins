{
  if (_persistence == null) {
    _persistence=(HRUserHistoryPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRUserHistoryPersistence.class.getName());
    ReferenceRegistry.registerReference(HRUserHistoryUtil.class,"_persistence");
