{
  String[] existingAttachments=DLServiceUtil.getFileNames(deportDefinition.getCompanyId(),CompanyConstants.SYSTEM,deportDefinition.getAttachmentsDir());
  ReportDesignRetriever reportDesignRetriever=new ServletContextReportDesignRetriever(servletContext,existingAttachments[0],deportDefinition.getAttachmentsDir(),StringPool.BLANK);
  ReportRequest request=new ReportRequest(reportDesignRetriever,deportDefinition.getReportFormat(),"/","/");
  return request;
}
