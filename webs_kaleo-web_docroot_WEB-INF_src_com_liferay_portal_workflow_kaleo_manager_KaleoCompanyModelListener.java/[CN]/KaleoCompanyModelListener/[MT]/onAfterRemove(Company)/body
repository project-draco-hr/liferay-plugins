{
  try {
    PortalKaleoManager portalKaleoManager=PortalKaleoManagerUtil.getPortalKaleoManager();
    portalKaleoManager.deleteKaleoData(companyModel);
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to delete Kaleo data.  " + "Please attempt to manually remove",e);
    }
  }
}
