{
  try {
    PortalKaleoManager portalKaleoManager=PortalKaleoManagerUtil.getPortalKaleoManager();
    portalKaleoManager.deployKaleoDefaults();
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn("Unable to deploy default Kaleo definitions.  " + "Please attempt to manually deploy",e);
    }
  }
}
