{
  KBArticle article=kbArticleLocalService.getArticle(resourcePrimKey);
  if (article.isTemplate()) {
    KBPermission.check(getPermissionChecker(),plid,KnowledgeBaseKeys.MANAGE_TEMPLATES);
  }
 else {
    KBArticlePermission.check(getPermissionChecker(),resourcePrimKey,ActionKeys.DELETE);
  }
  kbArticleLocalService.deleteArticle(resourcePrimKey);
}
