{
  KBArticle kbArticle=KBArticleLocalServiceUtil.getLatestKBArticle(resourcePrimKey,WorkflowConstants.STATUS_ANY);
  checkAttachmentPermissions(kbArticle.getGroupId(),PortletKeys.KNOWLEDGE_BASE_ADMIN,resourcePrimKey);
  kbArticleLocalService.addAttachment(getUserId(),resourcePrimKey,fileName,inputStream,mimeType);
}
