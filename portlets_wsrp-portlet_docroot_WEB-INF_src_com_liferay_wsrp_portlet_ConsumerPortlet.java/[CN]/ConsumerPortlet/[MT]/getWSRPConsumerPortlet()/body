{
  String portletName=getPortletConfig().getPortletName();
  int pos=portletName.indexOf(StringPool.UNDERLINE,PORTLET_NAME_PREFIX.length());
  String safeUUID=portletName.substring(pos + 1);
  String uuid=PortalUUIDUtil.fromSafeUuid(safeUUID);
  WSRPConsumerPortlet wsrpConsumerPortlet=WSRPConsumerPortletLocalServiceUtil.getWSRPConsumerPortlet(uuid);
  return wsrpConsumerPortlet;
}
