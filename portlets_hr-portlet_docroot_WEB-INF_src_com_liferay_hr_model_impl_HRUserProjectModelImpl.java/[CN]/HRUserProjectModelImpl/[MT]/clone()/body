{
  HRUserProjectImpl hrUserProjectImpl=new HRUserProjectImpl();
  hrUserProjectImpl.setHrUserProjectId(getHrUserProjectId());
  hrUserProjectImpl.setGroupId(getGroupId());
  hrUserProjectImpl.setCompanyId(getCompanyId());
  hrUserProjectImpl.setUserId(getUserId());
  hrUserProjectImpl.setUserName(getUserName());
  hrUserProjectImpl.setCreateDate(getCreateDate());
  hrUserProjectImpl.setModifiedDate(getModifiedDate());
  hrUserProjectImpl.setHrProjectBillingRateId(getHrProjectBillingRateId());
  hrUserProjectImpl.setHrProjectId(getHrProjectId());
  hrUserProjectImpl.setHrProjectRoleId(getHrProjectRoleId());
  hrUserProjectImpl.setHrUserId(getHrUserId());
  hrUserProjectImpl.setActualRate(getActualRate());
  hrUserProjectImpl.resetOriginalValues();
  return hrUserProjectImpl;
}
