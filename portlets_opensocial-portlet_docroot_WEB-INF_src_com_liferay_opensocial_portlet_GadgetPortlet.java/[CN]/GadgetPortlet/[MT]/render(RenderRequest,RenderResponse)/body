{
  try {
    Gadget gadget=getGadget();
    renderRequest.setAttribute(WebKeys.GADGET,gadget);
    super.render(renderRequest,renderResponse);
  }
 catch (  IOException ioe) {
    throw ioe;
  }
catch (  PortletException pe) {
    throw pe;
  }
catch (  Exception e) {
    throw new PortletException(e);
  }
}
