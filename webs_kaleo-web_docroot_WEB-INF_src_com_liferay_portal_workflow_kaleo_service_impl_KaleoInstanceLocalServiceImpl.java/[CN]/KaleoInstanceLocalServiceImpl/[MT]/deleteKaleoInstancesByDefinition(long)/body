{
  kaleoInstancePersistence.removeByKaleoDefinitionId(kaleoDefinitionId);
  kaleoInstanceTokenLocalService.deleteKaleoInstanceTokensByDefinition(kaleoDefinitionId);
  kaleoLogLocalService.deleteKaleoLogsByDefinition(kaleoDefinitionId);
  kaleoTaskInstanceTokenLocalService.deleteKaleoTaskInstanceTokensByDefinition(kaleoDefinitionId);
}
