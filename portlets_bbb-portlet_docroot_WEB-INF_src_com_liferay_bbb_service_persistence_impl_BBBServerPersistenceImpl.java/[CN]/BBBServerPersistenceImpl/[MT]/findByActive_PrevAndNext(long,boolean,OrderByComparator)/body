{
  BBBServer bbbServer=findByPrimaryKey(bbbServerId);
  Session session=null;
  try {
    session=openSession();
    BBBServer[] array=new BBBServerImpl[3];
    array[0]=getByActive_PrevAndNext(session,bbbServer,active,orderByComparator,true);
    array[1]=bbbServer;
    array[2]=getByActive_PrevAndNext(session,bbbServer,active,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
