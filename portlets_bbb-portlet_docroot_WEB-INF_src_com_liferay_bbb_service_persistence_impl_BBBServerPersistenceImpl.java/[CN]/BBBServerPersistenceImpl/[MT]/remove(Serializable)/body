{
  Session session=null;
  try {
    session=openSession();
    BBBServer bbbServer=(BBBServer)session.get(BBBServerImpl.class,primaryKey);
    if (bbbServer == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchServerException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(bbbServer);
  }
 catch (  NoSuchServerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
