{
  EntityCacheUtil.removeResult(KBCommentModelImpl.ENTITY_CACHE_ENABLED,KBCommentImpl.class,kbComment.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{kbComment.getUuid(),Long.valueOf(kbComment.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_U_C_C,new Object[]{Long.valueOf(kbComment.getUserId()),Long.valueOf(kbComment.getClassNameId()),Long.valueOf(kbComment.getClassPK())});
}
