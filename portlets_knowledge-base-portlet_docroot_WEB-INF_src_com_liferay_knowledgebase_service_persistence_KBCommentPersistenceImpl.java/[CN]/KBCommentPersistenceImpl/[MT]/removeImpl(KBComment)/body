{
  kbComment=toUnwrappedModel(kbComment);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,kbComment);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(kbComment);
  return kbComment;
}
