{
  EntityCacheUtil.putResult(KBCommentModelImpl.ENTITY_CACHE_ENABLED,KBCommentImpl.class,kbComment.getPrimaryKey(),kbComment);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{kbComment.getUuid(),kbComment.getGroupId()},kbComment);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_C_C,new Object[]{kbComment.getUserId(),kbComment.getClassNameId(),kbComment.getClassPK()},kbComment);
  kbComment.resetOriginalValues();
}
