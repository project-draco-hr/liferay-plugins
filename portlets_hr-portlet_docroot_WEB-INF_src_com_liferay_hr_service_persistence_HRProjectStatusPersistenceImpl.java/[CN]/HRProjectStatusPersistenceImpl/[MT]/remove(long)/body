{
  Session session=null;
  try {
    session=openSession();
    HRProjectStatus hrProjectStatus=(HRProjectStatus)session.get(HRProjectStatusImpl.class,Long.valueOf(hrProjectStatusId));
    if (hrProjectStatus == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrProjectStatusId);
      }
      throw new NoSuchProjectStatusException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrProjectStatusId);
    }
    return hrProjectStatusPersistence.remove(hrProjectStatus);
  }
 catch (  NoSuchProjectStatusException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
