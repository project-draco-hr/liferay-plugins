{
  try {
    Long presenceStatus=presenceUserLocalService.getPresenceUser(userId).getPresenceStatus();
    if (presenceStatus.equals(PresenceStatusConstants.STATUS_ONLINE)) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    throw new RUONException(e);
  }
}
