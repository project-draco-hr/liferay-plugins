{
  try {
    PortletPreferencesThreadLocal.setStrict(false);
    setupRole(companyId);
    setupUserGroup(companyId);
    setupExpando(companyId);
    setupLayoutSetPrototype(companyId);
    setInitialized(companyId);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
 finally {
    PortletPreferencesThreadLocal.setStrict(true);
  }
}
