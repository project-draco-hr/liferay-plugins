{
  Session session=null;
  try {
    session=openSession();
    if (BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(KBArticleResourceImpl.class,kbArticleResource.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(kbArticleResource);
    session.flush();
    return kbArticleResource;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(KBArticleResource.class.getName());
  }
}
