{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    serviceContext.setUserId(userId);
    List<Long> roleIds=RoleRetrievalUtil.getRoleIds(serviceContext);
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=KaleoTaskInstanceTokenLocalServiceUtil.getKaleoTaskInstanceTokens(roleIds,completed,start,end,orderByComparator,serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
