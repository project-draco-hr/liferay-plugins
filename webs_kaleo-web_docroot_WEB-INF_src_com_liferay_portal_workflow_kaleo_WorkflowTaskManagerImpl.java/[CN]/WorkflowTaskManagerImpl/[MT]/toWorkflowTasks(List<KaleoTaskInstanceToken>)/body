{
  List<WorkflowTask> workflowTasks=new ArrayList<WorkflowTask>(kaleoTaskInstanceTokens.size());
  for (  KaleoTaskInstanceToken kaleoTaskInstanceToken : kaleoTaskInstanceTokens) {
    WorkflowTask workflowTask=new WorkflowTaskAdapter(kaleoTaskInstanceToken,WorkflowContextUtil.convertTo(kaleoTaskInstanceToken.getWorkflowContext()));
    workflowTasks.add(workflowTask);
  }
  return workflowTasks;
}
