{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=KaleoTaskInstanceTokenLocalServiceUtil.getKaleoTaskInstanceTokens(workflowInstanceId,completed,start,end,orderByComparator,serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
