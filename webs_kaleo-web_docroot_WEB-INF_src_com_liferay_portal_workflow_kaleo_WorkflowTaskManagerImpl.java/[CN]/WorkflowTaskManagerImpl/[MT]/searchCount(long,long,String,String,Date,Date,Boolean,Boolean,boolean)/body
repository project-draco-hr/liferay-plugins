{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    serviceContext.setUserId(userId);
    return KaleoTaskInstanceTokenLocalServiceUtil.searchCount(taskName,assetType,dueDateGT,dueDateLT,completed,searchByUserRoles,andOperator,serviceContext);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
