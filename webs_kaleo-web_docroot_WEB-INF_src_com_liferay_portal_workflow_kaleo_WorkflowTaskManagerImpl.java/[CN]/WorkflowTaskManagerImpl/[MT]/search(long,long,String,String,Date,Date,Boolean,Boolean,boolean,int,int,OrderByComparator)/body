{
  try {
    ServiceContext serviceContext=new ServiceContext();
    serviceContext.setCompanyId(companyId);
    serviceContext.setUserId(userId);
    List<KaleoTaskInstanceToken> kaleoTaskInstanceTokens=KaleoTaskInstanceTokenLocalServiceUtil.search(taskName,assetType,dueDateGT,dueDateLT,completed,searchByUserRoles,andOperator,start,end,orderByComparator,serviceContext);
    return toWorkflowTasks(kaleoTaskInstanceTokens);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
