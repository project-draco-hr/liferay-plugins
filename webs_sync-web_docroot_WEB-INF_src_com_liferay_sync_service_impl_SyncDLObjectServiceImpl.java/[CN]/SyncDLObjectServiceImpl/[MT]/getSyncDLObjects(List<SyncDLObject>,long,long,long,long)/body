{
  List<SyncDLObject> curSyncDLObjects=syncDLObjectFinder.filterFindByC_M_R_P(companyId,lastAccessTime,repositoryId,parentFolderId);
  syncDLObjects.addAll(curSyncDLObjects);
  for (  SyncDLObject syncDLObject : curSyncDLObjects) {
    String type=syncDLObject.getType();
    if (type.equals("file")) {
      continue;
    }
    getSyncDLObjects(syncDLObjects,companyId,repositoryId,syncDLObject.getTypePK(),lastAccessTime);
  }
  return syncDLObjects;
}
