{
  ClassLoader portalClassLoader=PortalClassLoaderUtil.getClassLoader();
  Class<LicenseManager> licenseManagerClass=(Class<LicenseManager>)portalClassLoader.loadClass(LicenseManager.class.getName());
  Method method=ReflectionUtil.getDeclaredMethod(licenseManagerClass,"_digest",String.class,String.class,int.class);
  method.setAccessible(true);
  return (String)method.invoke(null,productId,uuid,licenseState);
}
