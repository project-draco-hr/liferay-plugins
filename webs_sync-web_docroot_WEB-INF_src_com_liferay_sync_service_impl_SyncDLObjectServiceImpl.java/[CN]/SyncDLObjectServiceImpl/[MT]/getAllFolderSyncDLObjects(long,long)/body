{
  try {
    SyncUtil.checkSyncEnabled(repositoryId);
    repositoryService.checkRepository(repositoryId);
    List<SyncDLObject> syncDLObjects=syncDLObjectPersistence.findByC_R_T(companyId,repositoryId,SyncConstants.TYPE_FOLDER);
    SyncDLObjectUpdate syncDLObjectUpdate=checkSyncDLObjects(syncDLObjects,repositoryId);
    return syncDLObjectUpdate.getSyncDLObjects();
  }
 catch (  PortalException pe) {
    throw new PortalException(SyncUtil.buildExceptionMessage(pe),pe);
  }
}
