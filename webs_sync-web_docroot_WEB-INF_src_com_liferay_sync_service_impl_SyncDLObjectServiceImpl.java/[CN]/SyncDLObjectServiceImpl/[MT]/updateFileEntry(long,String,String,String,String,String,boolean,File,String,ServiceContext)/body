{
  try {
    if (file != null) {
      validateChecksum(file,checksum);
    }
    FileEntry fileEntry=dlAppService.updateFileEntry(fileEntryId,sourceFileName,mimeType,title,description,changeLog,majorVersion,file,serviceContext);
    return SyncUtil.toSyncDLObject(fileEntry,SyncConstants.EVENT_UPDATE);
  }
 catch (  PortalException pe) {
    throw new PortalException(pe.getClass().getName(),pe);
  }
}
