{
  String type=syncDLObject.getType();
  if ((!type.equals(SyncConstants.TYPE_FILE) || !hasFileModelPermission) && (!type.equals(SyncConstants.TYPE_FOLDER) || !hasFolderModelPermission) && !checkedTypePKs.contains(syncDLObject.getTypePK())) {
    return false;
  }
  if (!hasFolderModelPermission && _PERMISSIONS_VIEW_DYNAMIC_INHERITANCE) {
    long[] parentFolderIds=StringUtil.split(syncDLObject.getTreePath(),StringPool.SLASH,0L);
    for (    long parentFolderId : parentFolderIds) {
      if ((parentFolderId > 0) && !checkedTypePKs.contains(parentFolderId)) {
        return false;
      }
    }
  }
  return true;
}
