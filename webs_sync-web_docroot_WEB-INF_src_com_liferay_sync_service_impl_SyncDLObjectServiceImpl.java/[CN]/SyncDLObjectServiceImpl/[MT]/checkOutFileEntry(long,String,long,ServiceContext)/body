{
  try {
    FileEntry fileEntry=dlAppService.checkOutFileEntry(fileEntryId,owner,expirationTime,serviceContext);
    return SyncUtil.toSyncDLObject(fileEntry,SyncConstants.EVENT_CHECK_OUT);
  }
 catch (  PortalException pe) {
    throw new PortalException(SyncUtil.buildExceptionMessage(pe),pe);
  }
}
