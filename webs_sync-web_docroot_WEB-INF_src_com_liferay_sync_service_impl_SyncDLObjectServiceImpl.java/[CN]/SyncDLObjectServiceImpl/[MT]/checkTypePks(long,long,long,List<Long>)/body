{
  if (typePKs.size() <= _SQL_DATA_MAX_PARAMETERS) {
    return syncDLObjectFinder.filterFindByC_R_U_T(companyId,repositoryId,userId,ArrayUtil.toLongArray(typePKs));
  }
 else {
    List<Long> sublist=typePKs.subList(0,_SQL_DATA_MAX_PARAMETERS);
    List<Long> checkedTypePKs=syncDLObjectFinder.filterFindByC_R_U_T(companyId,repositoryId,userId,ArrayUtil.toLongArray(sublist));
    checkedTypePKs.addAll(checkTypePks(companyId,repositoryId,userId,typePKs));
    return checkedTypePKs;
  }
}
