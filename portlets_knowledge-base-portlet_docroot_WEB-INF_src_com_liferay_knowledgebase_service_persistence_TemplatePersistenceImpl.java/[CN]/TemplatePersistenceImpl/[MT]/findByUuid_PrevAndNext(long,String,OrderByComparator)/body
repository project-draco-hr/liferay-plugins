{
  Template template=findByPrimaryKey(templateId);
  Session session=null;
  try {
    session=openSession();
    Template[] array=new TemplateImpl[3];
    array[0]=getByUuid_PrevAndNext(session,template,uuid,orderByComparator,true);
    array[1]=template;
    array[2]=getByUuid_PrevAndNext(session,template,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
