{
  Session session=null;
  try {
    session=openSession();
    Template template=(Template)session.get(TemplateImpl.class,Long.valueOf(templateId));
    if (template == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + templateId);
      }
      throw new NoSuchTemplateException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + templateId);
    }
    return templatePersistence.remove(template);
  }
 catch (  NoSuchTemplateException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
