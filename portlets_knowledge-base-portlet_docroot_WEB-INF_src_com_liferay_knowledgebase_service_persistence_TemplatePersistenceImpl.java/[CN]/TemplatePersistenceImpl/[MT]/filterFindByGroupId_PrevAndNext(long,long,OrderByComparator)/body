{
  if (!InlineSQLHelperUtil.isEnabled(groupId)) {
    return findByGroupId_PrevAndNext(templateId,groupId,orderByComparator);
  }
  Template template=findByPrimaryKey(templateId);
  Session session=null;
  try {
    session=openSession();
    Template[] array=new TemplateImpl[3];
    array[0]=filterGetByGroupId_PrevAndNext(session,template,groupId,orderByComparator,true);
    array[1]=template;
    array[2]=filterGetByGroupId_PrevAndNext(session,template,groupId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
