{
  template=toUnwrappedModel(template);
  Session session=null;
  try {
    session=openSession();
    if (template.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(TemplateImpl.class,template.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(template);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  TemplateModelImpl templateModelImpl=(TemplateModelImpl)template;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{templateModelImpl.getOriginalUuid(),new Long(templateModelImpl.getOriginalGroupId())});
  EntityCacheUtil.removeResult(TemplateModelImpl.ENTITY_CACHE_ENABLED,TemplateImpl.class,template.getPrimaryKey());
  return template;
}
