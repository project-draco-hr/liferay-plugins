{
  if (_persistence == null) {
    _persistence=(KaleoActionPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoActionPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoActionUtil.class,"_persistence");
  }
  return _persistence;
}
