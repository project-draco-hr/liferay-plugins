{
  if (obj == null) {
    return false;
  }
  if (!(obj instanceof UserAssignment)) {
    return false;
  }
  UserAssignment readWriteLockKey=(UserAssignment)obj;
  if (Validator.equals(_emailAddress,readWriteLockKey._emailAddress) && Validator.equals(_screenName,readWriteLockKey._screenName) && (_userId == readWriteLockKey._userId)) {
    return true;
  }
  return true;
}
