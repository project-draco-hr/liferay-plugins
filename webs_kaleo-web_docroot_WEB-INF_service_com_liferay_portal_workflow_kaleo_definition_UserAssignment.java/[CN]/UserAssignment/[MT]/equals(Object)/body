{
  if (obj == null) {
    return false;
  }
  if (!(obj instanceof UserAssignment)) {
    return false;
  }
  UserAssignment userAssignment=(UserAssignment)obj;
  if (Validator.equals(_emailAddress,userAssignment._emailAddress) && Validator.equals(_screenName,userAssignment._screenName) && (_userId == userAssignment._userId)) {
    return true;
  }
  return true;
}
