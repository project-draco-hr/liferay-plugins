{
  HREmploymentType hrEmploymentType=(HREmploymentType)EntityCacheUtil.getResult(HREmploymentTypeModelImpl.ENTITY_CACHE_ENABLED,HREmploymentTypeImpl.class,hrEmploymentTypeId,this);
  if (hrEmploymentType == null) {
    Session session=null;
    try {
      session=openSession();
      hrEmploymentType=(HREmploymentType)session.get(HREmploymentTypeImpl.class,Long.valueOf(hrEmploymentTypeId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrEmploymentType != null) {
        cacheResult(hrEmploymentType);
      }
      closeSession(session);
    }
  }
  return hrEmploymentType;
}
