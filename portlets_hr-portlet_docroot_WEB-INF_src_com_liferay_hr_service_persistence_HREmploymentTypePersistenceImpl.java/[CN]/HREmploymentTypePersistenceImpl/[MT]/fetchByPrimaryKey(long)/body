{
  HREmploymentType hrEmploymentType=(HREmploymentType)EntityCacheUtil.getResult(HREmploymentTypeModelImpl.ENTITY_CACHE_ENABLED,HREmploymentTypeImpl.class,hrEmploymentTypeId);
  if (hrEmploymentType == _nullHREmploymentType) {
    return null;
  }
  if (hrEmploymentType == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrEmploymentType=(HREmploymentType)session.get(HREmploymentTypeImpl.class,Long.valueOf(hrEmploymentTypeId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrEmploymentType != null) {
        cacheResult(hrEmploymentType);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HREmploymentTypeModelImpl.ENTITY_CACHE_ENABLED,HREmploymentTypeImpl.class,hrEmploymentTypeId,_nullHREmploymentType);
      }
      closeSession(session);
    }
  }
  return hrEmploymentType;
}
