{
  KaleoTimerInstanceToken kaleoTimerInstanceToken=fetchByPrimaryKey(kaleoTimerInstanceTokenId);
  if (kaleoTimerInstanceToken == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTimerInstanceTokenId);
    }
    throw new NoSuchTimerInstanceTokenException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTimerInstanceTokenId);
  }
  return kaleoTimerInstanceToken;
}
