{
  if (serviceContext == null) {
    return StringPool.BLANK;
  }
  ThemeDisplay themeDisplay=serviceContext.getThemeDisplay();
  if (themeDisplay == null) {
    return StringPool.BLANK;
  }
  PortletURL portletURL=PortletURLFactoryUtil.create(serviceContext.getRequest(),PortletKeys.CALENDAR,themeDisplay.getPlid(),ActionRequest.RENDER_PHASE);
  portletURL.setParameter("mvcPath","/view_calendar_booking.jsp");
  portletURL.setParameter("calendarBookingId",String.valueOf(calendarBookingId));
  return portletURL.toString();
}
