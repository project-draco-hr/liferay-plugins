{
  String content=getString(fileName);
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  Map<Locale,String> titleMap=new HashMap<Locale,String>();
  titleMap.put(Locale.US,title);
  JournalArticle journalArticle=JournalArticleLocalServiceUtil.addArticle(userId,groupId,0,0,StringPool.BLANK,true,JournalArticleConstants.VERSION_DEFAULT,titleMap,null,content,"general",structureId,templateId,StringPool.BLANK,1,1,2008,0,0,0,0,0,0,0,true,0,0,0,0,0,true,true,false,StringPool.BLANK,null,null,StringPool.BLANK,serviceContext);
  JournalArticleLocalServiceUtil.updateStatus(userId,groupId,journalArticle.getArticleId(),journalArticle.getVersion(),WorkflowConstants.STATUS_APPROVED,StringPool.BLANK,serviceContext);
  return journalArticle;
}
