{
  PortletPreferences portletSetup=PortletPreferencesFactoryUtil.getLayoutPortletSetup(layout,portletId);
  for (  Map.Entry<String,String> preferenceEntry : preferences.entrySet()) {
    String key=preferenceEntry.getKey();
    String value=preferenceEntry.getValue();
    portletSetup.setValue(key,value);
  }
  portletSetup.store();
  return portletSetup;
}
