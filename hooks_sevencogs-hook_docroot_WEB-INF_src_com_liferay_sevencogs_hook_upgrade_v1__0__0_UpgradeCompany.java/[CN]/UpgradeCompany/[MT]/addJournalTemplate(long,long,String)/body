{
  Map<Locale,String> descriptionMap=new HashMap<Locale,String>();
  descriptionMap.put(getLocale(),"A single image, optional URL");
  Map<Locale,String> nameMap=new HashMap<Locale,String>();
  nameMap.put(getLocale(),"Single Image");
  String xsl=getString(fileName);
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  return JournalTemplateLocalServiceUtil.addTemplate(userId,groupId,"SINGLE-IMAGE",false,"SINGLE-IMAGE",nameMap,descriptionMap,xsl,true,"vm",true,false,StringPool.BLANK,null,serviceContext);
}
