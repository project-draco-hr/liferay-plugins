{
  Map<Locale,String> nameMap=new HashMap<Locale,String>();
  setLocalizedValue(nameMap,"Single Image");
  Map<Locale,String> descriptionMap=new HashMap<Locale,String>();
  setLocalizedValue(descriptionMap,"A single image, optional link");
  String xsd=getString(fileName);
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddGroupPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  return JournalStructureLocalServiceUtil.addStructure(userId,groupId,"SINGLE-IMAGE",false,StringPool.BLANK,nameMap,descriptionMap,xsd,serviceContext);
}
