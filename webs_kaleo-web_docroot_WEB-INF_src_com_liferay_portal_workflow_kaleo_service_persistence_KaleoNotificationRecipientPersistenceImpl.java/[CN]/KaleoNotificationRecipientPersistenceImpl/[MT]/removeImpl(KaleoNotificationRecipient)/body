{
  kaleoNotificationRecipient=toUnwrappedModel(kaleoNotificationRecipient);
  Session session=null;
  try {
    session=openSession();
    if (kaleoNotificationRecipient.isCachedModel()) {
      kaleoNotificationRecipient=(KaleoNotificationRecipient)session.get(KaleoNotificationRecipientImpl.class,kaleoNotificationRecipient.getPrimaryKeyObj());
    }
    session.delete(kaleoNotificationRecipient);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(kaleoNotificationRecipient);
  return kaleoNotificationRecipient;
}
