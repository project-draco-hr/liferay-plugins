{
  kaleoNotificationRecipient=toUnwrappedModel(kaleoNotificationRecipient);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,kaleoNotificationRecipient,merge);
    kaleoNotificationRecipient.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(KaleoNotificationRecipientModelImpl.ENTITY_CACHE_ENABLED,KaleoNotificationRecipientImpl.class,kaleoNotificationRecipient.getPrimaryKey(),kaleoNotificationRecipient);
  return kaleoNotificationRecipient;
}
