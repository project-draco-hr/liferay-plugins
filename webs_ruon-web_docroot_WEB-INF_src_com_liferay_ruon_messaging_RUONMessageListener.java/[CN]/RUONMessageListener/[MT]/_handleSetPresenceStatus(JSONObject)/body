{
  Long requestUserId=setPresenceStatusRequestJSON.getLong("userId");
  String status=setPresenceStatusRequestJSON.getString("status");
  if (status.equalsIgnoreCase("online")) {
    UserPresenceLocalServiceUtil.setPresenceStatusOfUser(requestUserId,PresenceStatusConstants.STATUS_ONLINE);
    return "online";
  }
 else {
    UserPresenceLocalServiceUtil.setPresenceStatusOfUser(requestUserId,PresenceStatusConstants.STATUS_OFFLINE);
    return "offline";
  }
}
