{
  try {
    JRPdfExporter exporter=new JRPdfExporter();
    exporter.setParameter(JRExporterParameter.JASPER_PRINT,report);
    exporter.setParameter(JRExporterParameter.OUTPUT_STREAM,container.getOutputStream());
    exporter.exportReport();
  }
 catch (  Exception e) {
    throw new ReportExportException("Unable to export report in HTML format",e);
  }
}
