{
  HRAssetType hrAssetType=(HRAssetType)EntityCacheUtil.getResult(HRAssetTypeModelImpl.ENTITY_CACHE_ENABLED,HRAssetTypeImpl.class,hrAssetTypeId);
  if (hrAssetType == _nullHRAssetType) {
    return null;
  }
  if (hrAssetType == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrAssetType=(HRAssetType)session.get(HRAssetTypeImpl.class,Long.valueOf(hrAssetTypeId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrAssetType != null) {
        cacheResult(hrAssetType);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRAssetTypeModelImpl.ENTITY_CACHE_ENABLED,HRAssetTypeImpl.class,hrAssetTypeId,_nullHRAssetType);
      }
      closeSession(session);
    }
  }
  return hrAssetType;
}
