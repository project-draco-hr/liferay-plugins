{
  Session session=null;
  try {
    session=openSession();
    session.delete(ipInfo);
    session.flush();
    return ipInfo;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(IPInfo.class.getName());
  }
}
