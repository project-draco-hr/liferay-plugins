{
  JSONObject jsonObject=populateJSONObject(notificationMessage,executionContext);
  for (  NotificationRecipient notificationRecipient : notificationRecipients) {
    if (notificationRecipient.getUserId() > 0) {
      UserNotificationEventLocalServiceUtil.sendUserNotificationEvents(notificationRecipient.getUserId(),PortletKeys.MY_WORKFLOW_TASKS,UserNotificationDeliveryConstants.TYPE_WEBSITE,jsonObject);
    }
  }
}
