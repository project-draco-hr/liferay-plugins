def showPortletDetails(self, portletConfig, req):
    return (((((("\n        <table class='liferay-table'>\n        <tr>\n            <td>Portlet Name:</td>\n            <td> " + portletConfig.getPortletName()) + '</td>\n        </tr>\n        <tr>\n            <td>Preferences:</td>\n            <td>') + req.getPreferences().getMap()) + '</td>\n        </tr>\n        <tr>\n            <td>Parameters:</td>\n            <td>') + req.getParameterMap()) + '</td>\n        </tr>\n        </table>\n    ')
