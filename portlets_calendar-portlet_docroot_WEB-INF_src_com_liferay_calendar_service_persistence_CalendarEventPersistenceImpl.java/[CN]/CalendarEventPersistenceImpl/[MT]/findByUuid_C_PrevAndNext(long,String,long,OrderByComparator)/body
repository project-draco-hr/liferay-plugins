{
  CalendarEvent calendarEvent=findByPrimaryKey(calendarEventId);
  Session session=null;
  try {
    session=openSession();
    CalendarEvent[] array=new CalendarEventImpl[3];
    array[0]=getByUuid_C_PrevAndNext(session,calendarEvent,uuid,companyId,orderByComparator,true);
    array[1]=calendarEvent;
    array[2]=getByUuid_C_PrevAndNext(session,calendarEvent,uuid,companyId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
