{
  Session session=null;
  try {
    session=openSession();
    CalendarEvent calendarEvent=(CalendarEvent)session.get(CalendarEventImpl.class,Long.valueOf(calendarEventId));
    if (calendarEvent == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + calendarEventId);
      }
      throw new NoSuchEventException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + calendarEventId);
    }
    return calendarEventPersistence.remove(calendarEvent);
  }
 catch (  NoSuchEventException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
