{
  if (obj == null) {
    return false;
  }
  JIRAIssueImpl jiraIssue=null;
  try {
    jiraIssue=(JIRAIssueImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=jiraIssue.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
