{
  viewableParentResourcePrimKeys=getViewableParentResourcePrimKeys(groupId,status,viewableParentResourcePrimKeys);
  if (status == WorkflowConstants.STATUS_ANY) {
    return articlePersistence.filterCountByG_P_L(groupId,viewableParentResourcePrimKeys,new int[]{ArticleConstants.LATEST_VERSION});
  }
  return articlePersistence.filterCountByG_P_L_S(groupId,viewableParentResourcePrimKeys,ArticleConstants.LATEST_ANY,status);
}
