{
  ArticlePermission.check(getPermissionChecker(),resourcePrimKey,ActionKeys.VIEW);
  Article article=articleLocalService.getLatestArticle(resourcePrimKey);
  ListTree<Article> tree=new ListTree<Article>(article);
  populateLatestArticleTree(Arrays.asList(tree.getRootNode()),article.getGroupId(),new long[]{resourcePrimKey});
  return tree;
}
