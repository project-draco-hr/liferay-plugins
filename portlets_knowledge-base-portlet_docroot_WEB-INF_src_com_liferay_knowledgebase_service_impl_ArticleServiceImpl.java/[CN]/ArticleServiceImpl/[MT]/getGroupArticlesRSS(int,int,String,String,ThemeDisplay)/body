{
  Group group=themeDisplay.getScopeGroup();
  String descriptiveName=HtmlUtil.escape(group.getDescriptiveName());
  String name=descriptiveName;
  String description=descriptiveName;
  String feedURL=PortalUtil.getLayoutFullURL(themeDisplay);
  List<Article> articles=getRSSArticles(group.getGroupId(),ArticleConstants.DEFAULT_PARENT_RESOURCE_PRIM_KEY,status,rssDelta);
  return exportToRSS(rssDisplayStyle,rssFormat,name,description,feedURL,articles,themeDisplay);
}
