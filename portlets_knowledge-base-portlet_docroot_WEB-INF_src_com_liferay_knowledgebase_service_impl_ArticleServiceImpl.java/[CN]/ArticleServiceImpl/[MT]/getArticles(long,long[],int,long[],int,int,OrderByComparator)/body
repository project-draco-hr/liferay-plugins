{
  viewableParentResourcePrimKeys=getViewableParentResourcePrimKeys(groupId,status,viewableParentResourcePrimKeys);
  if (status == WorkflowConstants.STATUS_ANY) {
    return articlePersistence.filterFindByR_G_P_L(resourcePrimKeys,groupId,viewableParentResourcePrimKeys,new int[]{ArticleConstants.LATEST_VERSION},start,end,orderByComparator);
  }
  return articlePersistence.filterFindByR_G_P_L_S(resourcePrimKeys,groupId,viewableParentResourcePrimKeys,ArticleConstants.LATEST_ANY,status,start,end,orderByComparator);
}
