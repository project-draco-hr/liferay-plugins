{
  if (resourcePrimKey <= 0) {
    AdminPermission.check(getPermissionChecker(),groupId,ActionKeys.ADD_ARTICLE);
  }
 else {
    ArticlePermission.check(getPermissionChecker(),resourcePrimKey,ActionKeys.UPDATE);
  }
  articleLocalService.addAttachment(companyId,dirName,shortFileName,inputStream);
}
