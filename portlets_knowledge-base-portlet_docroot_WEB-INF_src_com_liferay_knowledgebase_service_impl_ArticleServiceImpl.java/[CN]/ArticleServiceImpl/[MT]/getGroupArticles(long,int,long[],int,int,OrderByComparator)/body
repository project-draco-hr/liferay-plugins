{
  viewableParentResourcePrimKeys=getViewableParentResourcePrimKeys(groupId,status,viewableParentResourcePrimKeys);
  if (status == WorkflowConstants.STATUS_ANY) {
    return articlePersistence.filterFindByG_P_L(groupId,viewableParentResourcePrimKeys,new int[]{ArticleConstants.LATEST_VERSION},start,end,orderByComparator);
  }
  return articlePersistence.filterFindByG_P_L_S(groupId,viewableParentResourcePrimKeys,ArticleConstants.LATEST_ANY,status,start,end,orderByComparator);
}
