{
  if (status == WorkflowConstants.STATUS_ANY) {
    return articlePersistence.filterFindByG_L(groupId,true,start,end,orderByComparator);
  }
 else   if (status == WorkflowConstants.STATUS_APPROVED) {
    return articlePersistence.filterFindByG_M(groupId,true,start,end,orderByComparator);
  }
  return articlePersistence.filterFindByG_S(groupId,status,start,end,orderByComparator);
}
