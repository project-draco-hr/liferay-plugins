{
  if (status == WorkflowConstants.STATUS_ANY) {
    return articlePersistence.filterFindByG_L(groupId,new int[]{ArticleConstants.LATEST_VERSION},start,end,orderByComparator);
  }
  return articlePersistence.filterFindByG_L_S(groupId,ArticleConstants.LATEST_ANY,status,start,end,orderByComparator);
}
