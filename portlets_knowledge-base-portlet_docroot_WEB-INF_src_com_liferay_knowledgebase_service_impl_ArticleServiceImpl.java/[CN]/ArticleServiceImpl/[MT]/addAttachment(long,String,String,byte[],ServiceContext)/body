{
  if (resourcePrimKey <= 0) {
    AdminPermission.check(getPermissionChecker(),serviceContext.getScopeGroupId(),ActionKeys.ADD_ARTICLE);
  }
 else {
    ArticlePermission.check(getPermissionChecker(),resourcePrimKey,ActionKeys.UPDATE);
  }
  articleLocalService.addAttachment(dirName,shortFileName,bytes,serviceContext);
}
