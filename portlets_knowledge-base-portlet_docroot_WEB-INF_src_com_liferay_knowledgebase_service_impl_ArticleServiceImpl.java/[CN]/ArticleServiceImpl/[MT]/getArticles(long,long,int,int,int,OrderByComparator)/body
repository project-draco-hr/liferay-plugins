{
  if (status == WorkflowConstants.STATUS_ANY) {
    return articlePersistence.filterFindByG_R_L(groupId,resourcePrimKey,ArticleConstants.LATEST_VERSION,start,end,orderByComparator);
  }
  return articlePersistence.filterFindByG_R_L_S(groupId,new long[]{resourcePrimKey},ArticleConstants.LATEST_ANY,status,start,end,orderByComparator);
}
