{
  if (obj == null) {
    return false;
  }
  HRProject hrProject=null;
  try {
    hrProject=(HRProject)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrProject.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
