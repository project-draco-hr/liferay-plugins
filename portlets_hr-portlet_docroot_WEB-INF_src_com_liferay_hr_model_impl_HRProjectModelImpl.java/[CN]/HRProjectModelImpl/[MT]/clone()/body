{
  HRProjectImpl hrProjectImpl=new HRProjectImpl();
  hrProjectImpl.setHrProjectId(getHrProjectId());
  hrProjectImpl.setGroupId(getGroupId());
  hrProjectImpl.setCompanyId(getCompanyId());
  hrProjectImpl.setUserId(getUserId());
  hrProjectImpl.setUserName(getUserName());
  hrProjectImpl.setCreateDate(getCreateDate());
  hrProjectImpl.setModifiedDate(getModifiedDate());
  hrProjectImpl.setHrClientId(getHrClientId());
  hrProjectImpl.setHrProjectStatusId(getHrProjectStatusId());
  hrProjectImpl.setName(getName());
  hrProjectImpl.setDescription(getDescription());
  hrProjectImpl.setEstimatedStartDate(getEstimatedStartDate());
  hrProjectImpl.setEstimatedEndDate(getEstimatedEndDate());
  hrProjectImpl.setEstimatedHours(getEstimatedHours());
  hrProjectImpl.setEstimatedHoursCost(getEstimatedHoursCost());
  hrProjectImpl.setEstimatedHoursCostCurrencyCode(getEstimatedHoursCostCurrencyCode());
  hrProjectImpl.setEstimatedExpenses(getEstimatedExpenses());
  hrProjectImpl.setEstimatedExpensesCurrencyCode(getEstimatedExpensesCurrencyCode());
  hrProjectImpl.setActualStartDate(getActualStartDate());
  hrProjectImpl.setActualEndDate(getActualEndDate());
  hrProjectImpl.setActualHours(getActualHours());
  hrProjectImpl.setActualHoursCost(getActualHoursCost());
  hrProjectImpl.setActualHoursCostCurrencyCode(getActualHoursCostCurrencyCode());
  hrProjectImpl.setActualExpenses(getActualExpenses());
  hrProjectImpl.setActualExpensesCurrencyCode(getActualExpensesCurrencyCode());
  hrProjectImpl.resetOriginalValues();
  return hrProjectImpl;
}
