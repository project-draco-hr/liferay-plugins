{
  if (obj == null) {
    return false;
  }
  AttachmentClp attachment=null;
  try {
    attachment=(AttachmentClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=attachment.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
