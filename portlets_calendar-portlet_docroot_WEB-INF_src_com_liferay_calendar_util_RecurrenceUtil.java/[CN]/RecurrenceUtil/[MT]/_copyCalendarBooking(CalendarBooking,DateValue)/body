{
  CalendarBooking newCalendarBooking=(CalendarBooking)calendarBooking.clone();
  Calendar jCalendar=JCalendarUtil.getJCalendar(calendarBooking.getStartDate());
  jCalendar=JCalendarUtil.getJCalendar(startDateValue.year(),startDateValue.month() - 1,startDateValue.day(),jCalendar.get(Calendar.HOUR_OF_DAY),jCalendar.get(Calendar.MINUTE),jCalendar.get(Calendar.SECOND),jCalendar.get(Calendar.MILLISECOND),TimeZone.getTimeZone(StringPool.UTC));
  newCalendarBooking.setEndDate(jCalendar.getTimeInMillis() + calendarBooking.getDuration());
  newCalendarBooking.setStartDate(jCalendar.getTimeInMillis());
  return newCalendarBooking;
}
