{
  List<CalendarBooking> expandedCalendarBookings=new ArrayList<CalendarBooking>();
  for (  CalendarBooking calendarBooking : calendarBookings) {
    List<CalendarBooking> expandedCalendarBooking=expandCalendarBooking(calendarBooking,startDate,endDate,maxSize);
    expandedCalendarBookings.addAll(expandedCalendarBooking);
  }
  return expandedCalendarBookings;
}
