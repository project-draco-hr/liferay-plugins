{
  List<WorkflowTaskAssignee> workflowTaskAssignees=new ArrayList<WorkflowTaskAssignee>(assignees.size());
  for (  Assignee assignee : assignees) {
    WorkflowTaskAssignee workflowTaskAssignee=new WorkflowTaskAssignee(assignee.getAssigneeClassName(),assignee.getAssigneeClassPK());
    workflowTaskAssignees.add(workflowTaskAssignee);
  }
  return workflowTaskAssignees;
}
