{
  List<WorkflowTaskAssignee> workflowTaskAssignees=new ArrayList<WorkflowTaskAssignee>(assignees.size());
  for (  Assignee assignee : assignees) {
    workflowTaskAssignees.add(new WorkflowTaskAssignee(assignee.getAssigneeClassName(),assignee.getAssigneeClassPK()));
  }
  return workflowTaskAssignees;
}
