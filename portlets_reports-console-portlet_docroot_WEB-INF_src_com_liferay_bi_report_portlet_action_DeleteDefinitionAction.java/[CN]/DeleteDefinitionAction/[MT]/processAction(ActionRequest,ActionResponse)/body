{
  long definitionId=ParamUtil.getLong(actionRequest,"definitionId");
  if (definitionId == -1) {
    return false;
  }
  try {
    ReportDefinitionLocalServiceUtil.deleteReportDefinition(definitionId);
    return true;
  }
 catch (  Exception e) {
    _log.error(e);
    throw new PortletException("Unable to delete new definition",e);
  }
}
