{
  if (_persistence == null) {
    _persistence=(KaleoTaskFormPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoTaskFormPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoTaskFormUtil.class,"_persistence");
  }
  return _persistence;
}
