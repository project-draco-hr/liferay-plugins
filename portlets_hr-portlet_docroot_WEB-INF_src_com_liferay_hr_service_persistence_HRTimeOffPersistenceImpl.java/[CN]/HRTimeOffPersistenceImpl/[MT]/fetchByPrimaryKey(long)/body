{
  HRTimeOff hrTimeOff=(HRTimeOff)EntityCacheUtil.getResult(HRTimeOffModelImpl.ENTITY_CACHE_ENABLED,HRTimeOffImpl.class,hrTimeOffId);
  if (hrTimeOff == _nullHRTimeOff) {
    return null;
  }
  if (hrTimeOff == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrTimeOff=(HRTimeOff)session.get(HRTimeOffImpl.class,Long.valueOf(hrTimeOffId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrTimeOff != null) {
        cacheResult(hrTimeOff);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRTimeOffModelImpl.ENTITY_CACHE_ENABLED,HRTimeOffImpl.class,hrTimeOffId,_nullHRTimeOff);
      }
      closeSession(session);
    }
  }
  return hrTimeOff;
}
