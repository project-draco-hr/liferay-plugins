{
  Session session=null;
  try {
    session=openSession();
    HRTimeOff hrTimeOff=(HRTimeOff)session.get(HRTimeOffImpl.class,Long.valueOf(hrTimeOffId));
    if (hrTimeOff == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeOffId);
      }
      throw new NoSuchTimeOffException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeOffId);
    }
    return hrTimeOffPersistence.remove(hrTimeOff);
  }
 catch (  NoSuchTimeOffException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
