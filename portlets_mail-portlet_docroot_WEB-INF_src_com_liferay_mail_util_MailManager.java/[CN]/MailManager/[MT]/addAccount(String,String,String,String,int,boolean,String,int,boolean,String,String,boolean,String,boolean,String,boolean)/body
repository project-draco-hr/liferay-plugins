{
  try {
    Mailbox mailbox=MailboxFactoryUtil.getMailbox(_user.getUserId(),protocol);
    mailbox.addAccount(address,personalName,protocol,incomingHostName,incomingPort,incomingSecure,outgoingHostName,outgoingPort,outgoingSecure,login,unencryptedPassword,savePassword,signature,useSignature,folderPrefix,defaultSender);
    return createJSONResult("success","account-has-been-created");
  }
 catch (  MailException me) {
    _log.error(me,me);
    return createJSONResult("failure","unable-to-add-account");
  }
}
