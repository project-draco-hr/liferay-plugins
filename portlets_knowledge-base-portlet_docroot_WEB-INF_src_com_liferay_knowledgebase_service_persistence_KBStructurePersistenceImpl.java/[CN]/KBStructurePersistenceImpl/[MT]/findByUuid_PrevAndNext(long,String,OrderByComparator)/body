{
  KBStructure kbStructure=findByPrimaryKey(kbStructureId);
  Session session=null;
  try {
    session=openSession();
    KBStructure[] array=new KBStructureImpl[3];
    array[0]=getByUuid_PrevAndNext(session,kbStructure,uuid,orderByComparator,true);
    array[1]=kbStructure;
    array[2]=getByUuid_PrevAndNext(session,kbStructure,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
