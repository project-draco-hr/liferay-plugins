{
  hrTimeOffType=toUnwrappedModel(hrTimeOffType);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,hrTimeOffType,merge);
    hrTimeOffType.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(HRTimeOffTypeModelImpl.ENTITY_CACHE_ENABLED,HRTimeOffTypeImpl.class,hrTimeOffType.getPrimaryKey(),hrTimeOffType);
  return hrTimeOffType;
}
