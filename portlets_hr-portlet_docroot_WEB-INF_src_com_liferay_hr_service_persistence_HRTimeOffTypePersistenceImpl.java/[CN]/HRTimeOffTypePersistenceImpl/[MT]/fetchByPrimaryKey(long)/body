{
  HRTimeOffType hrTimeOffType=(HRTimeOffType)EntityCacheUtil.getResult(HRTimeOffTypeModelImpl.ENTITY_CACHE_ENABLED,HRTimeOffTypeImpl.class,hrTimeOffTypeId,this);
  if (hrTimeOffType == null) {
    Session session=null;
    try {
      session=openSession();
      hrTimeOffType=(HRTimeOffType)session.get(HRTimeOffTypeImpl.class,Long.valueOf(hrTimeOffTypeId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrTimeOffType != null) {
        cacheResult(hrTimeOffType);
      }
      closeSession(session);
    }
  }
  return hrTimeOffType;
}
