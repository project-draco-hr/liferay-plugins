{
  if (_persistence == null) {
    _persistence=(HRExpenseAccountPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRExpenseAccountPersistence.class.getName());
    ReferenceRegistry.registerReference(HRExpenseAccountUtil.class,"_persistence");
  }
  return _persistence;
}
