{
  if (_persistence == null) {
    _persistence=(HRBranchPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRBranchPersistence.class.getName());
    ReferenceRegistry.registerReference(HRBranchUtil.class,"_persistence");
  }
  return _persistence;
}
