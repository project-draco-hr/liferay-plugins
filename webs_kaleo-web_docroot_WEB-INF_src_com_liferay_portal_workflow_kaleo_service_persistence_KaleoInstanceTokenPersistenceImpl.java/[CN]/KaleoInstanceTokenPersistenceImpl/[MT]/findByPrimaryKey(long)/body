{
  KaleoInstanceToken kaleoInstanceToken=fetchByPrimaryKey(kaleoInstanceTokenId);
  if (kaleoInstanceToken == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoInstanceTokenId);
    }
    throw new NoSuchInstanceTokenException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoInstanceTokenId);
  }
  return kaleoInstanceToken;
}
