{
  for (  Layout layout : layouts) {
    LayoutTypePortlet layoutTypePortlet=(LayoutTypePortlet)layout.getLayoutType();
    String articleURL=getArticleURL(layout.getGroupId(),layout.getPlid(),resourcePrimKey,portalURL,layoutTypePortlet);
    if (Validator.isNotNull(articleURL)) {
      return articleURL;
    }
  }
  return null;
}
