{
  long id=0;
  try {
    id=((WorkflowInstanceImpl)entity).getPrimaryKey();
    WorkflowInstanceUtil.remove(id);
  }
 catch (  NoSuchWorkflowInstanceException nswie) {
    throw new ProcessException("Could not delete workflow instance with id " + id,nswie);
  }
catch (  SystemException se) {
    throw new ProcessException("Could not delete workflow instance with id " + id,se);
  }
}
