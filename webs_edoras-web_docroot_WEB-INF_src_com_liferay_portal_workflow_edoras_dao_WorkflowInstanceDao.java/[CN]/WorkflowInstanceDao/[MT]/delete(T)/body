{
  long primaryKey=0;
  try {
    WorkflowInstanceBridge workflowInstanceBridge=(WorkflowInstanceBridge)workflowEntity;
    primaryKey=workflowInstanceBridge.getPrimaryKey();
    WorkflowInstanceUtil.remove(primaryKey);
  }
 catch (  NoSuchWorkflowInstanceException nswie) {
    throw new ProcessException("Could not delete workflow instance with id " + primaryKey,nswie);
  }
catch (  SystemException se) {
    throw new ProcessException("Could not delete workflow instance with id " + primaryKey,se);
  }
}
