{
  AbstractServiceBuilderObjectIdentity sbObjectIdentity=(AbstractServiceBuilderObjectIdentity)objectIdentity;
  try {
    BasePersistence<?> basePersistence=PortalUtil.getBasePersistence(sbObjectIdentity.getServletContextName(),sbObjectIdentity.getTypeName());
    BaseModel<?> baseModel=basePersistence.findByPrimaryKey(sbObjectIdentity.getPrimaryKey());
    return (T)baseModel;
  }
 catch (  NoSuchModelException nsme) {
    if (_log.isWarnEnabled()) {
      _log.warn("Could not find referenced attribute",nsme);
    }
    return null;
  }
catch (  Exception e) {
    throw new ProcessException(e.getMessage(),e);
  }
}
