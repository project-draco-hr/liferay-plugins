{
  try {
    List<WorkflowInstance> workflowInstances=WorkflowInstanceUtil.findByFinished(false);
    return (List<? extends MutableProcessInstance>)WorkflowEntityBridgeUtil.wrapWorkflowInstances(workflowInstances,null,true);
  }
 catch (  Exception e) {
    throw new ProcessException(e.getMessage(),e);
  }
}
