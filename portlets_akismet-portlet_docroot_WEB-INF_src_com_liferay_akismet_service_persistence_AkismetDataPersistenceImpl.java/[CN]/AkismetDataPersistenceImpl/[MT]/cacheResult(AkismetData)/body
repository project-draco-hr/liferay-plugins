{
  EntityCacheUtil.putResult(AkismetDataModelImpl.ENTITY_CACHE_ENABLED,AkismetDataImpl.class,akismetData.getPrimaryKey(),akismetData);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_MBMESSAGEID,new Object[]{Long.valueOf(akismetData.getMbMessageId())},akismetData);
  akismetData.resetOriginalValues();
}
