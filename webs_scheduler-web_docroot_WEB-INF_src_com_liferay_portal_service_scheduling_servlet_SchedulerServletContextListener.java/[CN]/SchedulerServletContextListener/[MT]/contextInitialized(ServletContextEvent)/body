{
  try {
    ApplicationContext ctx=new ClassPathXmlApplicationContext(event.getServletContext().getInitParameter("springConfigLocation"));
    SchedulingEngine engine=(SchedulingEngine)ctx.getBean(SchedulingEngine.class.getName());
    SchedulerUtil.init(engine);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
