{
  if (obj == null) {
    return false;
  }
  HRProjectClp hrProject=null;
  try {
    hrProject=(HRProjectClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrProject.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
