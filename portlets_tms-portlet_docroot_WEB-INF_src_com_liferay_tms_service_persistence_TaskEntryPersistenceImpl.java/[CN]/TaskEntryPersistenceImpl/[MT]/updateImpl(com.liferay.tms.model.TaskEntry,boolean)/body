{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,taskEntry,merge);
    taskEntry.setNew(false);
    return taskEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(TaskEntry.class.getName());
  }
}
