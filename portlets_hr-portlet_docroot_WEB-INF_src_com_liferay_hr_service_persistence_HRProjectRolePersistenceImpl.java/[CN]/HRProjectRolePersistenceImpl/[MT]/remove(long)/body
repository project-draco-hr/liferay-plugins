{
  Session session=null;
  try {
    session=openSession();
    HRProjectRole hrProjectRole=(HRProjectRole)session.get(HRProjectRoleImpl.class,Long.valueOf(hrProjectRoleId));
    if (hrProjectRole == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrProjectRoleId);
      }
      throw new NoSuchProjectRoleException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrProjectRoleId);
    }
    return hrProjectRolePersistence.remove(hrProjectRole);
  }
 catch (  NoSuchProjectRoleException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
