{
  HRProjectRole hrProjectRole=(HRProjectRole)EntityCacheUtil.getResult(HRProjectRoleModelImpl.ENTITY_CACHE_ENABLED,HRProjectRoleImpl.class,hrProjectRoleId,this);
  if (hrProjectRole == null) {
    Session session=null;
    try {
      session=openSession();
      hrProjectRole=(HRProjectRole)session.get(HRProjectRoleImpl.class,Long.valueOf(hrProjectRoleId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrProjectRole != null) {
        cacheResult(hrProjectRole);
      }
      closeSession(session);
    }
  }
  return hrProjectRole;
}
