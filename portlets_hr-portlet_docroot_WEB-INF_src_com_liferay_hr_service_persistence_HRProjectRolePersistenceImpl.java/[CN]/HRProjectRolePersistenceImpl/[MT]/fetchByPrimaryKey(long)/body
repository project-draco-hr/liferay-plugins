{
  HRProjectRole hrProjectRole=(HRProjectRole)EntityCacheUtil.getResult(HRProjectRoleModelImpl.ENTITY_CACHE_ENABLED,HRProjectRoleImpl.class,hrProjectRoleId,this);
  if (hrProjectRole == _nullHRProjectRole) {
    return null;
  }
  if (hrProjectRole == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrProjectRole=(HRProjectRole)session.get(HRProjectRoleImpl.class,Long.valueOf(hrProjectRoleId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrProjectRole != null) {
        cacheResult(hrProjectRole);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRProjectRoleModelImpl.ENTITY_CACHE_ENABLED,HRProjectRoleImpl.class,hrProjectRoleId,_nullHRProjectRole);
      }
      closeSession(session);
    }
  }
  return hrProjectRole;
}
