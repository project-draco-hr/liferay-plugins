{
  KaleoAction kaleoAction=(KaleoAction)EntityCacheUtil.getResult(KaleoActionModelImpl.ENTITY_CACHE_ENABLED,KaleoActionImpl.class,kaleoActionId,this);
  if (kaleoAction == null) {
    Session session=null;
    try {
      session=openSession();
      kaleoAction=(KaleoAction)session.get(KaleoActionImpl.class,new Long(kaleoActionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (kaleoAction != null) {
        cacheResult(kaleoAction);
      }
      closeSession(session);
    }
  }
  return kaleoAction;
}
