{
  kaleoAction=toUnwrappedModel(kaleoAction);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,kaleoAction);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(kaleoAction);
  return kaleoAction;
}
