{
  if (obj == null) {
    return false;
  }
  HRUserTask hrUserTask=null;
  try {
    hrUserTask=(HRUserTask)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrUserTask.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
