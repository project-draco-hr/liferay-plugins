{
  Session session=null;
  try {
    session=openSession();
    HRUserTask hrUserTask=(HRUserTask)session.get(HRUserTaskImpl.class,Long.valueOf(hrUserTaskId));
    if (hrUserTask == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrUserTaskId);
      }
      throw new NoSuchUserTaskException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrUserTaskId);
    }
    return hrUserTaskPersistence.remove(hrUserTask);
  }
 catch (  NoSuchUserTaskException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
