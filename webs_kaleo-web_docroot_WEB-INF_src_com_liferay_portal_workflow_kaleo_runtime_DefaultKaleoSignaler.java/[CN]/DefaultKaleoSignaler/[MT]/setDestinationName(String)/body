{
  DefaultSingleDestinationMessageSender singleDestinationMessageSender=new DefaultSingleDestinationMessageSender();
  singleDestinationMessageSender.setDestinationName(destinationName);
  singleDestinationMessageSender.setMessageBus(MessageBusUtil.getMessageBus());
  _singleDestinationMessageSender=singleDestinationMessageSender;
}
