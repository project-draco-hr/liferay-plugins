{
  Session session=null;
  try {
    session=openSession();
    AMSType amsType=(AMSType)session.get(AMSTypeImpl.class,new Long(amsTypeId));
    if (amsType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + amsTypeId);
      }
      throw new NoSuchTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + amsTypeId);
    }
    return remove(amsType);
  }
 catch (  NoSuchTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
