{
  if (cause instanceof ArticleContentException || cause instanceof ArticlePriorityException || cause instanceof ArticleTitleException|| cause instanceof CommentContentException|| cause instanceof DuplicateFileException|| cause instanceof FileSizeException|| cause instanceof NoSuchArticleException|| cause instanceof NoSuchCommentException|| cause instanceof NoSuchFileException|| cause instanceof PrincipalException) {
    return true;
  }
  return false;
}
