{
  List<Group> scopeGroups=new ArrayList<Group>();
  for (  long curScopeGroupId : scopeGroupIds) {
    Group group=null;
    try {
      group=GroupLocalServiceUtil.getGroup(curScopeGroupId);
    }
 catch (    NoSuchGroupException nsge) {
      continue;
    }
    scopeGroups.add(group);
  }
  return scopeGroups;
}
