{
  try {
    clearSemanticsFiles.clear(pk);
    for (    String semanticsFilePK : semanticsFilePKs) {
      addSemanticsFile.add(pk,semanticsFilePK);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("InterestGroups_SemanticsFiles");
  }
}
