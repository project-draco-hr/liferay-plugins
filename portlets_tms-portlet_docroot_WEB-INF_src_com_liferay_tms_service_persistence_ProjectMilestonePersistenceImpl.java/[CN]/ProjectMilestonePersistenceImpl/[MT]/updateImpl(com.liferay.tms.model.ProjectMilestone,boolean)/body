{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,projectMilestone,merge);
    projectMilestone.setNew(false);
    return projectMilestone;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(ProjectMilestone.class.getName());
  }
}
