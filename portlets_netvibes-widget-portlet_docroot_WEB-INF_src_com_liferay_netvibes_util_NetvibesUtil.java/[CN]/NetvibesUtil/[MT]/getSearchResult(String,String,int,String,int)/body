{
  WebCacheItem wci=new NetvibesSearchResultWebCacheItem(query,sort,category,region,page);
  String key=NetvibesUtil.class.getName() + StringPool.PERIOD + query+ StringPool.PERIOD+ sort+ StringPool.PERIOD+ category+ StringPool.PERIOD+ region+ StringPool.PERIOD+ page;
  String result=(String)WebCachePoolUtil.get(key,wci);
  if (Validator.isNotNull(result)) {
    return result;
  }
 else {
    WebCachePoolUtil.remove(key);
    return (String)WebCachePoolUtil.get(key,wci);
  }
}
