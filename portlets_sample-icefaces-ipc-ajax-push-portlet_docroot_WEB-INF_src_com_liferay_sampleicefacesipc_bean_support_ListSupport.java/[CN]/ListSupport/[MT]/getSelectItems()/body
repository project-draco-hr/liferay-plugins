{
  if (_selectItems == null) {
    _selectItems=new ArrayList<SelectItem>();
    List<BookingType> bookingTypes=_bookingTypeService.getAllBookingTypes();
    for (    BookingType bookingType : bookingTypes) {
      _selectItems.add(new SelectItem(bookingType.getBookingTypeId(),bookingType.getBookingTypeName()));
    }
  }
  return _selectItems;
}
