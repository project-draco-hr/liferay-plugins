{
  type=toUnwrappedModel(type);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(type)) {
      type=(Type)session.get(TypeImpl.class,type.getPrimaryKeyObj());
    }
    if (type != null) {
      session.delete(type);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (type != null) {
    clearCache(type);
  }
  return type;
}
