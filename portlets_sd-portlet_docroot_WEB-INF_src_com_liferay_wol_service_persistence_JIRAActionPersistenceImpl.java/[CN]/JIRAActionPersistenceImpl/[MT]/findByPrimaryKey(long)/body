{
  JIRAAction jiraAction=fetchByPrimaryKey(jiraActionId);
  if (jiraAction == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No JIRAAction exists with the primary key " + jiraActionId);
    }
    throw new NoSuchJIRAActionException("No JIRAAction exists with the primary key " + jiraActionId);
  }
  return jiraAction;
}
