{
  Map<String,Object> inputObjects=ScriptingContextBuilder.buildScriptingContext(executionContext);
  String assigneeScript=kaleoTaskAssignment.getAssigneeScript();
  String assigneeScriptingLanguage=kaleoTaskAssignment.getAssigneeScriptLanguage();
  Map<String,Object> results=ScriptingUtil.eval(null,inputObjects,_outputNames,assigneeScriptingLanguage,assigneeScript,classLoaders);
  return getKaleoTaskAssignments(results);
}
