{
  if (_persistence == null) {
    _persistence=(TasksEntryPersistence)PortletBeanLocatorUtil.locate(com.liferay.tms.service.ClpSerializer.getServletContextName(),TasksEntryPersistence.class.getName());
    ReferenceRegistry.registerReference(TasksEntryUtil.class,"_persistence");
  }
  return _persistence;
}
