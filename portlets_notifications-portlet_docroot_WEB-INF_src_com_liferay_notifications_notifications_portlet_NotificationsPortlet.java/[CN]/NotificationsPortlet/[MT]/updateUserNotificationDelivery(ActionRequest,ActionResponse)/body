{
  boolean isDeliver=ParamUtil.getBoolean(actionRequest,"isDeliver",true);
  long userNotificationDeliveryId=ParamUtil.getLong(actionRequest,"userNotificationDeliveryId");
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject();
  try {
    UserNotificationDeliveryLocalServiceUtil.updateUserNotificationDelivery(userNotificationDeliveryId,isDeliver);
    jsonObject.put("success",Boolean.TRUE);
  }
 catch (  Exception e) {
    jsonObject.put("success",Boolean.FALSE);
  }
}
