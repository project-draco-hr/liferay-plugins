{
  List<String> inactiveMetricAlarms=new ArrayList<String>();
  for (  String autoScalingGroupName : autoScalingGroupsMetricAlarms.keySet()) {
    if (!activeAutoScalingGroups.contains(autoScalingGroupName)) {
      inactiveMetricAlarms.add(autoScalingGroupsMetricAlarms.get(autoScalingGroupName));
    }
  }
  return inactiveMetricAlarms;
}
