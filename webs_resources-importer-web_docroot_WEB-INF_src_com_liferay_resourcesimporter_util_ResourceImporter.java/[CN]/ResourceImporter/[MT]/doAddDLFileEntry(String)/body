{
  long parentFolderId=0;
  String folderPath=FileUtil.getPath(resourcePath).concat(StringPool.SLASH);
  if (_folderIds.containsKey(folderPath)) {
    parentFolderId=_folderIds.get(folderPath);
  }
  URL url=servletContext.getResource(resourcePath);
  URLConnection urlConnection=url.openConnection();
  doAddDLFileEntry(parentFolderId,FileUtil.getShortFileName(resourcePath),urlConnection.getInputStream(),urlConnection.getContentLength());
}
