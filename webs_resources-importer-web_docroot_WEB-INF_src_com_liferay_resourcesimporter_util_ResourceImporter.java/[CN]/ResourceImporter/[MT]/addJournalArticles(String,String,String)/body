{
  Set<String> resourcePaths=servletContext.getResourcePaths(resourcesDir.concat(articlesDirName));
  if (resourcePaths == null) {
    return;
  }
  for (  String resourcePath : resourcePaths) {
    if (resourcePath.endsWith(StringPool.SLASH)) {
      continue;
    }
    int pos=resourcePath.lastIndexOf(StringPool.SLASH);
    String name=resourcePath.substring(pos + 1);
    URL url=servletContext.getResource(resourcePath);
    URLConnection urlConnection=url.openConnection();
    doAddJournalArticles(journalStructureId,journalTemplateId,name,urlConnection.getInputStream());
  }
}
