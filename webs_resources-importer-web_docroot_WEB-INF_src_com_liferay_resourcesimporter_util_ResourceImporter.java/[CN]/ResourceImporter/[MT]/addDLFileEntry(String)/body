{
  String folderPath=FileUtil.getPath(resourcePath);
  Long parentFolderId=_folderIds.get(folderPath.concat(StringPool.SLASH));
  if (parentFolderId == null) {
    parentFolderId=0L;
  }
  URL url=servletContext.getResource(resourcePath);
  URLConnection urlConnection=url.openConnection();
  addDLFileEntry(parentFolderId,FileUtil.getShortFileName(resourcePath),urlConnection.getInputStream(),urlConnection.getContentLength());
}
