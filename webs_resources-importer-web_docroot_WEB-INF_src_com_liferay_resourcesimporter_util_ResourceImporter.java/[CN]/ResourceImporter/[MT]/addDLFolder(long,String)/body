{
  String folderPath=FileUtil.getPath(resourcePath);
  long folderId=super.addDLFolder(parentFolderId,FileUtil.getShortFileName(folderPath));
  _folderIds.put(resourcePath,folderId);
  Set<String> resourcePaths=servletContext.getResourcePaths(resourcePath);
  if ((resourcePaths == null) || resourcePaths.isEmpty()) {
    return folderId;
  }
  for (  String curResourcePath : resourcePaths) {
    if (curResourcePath.endsWith(StringPool.SLASH)) {
      addDLFolder(folderId,curResourcePath);
    }
 else {
      doAddDLFileEntry(curResourcePath);
    }
  }
  return folderId;
}
