{
  String settings="";
  try {
    User user=UserLocalServiceUtil.getUser(calendar.getUserId());
    Properties properties=new Properties();
    properties.setProperty(PortletPropsKeys.CALENDAR_NOTIFICATION_SENDER_EMAIL,user.getEmailAddress());
    properties.setProperty(PortletPropsKeys.CALENDAR_NOTIFICATION_SENDER_NAME,calendar.getUserName());
    Writer writer=new StringWriter();
    properties.list(new PrintWriter(writer));
    settings=writer.toString();
  }
 catch (  Exception e) {
    settings="";
  }
  return settings;
}
