{
  Session session=null;
  try {
    session=openSession();
    HRTimeSheetDay hrTimeSheetDay=(HRTimeSheetDay)session.get(HRTimeSheetDayImpl.class,Long.valueOf(hrTimeSheetDayId));
    if (hrTimeSheetDay == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeSheetDayId);
      }
      throw new NoSuchTimeSheetDayException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeSheetDayId);
    }
    return hrTimeSheetDayPersistence.remove(hrTimeSheetDay);
  }
 catch (  NoSuchTimeSheetDayException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
