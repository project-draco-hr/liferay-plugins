{
  HRTimeSheetDay hrTimeSheetDay=(HRTimeSheetDay)EntityCacheUtil.getResult(HRTimeSheetDayModelImpl.ENTITY_CACHE_ENABLED,HRTimeSheetDayImpl.class,hrTimeSheetDayId,this);
  if (hrTimeSheetDay == null) {
    Session session=null;
    try {
      session=openSession();
      hrTimeSheetDay=(HRTimeSheetDay)session.get(HRTimeSheetDayImpl.class,Long.valueOf(hrTimeSheetDayId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrTimeSheetDay != null) {
        cacheResult(hrTimeSheetDay);
      }
      closeSession(session);
    }
  }
  return hrTimeSheetDay;
}
