{
  if (isEscapedModel()) {
    return (PresenceStatus)this;
  }
 else {
    PresenceStatus model=new PresenceStatusImpl();
    model.setEscapedModel(true);
    model.setPresenceStatusId(getPresenceStatusId());
    model.setPresenceStatusMessage(HtmlUtil.escape(getPresenceStatusMessage()));
    model=(PresenceStatus)Proxy.newProxyInstance(PresenceStatus.class.getClassLoader(),new Class[]{PresenceStatus.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
