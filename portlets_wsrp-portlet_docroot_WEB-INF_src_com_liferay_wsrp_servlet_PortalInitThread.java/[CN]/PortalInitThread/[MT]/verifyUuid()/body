{
  List<WSRPConsumer> wsrpConsumers=WSRPConsumerLocalServiceUtil.getWSRPConsumers(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  WSRPConsumer wsrpConsumer : wsrpConsumers) {
    if (Validator.isNull(wsrpConsumer.getUuid())) {
      WSRPConsumerLocalServiceUtil.updateWSRPConsumer(wsrpConsumer);
    }
  }
  List<WSRPConsumerPortlet> wsrpConsumerPortlets=WSRPConsumerPortletLocalServiceUtil.getWSRPConsumerPortlets(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  WSRPConsumerPortlet wsrpConsumerPortlet : wsrpConsumerPortlets) {
    if (Validator.isNull(wsrpConsumerPortlet.getUuid())) {
      WSRPConsumerPortletLocalServiceUtil.updateWSRPConsumerPortlet(wsrpConsumerPortlet);
    }
  }
  List<WSRPProducer> wsrpProducers=WSRPProducerLocalServiceUtil.getWSRPProducers(QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  for (  WSRPProducer wsrpProducer : wsrpProducers) {
    if (Validator.isNull(wsrpProducer.getUuid())) {
      WSRPProducerLocalServiceUtil.updateWSRPProducer(wsrpProducer);
    }
  }
}
