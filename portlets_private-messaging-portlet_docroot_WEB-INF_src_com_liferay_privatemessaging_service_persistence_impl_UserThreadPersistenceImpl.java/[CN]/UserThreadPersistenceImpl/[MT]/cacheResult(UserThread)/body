{
  EntityCacheUtil.putResult(UserThreadModelImpl.ENTITY_CACHE_ENABLED,UserThreadImpl.class,userThread.getPrimaryKey(),userThread);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_M,new Object[]{userThread.getUserId(),userThread.getMbThreadId()},userThread);
  userThread.resetOriginalValues();
}
