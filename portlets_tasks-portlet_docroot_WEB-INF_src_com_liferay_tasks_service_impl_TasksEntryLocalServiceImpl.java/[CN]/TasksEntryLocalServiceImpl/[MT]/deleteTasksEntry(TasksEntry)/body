{
  tasksEntryPersistence.remove(tasksEntry);
  AssetEntryLocalServiceUtil.deleteEntry(TasksEntry.class.getName(),tasksEntry.getTasksEntryId());
  MBMessageLocalServiceUtil.deleteDiscussionMessages(TasksEntry.class.getName(),tasksEntry.getTasksEntryId());
  AssetEntry assetEntry=AssetEntryLocalServiceUtil.getEntry(TasksEntry.class.getName(),tasksEntry.getTasksEntryId());
  SocialActivityLocalServiceUtil.deleteActivities(assetEntry);
}
