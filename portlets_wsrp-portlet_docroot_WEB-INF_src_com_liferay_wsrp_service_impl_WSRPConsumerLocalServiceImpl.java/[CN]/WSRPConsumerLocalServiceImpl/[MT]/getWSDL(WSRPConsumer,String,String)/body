{
  try {
    wsrpConsumer.setForwardCookies(forwardCookies);
    WSRPConsumerManager wsrpConsumerManager=WSRPConsumerManagerFactory.getWSRPConsumerManager(wsrpConsumer,userToken);
    return wsrpConsumerManager.getWsdl();
  }
 catch (  Exception e) {
    throw new WSRPConsumerWSDLException(e);
  }
}
