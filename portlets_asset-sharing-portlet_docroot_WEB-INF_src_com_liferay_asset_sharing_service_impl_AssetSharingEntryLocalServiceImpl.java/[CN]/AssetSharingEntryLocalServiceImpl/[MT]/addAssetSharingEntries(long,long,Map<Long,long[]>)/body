{
  for (  Long sharedToClassNameId : sharedToClassNameIdsClassPKs.keySet()) {
    long[] sharedToClassPKs=sharedToClassNameIdsClassPKs.get(sharedToClassNameId);
    if (sharedToClassPKs.length == 0) {
      continue;
    }
    for (    long sharedToClassPK : sharedToClassPKs) {
      addAssetSharingEntry(classNameId,classPK,sharedToClassNameId,sharedToClassPK);
    }
  }
}
