{
  AssetSharingEntryPK pk=new AssetSharingEntryPK(classNameId,classPK,sharedToClassNameId,sharedToClassPK);
  AssetSharingEntry assetSharingEntry=assetSharingEntryPersistence.fetchByPrimaryKey(pk);
  if (assetSharingEntry == null) {
    assetSharingEntry=assetSharingEntryPersistence.create(pk);
    assetSharingEntryPersistence.update(assetSharingEntry);
  }
}
