{
  AssetSharingEntryPK assetSharingEntryPK=new AssetSharingEntryPK(classNameId,classPK,sharedToClassNameId,sharedToClassPK);
  AssetSharingEntry assetSharingEntry=assetSharingEntryPersistence.fetchByPrimaryKey(assetSharingEntryPK);
  if (assetSharingEntry == null) {
    assetSharingEntry=assetSharingEntryPersistence.create(assetSharingEntryPK);
    assetSharingEntryPersistence.update(assetSharingEntry);
  }
}
