{
  message=toUnwrappedModel(message);
  Session session=null;
  try {
    session=openSession();
    if (message.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(MessageImpl.class,message.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(message);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(MessageModelImpl.ENTITY_CACHE_ENABLED,MessageImpl.class,message.getPrimaryKey());
  return message;
}
