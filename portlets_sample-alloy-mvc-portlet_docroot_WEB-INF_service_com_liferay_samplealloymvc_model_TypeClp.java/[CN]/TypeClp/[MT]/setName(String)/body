{
  _name=name;
  if (_typeRemoteModel != null) {
    try {
      Class<?> clazz=_typeRemoteModel.getClass();
      Method method=clazz.getMethod("setName",String.class);
      method.invoke(_typeRemoteModel,name);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
