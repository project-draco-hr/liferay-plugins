{
  Session session=null;
  try {
    session=openSession();
    MeetupRegistration meetupRegistration=(MeetupRegistration)session.get(MeetupRegistrationImpl.class,new Long(meetupRegistrationId));
    if (meetupRegistration == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No MeetupRegistration exists with the primary key " + meetupRegistrationId);
      }
      throw new NoSuchMeetupRegistrationException("No MeetupRegistration exists with the primary key " + meetupRegistrationId);
    }
    return remove(meetupRegistration);
  }
 catch (  NoSuchMeetupRegistrationException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
  }
}
