{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),AccountLocalService.class.getName());
    _service=new AccountLocalServiceClp(invokableLocalService);
    ReferenceRegistry.registerReference(AccountLocalServiceUtil.class,"_service");
    MethodCache.remove(AccountLocalService.class);
  }
  return _service;
}
