{
  Map<Long,Set<Semantics>> communitySemantics=new HashMap();
  for (  Long communityId : communityIds) {
    try {
      Community community=communityPersistence.fetchByPrimaryKey(communityId);
      Set<Semantics> semanticsSet=new TreeSet(communityPersistence.getSemanticss(communityId));
      communitySemantics.put(communityId,semanticsSet);
    }
 catch (    SystemException ex) {
      throw new IWebException(ex);
    }
  }
  return communitySemantics;
}
