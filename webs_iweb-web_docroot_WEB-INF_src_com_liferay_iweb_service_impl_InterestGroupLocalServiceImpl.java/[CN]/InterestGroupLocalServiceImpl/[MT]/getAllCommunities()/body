{
  Set<InterestGroup> igset=new TreeSet();
  try {
    igset.addAll(interestGroupPersistence.findAll());
    return igset;
  }
 catch (  SystemException e) {
    if (e.getCause() instanceof com.liferay.portal.NoSuchResourceException) {
      return igset;
    }
 else {
      throw new IWebException(e);
    }
  }
}
