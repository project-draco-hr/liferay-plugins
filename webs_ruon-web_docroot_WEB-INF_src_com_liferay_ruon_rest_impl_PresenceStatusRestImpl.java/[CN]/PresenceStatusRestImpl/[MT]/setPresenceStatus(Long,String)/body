{
  try {
    if (status.equalsIgnoreCase("online")) {
      UserPresenceLocalServiceUtil.setPresenceStatusOfUser(userId,PresenceStatusConstants.STATUS_ONLINE);
    }
 else {
      UserPresenceLocalServiceUtil.setPresenceStatusOfUser(userId,PresenceStatusConstants.STATUS_OFFLINE);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
