{
  long[] childCalendarIds=getChildCalendarIds(calendarBookingId,calendarId);
  return updateCalendarBooking(userId,calendarBookingId,calendarId,childCalendarIds,titleMap,descriptionMap,location,startDate,endDate,allDay,recurrence,firstReminder,firstReminderType,secondReminder,secondReminderType,status,serviceContext);
}
