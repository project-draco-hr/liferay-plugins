{
  List<CalendarBooking> calendarBookings=calendarBookingFinder.findByC_G_C_C_P_T_D_L_S_E_S(companyId,groupIds,calendarIds,calendarResourceIds,parentCalendarBookingId,title,description,location,startDate,endDate,statuses,recurring,andOperator,start,end,orderByComparator);
  if (recurring) {
    calendarBookings=RecurrenceUtil.expandCalendarBookings(calendarBookings,endDate);
  }
  return calendarBookings;
}
