{
  try {
    Date now=new Date();
    List<CalendarBooking> calendarBookings=calendarBookingFinder.findByFutureReminders(now);
    for (    CalendarBooking calendarBooking : calendarBookings) {
      NotificationUtil.notifyCalendarBookingReminders(calendarBooking);
    }
  }
 catch (  Exception e) {
  }
}
