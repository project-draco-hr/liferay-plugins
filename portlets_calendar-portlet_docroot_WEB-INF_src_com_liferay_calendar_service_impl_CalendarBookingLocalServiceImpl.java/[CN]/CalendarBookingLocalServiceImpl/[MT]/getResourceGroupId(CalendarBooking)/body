{
  CalendarResource calendarResource=calendarBooking.getCalendarResource();
  if (calendarResource.isUser()) {
    Group group=GroupLocalServiceUtil.getUserGroup(calendarBooking.getCompanyId(),calendarResource.getClassPK());
    return group.getGroupId();
  }
 else {
    return calendarBooking.getResourceGroupId();
  }
}
