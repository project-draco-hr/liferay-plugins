{
  jiraAction=toUnwrappedModel(jiraAction);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,jiraAction);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(jiraAction);
  return jiraAction;
}
