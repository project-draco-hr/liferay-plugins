{
  Session session=null;
  try {
    session=openSession();
    if (jiraAction.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(JIRAActionImpl.class,jiraAction.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(jiraAction);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(JIRAActionModelImpl.ENTITY_CACHE_ENABLED,JIRAActionImpl.class,jiraAction.getPrimaryKey());
  return jiraAction;
}
