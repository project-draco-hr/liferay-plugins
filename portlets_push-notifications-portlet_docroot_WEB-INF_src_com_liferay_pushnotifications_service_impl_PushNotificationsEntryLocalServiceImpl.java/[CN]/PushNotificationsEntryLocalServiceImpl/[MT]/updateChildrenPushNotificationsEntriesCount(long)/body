{
  if (parentPushNotificationsEntryId == 0) {
    return null;
  }
  PushNotificationsEntry pushNotificationsEntry=pushNotificationsEntryPersistence.findByPrimaryKey(parentPushNotificationsEntryId);
  int childrenPushNotificationsEntriesCount=pushNotificationsEntryPersistence.countByParentPushNotificationsEntryId(parentPushNotificationsEntryId);
  pushNotificationsEntry.setChildrenPushNotificationsEntriesCount(childrenPushNotificationsEntriesCount);
  pushNotificationsEntryPersistence.update(pushNotificationsEntry);
  return pushNotificationsEntry;
}
