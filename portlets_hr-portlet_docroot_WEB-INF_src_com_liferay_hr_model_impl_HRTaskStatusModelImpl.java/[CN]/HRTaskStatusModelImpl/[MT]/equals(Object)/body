{
  if (obj == null) {
    return false;
  }
  HRTaskStatus hrTaskStatus=null;
  try {
    hrTaskStatus=(HRTaskStatus)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrTaskStatus.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
