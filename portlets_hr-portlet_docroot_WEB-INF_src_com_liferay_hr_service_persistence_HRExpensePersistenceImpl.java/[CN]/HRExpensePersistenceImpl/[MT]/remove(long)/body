{
  Session session=null;
  try {
    session=openSession();
    HRExpense hrExpense=(HRExpense)session.get(HRExpenseImpl.class,Long.valueOf(hrExpenseId));
    if (hrExpense == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrExpenseId);
      }
      throw new NoSuchExpenseException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrExpenseId);
    }
    return hrExpensePersistence.remove(hrExpense);
  }
 catch (  NoSuchExpenseException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
