{
  HRExpense hrExpense=(HRExpense)EntityCacheUtil.getResult(HRExpenseModelImpl.ENTITY_CACHE_ENABLED,HRExpenseImpl.class,hrExpenseId,this);
  if (hrExpense == null) {
    Session session=null;
    try {
      session=openSession();
      hrExpense=(HRExpense)session.get(HRExpenseImpl.class,Long.valueOf(hrExpenseId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrExpense != null) {
        cacheResult(hrExpense);
      }
      closeSession(session);
    }
  }
  return hrExpense;
}
