    }
    if ((kbArticle.getCompanyId() != kbArticleModelImpl.getOriginalCompanyId()) || (kbArticle.getMain() != kbArticleModelImpl.getOriginalMain())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_C_M,new Object[]{Long.valueOf(kbArticleModelImpl.getOriginalCompanyId()),Boolean.valueOf(kbArticleModelImpl.getOriginalMain())});
