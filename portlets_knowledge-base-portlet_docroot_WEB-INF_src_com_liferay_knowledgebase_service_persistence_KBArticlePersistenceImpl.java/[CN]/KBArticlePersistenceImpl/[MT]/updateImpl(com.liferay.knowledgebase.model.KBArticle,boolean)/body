    }
    if ((kbArticle.getResourcePrimKey() != kbArticleModelImpl.getOriginalResourcePrimKey()) || (kbArticle.getMain() != kbArticleModelImpl.getOriginalMain())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_R_M,new Object[]{Long.valueOf(kbArticleModelImpl.getOriginalResourcePrimKey()),Boolean.valueOf(kbArticleModelImpl.getOriginalMain())});
    }
    if ((kbArticle.getResourcePrimKey() != kbArticleModelImpl.getOriginalResourcePrimKey()) || (kbArticle.getStatus() != kbArticleModelImpl.getOriginalStatus())) {
