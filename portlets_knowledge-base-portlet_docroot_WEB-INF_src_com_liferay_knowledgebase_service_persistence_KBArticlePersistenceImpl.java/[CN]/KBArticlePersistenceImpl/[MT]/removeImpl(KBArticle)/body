{
  kbArticle=toUnwrappedModel(kbArticle);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,kbArticle);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(kbArticle);
  return kbArticle;
}
