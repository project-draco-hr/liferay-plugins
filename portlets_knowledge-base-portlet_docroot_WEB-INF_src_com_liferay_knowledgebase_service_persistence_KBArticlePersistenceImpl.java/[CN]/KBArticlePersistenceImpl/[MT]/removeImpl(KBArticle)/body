{
  Session session=null;
  try {
    session=openSession();
    session.delete(kbArticle);
    session.flush();
    return kbArticle;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(KBArticle.class.getName());
  }
}
