{
  if (attributeId.equalsIgnoreCase("objectclass")) {
    if (value.equalsIgnoreCase("organizationalUnit") || value.equalsIgnoreCase("top") || value.equalsIgnoreCase("*")) {
      return true;
    }
  }
 else   if (attributeId.equalsIgnoreCase("ou")) {
    if (value.equalsIgnoreCase("Roles") || value.equalsIgnoreCase("*")) {
      return true;
    }
  }
  return false;
}
