{
  BBBMeeting bbbMeeting=BBBMeetingLocalServiceUtil.getBBBMeeting(bbbParticipant.getBbbMeetingId());
  return DigesterUtil.digestHex(Digester.SHA_1,String.valueOf(bbbMeeting.getBbbMeetingId()),String.valueOf(bbbParticipant.getBbbParticipantId()),String.valueOf(bbbParticipant.getCreateDate()),bbbParticipant.getEmailAddress());
}
