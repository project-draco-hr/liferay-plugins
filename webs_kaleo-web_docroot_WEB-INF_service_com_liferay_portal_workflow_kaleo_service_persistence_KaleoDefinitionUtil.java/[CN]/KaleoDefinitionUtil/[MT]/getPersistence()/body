{
  if (_persistence == null) {
    _persistence=(KaleoDefinitionPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoDefinitionPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoDefinitionUtil.class,"_persistence");
  }
  return _persistence;
}
