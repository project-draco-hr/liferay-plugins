{
  int count=countByME_S(meetupsEntryId,status);
  List<MeetupsRegistration> list=findByME_S(meetupsEntryId,status,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("meetupsEntryId=");
    msg.append(meetupsEntryId);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchMeetupsRegistrationException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
