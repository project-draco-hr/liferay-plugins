{
  if (_persistence == null) {
    _persistence=(KaleoTransitionPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoTransitionPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoTransitionUtil.class,"_persistence");
  }
  return _persistence;
}
