{
  HRUser hrUser=(HRUser)EntityCacheUtil.getResult(HRUserModelImpl.ENTITY_CACHE_ENABLED,HRUserImpl.class,hrUserId);
  if (hrUser == _nullHRUser) {
    return null;
  }
  if (hrUser == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrUser=(HRUser)session.get(HRUserImpl.class,Long.valueOf(hrUserId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrUser != null) {
        cacheResult(hrUser);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRUserModelImpl.ENTITY_CACHE_ENABLED,HRUserImpl.class,hrUserId,_nullHRUser);
      }
      closeSession(session);
    }
  }
  return hrUser;
}
