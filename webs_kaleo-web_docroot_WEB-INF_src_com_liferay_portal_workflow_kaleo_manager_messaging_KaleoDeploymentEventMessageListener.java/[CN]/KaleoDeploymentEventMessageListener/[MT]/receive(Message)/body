{
  String contextName=(String)message.get("servletContextName");
  if (!_contextName.equals(contextName)) {
    return;
  }
  try {
    _portalKaleoManager.deployKaleoDefaults();
  }
 catch (  Exception e) {
    if (_log.isErrorEnabled()) {
      _log.error("Unable to deploy Kaleo defaults",e);
    }
  }
}
