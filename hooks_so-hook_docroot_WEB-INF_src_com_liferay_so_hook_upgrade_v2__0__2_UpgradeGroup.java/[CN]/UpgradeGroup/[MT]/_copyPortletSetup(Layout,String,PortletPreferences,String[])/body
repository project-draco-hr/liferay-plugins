{
  PortletPreferences newPortletSetup=PortletPreferencesFactoryUtil.getLayoutPortletSetup(layout,portletId);
  for (  String key : keysToCopy) {
    String oldValue=oldPortletSetup.getValue(key,"");
    if (Validator.isNotNull(oldValue)) {
      newPortletSetup.setValue(key,oldValue);
    }
  }
  newPortletSetup.store();
}
