{
  try {
    Directory directory=getDirectory();
    Directory.Members members=directory.members();
    Member member=new Member();
    member.setEmail(emailAddress);
    Directory.Members.Insert insert=members.insert(groupEmailAddress,member);
    for (int i=0; i < PortletPropsValues.GOOGLE_API_MAX_RETRIES; i++) {
      try {
        insert.execute();
        return;
      }
 catch (      GoogleJsonResponseException gjre) {
        if (gjre.getStatusCode() == _ERROR_CONFLICT) {
          return;
        }
        if (i >= (PortletPropsValues.GOOGLE_API_MAX_RETRIES - 1)) {
          throw new PortalException(gjre);
        }
 else {
          Thread.sleep(PortletPropsValues.GOOGLE_API_RETRY_WAIT_TIME);
        }
      }
    }
  }
 catch (  Exception e) {
    throw new PortalException(e);
  }
}
