{
  String s1=request.getParameter("param1");
  String s2=request.getParameter("param2");
  if ("wildcardMatch".equals(s1)) {
    try {
      QName qName=new QName("http://wildcarding.com","a.b.c.d","x");
      String pincode_Data="wildcard";
      response.setEvent(qName,pincode_Data);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  if ("multipleEvents".equals(s2)) {
    try {
      QName qName=new QName("http://event.com","a1","x");
      String a1_Data="data1";
      response.setEvent(qName,a1_Data);
      String a2_Event="a2";
      String a2_Data="data2";
      response.setEvent(a2_Event,a2_Data);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
