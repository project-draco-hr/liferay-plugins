{
  if (syncVersion == null) {
    syncVersion=getLatestSyncVersion();
  }
  if (syncVersion == null) {
    return;
  }
  HttpResponse httpResponse=execute(syncVersion.getUrl());
  if (httpResponse == null) {
    return;
  }
  HttpEntity httpEntity=httpResponse.getEntity();
  Files.copy(httpEntity.getContent(),getFilePath(httpResponse),StandardCopyOption.REPLACE_EXISTING);
  Desktop desktop=Desktop.getDesktop();
  desktop.open(filePath.toFile());
}
