{
  EntityCacheUtil.removeResult(GadgetModelImpl.ENTITY_CACHE_ENABLED,GadgetImpl.class,gadget.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_U,new Object[]{Long.valueOf(gadget.getCompanyId()),gadget.getUrl()});
}
