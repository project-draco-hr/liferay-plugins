{
  GadgetModelImpl gadgetModelImpl=(GadgetModelImpl)gadget;
  Object[] args=new Object[]{Long.valueOf(gadget.getCompanyId()),gadget.getUrl()};
  FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_C_U,args);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_U,args);
  if ((gadgetModelImpl.getColumnBitmask() & FINDER_PATH_FETCH_BY_C_U.getColumnBitmask()) != 0) {
    args=new Object[]{Long.valueOf(gadgetModelImpl.getOriginalCompanyId()),gadgetModelImpl.getOriginalUrl()};
    FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_C_U,args);
    FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_U,args);
  }
}
