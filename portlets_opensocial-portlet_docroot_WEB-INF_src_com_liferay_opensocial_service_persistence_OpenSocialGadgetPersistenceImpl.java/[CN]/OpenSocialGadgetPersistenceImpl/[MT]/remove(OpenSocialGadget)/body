{
  for (  ModelListener<OpenSocialGadget> listener : listeners) {
    listener.onBeforeRemove(openSocialGadget);
  }
  openSocialGadget=removeImpl(openSocialGadget);
  for (  ModelListener<OpenSocialGadget> listener : listeners) {
    listener.onAfterRemove(openSocialGadget);
  }
  return openSocialGadget;
}
