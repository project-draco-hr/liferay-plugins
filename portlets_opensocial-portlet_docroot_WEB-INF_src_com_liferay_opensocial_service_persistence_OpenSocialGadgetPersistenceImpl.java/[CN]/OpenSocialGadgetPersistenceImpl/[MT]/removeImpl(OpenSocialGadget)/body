{
  openSocialGadget=toUnwrappedModel(openSocialGadget);
  Session session=null;
  try {
    session=openSession();
    if (openSocialGadget.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(OpenSocialGadgetImpl.class,openSocialGadget.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(openSocialGadget);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(OpenSocialGadgetModelImpl.ENTITY_CACHE_ENABLED,OpenSocialGadgetImpl.class,openSocialGadget.getPrimaryKey());
  return openSocialGadget;
}
