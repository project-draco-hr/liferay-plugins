{
  OpenSocialGadget openSocialGadget=fetchByPrimaryKey(openSocialGadgetId);
  if (openSocialGadget == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + openSocialGadgetId);
    }
    throw new NoSuchGadgetException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + openSocialGadgetId);
  }
  return openSocialGadget;
}
