{
  Session session=null;
  try {
    session=openSession();
    OpenSocialGadget openSocialGadget=(OpenSocialGadget)session.get(OpenSocialGadgetImpl.class,new Long(openSocialGadgetId));
    if (openSocialGadget == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + openSocialGadgetId);
      }
      throw new NoSuchGadgetException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + openSocialGadgetId);
    }
    return remove(openSocialGadget);
  }
 catch (  NoSuchGadgetException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
