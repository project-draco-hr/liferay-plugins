{
  JIRAChangeGroup jiraChangeGroup=fetchByPrimaryKey(jiraChangeGroupId);
  if (jiraChangeGroup == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No JIRAChangeGroup exists with the primary key " + jiraChangeGroupId);
    }
    throw new NoSuchJIRAChangeGroupException("No JIRAChangeGroup exists with the primary key " + jiraChangeGroupId);
  }
  return jiraChangeGroup;
}
