{
  try {
    List<Long> activeSyncSiteIds=_activeSyncSiteIdsMap.get(syncAccountId);
    if ((activeSyncSiteIds != null) && !activeSyncSiteIds.isEmpty()) {
      return activeSyncSiteIds;
    }
    activeSyncSiteIds=_syncSitePersistence.findByA_S(true,syncAccountId);
    _activeSyncSiteIdsMap.put(syncAccountId,activeSyncSiteIds);
    return activeSyncSiteIds;
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
    return Collections.emptyList();
  }
}
