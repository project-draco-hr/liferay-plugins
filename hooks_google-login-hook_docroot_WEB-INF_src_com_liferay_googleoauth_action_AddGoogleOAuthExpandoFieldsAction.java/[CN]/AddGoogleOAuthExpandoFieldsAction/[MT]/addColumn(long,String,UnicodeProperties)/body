{
  ExpandoColumn expandoColum=ExpandoColumnLocalServiceUtil.getColumn(tableId,name);
  if (expandoColum != null) {
    return;
  }
  expandoColum=ExpandoColumnLocalServiceUtil.addColumn(tableId,name,ExpandoColumnConstants.STRING);
  ExpandoColumnLocalServiceUtil.updateTypeSettings(expandoColum.getColumnId(),properties.toString());
}
