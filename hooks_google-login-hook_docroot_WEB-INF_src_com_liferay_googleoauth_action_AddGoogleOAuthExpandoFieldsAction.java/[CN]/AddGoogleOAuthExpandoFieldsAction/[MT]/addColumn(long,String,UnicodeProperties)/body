{
  ExpandoColumn expandoColumn=ExpandoColumnLocalServiceUtil.getColumn(tableId,name);
  if (expandoColumn != null) {
    return;
  }
  expandoColumn=ExpandoColumnLocalServiceUtil.addColumn(tableId,name,ExpandoColumnConstants.STRING);
  ExpandoColumnLocalServiceUtil.updateTypeSettings(expandoColumn.getColumnId(),properties.toString());
}
