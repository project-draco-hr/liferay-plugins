{
  String role=_permission.getRole();
  if (extRepositoryPermission.equals(ExtRepositoryPermission.ACCESS) || extRepositoryPermission.equals(ExtRepositoryPermission.VIEW)) {
    return true;
  }
 else   if (extRepositoryPermission.equals(ExtRepositoryPermission.ADD_DOCUMENT) || extRepositoryPermission.equals(ExtRepositoryPermission.ADD_FOLDER) || extRepositoryPermission.equals(ExtRepositoryPermission.ADD_SUBFOLDER)|| extRepositoryPermission.equals(ExtRepositoryPermission.UPDATE)) {
    return isOwnerOrWriter(role);
  }
 else   if (extRepositoryPermission.equals(ExtRepositoryPermission.DELETE)) {
    return isOwner(role);
  }
  return false;
}
