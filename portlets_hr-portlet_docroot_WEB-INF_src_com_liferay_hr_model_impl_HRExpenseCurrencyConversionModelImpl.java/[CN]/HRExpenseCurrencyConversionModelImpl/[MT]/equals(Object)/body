{
  if (obj == null) {
    return false;
  }
  HRExpenseCurrencyConversion hrExpenseCurrencyConversion=null;
  try {
    hrExpenseCurrencyConversion=(HRExpenseCurrencyConversion)obj;
  }
 catch (  ClassCastException cce) {
