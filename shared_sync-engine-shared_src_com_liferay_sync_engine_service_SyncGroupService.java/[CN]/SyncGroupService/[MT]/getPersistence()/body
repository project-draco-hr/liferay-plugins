{
  if (_syncGroupPersistence != null) {
    return _syncGroupPersistence;
  }
  try {
    _syncGroupPersistence=new SyncGroupPersistence();
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
  }
  return _syncGroupPersistence;
}
