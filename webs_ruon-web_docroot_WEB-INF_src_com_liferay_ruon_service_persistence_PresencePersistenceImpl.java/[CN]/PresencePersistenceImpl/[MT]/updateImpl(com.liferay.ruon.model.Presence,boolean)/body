{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,presence,merge);
    presence.setNew(false);
    return presence;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Presence.class.getName());
  }
}
