{
  Group group=GroupLocalServiceUtil.getGroup(groupId);
  WSRPConsumerPortlet consumerPortlet=WSRPConsumerPortletLocalServiceUtil.fetchWSRPConsumerPortletByUuidAndCompanyId(uuid,group.getCompanyId());
  if (consumerPortlet != null) {
    WSRPConsumerPortletLocalServiceUtil.deleteWSRPConsumerPortlet(consumerPortlet);
  }
}
