{
  KBArticle article=kbArticleLocalService.getArticle(groupId,title,version);
  if (article.isDraft()) {
    KBPermission.check(getPermissionChecker(),plid,ActionKeys.ADD_ARTICLE);
  }
 else {
    KBArticlePermission.check(getPermissionChecker(),groupId,title,ActionKeys.VIEW);
  }
  return article;
}
