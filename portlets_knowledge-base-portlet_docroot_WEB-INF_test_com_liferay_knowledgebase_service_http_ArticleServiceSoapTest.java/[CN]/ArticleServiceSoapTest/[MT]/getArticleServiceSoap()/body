{
  int pos=URL.indexOf("://");
  String protocol=URL.substring(0,pos + 3);
  String host=URL.substring(pos + 3,URL.length());
  String path="/knowledge-base-portlet/secure/axis/Plugin_KB_ArticleService";
  URL url=new URL(protocol + USER_ID + StringPool.COLON+ PASSWORD+ StringPool.AT+ host+ path);
  ArticleServiceSoapServiceLocator locator=new ArticleServiceSoapServiceLocator();
  return locator.getPlugin_KB_ArticleService(url);
}
