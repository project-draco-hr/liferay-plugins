{
  KBTemplate kbTemplate1=(KBTemplate)obj1;
  KBTemplate kbTemplate2=(KBTemplate)obj2;
  int value=0;
  if (kbTemplate1.getEngineType() < kbTemplate2.getEngineType()) {
    value=-1;
  }
 else   if (kbTemplate1.getEngineType() > kbTemplate2.getEngineType()) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
