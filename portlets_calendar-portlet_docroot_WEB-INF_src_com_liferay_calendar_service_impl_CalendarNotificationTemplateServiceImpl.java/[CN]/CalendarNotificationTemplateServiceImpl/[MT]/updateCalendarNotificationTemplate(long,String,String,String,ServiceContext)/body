{
  CalendarNotificationTemplate calendarNotificationTemplate=calendarNotificationTemplatePersistence.findByPrimaryKey(calendarNotificationTemplateId);
  CalendarPermission.check(getPermissionChecker(),calendarNotificationTemplate.getCalendarId(),ActionKeys.UPDATE);
  return calendarNotificationTemplateLocalService.updateCalendarNotificationTemplate(calendarNotificationTemplateId,subject,body,typeSettings,serviceContext);
}
