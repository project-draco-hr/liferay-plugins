{
  DescribeAvailabilityZonesResult availabilityZonesResult=amazonEC2Client.describeAvailabilityZones();
  List<AvailabilityZone> availabilityZones=availabilityZonesResult.getAvailabilityZones();
  for (  AvailabilityZone availabilityZone : availabilityZones) {
    String availabilityZoneName=availabilityZone.getZoneName();
    if (availabilityZoneName.equals(zoneName)) {
      return true;
    }
  }
  return false;
}
