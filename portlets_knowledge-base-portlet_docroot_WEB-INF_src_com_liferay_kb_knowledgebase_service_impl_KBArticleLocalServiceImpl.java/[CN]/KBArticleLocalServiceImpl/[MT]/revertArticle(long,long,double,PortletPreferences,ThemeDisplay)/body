{
  KBArticle article=getArticle(resourcePrimKey);
  KBArticle oldArticle=getArticle(resourcePrimKey,version);
  return updateArticle(userId,resourcePrimKey,0,oldArticle.getTitle(),oldArticle.getContent(),oldArticle.getDescription(),false,oldArticle.isTemplate(),article.getDraft(),null,prefs,themeDisplay);
}
