{
  HRTask hrTask=(HRTask)EntityCacheUtil.getResult(HRTaskModelImpl.ENTITY_CACHE_ENABLED,HRTaskImpl.class,hrTaskId,this);
  if (hrTask == null) {
    Session session=null;
    try {
      session=openSession();
      hrTask=(HRTask)session.get(HRTaskImpl.class,Long.valueOf(hrTaskId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrTask != null) {
        cacheResult(hrTask);
      }
      closeSession(session);
    }
  }
  return hrTask;
}
