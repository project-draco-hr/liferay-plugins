{
  try {
    Message message=new Message();
    message.put("command","delegateToNetwork");
    message.put("name",network);
    message.put("jsonRequest",jsonRequest);
    JSONObject response=(JSONObject)MessageBusUtil.sendSynchronousMessage(DestinationNames.RUON,message);
    String responseOutput=response.getString("output");
    if (responseOutput.equalsIgnoreCase("success")) {
      return "success";
    }
 else {
      return "failure";
    }
  }
 catch (  Exception e) {
    _log.error("Unable to process message " + jsonRequest,e);
    return "failure";
  }
}
