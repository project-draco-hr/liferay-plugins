{
  TasksEntry tasksEntry=(TasksEntry)EntityCacheUtil.getResult(TasksEntryModelImpl.ENTITY_CACHE_ENABLED,TasksEntryImpl.class,tasksEntryId,this);
  if (tasksEntry == null) {
    Session session=null;
    try {
      session=openSession();
      tasksEntry=(TasksEntry)session.get(TasksEntryImpl.class,Long.valueOf(tasksEntryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (tasksEntry != null) {
        cacheResult(tasksEntry);
      }
      closeSession(session);
    }
  }
  return tasksEntry;
}
