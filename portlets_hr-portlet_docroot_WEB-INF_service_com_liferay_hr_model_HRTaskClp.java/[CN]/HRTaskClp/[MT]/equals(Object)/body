{
  if (obj == null) {
    return false;
  }
  HRTaskClp hrTask=null;
  try {
    hrTask=(HRTaskClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrTask.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
