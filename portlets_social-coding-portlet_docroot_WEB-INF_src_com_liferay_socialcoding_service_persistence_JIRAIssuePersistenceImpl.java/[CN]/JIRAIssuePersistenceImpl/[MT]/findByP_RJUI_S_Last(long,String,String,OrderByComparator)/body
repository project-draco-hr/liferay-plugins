{
  int count=countByP_RJUI_S(projectId,reporterJiraUserId,status);
  List<JIRAIssue> list=findByP_RJUI_S(projectId,reporterJiraUserId,status,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("projectId=");
    msg.append(projectId);
    msg.append(", reporterJiraUserId=");
    msg.append(reporterJiraUserId);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchJIRAIssueException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
