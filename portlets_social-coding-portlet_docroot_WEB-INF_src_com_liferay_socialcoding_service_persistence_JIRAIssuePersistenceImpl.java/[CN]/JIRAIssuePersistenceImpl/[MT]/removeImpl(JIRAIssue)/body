{
  jiraIssue=toUnwrappedModel(jiraIssue);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,jiraIssue);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  JIRAIssueModelImpl jiraIssueModelImpl=(JIRAIssueModelImpl)jiraIssue;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_KEY,new Object[]{jiraIssueModelImpl.getKey()});
  EntityCacheUtil.removeResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssue.getPrimaryKey());
  return jiraIssue;
}
