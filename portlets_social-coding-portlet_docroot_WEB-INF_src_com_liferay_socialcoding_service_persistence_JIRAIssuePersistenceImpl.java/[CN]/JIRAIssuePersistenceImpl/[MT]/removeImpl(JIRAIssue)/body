{
  jiraIssue=toUnwrappedModel(jiraIssue);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,jiraIssue);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(jiraIssue);
  return jiraIssue;
}
