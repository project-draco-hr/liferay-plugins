{
  JIRAIssue jiraIssue=(JIRAIssue)EntityCacheUtil.getResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssueId,this);
  if (jiraIssue == _nullJIRAIssue) {
    return null;
  }
  if (jiraIssue == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      jiraIssue=(JIRAIssue)session.get(JIRAIssueImpl.class,Long.valueOf(jiraIssueId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (jiraIssue != null) {
        cacheResult(jiraIssue);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssueId,_nullJIRAIssue);
      }
      closeSession(session);
    }
  }
  return jiraIssue;
}
