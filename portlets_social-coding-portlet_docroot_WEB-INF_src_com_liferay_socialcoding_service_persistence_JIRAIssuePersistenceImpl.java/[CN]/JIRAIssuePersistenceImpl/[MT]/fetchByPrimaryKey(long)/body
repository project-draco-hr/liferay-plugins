{
  JIRAIssue jiraIssue=(JIRAIssue)EntityCacheUtil.getResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssueId);
  if (jiraIssue == _nullJIRAIssue) {
    return null;
  }
  if (jiraIssue == null) {
    Session session=null;
    try {
      session=openSession();
      jiraIssue=(JIRAIssue)session.get(JIRAIssueImpl.class,Long.valueOf(jiraIssueId));
      if (jiraIssue != null) {
        cacheResult(jiraIssue);
      }
 else {
        EntityCacheUtil.putResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssueId,_nullJIRAIssue);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssueId);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return jiraIssue;
}
