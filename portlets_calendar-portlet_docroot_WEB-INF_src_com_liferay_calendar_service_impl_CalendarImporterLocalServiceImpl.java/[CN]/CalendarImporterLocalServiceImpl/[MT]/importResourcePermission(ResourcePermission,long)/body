{
  CalendarBooking calendarBooking=calendarBookingPersistence.findByPrimaryKey(calendarBookingId);
  long actionIds=getActionIds(resourcePermission);
  resourceBlockLocalService.updateIndividualScopePermissions(calendarBooking.getCompanyId(),calendarBooking.getResourceGroupId(),CalendarBooking.class.getName(),calendarBooking,resourcePermission.getRoleId(),actionIds,ResourceBlockConstants.OPERATOR_SET);
}
