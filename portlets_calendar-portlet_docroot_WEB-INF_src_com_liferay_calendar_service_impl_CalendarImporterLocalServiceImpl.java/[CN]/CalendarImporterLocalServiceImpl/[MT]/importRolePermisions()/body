{
  long[] companyIds=PortalUtil.getCompanyIds();
  String[][] className={new String[]{"com.liferay.portlet.calendar","com.liferay.calendar.model.Calendar"},new String[]{"com.liferay.portlet.calendar.model.CalEvent","com.liferay.calendar.model.CalendarBooking"}};
  for (int i=0; i < className.length; i++) {
    for (int j=0; j < companyIds.length; j++) {
      importResourcePermissions(className[i][0],className[i][1],companyIds[j],ResourceConstants.SCOPE_COMPANY);
      importResourcePermissions(className[i][0],className[i][1],companyIds[j],ResourceConstants.SCOPE_GROUP_TEMPLATE);
      importResourcePermissions(className[i][0],className[i][1],companyIds[j],ResourceConstants.SCOPE_GROUP);
    }
  }
}
