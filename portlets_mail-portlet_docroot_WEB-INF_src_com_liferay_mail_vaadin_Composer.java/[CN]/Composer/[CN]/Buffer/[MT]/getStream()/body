{
  if (file == null) {
    return null;
  }
  try {
    return new FileInputStream(file);
  }
 catch (  final FileNotFoundException e) {
    getWindow().showNotification(Lang.get("unable-to-attach-files"),Notification.TYPE_ERROR_MESSAGE);
    _log.warn("Unable to attach files",e);
  }
  return null;
}
