{
  List<String> ids=new ArrayList<String>();
switch (idSpec.getType()) {
case OWNER:
    ids.add(token.getOwnerId());
  break;
case VIEWER:
ids.add(token.getViewerId());
break;
case OWNER_FRIENDS:
break;
case VIEWER_FRIENDS:
break;
case USER_IDS:
ids.addAll(idSpec.fetchUserIds());
break;
}
return ids;
}
