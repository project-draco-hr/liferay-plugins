{
  try {
    String url=_URL + twitterScreenName + ".json";
    if (sinceId > 0) {
      url+="?since_id=" + sinceId;
    }
    return JSONFactoryUtil.createJSONArray(HttpUtil.URLtoString(url));
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e,e);
    }
    return null;
  }
}
