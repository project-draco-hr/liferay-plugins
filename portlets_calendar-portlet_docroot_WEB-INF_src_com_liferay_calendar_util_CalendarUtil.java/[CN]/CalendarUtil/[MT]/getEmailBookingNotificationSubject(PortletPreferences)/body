{
  String emailBookingNotificationSubject=preferences.getValue("emailBookingNotificationSubject",StringPool.BLANK);
  if (Validator.isNotNull(emailBookingNotificationSubject)) {
    return emailBookingNotificationSubject;
  }
 else {
    return ContentUtil.get(PortletProps.get(PortletPropsKeys.CALENDAR_EMAIL_BOOKING_NOTIFICATION_SUBJECT));
  }
}
