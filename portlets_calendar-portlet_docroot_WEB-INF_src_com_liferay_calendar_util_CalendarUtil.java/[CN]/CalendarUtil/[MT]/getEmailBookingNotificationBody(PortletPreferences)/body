{
  String emailBookingNotificationBody=preferences.getValue("emailBookingNotificationBody",StringPool.BLANK);
  if (Validator.isNotNull(emailBookingNotificationBody)) {
    return emailBookingNotificationBody;
  }
 else {
    return ContentUtil.get(PortletProps.get(PortletPropsKeys.CALENDAR_EMAIL_BOOKING_NOTIFICATION_BODY));
  }
}
