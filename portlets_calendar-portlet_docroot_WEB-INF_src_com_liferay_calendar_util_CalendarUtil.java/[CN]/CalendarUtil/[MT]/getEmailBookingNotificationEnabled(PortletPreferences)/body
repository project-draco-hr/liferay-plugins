{
  String emailBookingNotificationEnabled=preferences.getValue("emailBookingNotificationEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailBookingNotificationEnabled)) {
    return GetterUtil.getBoolean(emailBookingNotificationEnabled);
  }
 else {
    return PortletPropsValues.CALENDAR_EMAIL_BOOKING_NOTIFICATION_ENABLED;
  }
}
