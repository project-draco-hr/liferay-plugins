{
  String emailBookingReminderBody=preferences.getValue("emailBookingReminderBody",StringPool.BLANK);
  if (Validator.isNotNull(emailBookingReminderBody)) {
    return emailBookingReminderBody;
  }
 else {
    return ContentUtil.get(PortletProps.get(PortletPropsKeys.CALENDAR_EMAIL_BOOKING_REMINDER_BODY));
  }
}
