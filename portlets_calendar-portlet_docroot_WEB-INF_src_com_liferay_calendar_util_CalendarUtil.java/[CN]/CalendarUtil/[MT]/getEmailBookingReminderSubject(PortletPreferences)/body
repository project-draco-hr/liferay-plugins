{
  String emailBookingReminderSubject=preferences.getValue("emailBookingReminderSubject",StringPool.BLANK);
  if (Validator.isNotNull(emailBookingReminderSubject)) {
    return emailBookingReminderSubject;
  }
 else {
    return ContentUtil.get(PortletProps.get(PortletPropsKeys.CALENDAR_EMAIL_BOOKING_REMINDER_SUBJECT));
  }
}
