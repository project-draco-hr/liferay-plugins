{
  _key=key;
  if (_jiraIssueRemoteModel != null) {
    try {
      Class<?> clazz=_jiraIssueRemoteModel.getClass();
      Method method=clazz.getMethod("setKey",String.class);
      method.invoke(_jiraIssueRemoteModel,key);
    }
