{
  String id=ParamUtil.getString(req,"id");
  String callback=ParamUtil.getString(req,"callback");
  JSONObject jsonObj=new JSONObject();
  jsonObj.put("id",id);
  ServletContext ctx=getServletContext();
  String script=StringUtil.read(ctx.getResourceAsStream("/click_image.js"));
  String jsonObjString=jsonObj.toString();
  jsonObjString=jsonObjString.substring(0,jsonObjString.length() - 1) + "," + script+ "}";
  return callback + "(" + jsonObjString+ ");";
}
