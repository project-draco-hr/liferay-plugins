{
  if (_persistence == null) {
    _persistence=(HRWageTypePersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRWageTypePersistence.class.getName());
    ReferenceRegistry.registerReference(HRWageTypeUtil.class,"_persistence");
  }
  return _persistence;
}
