{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),AttachmentLocalService.class.getName());
    _service=new AttachmentLocalServiceClp(invokableLocalService);
    ReferenceRegistry.registerReference(AttachmentLocalServiceUtil.class,"_service");
    MethodCache.remove(AttachmentLocalService.class);
  }
  return _service;
}
