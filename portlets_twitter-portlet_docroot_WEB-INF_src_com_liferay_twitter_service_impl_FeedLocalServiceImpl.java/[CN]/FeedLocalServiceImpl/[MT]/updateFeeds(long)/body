{
  ShardUtil.pushCompanyService(companyId);
  try {
    LinkedHashMap userParams=new LinkedHashMap();
    userParams.put("contactTwitterSn",Boolean.TRUE);
    List<User> users=userLocalService.search(companyId,null,0,userParams,QueryUtil.ALL_POS,QueryUtil.ALL_POS,(OrderByComparator)null);
    for (    User user : users) {
      try {
        updateFeed(user);
      }
 catch (      Exception e) {
        if (_log.isWarnEnabled()) {
          _log.warn(e,e);
        }
      }
    }
  }
  finally {
    ShardUtil.popCompanyService();
  }
}
