{
  Session session=null;
  try {
    session=openSession();
    AssetType assetType=(AssetType)session.get(AssetTypeImpl.class,new Long(assetTypeId));
    if (assetType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No AssetType exists with the primary key " + assetTypeId);
      }
      throw new NoSuchAssetTypeException("No AssetType exists with the primary key " + assetTypeId);
    }
    return remove(assetType);
  }
 catch (  NoSuchAssetTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
