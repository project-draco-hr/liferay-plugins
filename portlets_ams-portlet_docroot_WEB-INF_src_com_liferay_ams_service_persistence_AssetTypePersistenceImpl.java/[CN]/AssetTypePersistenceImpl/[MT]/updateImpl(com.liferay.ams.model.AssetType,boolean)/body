{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,assetType,merge);
    assetType.setNew(false);
    return assetType;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(AssetType.class.getName());
  }
}
