{
  JIRAIssue jiraIssue=findByPrimaryKey(jiraIssueId);
  Session session=null;
  try {
    session=openSession();
    JIRAIssue[] array=new JIRAIssueImpl[3];
    array[0]=getByProjectId_PrevAndNext(session,jiraIssue,projectId,orderByComparator,true);
    array[1]=jiraIssue;
    array[2]=getByProjectId_PrevAndNext(session,jiraIssue,projectId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
