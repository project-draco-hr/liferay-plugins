{
  JIRAIssue jiraIssue=findByPrimaryKey(jiraIssueId);
  Session session=null;
  try {
    session=openSession();
    JIRAIssue[] array=new JIRAIssueImpl[3];
    array[0]=getByMD_P_AJUI_PrevAndNext(session,jiraIssue,modifiedDate,projectId,assigneeJiraUserId,orderByComparator,true);
    array[1]=jiraIssue;
    array[2]=getByMD_P_AJUI_PrevAndNext(session,jiraIssue,modifiedDate,projectId,assigneeJiraUserId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
