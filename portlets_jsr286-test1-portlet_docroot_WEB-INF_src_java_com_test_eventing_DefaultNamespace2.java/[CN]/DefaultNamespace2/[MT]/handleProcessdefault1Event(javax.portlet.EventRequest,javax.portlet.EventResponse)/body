{
  javax.portlet.Event event=request.getEvent();
  java.lang.String value=(java.lang.String)event.getValue();
  response.setRenderParameter("key",value);
  response.setRenderParameter("localpart",event.getQName().getLocalPart());
  response.setRenderParameter("namespaceURI",event.getQName().getNamespaceURI());
}
