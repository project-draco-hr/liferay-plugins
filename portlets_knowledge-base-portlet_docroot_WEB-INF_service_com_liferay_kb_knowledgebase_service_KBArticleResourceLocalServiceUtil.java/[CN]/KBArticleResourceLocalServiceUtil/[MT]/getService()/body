{
  if (_service == null) {
    Object obj=PortletBeanLocatorUtil.locate("knowledge-base-portlet",KBArticleResourceLocalServiceUtil.class.getName());
    ClassLoader portletClassLoader=(ClassLoader)PortletBeanLocatorUtil.locate("chat-portlet","portletClassLoader");
    ClassLoaderProxy classLoaderProxy=new ClassLoaderProxy(obj,portletClassLoader);
    _service=new KBArticleResourceLocalServiceClp(classLoaderProxy);
  }
  return _service;
}
