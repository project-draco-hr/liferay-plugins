{
  if (loggedInUserId.equals(new Long(0)) || !userPresenceLocalService.isUserOnline(userId) || userId.equals(loggedInUserId)) {
    return "";
  }
  try {
    UserPresence pUser=userPresenceLocalService.getUserPresence(userId);
    if (pUser != null && _isChatPortletDeployed()) {
      return ("<a onclick=\"Liferay.Chat.openChatWindow('" + userId + "');\" href=\"javascript: ;\">"+ "Chat"+ "</a>");
    }
    return "";
  }
 catch (  Exception e) {
    throw new RUONException(e);
  }
}
