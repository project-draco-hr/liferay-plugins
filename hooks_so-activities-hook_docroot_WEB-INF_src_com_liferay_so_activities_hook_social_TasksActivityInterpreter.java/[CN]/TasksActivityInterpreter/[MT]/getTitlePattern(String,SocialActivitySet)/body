{
  if (activitySet.getType() == SocialActivityKeyConstants.TASKS_ADD_ENTRY) {
    return "created-x-new-tasks";
  }
 else   if (activitySet.getType() == SocialActivityKeyConstants.TASKS_REOPEN_ENTRY) {
    return "reopened-x-tasks";
  }
 else   if (activitySet.getType() == SocialActivityKeyConstants.TASKS_RESOLVE_ENTRY) {
    return "resolved-x-tasks";
  }
 else   if (activitySet.getType() == SocialActivityKeyConstants.TASKS_UPDATE_ENTRY) {
    return "made-x-updates-to-a-task";
  }
  return StringPool.BLANK;
}
