{
  KaleoTimer kaleoTimer=(KaleoTimer)EntityCacheUtil.getResult(KaleoTimerModelImpl.ENTITY_CACHE_ENABLED,KaleoTimerImpl.class,kaleoTimerId,this);
  if (kaleoTimer == null) {
    Session session=null;
    try {
      session=openSession();
      kaleoTimer=(KaleoTimer)session.get(KaleoTimerImpl.class,Long.valueOf(kaleoTimerId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (kaleoTimer != null) {
        cacheResult(kaleoTimer);
      }
      closeSession(session);
    }
  }
  return kaleoTimer;
}
