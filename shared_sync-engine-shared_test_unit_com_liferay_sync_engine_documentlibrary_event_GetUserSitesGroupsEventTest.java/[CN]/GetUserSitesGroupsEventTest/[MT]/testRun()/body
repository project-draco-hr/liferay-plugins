{
  setPostResponse("dependencies/get_user_sites_groups.json");
  GetUserSitesGroupsEvent getUserSitesGroupsEvent=new GetUserSitesGroupsEvent(syncAccount.getSyncAccountId(),null);
  getUserSitesGroupsEvent.run();
  _syncSites=SyncSiteService.findSyncSites(syncAccount.getSyncAccountId());
  Assert.assertEquals(2,_syncSites.size());
}
