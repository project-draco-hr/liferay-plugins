{
  setMockPostResponse("dependencies/get_user_sites_groups.json");
  GetUserSitesGroupsEvent getUserSitesGroupsEvent=new GetUserSitesGroupsEvent(_syncAccount.getSyncAccountId(),null);
  getUserSitesGroupsEvent.run();
