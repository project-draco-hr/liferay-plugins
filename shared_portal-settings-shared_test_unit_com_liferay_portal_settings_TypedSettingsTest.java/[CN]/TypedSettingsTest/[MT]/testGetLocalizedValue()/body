{
  LocalizedValue localizedValue=_typedSettings.getLocalizedValue(_KEY);
  Assert.assertEquals("value_es_ES",localizedValue.getDefaultValue());
  Assert.assertEquals("value_es_ES",localizedValue.get(new Locale("es","ES")));
  Assert.assertEquals("value_en_US",localizedValue.get(new Locale("en","US")));
  Assert.assertEquals("value_en_GB",localizedValue.get(new Locale("en","GB")));
}
