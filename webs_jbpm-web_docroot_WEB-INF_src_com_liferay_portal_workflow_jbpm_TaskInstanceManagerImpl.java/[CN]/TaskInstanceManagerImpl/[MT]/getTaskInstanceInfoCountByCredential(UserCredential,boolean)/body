{
  long[] roleIds=ArrayUtil.toArray(userCredential.getRoleIds().toArray(new Long[0]));
  int pooledActorCount=getTaskInstanceInfoCount(roleIds,true,completed);
  int singleActorCount=getTaskInstanceInfoCount(new long[]{userCredential.getUserId()},false,completed);
  return pooledActorCount + singleActorCount;
}
