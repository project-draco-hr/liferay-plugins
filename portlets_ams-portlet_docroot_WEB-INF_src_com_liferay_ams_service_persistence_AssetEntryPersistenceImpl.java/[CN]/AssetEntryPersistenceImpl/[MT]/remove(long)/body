{
  Session session=null;
  try {
    session=openSession();
    AssetEntry assetEntry=(AssetEntry)session.get(AssetEntryImpl.class,new Long(assetEntryId));
    if (assetEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No AssetEntry exists with the primary key " + assetEntryId);
      }
      throw new NoSuchAssetEntryException("No AssetEntry exists with the primary key " + assetEntryId);
    }
    return remove(assetEntry);
  }
 catch (  NoSuchAssetEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
