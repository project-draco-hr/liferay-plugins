{
  long layoutSetPrototypeClassNameId=PortalUtil.getClassNameId(LayoutSetPrototype.class.getName());
  int count=GroupLocalServiceUtil.searchCount(companyId,new long[]{layoutSetPrototypeClassNameId},name,StringPool.BLANK,null);
  if (count > 0) {
    return true;
  }
 else {
    return false;
  }
}
