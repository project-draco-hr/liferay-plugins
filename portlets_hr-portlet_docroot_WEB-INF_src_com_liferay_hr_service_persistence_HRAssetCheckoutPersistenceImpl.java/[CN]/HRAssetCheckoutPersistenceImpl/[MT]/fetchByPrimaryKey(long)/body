{
  HRAssetCheckout hrAssetCheckout=(HRAssetCheckout)EntityCacheUtil.getResult(HRAssetCheckoutModelImpl.ENTITY_CACHE_ENABLED,HRAssetCheckoutImpl.class,hrAssetCheckoutId);
  if (hrAssetCheckout == _nullHRAssetCheckout) {
    return null;
  }
  if (hrAssetCheckout == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrAssetCheckout=(HRAssetCheckout)session.get(HRAssetCheckoutImpl.class,Long.valueOf(hrAssetCheckoutId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrAssetCheckout != null) {
        cacheResult(hrAssetCheckout);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRAssetCheckoutModelImpl.ENTITY_CACHE_ENABLED,HRAssetCheckoutImpl.class,hrAssetCheckoutId,_nullHRAssetCheckout);
      }
      closeSession(session);
    }
  }
  return hrAssetCheckout;
}
