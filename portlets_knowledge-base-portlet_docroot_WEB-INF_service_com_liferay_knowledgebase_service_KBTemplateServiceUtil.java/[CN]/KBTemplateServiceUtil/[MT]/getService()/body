{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KBTemplateService.class.getName());
    _service=new KBTemplateServiceClp(invokableService);
    ReferenceRegistry.registerReference(KBTemplateServiceUtil.class,"_service");
    MethodCache.remove(KBTemplateService.class);
  }
  return _service;
}
