{
  List<Customer> allCustomers=(List<Customer>)PortletSessionUtil.getSharedSessionAttribute(PortletSessionUtil.ATTR_CUSTOMER_LIST);
  if (allCustomers == null) {
    allCustomers=getCustomerService().getAllCustomers();
    PortletSessionUtil.setSharedSessionAttribute(PortletSessionUtil.ATTR_CUSTOMER_LIST,allCustomers);
  }
  return allCustomers;
}
