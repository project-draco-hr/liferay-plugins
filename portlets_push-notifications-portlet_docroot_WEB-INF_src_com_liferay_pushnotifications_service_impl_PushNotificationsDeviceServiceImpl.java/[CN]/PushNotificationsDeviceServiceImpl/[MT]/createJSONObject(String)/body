{
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject();
  jsonObject.put(PushNotificationsConstants.PAYLOAD,JSONFactoryUtil.createJSONObject(payload));
  JSONObject fromUserJSONObject=JSONFactoryUtil.createJSONObject();
  fromUserJSONObject.put(PushNotificationsConstants.USER_ID,getUserId());
  jsonObject.put(PushNotificationsConstants.FROM_USER,fromUserJSONObject);
  return jsonObject;
}
