{
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject();
  jsonObject.put(PushNotificationsConstants.KEY_PAYLOAD,JSONFactoryUtil.createJSONObject(payload));
  JSONObject fromUserJSONObject=JSONFactoryUtil.createJSONObject();
  fromUserJSONObject.put(PushNotificationsConstants.KEY_USER_ID,getUserId());
  jsonObject.put(PushNotificationsConstants.KEY_FROM_USER,fromUserJSONObject);
  return jsonObject;
}
