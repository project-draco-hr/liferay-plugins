{
  PushNotificationsPermission.check(getPermissionChecker(),ActionKeys.SEND_NOTIFICATION);
  JSONObject jsonObject=createJSONObject(message);
  if (_log.isDebugEnabled()) {
    _log.debug("Sending message " + jsonObject + " to user "+ toUserId);
  }
  pushNotificationsDeviceLocalService.sendPushNotification(toUserId,jsonObject,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
}
