{
  String url=StringUtil.replace(_DELETE_FOLDER_TEMPLATE,new String[]{"$SERVER_URL","$REPOSITORY_ID","$FOLDER_ID"},new String[]{AppPropsValues.SERVER_URL,AppPropsValues.REPOSITORY_ID,Long.toString(folderData.getId())});
  AsyncHandler<Response> asyncHandler=new DeleteFolderHandler(folderData);
  _executeRequest(url,"POST",asyncHandler);
}
