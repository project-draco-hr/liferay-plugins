{
  if (_persistence == null) {
    _persistence=(BBBMeetingPersistence)PortletBeanLocatorUtil.locate(com.liferay.bbb.service.ClpSerializer.getServletContextName(),BBBMeetingPersistence.class.getName());
    ReferenceRegistry.registerReference(BBBMeetingUtil.class,"_persistence");
  }
  return _persistence;
}
