{
  KBArticle article1=(KBArticle)obj1;
  KBArticle article2=(KBArticle)obj2;
  int value=0;
  if (article1.getVersion() < article2.getVersion()) {
    value=-1;
  }
 else   if (article1.getVersion() > article2.getVersion()) {
    value=1;
  }
  if (_asc) {
    return value;
  }
 else {
    return -value;
  }
}
