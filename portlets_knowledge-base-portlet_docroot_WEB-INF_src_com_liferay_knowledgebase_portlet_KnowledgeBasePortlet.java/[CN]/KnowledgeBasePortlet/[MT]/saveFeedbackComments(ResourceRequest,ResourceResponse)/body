{
  long articleResourcePrimKey=ParamUtil.getLong(resourceRequest,"articleResourcePrimKey");
  long userId=ParamUtil.getLong(resourceRequest,"userId");
  String comments=ParamUtil.getString(resourceRequest,"comments");
  KBFeedbackEntryLocalServiceUtil.updateComments(articleResourcePrimKey,userId,comments);
  Map<String,String> parameterMap=new HashMap<String,String>();
  sendJSON(resourceResponse,parameterMap);
}
