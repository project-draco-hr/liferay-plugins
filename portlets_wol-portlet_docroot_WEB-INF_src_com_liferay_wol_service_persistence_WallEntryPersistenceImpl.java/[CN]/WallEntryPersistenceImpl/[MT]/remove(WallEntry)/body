{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(wallEntry);
    }
  }
  wallEntry=removeImpl(wallEntry);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(wallEntry);
    }
  }
  return wallEntry;
}
