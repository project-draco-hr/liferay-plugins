{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(wallEntry);
    }
 else {
      if (wallEntry.isNew()) {
        session.save(wallEntry);
      }
    }
    session.flush();
    wallEntry.setNew(false);
    return wallEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(WallEntry.class.getName());
  }
}
