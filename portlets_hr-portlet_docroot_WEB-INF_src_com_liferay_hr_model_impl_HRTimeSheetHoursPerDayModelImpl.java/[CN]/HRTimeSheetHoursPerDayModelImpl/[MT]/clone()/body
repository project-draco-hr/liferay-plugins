  hrTimeSheetHoursPerDayImpl.setHrOfficeId(getHrOfficeId());
  hrTimeSheetHoursPerDayImpl.setHoursPerDay(getHoursPerDay());
  hrTimeSheetHoursPerDayImpl.resetOriginalValues();
