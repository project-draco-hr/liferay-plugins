{
  Map<String,List<String>> suggestions=new HashMap<String,List<String>>();
  String localizedFieldName=DocumentImpl.getLocalizedName(searchContext.getLanguageId(),Field.SPELL_CHECK_WORD);
  List<String> keywordTokens=TokenizerUtil.tokenize(localizedFieldName,searchContext.getKeywords(),searchContext.getLanguageId());
  for (  String keywordToken : keywordTokens) {
    List<String> keywordSuggestions=suggestKeywords(searchContext,maxSuggestions,keywordToken);
    suggestions.put(keywordToken,keywordSuggestions);
  }
  return suggestions;
}
