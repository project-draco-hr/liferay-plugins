{
  if (_log.isInfoEnabled()) {
    _log.info("Starting to sync server with root " + AppPropsValues.ROOT_FOLDER);
  }
  if (_log.isInfoEnabled()) {
    _log.info("Starting initial sync");
  }
  Folder folderData=new Folder();
  folderData.setFolderId(0);
  RemoteFileSystemUtil.bootstrapFolders(folderData,new BootstrapFolderListener());
}
