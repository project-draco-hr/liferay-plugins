{
  _createDate=createDate;
  if (_kbCommentRemoteModel != null) {
    try {
      Class<?> clazz=_kbCommentRemoteModel.getClass();
      Method method=clazz.getMethod("setCreateDate",Date.class);
      method.invoke(_kbCommentRemoteModel,createDate);
    }
