{
  _userRating=userRating;
  if (_kbCommentRemoteModel != null) {
    try {
      Class<?> clazz=_kbCommentRemoteModel.getClass();
      Method method=clazz.getMethod("setUserRating",int.class);
      method.invoke(_kbCommentRemoteModel,userRating);
    }
