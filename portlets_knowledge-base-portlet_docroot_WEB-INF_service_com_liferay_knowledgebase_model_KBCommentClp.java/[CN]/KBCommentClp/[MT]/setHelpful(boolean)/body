{
  _helpful=helpful;
  if (_kbCommentRemoteModel != null) {
    try {
      Class<?> clazz=_kbCommentRemoteModel.getClass();
      Method method=clazz.getMethod("setHelpful",boolean.class);
      method.invoke(_kbCommentRemoteModel,helpful);
    }
