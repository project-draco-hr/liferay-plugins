{
  _uuid=uuid;
  if (_kbCommentRemoteModel != null) {
    try {
      Class<?> clazz=_kbCommentRemoteModel.getClass();
      Method method=clazz.getMethod("setUuid",String.class);
      method.invoke(_kbCommentRemoteModel,uuid);
    }
