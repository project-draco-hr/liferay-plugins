{
  if (obj == null) {
    return false;
  }
  KBCommentClp kbComment=null;
  try {
    kbComment=(KBCommentClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=kbComment.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
