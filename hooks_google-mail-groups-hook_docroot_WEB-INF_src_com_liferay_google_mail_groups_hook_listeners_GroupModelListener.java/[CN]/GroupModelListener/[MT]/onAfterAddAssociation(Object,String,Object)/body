{
  try {
    Group group=GroupLocalServiceUtil.getGroup((Long)classPK);
    List<User> users=getUsers(classPK,associationClassName,associationClassPK);
    MessageBusUtil.sendMessage(DestinationNames.ASYNC_SERVICE,new OnAssociationProcessCallable(group,users,"addGroupMembers"));
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
