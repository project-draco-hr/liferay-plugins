{
  try {
    new OnAssociation(classPK,associationClassName,associationClassPK){
      @Override public void onAssociation(      User user,      Group group) throws Exception {
        GoogleMailGroupsUtil.addGGroupMember(GoogleMailGroupsUtil.getGroupEmailAddress(group),GoogleMailGroupsUtil.getUserEmailAddress(user));
      }
    }
;
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
