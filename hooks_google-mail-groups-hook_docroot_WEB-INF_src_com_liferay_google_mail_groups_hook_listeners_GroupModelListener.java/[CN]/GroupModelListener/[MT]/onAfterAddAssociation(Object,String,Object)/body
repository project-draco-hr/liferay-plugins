{
  try {
    new OnAssociation(classPK,associationClassName,associationClassPK){
      @Override public void onAssociation(      User user,      Group group) throws Exception {
        GoogleDirectoryUtil.addGroupMember(GoogleMailGroupsUtil.getGroupEmailAddress(group),GoogleMailGroupsUtil.getUserEmailAddress(user));
        GoogleMailGroupsUtil.checkLargeGroup(group);
      }
    }
;
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
