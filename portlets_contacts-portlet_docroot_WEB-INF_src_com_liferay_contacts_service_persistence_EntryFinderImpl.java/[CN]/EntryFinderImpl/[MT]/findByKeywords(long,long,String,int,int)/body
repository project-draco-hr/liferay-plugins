{
  String[] fullNames=null;
  String[] emailAddresses=null;
  boolean andOperator=false;
  if (Validator.isNotNull(keywords)) {
    fullNames=CustomSQLUtil.keywords(keywords);
    emailAddresses=CustomSQLUtil.keywords(keywords);
  }
 else {
    andOperator=true;
  }
  fullNames=CustomSQLUtil.keywords(fullNames);
  emailAddresses=CustomSQLUtil.keywords(emailAddresses);
  return findByC_U_FN_EA(companyId,userId,fullNames,emailAddresses,andOperator,start,end);
}
