{
  if (obj == null) {
    return false;
  }
  HRProjectRoleClp hrProjectRole=null;
  try {
    hrProjectRole=(HRProjectRoleClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrProjectRole.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
