{
  Path filePath=Paths.get(syncWatchEvent.getFilePathName());
  SyncFile syncFile=SyncFileService.fetchSyncFileByFileKey(FileUtil.getFileKey(filePath),_syncAccountId);
  if ((syncFile == null) || !FileUtil.hasFileChanged(syncFile)) {
    return;
  }
  SyncFileService.updateFileSyncFile(filePath,_syncAccountId,syncFile,false);
}
