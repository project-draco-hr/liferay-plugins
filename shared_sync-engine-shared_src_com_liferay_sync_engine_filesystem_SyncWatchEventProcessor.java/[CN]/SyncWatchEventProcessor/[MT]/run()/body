{
  if (_logger.isTraceEnabled()) {
    _logger.trace("Processing sync watch events");
  }
  List<SyncWatchEvent> syncWatchEvents=SyncWatchEventService.findAll();
  for (  SyncWatchEvent syncWatchEvent : syncWatchEvents) {
    if (_logger.isDebugEnabled()) {
      _logger.debug("Event file path {} file type {} kind {} timestamp {}",syncWatchEvent.getFilePath(),syncWatchEvent.getFileType(),syncWatchEvent.getKind(),syncWatchEvent.getTimestamp());
    }
    SyncWatchEventService.deleteSyncWatchEvent(syncWatchEvent.getSyncWatchEventId());
  }
}
