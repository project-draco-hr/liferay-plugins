{
  if (_persistence == null) {
    _persistence=(PollsQuestionPersistence)PortletBeanLocatorUtil.locate(com.liferay.polls.service.ClpSerializer.getServletContextName(),PollsQuestionPersistence.class.getName());
    ReferenceRegistry.registerReference(PollsQuestionUtil.class,"_persistence");
  }
  return _persistence;
}
