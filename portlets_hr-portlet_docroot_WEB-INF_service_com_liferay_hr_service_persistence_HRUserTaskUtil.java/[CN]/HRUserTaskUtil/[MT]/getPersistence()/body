{
  if (_persistence == null) {
    _persistence=(HRUserTaskPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRUserTaskPersistence.class.getName());
    ReferenceRegistry.registerReference(HRUserTaskUtil.class,"_persistence");
  }
  return _persistence;
}
