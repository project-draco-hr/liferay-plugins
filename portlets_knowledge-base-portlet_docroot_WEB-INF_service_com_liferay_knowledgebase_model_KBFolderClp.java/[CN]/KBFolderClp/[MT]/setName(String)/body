{
  _name=name;
  if (_kbFolderRemoteModel != null) {
    try {
      Class<?> clazz=_kbFolderRemoteModel.getClass();
      Method method=clazz.getMethod("setName",String.class);
      method.invoke(_kbFolderRemoteModel,name);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
