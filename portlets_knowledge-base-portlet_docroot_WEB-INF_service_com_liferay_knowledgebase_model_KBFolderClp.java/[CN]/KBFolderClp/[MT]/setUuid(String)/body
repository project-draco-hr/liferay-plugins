{
  _uuid=uuid;
  if (_kbFolderRemoteModel != null) {
    try {
      Class<?> clazz=_kbFolderRemoteModel.getClass();
      Method method=clazz.getMethod("setUuid",String.class);
      method.invoke(_kbFolderRemoteModel,uuid);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
