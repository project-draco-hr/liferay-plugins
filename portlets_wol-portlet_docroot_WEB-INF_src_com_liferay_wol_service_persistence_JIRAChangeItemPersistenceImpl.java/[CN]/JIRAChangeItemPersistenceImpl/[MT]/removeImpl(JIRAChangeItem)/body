{
  Session session=null;
  try {
    session=openSession();
    session.delete(jiraChangeItem);
    session.flush();
    return jiraChangeItem;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(JIRAChangeItem.class.getName());
  }
}
