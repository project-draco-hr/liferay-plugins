{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(jiraChangeItem);
    }
  }
  jiraChangeItem=removeImpl(jiraChangeItem);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(jiraChangeItem);
    }
  }
  return jiraChangeItem;
}
