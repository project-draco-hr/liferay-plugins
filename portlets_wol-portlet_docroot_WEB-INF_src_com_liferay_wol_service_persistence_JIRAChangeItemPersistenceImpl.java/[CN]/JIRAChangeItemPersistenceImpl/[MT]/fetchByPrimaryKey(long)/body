{
  JIRAChangeItem jiraChangeItem=(JIRAChangeItem)EntityCacheUtil.getResult(JIRAChangeItemModelImpl.ENTITY_CACHE_ENABLED,JIRAChangeItemImpl.class,jiraChangeItemId,this);
  if (jiraChangeItem == null) {
    Session session=null;
    try {
      session=openSession();
      jiraChangeItem=(JIRAChangeItem)session.get(JIRAChangeItemImpl.class,new Long(jiraChangeItemId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (jiraChangeItem != null) {
        cacheResult(jiraChangeItem);
      }
      closeSession(session);
    }
  }
  return jiraChangeItem;
}
