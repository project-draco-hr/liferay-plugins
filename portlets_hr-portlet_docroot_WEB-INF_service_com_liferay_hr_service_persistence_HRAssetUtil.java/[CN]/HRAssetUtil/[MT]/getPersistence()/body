{
  if (_persistence == null) {
    _persistence=(HRAssetPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRAssetPersistence.class.getName());
    ReferenceRegistry.registerReference(HRAssetUtil.class,"_persistence");
  }
  return _persistence;
}
