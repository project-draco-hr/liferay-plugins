{
  WSRPConsumer wsrpConsumer=(WSRPConsumer)EntityCacheUtil.getResult(WSRPConsumerModelImpl.ENTITY_CACHE_ENABLED,WSRPConsumerImpl.class,wsrpConsumerId,this);
  if (wsrpConsumer == null) {
    Session session=null;
    try {
      session=openSession();
      wsrpConsumer=(WSRPConsumer)session.get(WSRPConsumerImpl.class,new Long(wsrpConsumerId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (wsrpConsumer != null) {
        cacheResult(wsrpConsumer);
      }
      closeSession(session);
    }
  }
  return wsrpConsumer;
}
