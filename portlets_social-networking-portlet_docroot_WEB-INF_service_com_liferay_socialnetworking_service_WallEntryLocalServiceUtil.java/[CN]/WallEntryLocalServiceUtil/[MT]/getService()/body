{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),WallEntryLocalService.class.getName());
    _service=new WallEntryLocalServiceClp(invokableLocalService);
    ReferenceRegistry.registerReference(WallEntryLocalServiceUtil.class,"_service");
    MethodCache.remove(WallEntryLocalService.class);
  }
  return _service;
}
