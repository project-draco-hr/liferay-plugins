{
  KaleoInstanceToken kaleoInstanceToken=executionContext.getKaleoInstanceToken();
  List<KaleoTimerInstanceToken> kaleoTimerInstanceTokens=KaleoTimerInstanceTokenLocalServiceUtil.getKaleoTimerInstanceTokens(kaleoInstanceToken.getKaleoInstanceId());
  for (  KaleoTimerInstanceToken kaleoTimerInstanceToken : kaleoTimerInstanceTokens) {
    if (!kaleoTimerInstanceToken.isCompleted()) {
      return true;
    }
  }
  return false;
}
