{
  KaleoInstanceToken kaleoInstanceToken=executionContext.getKaleoInstanceToken();
  int kaleoTimerInstanceTokensCount=KaleoTimerInstanceTokenLocalServiceUtil.getKaleoTimerInstanceTokensCount(kaleoInstanceToken.getKaleoInstanceId(),false,executionContext.getServiceContext());
  if (kaleoTimerInstanceTokensCount > 0) {
    return true;
  }
  return false;
}
