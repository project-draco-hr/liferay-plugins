{
  KaleoInstanceToken kaleoInstanceToken=executionContext.getKaleoInstanceToken();
  int count=KaleoTimerInstanceTokenLocalServiceUtil.getKaleoTimerInstanceTokensCount(kaleoInstanceToken.getKaleoInstanceId(),false,executionContext.getServiceContext());
  if (count > 0) {
    return true;
  }
  return false;
}
