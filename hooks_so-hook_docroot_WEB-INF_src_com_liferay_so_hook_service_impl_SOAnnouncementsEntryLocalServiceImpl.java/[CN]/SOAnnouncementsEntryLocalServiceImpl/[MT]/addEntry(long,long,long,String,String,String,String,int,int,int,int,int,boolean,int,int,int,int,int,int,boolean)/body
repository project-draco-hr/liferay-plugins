{
  AnnouncementsEntry announcementEntry=super.addEntry(userId,classNameId,classPK,title,content,url,type,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,expirationDateMonth,expirationDateDay,expirationDateYear,expirationDateHour,expirationDateMinute,priority,alert);
  if (announcementEntry != null) {
    Date displayDate=announcementEntry.getDisplayDate();
    if (!displayDate.after(announcementEntry.getCreateDate())) {
      sendNotificationEvent(announcementEntry);
    }
  }
  return announcementEntry;
}
