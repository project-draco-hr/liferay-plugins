{
  AnnouncementsEntry announcementEntry=super.addEntry(plid,classNameId,classPK,title,content,url,type,displayDateMonth,displayDateDay,displayDateYear,displayDateHour,displayDateMinute,expirationDateMonth,expirationDateDay,expirationDateYear,expirationDateHour,expirationDateMinute,priority,alert);
  if (announcementEntry != null) {
    sendNotificationEvent(announcementEntry);
  }
  return announcementEntry;
}
