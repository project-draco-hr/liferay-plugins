{
  HRTimeOffTypeImpl hrTimeOffTypeImpl=new HRTimeOffTypeImpl();
  hrTimeOffTypeImpl.setHrTimeOffTypeId(getHrTimeOffTypeId());
  hrTimeOffTypeImpl.setGroupId(getGroupId());
  hrTimeOffTypeImpl.setCompanyId(getCompanyId());
  hrTimeOffTypeImpl.setUserId(getUserId());
  hrTimeOffTypeImpl.setUserName(getUserName());
  hrTimeOffTypeImpl.setCreateDate(getCreateDate());
  hrTimeOffTypeImpl.setModifiedDate(getModifiedDate());
  hrTimeOffTypeImpl.setName(getName());
  hrTimeOffTypeImpl.setDescription(getDescription());
  hrTimeOffTypeImpl.resetOriginalValues();
  return hrTimeOffTypeImpl;
}
