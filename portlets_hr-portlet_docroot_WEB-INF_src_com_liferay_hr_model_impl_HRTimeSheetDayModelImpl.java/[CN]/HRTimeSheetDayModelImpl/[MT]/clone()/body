{
  HRTimeSheetDayImpl hrTimeSheetDayImpl=new HRTimeSheetDayImpl();
  hrTimeSheetDayImpl.setHrTimeSheetDayId(getHrTimeSheetDayId());
  hrTimeSheetDayImpl.setGroupId(getGroupId());
  hrTimeSheetDayImpl.setCompanyId(getCompanyId());
  hrTimeSheetDayImpl.setUserId(getUserId());
  hrTimeSheetDayImpl.setUserName(getUserName());
  hrTimeSheetDayImpl.setCreateDate(getCreateDate());
  hrTimeSheetDayImpl.setModifiedDate(getModifiedDate());
  hrTimeSheetDayImpl.setHrTimeSheetId(getHrTimeSheetId());
  hrTimeSheetDayImpl.setHrUserId(getHrUserId());
  hrTimeSheetDayImpl.setDayOfYear(getDayOfYear());
  hrTimeSheetDayImpl.setYear(getYear());
  hrTimeSheetDayImpl.setHours(getHours());
  hrTimeSheetDayImpl.resetOriginalValues();
  return hrTimeSheetDayImpl;
}
