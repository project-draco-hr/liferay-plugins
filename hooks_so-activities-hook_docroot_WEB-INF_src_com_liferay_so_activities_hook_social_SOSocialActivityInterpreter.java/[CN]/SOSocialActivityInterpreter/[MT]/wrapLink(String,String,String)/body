{
  StringBundler sb=new StringBundler(5);
  sb.append("<a href=\"");
  sb.append(link);
  sb.append("\">");
  sb.append("<span><img class=\"icon\" src=\"");
  sb.append(iconPath);
  sb.append("\"></span><span>");
  sb.append(text);
  sb.append("</span>");
  sb.append("</a>");
  return sb.toString();
}
