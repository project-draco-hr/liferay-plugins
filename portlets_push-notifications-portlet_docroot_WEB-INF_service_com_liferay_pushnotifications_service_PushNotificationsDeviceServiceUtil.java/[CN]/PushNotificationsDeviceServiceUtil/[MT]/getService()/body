{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),PushNotificationsDeviceService.class.getName());
    if (invokableService instanceof PushNotificationsDeviceService) {
      _service=(PushNotificationsDeviceService)invokableService;
    }
 else {
      _service=new PushNotificationsDeviceServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(PushNotificationsDeviceServiceUtil.class,"_service");
  }
  return _service;
}
