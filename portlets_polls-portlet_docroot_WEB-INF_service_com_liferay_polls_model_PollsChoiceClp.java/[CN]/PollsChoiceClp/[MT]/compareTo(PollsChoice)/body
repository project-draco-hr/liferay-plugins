{
  int value=0;
  if (getPollsQuestionId() < pollsChoice.getPollsQuestionId()) {
    value=-1;
  }
 else   if (getPollsQuestionId() > pollsChoice.getPollsQuestionId()) {
    value=1;
  }
 else {
    value=0;
  }
  if (value != 0) {
    return value;
  }
  value=getName().compareTo(pollsChoice.getName());
  if (value != 0) {
    return value;
  }
  return 0;
}
