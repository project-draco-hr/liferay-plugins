{
  HRTerminationType hrTerminationType=fetchByPrimaryKey(hrTerminationTypeId);
  if (hrTerminationType == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTerminationTypeId);
    }
    throw new NoSuchTerminationTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTerminationTypeId);
  }
  return hrTerminationType;
}
