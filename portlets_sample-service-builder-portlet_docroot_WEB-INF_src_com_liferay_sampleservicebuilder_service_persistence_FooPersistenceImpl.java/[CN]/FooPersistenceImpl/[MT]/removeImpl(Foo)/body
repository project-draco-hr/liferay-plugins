{
  foo=toUnwrappedModel(foo);
  Session session=null;
  try {
    session=openSession();
    if (foo.isCachedModel()) {
      foo=(Foo)session.get(FooImpl.class,foo.getPrimaryKeyObj());
    }
    if (foo != null) {
      session.delete(foo);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (foo != null) {
    clearCache(foo);
  }
  return foo;
}
