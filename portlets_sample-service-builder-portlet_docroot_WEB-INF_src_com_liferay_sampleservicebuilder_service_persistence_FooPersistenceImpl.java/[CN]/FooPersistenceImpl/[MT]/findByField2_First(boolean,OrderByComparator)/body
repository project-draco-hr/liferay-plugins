{
  List<Foo> list=findByField2(field2,0,1,obc);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("field2=");
    msg.append(field2);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchFooException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
