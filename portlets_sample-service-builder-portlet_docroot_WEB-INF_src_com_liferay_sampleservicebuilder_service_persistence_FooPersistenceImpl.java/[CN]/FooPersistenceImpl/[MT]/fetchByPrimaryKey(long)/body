{
  Foo foo=(Foo)EntityCacheUtil.getResult(FooModelImpl.ENTITY_CACHE_ENABLED,FooImpl.class,fooId,this);
  if (foo == null) {
    Session session=null;
    try {
      session=openSession();
      foo=(Foo)session.get(FooImpl.class,new Long(fooId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (foo != null) {
        cacheResult(foo);
      }
      closeSession(session);
    }
  }
  return foo;
}
