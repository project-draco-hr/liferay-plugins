{
  LayoutSetLocalServiceUtil.updateLookAndFeel(group.getGroupId(),"so_WAR_sotheme","01","",false);
  Layout layout=addLayout(group,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID,"Home","2_columns_ii");
  addPortlets(group,layout,"/home");
  updatePermissions(layout,false);
  addApplications(group,layout.getLayoutId());
  layout=addLayout(group,LayoutConstants.DEFAULT_PARENT_LAYOUT_ID,"Profile","2_columns_ii");
  addPortlets(group,layout,"/profile");
  updatePermissions(layout,true);
}
