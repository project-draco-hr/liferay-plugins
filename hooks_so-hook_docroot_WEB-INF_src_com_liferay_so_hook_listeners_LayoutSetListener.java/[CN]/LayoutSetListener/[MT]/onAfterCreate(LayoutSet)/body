{
  try {
    Group group=GroupLocalServiceUtil.getGroup(layoutSet.getGroupId());
    if (!group.isUser()) {
      return;
    }
    if (layoutSet.isPrivateLayout()) {
      addPrivateUserLayouts(group);
    }
 else {
      addPublicUserLayouts(group);
    }
  }
 catch (  Exception e) {
    throw new ModelListenerException(e);
  }
}
