{
  try {
    clearSemanticsElements.clear(pk);
    for (    String semanticsElementPK : semanticsElementPKs) {
      addSemanticsElement.add(pk,semanticsElementPK);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    FinderCacheUtil.clearCache("Posts_SemanticsElements");
  }
}
