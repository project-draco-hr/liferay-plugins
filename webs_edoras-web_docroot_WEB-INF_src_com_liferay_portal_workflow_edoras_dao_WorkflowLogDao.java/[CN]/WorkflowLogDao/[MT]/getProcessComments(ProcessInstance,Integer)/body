{
  try {
    List<WorkflowLog> workflowLogs=null;
    if (commentType == null) {
      workflowLogs=WorkflowLogUtil.findByW_T(processInstance.getPrimaryKey(),ProcessLogType.COMMENT.getId());
    }
 else {
      workflowLogs=WorkflowLogUtil.findByW_T_T(processInstance.getPrimaryKey(),ProcessLogType.COMMENT.getId(),commentType.intValue());
    }
    return (List<? extends CommentLog>)WorkflowEntityBridgeUtil.wrapWorkflowLogs(workflowLogs);
  }
 catch (  Exception e) {
    throw new ProcessException(e.getMessage(),e);
  }
}
