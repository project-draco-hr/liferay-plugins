{
  try {
    Document document=execute(bbbServer,BBBConstants.API_METHOD_GET_MEETINGS,StringPool.BLANK);
    Element element=document.getRootElement();
    String returnCode=getText(element,BBBConstants.API_RESPONSE_RETURN_CODE);
    if (returnCode.equals(BBBConstants.API_RESPONSE_SUCCESS)) {
      return true;
    }
  }
 catch (  Exception e) {
  }
  return false;
}
