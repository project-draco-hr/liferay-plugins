{
  if (_persistence == null) {
    _persistence=(KaleoTaskAssignmentPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoTaskAssignmentPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoTaskAssignmentUtil.class,"_persistence");
  }
  return _persistence;
}
