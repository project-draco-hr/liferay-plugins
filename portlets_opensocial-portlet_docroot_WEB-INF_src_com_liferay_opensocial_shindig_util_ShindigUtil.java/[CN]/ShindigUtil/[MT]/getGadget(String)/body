{
  int pos=portletName.indexOf(StringPool.UNDERLINE,GadgetPortlet.PORTLET_NAME_PREFIX.length());
  String uuid=GetterUtil.getString(portletName.substring(pos + 1));
  uuid=PortalUUIDUtil.fromJsSafeUuid(uuid);
  com.liferay.opensocial.model.Gadget gadget=GadgetLocalServiceUtil.getGadget(uuid);
  return gadget;
}
