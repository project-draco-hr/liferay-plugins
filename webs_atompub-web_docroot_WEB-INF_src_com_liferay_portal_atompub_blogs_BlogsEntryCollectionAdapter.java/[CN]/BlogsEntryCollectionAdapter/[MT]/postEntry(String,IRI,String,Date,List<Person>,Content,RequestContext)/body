{
  long groupId=GetterUtil.getLong(requestContext.getParameter("groupId"),0L);
  long userId=GetterUtil.getLong(requestContext.getParameter("userId"),0L);
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setScopeGroupId(groupId);
  try {
    return BlogsEntryLocalServiceUtil.addEntry(userId,title,content.getText(),date.getMonth(),date.getDay(),date.getYear(),date.getHours(),date.getMinutes(),false,true,new String[0],serviceContext);
  }
 catch (  Exception e) {
    throw new ResponseContextException(0,e);
  }
}
