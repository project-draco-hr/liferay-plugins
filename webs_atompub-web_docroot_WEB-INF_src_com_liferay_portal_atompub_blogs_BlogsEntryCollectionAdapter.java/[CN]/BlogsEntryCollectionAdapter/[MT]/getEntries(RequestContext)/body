{
  try {
    long groupId=GetterUtil.getLong(requestContext.getParameter("groupId"),0L);
    long companyId=GetterUtil.getLong(requestContext.getParameter("companyId"),0L);
    int start=GetterUtil.getInteger(requestContext.getParameter("start"),0);
    int end=GetterUtil.getInteger(requestContext.getParameter("end"),10);
    if (groupId > 0) {
      return BlogsEntryLocalServiceUtil.getGroupEntries(groupId,start,end);
    }
 else     if (companyId > 0) {
      return BlogsEntryLocalServiceUtil.getCompanyEntries(companyId,start,end);
    }
 else {
      return BlogsEntryLocalServiceUtil.getBlogsEntries(start,end);
    }
  }
 catch (  SystemException se) {
    throw new ResponseContextException(0,se);
  }
}
