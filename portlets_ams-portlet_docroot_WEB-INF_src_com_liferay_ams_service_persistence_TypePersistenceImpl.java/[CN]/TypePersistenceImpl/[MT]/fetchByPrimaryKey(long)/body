{
  Type type=(Type)EntityCacheUtil.getResult(TypeModelImpl.ENTITY_CACHE_ENABLED,TypeImpl.class,typeId,this);
  if (type == null) {
    Session session=null;
    try {
      session=openSession();
      type=(Type)session.get(TypeImpl.class,new Long(typeId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (type != null) {
        cacheResult(type);
      }
      closeSession(session);
    }
  }
  return type;
}
