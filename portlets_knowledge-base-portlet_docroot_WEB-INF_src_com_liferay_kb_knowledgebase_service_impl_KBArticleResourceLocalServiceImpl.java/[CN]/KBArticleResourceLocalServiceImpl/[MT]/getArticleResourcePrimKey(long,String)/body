{
  KBArticleResource articleResource=kbArticleResourcePersistence.fetchByG_T(groupId,title);
  if (articleResource == null) {
    long articleResourcePrimKey=CounterLocalServiceUtil.increment();
    articleResource=kbArticleResourcePersistence.create(articleResourcePrimKey);
    articleResource.setGroupId(groupId);
    articleResource.setTitle(title);
    kbArticleResourcePersistence.update(articleResource,false);
  }
  return articleResource.getResourcePrimKey();
}
