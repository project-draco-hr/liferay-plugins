{
  Session session=null;
  try {
    session=openSession();
    OAuthConsumer oAuthConsumer=(OAuthConsumer)session.get(OAuthConsumerImpl.class,new Long(oAuthConsumerId));
    if (oAuthConsumer == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + oAuthConsumerId);
      }
      throw new NoSuchOAuthConsumerException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + oAuthConsumerId);
    }
    return oAuthConsumerPersistence.remove(oAuthConsumer);
  }
 catch (  NoSuchOAuthConsumerException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
