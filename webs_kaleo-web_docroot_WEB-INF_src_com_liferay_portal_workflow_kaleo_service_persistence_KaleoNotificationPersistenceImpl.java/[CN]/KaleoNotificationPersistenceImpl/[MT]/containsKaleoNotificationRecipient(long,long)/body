{
  Object[] finderArgs=new Object[]{pk,kaleoNotificationRecipientPK};
  Boolean value=(Boolean)FinderCacheUtil.getResult(FINDER_PATH_CONTAINS_KALEONOTIFICATIONRECIPIENT,finderArgs,this);
  if (value == null) {
