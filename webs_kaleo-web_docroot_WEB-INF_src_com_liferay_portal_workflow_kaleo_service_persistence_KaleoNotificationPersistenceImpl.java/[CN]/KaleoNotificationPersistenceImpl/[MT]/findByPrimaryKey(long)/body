{
  KaleoNotification kaleoNotification=fetchByPrimaryKey(kaleoNotificationId);
  if (kaleoNotification == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoNotificationId);
    }
    throw new NoSuchNotificationException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoNotificationId);
  }
  return kaleoNotification;
}
