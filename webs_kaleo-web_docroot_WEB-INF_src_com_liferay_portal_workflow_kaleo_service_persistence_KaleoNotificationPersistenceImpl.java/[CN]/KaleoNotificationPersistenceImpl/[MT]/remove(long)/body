{
  Session session=null;
  try {
    session=openSession();
    KaleoNotification kaleoNotification=(KaleoNotification)session.get(KaleoNotificationImpl.class,new Long(kaleoNotificationId));
    if (kaleoNotification == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoNotificationId);
      }
      throw new NoSuchNotificationException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoNotificationId);
    }
    return remove(kaleoNotification);
  }
 catch (  NoSuchNotificationException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
