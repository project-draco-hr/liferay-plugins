{
  Session session=null;
  try {
    session=openSession();
    session.delete(svnRepository);
    session.flush();
    return svnRepository;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(SVNRepository.class.getName());
  }
}
