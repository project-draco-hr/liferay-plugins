{
  DynamicQuery dynamicQuery=getUserCredentialDynamicQuery(userCredential,true);
  dynamicQuery.add(RestrictionsFactoryUtil.eq("completed",completed));
  try {
    List<Object> count=WorkflowTaskUtil.findWithDynamicQuery(dynamicQuery);
    return (Integer)count.get(0);
  }
 catch (  Exception e) {
    throw new WorkflowException(e.getMessage(),e);
  }
}
