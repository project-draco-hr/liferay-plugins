{
  DynamicQuery dynamicQuery=getUserCredentialDynamicQuery(userCredential,true);
  addOrder(dynamicQuery,orderByComparator);
  try {
    List workflowTasks=WorkflowTaskUtil.findWithDynamicQuery(dynamicQuery,start,end);
    return WorkflowManagerUtil.wrapWorkflowTasks(workflowTasks);
  }
 catch (  Exception e) {
    throw new WorkflowException(e.getMessage(),e);
  }
}
