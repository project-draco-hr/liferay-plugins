{
  long fileEntryId=ParamUtil.getLong(resourceRequest,"fileEntryId");
  FileEntry fileEntry=DLAppServiceUtil.getFileEntry(fileEntryId);
  String fileEntryTitle=ParamUtil.getString(resourceRequest,"fileEntryTitle");
  byte[] bytes=null;
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setCreateDate(fileEntry.getCreateDate());
  serviceContext.setModifiedDate(fileEntry.getModifiedDate());
  DLAppServiceUtil.updateFileEntry(fileEntryId,fileEntryTitle,fileEntryTitle,resourceRequest.getContentType(),fileEntry.getDescription(),StringPool.BLANK,false,bytes,serviceContext);
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject();
  writeJSON(resourceRequest,resourceResponse,jsonObject.toString());
}
