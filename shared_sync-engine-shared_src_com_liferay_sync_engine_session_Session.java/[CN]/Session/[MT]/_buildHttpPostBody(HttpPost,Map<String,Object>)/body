{
  MultipartEntityBuilder multipartEntityBuilder=_getMultipartEntityBuilder(parameters);
  if (parameters.get("filePath") != null) {
    multipartEntityBuilder.addPart("file",_getFileBody((Path)parameters.get("filePath"),(String)parameters.get("mimeType"),(String)parameters.get("title")));
  }
  httpPost.setEntity(multipartEntityBuilder.build());
}
