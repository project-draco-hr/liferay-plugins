{
  CmdLineParser parser=new CmdLineParser();
  CmdLineParser.Option propertiesFileNameOption=parser.addStringOption("properties.file.name");
  CmdLineParser.Option imageNameOption=parser.addStringOption("image.name");
  parser.parse(args);
  try {
    new AsgardAMIDeployer((String)parser.getOptionValue(propertiesFileNameOption),(String)parser.getOptionValue(imageNameOption));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
