{
  super(propertiesFileName);
  _imageName=imageName;
  _jsonWebServiceClient=getJSONWebServiceClient(properties.getProperty("asgard.host.name"),Integer.valueOf(properties.getProperty("asgard.host.port")));
  String autoScalingGroupName=createAutoScalingGroup();
  if (autoScalingGroupName != null) {
    checkAutoScalingGroup(autoScalingGroupName);
  }
}
