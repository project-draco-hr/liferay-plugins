{
  if (obj == null) {
    return false;
  }
  HRAssetProduct hrAssetProduct=null;
  try {
    hrAssetProduct=(HRAssetProduct)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrAssetProduct.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
