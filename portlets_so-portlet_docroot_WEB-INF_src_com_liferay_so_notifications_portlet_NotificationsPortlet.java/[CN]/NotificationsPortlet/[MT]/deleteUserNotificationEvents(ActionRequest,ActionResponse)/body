{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String[] userNotificationEventUuids=StringUtil.split(ParamUtil.getString(actionRequest,"userNotificationEventUuids"));
  for (  String userNotificationEventUuid : userNotificationEventUuids) {
    ChannelHubManagerUtil.deleteUserNotificiationEvent(themeDisplay.getCompanyId(),themeDisplay.getUserId(),userNotificationEventUuid);
  }
}
