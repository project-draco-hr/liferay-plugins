{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),AssetSharingEntryService.class.getName());
    if (invokableService instanceof AssetSharingEntryService) {
      _service=(AssetSharingEntryService)invokableService;
    }
 else {
      _service=new AssetSharingEntryServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(AssetSharingEntryServiceUtil.class,"_service");
  }
  return _service;
}
