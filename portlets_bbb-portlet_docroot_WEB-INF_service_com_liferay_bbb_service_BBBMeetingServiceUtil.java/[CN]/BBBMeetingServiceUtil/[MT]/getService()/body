{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),BBBMeetingService.class.getName());
    if (invokableService instanceof BBBMeetingService) {
      _service=(BBBMeetingService)invokableService;
    }
 else {
      _service=new BBBMeetingServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(BBBMeetingServiceUtil.class,"_service");
  }
  return _service;
}
