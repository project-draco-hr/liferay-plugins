{
  portletDataContext.addPortletPermissions(RESOURCE_NAME);
  long recordSetId=GetterUtil.getLong(portletPreferences.getValue("recordSetId",null));
  if (recordSetId == 0) {
    return portletPreferences;
  }
  DDLRecordSet recordSet=DDLRecordSetLocalServiceUtil.getRecordSet(recordSetId);
  StagedModelDataHandlerUtil.exportReferenceStagedModel(portletDataContext,portletId,recordSet);
  return portletPreferences;
}
