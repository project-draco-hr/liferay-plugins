{
  portletDataContext.addPermissions("com.liferay.portlet.dynamicdatalist",portletDataContext.getScopeGroupId());
  long recordSetId=GetterUtil.getLong(portletPreferences.getValue("recordSetId",null));
  if (recordSetId == 0) {
    return StringPool.BLANK;
  }
  Element rootElement=addExportRootElement();
  DDLRecordSet recordSet=DDLRecordSetLocalServiceUtil.getRecordSet(recordSetId);
  StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,rootElement,recordSet);
  return rootElement.formattedString();
}
