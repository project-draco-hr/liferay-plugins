{
  portletDataContext.addPermissions(RESOURCE_NAME,portletDataContext.getScopeGroupId());
  long recordSetId=GetterUtil.getLong(portletPreferences.getValue("recordSetId",null));
  if (recordSetId == 0) {
    return StringPool.BLANK;
  }
  DDLRecordSet recordSet=DDLRecordSetLocalServiceUtil.getRecordSet(recordSetId);
  StagedModelDataHandlerUtil.exportStagedModel(portletDataContext,recordSet);
  Element rootElement=addExportDataRootElement(portletDataContext);
  return getExportDataRootElementString(rootElement);
}
