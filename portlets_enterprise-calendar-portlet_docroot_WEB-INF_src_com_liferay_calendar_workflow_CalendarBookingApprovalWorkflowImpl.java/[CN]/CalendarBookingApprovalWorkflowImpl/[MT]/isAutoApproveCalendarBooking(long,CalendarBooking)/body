{
  if (calendarBooking.getStatus() == CalendarBookingWorkflowConstants.STATUS_DENIED) {
    return false;
  }
  if (userId != calendarBooking.getUserId()) {
    return false;
  }
  CalendarResource calendarResource=calendarBooking.getCalendarResource();
  if (userId != calendarResource.getUserId()) {
    return false;
  }
  return true;
}
