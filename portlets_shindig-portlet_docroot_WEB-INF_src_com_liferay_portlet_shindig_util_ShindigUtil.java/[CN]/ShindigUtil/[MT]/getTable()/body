{
  ExpandoTable expandoTable=null;
  try {
    expandoTable=ExpandoTableLocalServiceUtil.getTable(User.class.getName(),ShindigUtil.OPEN_SOCIAL_DATA);
  }
 catch (  Exception e) {
  }
  if (expandoTable == null) {
    try {
      expandoTable=ExpandoTableLocalServiceUtil.addTable(User.class.getName(),ShindigUtil.OPEN_SOCIAL_DATA);
    }
 catch (    Exception e) {
    }
  }
  return expandoTable;
}
