{
  if (array == null) {
    return null;
  }
  List<MediaItem> items=new ArrayList<MediaItem>();
  for (int i=0; i < array.length(); i++) {
    try {
      JSONObject jsonItem=array.getJSONObject(i);
      MediaItem item=new MediaItem(jsonItem.getString("mimeType"),Type.valueOf(jsonItem.getString("type")),jsonItem.getString("url"));
      items.add(item);
    }
 catch (    JSONException je) {
    }
  }
  return items;
}
