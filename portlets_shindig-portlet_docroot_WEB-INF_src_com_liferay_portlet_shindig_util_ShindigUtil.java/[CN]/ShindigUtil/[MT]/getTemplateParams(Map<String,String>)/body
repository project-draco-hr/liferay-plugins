{
  if (map == null) {
    return null;
  }
  JSONArray items=new JSONArray();
  for (  Entry<String,String> entry : map.entrySet()) {
    try {
      JSONObject jsonEntry=new JSONObject();
      jsonEntry.put(entry.getKey(),entry.getValue());
      items.put(jsonEntry);
    }
 catch (    JSONException je) {
    }
  }
  return items;
}
