{
  if (key == null || key.length() == 0) {
    return false;
  }
  for (int i=0; i < key.length(); ++i) {
    char c=key.charAt(i);
    if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z') || (c >= '0' && c <= '9')|| (c == '-')|| (c == '_')|| (c == '.')) {
      continue;
    }
    return false;
  }
  return true;
}
