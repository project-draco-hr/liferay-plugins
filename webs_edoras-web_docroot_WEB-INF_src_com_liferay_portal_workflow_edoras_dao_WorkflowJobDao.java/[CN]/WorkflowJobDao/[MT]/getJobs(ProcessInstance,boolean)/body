{
  try {
    List<WorkflowJob> jobList=WorkflowJobUtil.findByWorkflowInstanceId(instance.getPrimaryKey());
    return WorkflowEntityBridgeUtil.wrapWorkflowJobList(jobList);
  }
 catch (  SystemException se) {
    throw new ProcessException("Could not load open jobs related to workflow instance with id [" + instance.getPrimaryKey() + "]",se);
  }
}
