{
  SyncAccount syncAccount=SyncAccountService.fetchSyncAccount(syncAccountId);
  URL url=new URL(syncAccount.getUrl());
  HttpHost httpHost=new HttpHost(url.getHost(),url.getPort(),url.getProtocol());
  HttpGet httpGet=new HttpGet(urlPath);
  HttpClient httpClient=_getHttpClient(syncAccount,url);
  return httpClient.execute(httpHost,httpGet,new BaseHandler(),_getBasicHttpContext(httpHost));
}
