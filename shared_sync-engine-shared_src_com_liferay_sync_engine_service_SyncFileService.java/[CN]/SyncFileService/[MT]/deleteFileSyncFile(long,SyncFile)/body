{
  syncFile.setUiEvent(SyncFile.UI_EVENT_DELETED_LOCAL);
  deleteSyncFile(syncFile);
  if (syncFile.getState() == SyncFile.STATE_DELETED) {
    return syncFile;
  }
  Map<String,Object> parameters=new HashMap<String,Object>();
  parameters.put("fileEntryId",syncFile.getTypePK());
  MoveFileEntryToTrashEvent moveFileEntryToTrashEvent=new MoveFileEntryToTrashEvent(syncAccountId,parameters);
  moveFileEntryToTrashEvent.run();
  return syncFile;
}
