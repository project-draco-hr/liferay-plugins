{
  Map<String,String> headers=serviceContext.getHeaders();
  if (headers == null) {
    return false;
  }
  String userAgent=headers.get(HttpHeaders.USER_AGENT);
  if (Validator.isNull(userAgent)) {
    return false;
  }
  String userIP=serviceContext.getRemoteAddr();
  if (Validator.isNull(userIP)) {
    return false;
  }
  return true;
}
