{
  if (_finder == null) {
    _finder=(KaleoDefinitionFinder)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.getServletContextName(),KaleoDefinitionFinder.class.getName());
    ReferenceRegistry.registerReference(KaleoDefinitionFinderUtil.class,"_finder");
  }
  return _finder;
}
