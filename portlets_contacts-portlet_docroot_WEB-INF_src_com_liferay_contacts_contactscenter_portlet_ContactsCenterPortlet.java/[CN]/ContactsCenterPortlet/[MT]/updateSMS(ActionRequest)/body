{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  User user=themeDisplay.getUser();
  Contact contact=themeDisplay.getContact();
  String smsSn=ParamUtil.getString(actionRequest,"smsSn");
  updateProfile(actionRequest,contact.getAimSn(),user.getComments(),contact.getFacebookSn(),contact.getIcqSn(),contact.getJabberSn(),contact.getMsnSn(),contact.getMySpaceSn(),contact.getSkypeSn(),smsSn,contact.getTwitterSn(),contact.getYmSn());
}
