{
  if (obj == null) {
    return false;
  }
  HRProjectRole hrProjectRole=null;
  try {
    hrProjectRole=(HRProjectRole)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrProjectRole.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
