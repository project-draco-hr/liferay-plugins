{
  Object[] finderArgs=new Object[]{new Long(fromUserId)};
  List<Entry> list=(List<Entry>)FinderCacheUtil.getResult(FINDER_PATH_FIND_BY_FROMUSERID,finderArgs,this);
  if (list == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT entry FROM Entry entry WHERE ");
      query.append("entry.fromUserId = ?");
      query.append(" ");
      query.append("ORDER BY ");
      query.append("entry.createDate DESC");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(fromUserId);
      list=q.list();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (list == null) {
        list=new ArrayList<Entry>();
      }
      cacheResult(list);
      FinderCacheUtil.putResult(FINDER_PATH_FIND_BY_FROMUSERID,finderArgs,list);
      closeSession(session);
    }
  }
  return list;
}
