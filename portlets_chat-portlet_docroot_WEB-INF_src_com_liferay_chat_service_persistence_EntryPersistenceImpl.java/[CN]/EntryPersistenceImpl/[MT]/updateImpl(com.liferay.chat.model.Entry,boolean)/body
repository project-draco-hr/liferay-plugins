{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,entry,merge);
    entry.setNew(false);
    return entry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Entry.class.getName());
  }
}
