{
  Object[] finderArgs=new Object[]{new Long(createDate),new Long(toUserId)};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_C_T,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBuilder query=new StringBuilder();
      query.append("SELECT COUNT(entry) ");
      query.append("FROM Entry entry WHERE ");
      query.append("entry.createDate = ?");
      query.append(" AND ");
      query.append("entry.toUserId = ?");
      query.append(" ");
      Query q=session.createQuery(query.toString());
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(createDate);
      qPos.add(toUserId);
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_T,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
