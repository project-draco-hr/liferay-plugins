{
  Entry entry=(Entry)EntityCacheUtil.getResult(EntryModelImpl.ENTITY_CACHE_ENABLED,EntryImpl.class,entryId,this);
  if (entry == null) {
    Session session=null;
    try {
      session=openSession();
      entry=(Entry)session.get(EntryImpl.class,new Long(entryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (entry != null) {
        cacheResult(entry);
      }
      closeSession(session);
    }
  }
  return entry;
}
