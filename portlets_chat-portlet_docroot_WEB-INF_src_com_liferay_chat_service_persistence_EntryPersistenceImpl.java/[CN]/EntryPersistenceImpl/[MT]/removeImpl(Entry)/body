{
  Session session=null;
  try {
    session=openSession();
    session.delete(entry);
    session.flush();
    return entry;
  }
 catch (  Exception e) {
    throw HibernateUtil.processException(e);
  }
 finally {
    closeSession(session);
    FinderCache.clearCache(Entry.class.getName());
  }
}
