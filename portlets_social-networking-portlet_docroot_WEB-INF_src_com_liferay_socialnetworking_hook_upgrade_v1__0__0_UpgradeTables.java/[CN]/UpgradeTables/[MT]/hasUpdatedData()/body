{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  for (  String tableName : _tableNames) {
    try {
      con=DataAccess.getConnection();
      ps=con.prepareStatement("select * from " + tableName);
      rs=ps.executeQuery();
      if (rs.next()) {
        _log.info("Existing data in table " + tableName + "SN info from WOL won't be updated.");
        return true;
      }
    }
 catch (    Exception e) {
    }
 finally {
      DataAccess.cleanUp(con,ps,rs);
    }
  }
  return false;
}
