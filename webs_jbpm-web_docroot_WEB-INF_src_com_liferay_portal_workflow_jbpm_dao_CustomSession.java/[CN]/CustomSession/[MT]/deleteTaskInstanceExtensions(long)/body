{
  List<TaskInstance> taskInstances=findTaskInstances(processInstanceId,1,null,false,null,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  for (  TaskInstance taskInstance : taskInstances) {
    TaskInstanceExtensionImpl taskInstanceExtensionImpl=findTaskInstanceExtension(taskInstance.getId());
    _session.delete(taskInstanceExtensionImpl);
  }
}
