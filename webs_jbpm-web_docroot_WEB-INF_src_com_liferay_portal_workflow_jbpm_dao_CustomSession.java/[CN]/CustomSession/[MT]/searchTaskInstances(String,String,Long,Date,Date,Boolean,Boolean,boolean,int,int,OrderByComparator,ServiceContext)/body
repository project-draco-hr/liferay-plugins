{
  try {
    Criteria criteria=buildTaskInstanceExtensionSearchCriteria(taskName,assetType,assetPrimaryKey,dueDateGT,dueDateLT,completed,searchByUserRoles,andOperator,serviceContext);
    addPagination(criteria,start,end);
    addOrder(criteria,orderByComparator);
    return toTaskIntances(criteria.list());
  }
 catch (  Exception e) {
    throw new JbpmException(e);
  }
}
