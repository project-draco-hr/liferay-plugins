{
  Session session=null;
  try {
    session=openSession();
    WorkflowJob workflowJob=(WorkflowJob)session.get(WorkflowJobImpl.class,new Long(workflowJobId));
    if (workflowJob == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No WorkflowJob exists with the primary key " + workflowJobId);
      }
      throw new NoSuchWorkflowJobException("No WorkflowJob exists with the primary key " + workflowJobId);
    }
    return remove(workflowJob);
  }
 catch (  NoSuchWorkflowJobException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
