{
  WorkflowJob workflowJob=(WorkflowJob)EntityCacheUtil.getResult(WorkflowJobModelImpl.ENTITY_CACHE_ENABLED,WorkflowJobImpl.class,workflowJobId,this);
  if (workflowJob == null) {
    Session session=null;
    try {
      session=openSession();
      workflowJob=(WorkflowJob)session.get(WorkflowJobImpl.class,new Long(workflowJobId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (workflowJob != null) {
        cacheResult(workflowJob);
      }
      closeSession(session);
    }
  }
  return workflowJob;
}
