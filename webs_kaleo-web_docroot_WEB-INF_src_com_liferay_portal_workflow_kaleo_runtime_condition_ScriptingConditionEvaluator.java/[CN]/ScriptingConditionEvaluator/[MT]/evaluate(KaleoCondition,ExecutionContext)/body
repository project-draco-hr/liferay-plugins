{
  Map<String,Object> inputObjects=ScriptingContextBuilder.buildScriptingContext(executionContext);
  Map<String,Object> returnResults=ScriptingUtil.eval(null,inputObjects,CONDITION_RETURN_PARAMTERS,kaleoCondition.getScriptLanguage(),kaleoCondition.getScript());
  Boolean conditionValue=(Boolean)returnResults.get(RETURN_PARAMETER_NAME);
  if (conditionValue == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("Conditional script did not return value: " + conditionValue + ": "+ kaleoCondition.getScript());
    }
    return false;
  }
  return conditionValue;
}
