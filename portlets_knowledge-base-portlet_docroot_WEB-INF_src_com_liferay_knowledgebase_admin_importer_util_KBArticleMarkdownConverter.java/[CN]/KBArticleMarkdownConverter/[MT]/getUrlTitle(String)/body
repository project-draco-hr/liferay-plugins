{
  String urlTitle=null;
  int idIndex=heading.indexOf("[](id=");
  int closeParentheses=heading.indexOf(StringPool.CLOSE_PARENTHESIS,idIndex);
  if (closeParentheses > (idIndex + 1)) {
    int equalsSign=heading.indexOf(StringPool.EQUAL,idIndex);
    urlTitle=heading.substring(equalsSign + 1,closeParentheses);
    urlTitle=StringUtil.replace(urlTitle,StringPool.SPACE,StringPool.DASH);
    urlTitle=StringUtil.toLowerCase(urlTitle);
  }
  return urlTitle;
}
