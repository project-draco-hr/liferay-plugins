{
  List<Article> articles=getSiblingArticles(groupId,parentResourcePrimKey,WorkflowConstants.STATUS_ANY,0,1,new ArticlePriorityComparator());
  if (articles.isEmpty()) {
    return ArticleConstants.DEFAULT_PRIORITY;
  }
  Article article=articles.get(0);
  return Math.floor(article.getPriority()) + 1;
}
