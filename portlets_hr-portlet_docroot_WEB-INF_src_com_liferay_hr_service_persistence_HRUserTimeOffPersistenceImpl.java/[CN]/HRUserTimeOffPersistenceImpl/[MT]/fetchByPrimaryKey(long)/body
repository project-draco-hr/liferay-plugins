{
  HRUserTimeOff hrUserTimeOff=(HRUserTimeOff)EntityCacheUtil.getResult(HRUserTimeOffModelImpl.ENTITY_CACHE_ENABLED,HRUserTimeOffImpl.class,hrUserTimeOffId,this);
  if (hrUserTimeOff == null) {
    Session session=null;
    try {
      session=openSession();
      hrUserTimeOff=(HRUserTimeOff)session.get(HRUserTimeOffImpl.class,Long.valueOf(hrUserTimeOffId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrUserTimeOff != null) {
        cacheResult(hrUserTimeOff);
      }
      closeSession(session);
    }
  }
  return hrUserTimeOff;
}
