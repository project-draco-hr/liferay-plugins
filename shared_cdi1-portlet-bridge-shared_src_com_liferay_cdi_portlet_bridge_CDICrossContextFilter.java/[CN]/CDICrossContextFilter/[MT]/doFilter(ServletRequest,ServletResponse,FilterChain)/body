{
  HttpServletRequest httpServletRequest=(HttpServletRequest)servletRequest;
  CDIRequestFactory cdiRequestFactory=(CDIRequestFactory)CDIUtil.getManagedBeanReference(CDIRequestFactory.class);
  CDICrossContextRequest cdiCrossContextRequest=cdiRequestFactory.getCDICrossContextRequest(httpServletRequest);
  filterChain.doFilter(cdiCrossContextRequest,servletResponse);
}
