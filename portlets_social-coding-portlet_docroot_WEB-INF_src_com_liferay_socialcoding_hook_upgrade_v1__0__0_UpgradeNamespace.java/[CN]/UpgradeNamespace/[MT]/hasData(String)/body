{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select count(*) from " + tableName);
    rs=ps.executeQuery();
    while (rs.next()) {
      long count=rs.getLong(1);
      if (count > 0) {
        return true;
      }
    }
  }
 catch (  Exception e) {
    if (_log.isWarnEnabled()) {
      _log.warn(e.getMessage());
    }
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return false;
}
