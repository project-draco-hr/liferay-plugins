{
  if (_persistence == null) {
    _persistence=(WallEntryPersistence)PortletBeanLocatorUtil.locate(com.liferay.socialnetworking.service.ClpSerializer.getServletContextName(),WallEntryPersistence.class.getName());
    ReferenceRegistry.registerReference(WallEntryUtil.class,"_persistence");
  }
  return _persistence;
}
