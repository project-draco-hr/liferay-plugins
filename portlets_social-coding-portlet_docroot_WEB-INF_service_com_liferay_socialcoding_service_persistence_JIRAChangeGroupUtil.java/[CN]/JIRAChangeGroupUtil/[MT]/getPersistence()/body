{
  if (_persistence == null) {
    _persistence=(JIRAChangeGroupPersistence)PortletBeanLocatorUtil.locate(com.liferay.socialcoding.service.ClpSerializer.SERVLET_CONTEXT_NAME,JIRAChangeGroupPersistence.class.getName());
