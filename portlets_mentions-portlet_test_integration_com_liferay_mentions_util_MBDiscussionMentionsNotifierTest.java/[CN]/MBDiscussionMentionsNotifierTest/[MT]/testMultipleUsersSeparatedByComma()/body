{
  MentionsNotifier mentionsNotifier=new MentionsNotifier();
  String[] mentionedUsersScreenNames=mentionsNotifier.getMentionedUsersScreenNames(TestPropsValues.getUserId(),"hey @ana, @sergio can you check this out?");
  Assert.assertEquals(2,mentionedUsersScreenNames.length);
  Assert.assertTrue(ArrayUtil.contains(mentionedUsersScreenNames,"ana"));
  Assert.assertTrue(ArrayUtil.contains(mentionedUsersScreenNames,"sergio"));
}
