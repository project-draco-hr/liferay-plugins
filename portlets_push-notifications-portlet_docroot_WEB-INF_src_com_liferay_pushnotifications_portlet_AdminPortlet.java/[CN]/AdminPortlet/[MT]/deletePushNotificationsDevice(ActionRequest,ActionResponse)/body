{
  long pushNotificationsDeviceId=ParamUtil.getLong(actionRequest,"pushNotificationsDeviceId");
  try {
    PushNotificationsDeviceLocalServiceUtil.deletePushNotificationsDevice(pushNotificationsDeviceId);
    SessionMessages.add(actionRequest,"deviceDeleted");
  }
 catch (  PortalException pe) {
    SessionErrors.add(actionRequest,pe.getClass());
  }
  sendRedirect(actionRequest,actionResponse);
}
