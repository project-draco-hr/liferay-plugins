{
  if (obj == null) {
    return false;
  }
  HRTimeSheetDayClp hrTimeSheetDay=null;
  try {
    hrTimeSheetDay=(HRTimeSheetDayClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrTimeSheetDay.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
