{
  Object[] finderArgs=new Object[]{kaleoClassName,kaleoClassPK};
  Object result=null;
  if (retrieveFromCache) {
    result=FinderCacheUtil.getResult(FINDER_PATH_FETCH_BY_KALEOCN_PK,finderArgs,this);
  }
  if (result == null) {
    StringBundler query=new StringBundler(4);
    query.append(_SQL_SELECT_KALEOCONDITION_WHERE);
    if (kaleoClassName == null) {
      query.append(_FINDER_COLUMN_KALEOCN_PK_KALEOCLASSNAME_1);
    }
 else {
      if (kaleoClassName.equals(StringPool.BLANK)) {
        query.append(_FINDER_COLUMN_KALEOCN_PK_KALEOCLASSNAME_3);
      }
 else {
        query.append(_FINDER_COLUMN_KALEOCN_PK_KALEOCLASSNAME_2);
      }
    }
    query.append(_FINDER_COLUMN_KALEOCN_PK_KALEOCLASSPK_2);
    query.append(KaleoConditionModelImpl.ORDER_BY_JPQL);
    String sql=query.toString();
    Session session=null;
    try {
      session=openSession();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      if (kaleoClassName != null) {
        qPos.add(kaleoClassName);
      }
      qPos.add(kaleoClassPK);
      List<KaleoCondition> list=q.list();
      result=list;
      KaleoCondition kaleoCondition=null;
      if (list.isEmpty()) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_KALEOCN_PK,finderArgs,list);
      }
 else {
        kaleoCondition=list.get(0);
        cacheResult(kaleoCondition);
        if ((kaleoCondition.getKaleoClassName() == null) || !kaleoCondition.getKaleoClassName().equals(kaleoClassName) || (kaleoCondition.getKaleoClassPK() != kaleoClassPK)) {
          FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_KALEOCN_PK,finderArgs,kaleoCondition);
        }
      }
      return kaleoCondition;
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (result == null) {
        FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_KALEOCN_PK,finderArgs,new ArrayList<KaleoCondition>());
      }
      closeSession(session);
    }
  }
 else {
    if (result instanceof List<?>) {
      return null;
    }
 else {
      return (KaleoCondition)result;
    }
  }
}
