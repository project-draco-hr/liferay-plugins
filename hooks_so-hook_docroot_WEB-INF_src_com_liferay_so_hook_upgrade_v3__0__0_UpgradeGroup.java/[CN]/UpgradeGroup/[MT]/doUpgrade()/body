{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select classPK from ExpandoValue inner join ExpandoColumn " + "on (ExpandoValue.columnId = ExpandoColumn.columnId and " + "ExpandoColumn.name = 'socialOfficeEnabled') "+ "inner join ExpandoTable on (ExpandoValue."+ "tableId = ExpandoTable.tableId and "+ "ExpandoTable.name = 'CUSTOM_FIELDS') "+ "where ExpandoValue.classNameId = " + PortalUtil.getClassNameId(Group.class) + " and ExpandoValue.data_ "+ "= 'true'");
    rs=ps.executeQuery();
    while (rs.next()) {
      long classPK=rs.getLong("classPK");
      LayoutSetLocalServiceUtil.updateLookAndFeel(classPK,"so_WAR_sotheme","01",StringPool.BLANK,false);
    }
  }
  finally {
    DataAccess.cleanUp(con,ps,rs);
  }
}
