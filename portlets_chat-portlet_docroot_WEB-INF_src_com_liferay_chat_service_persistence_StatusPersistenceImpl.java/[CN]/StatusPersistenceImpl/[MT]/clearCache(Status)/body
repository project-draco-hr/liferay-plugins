{
  EntityCacheUtil.removeResult(StatusModelImpl.ENTITY_CACHE_ENABLED,StatusImpl.class,status.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_USERID,new Object[]{Long.valueOf(status.getUserId())});
}
