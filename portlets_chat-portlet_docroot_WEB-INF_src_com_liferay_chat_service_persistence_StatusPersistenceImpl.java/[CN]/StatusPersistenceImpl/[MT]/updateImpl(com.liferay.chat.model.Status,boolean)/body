{
  status=toUnwrappedModel(status);
  boolean isNew=status.isNew();
  StatusModelImpl statusModelImpl=(StatusModelImpl)status;
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,status,merge);
    status.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  if (isNew) {
    FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  }
 else {
    if (status.getModifiedDate() != statusModelImpl.getOriginalModifiedDate()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_MODIFIEDDATE,new Object[]{Long.valueOf(statusModelImpl.getOriginalModifiedDate())});
    }
    if (status.getOnline() != statusModelImpl.getOriginalOnline()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_ONLINE,new Object[]{Boolean.valueOf(statusModelImpl.getOriginalOnline())});
    }
    if ((status.getModifiedDate() != statusModelImpl.getOriginalModifiedDate()) || (status.getOnline() != statusModelImpl.getOriginalOnline())) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_M_O,new Object[]{Long.valueOf(statusModelImpl.getOriginalModifiedDate()),Boolean.valueOf(statusModelImpl.getOriginalOnline())});
    }
  }
  EntityCacheUtil.putResult(StatusModelImpl.ENTITY_CACHE_ENABLED,StatusImpl.class,status.getPrimaryKey(),status);
  if (isNew) {
    FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_USERID,new Object[]{Long.valueOf(status.getUserId())},status);
  }
 else {
    if (status.getUserId() != statusModelImpl.getOriginalUserId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_USERID,new Object[]{Long.valueOf(statusModelImpl.getOriginalUserId())});
      FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_USERID,new Object[]{Long.valueOf(status.getUserId())},status);
    }
  }
  return status;
}
