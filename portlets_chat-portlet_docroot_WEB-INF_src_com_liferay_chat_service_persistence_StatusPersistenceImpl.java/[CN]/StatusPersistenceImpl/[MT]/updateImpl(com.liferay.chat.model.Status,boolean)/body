{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,status,merge);
    status.setNew(false);
    return status;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Status.class.getName());
  }
}
