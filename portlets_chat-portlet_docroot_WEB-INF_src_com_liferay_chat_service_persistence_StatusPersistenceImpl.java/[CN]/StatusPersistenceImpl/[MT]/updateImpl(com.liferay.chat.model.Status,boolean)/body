{
  Session session=null;
  try {
    session=openSession();
    if (merge) {
      session.merge(status);
    }
 else {
      if (status.isNew()) {
        session.save(status);
      }
    }
    session.flush();
    status.setNew(false);
    return status;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Status.class.getName());
  }
}
