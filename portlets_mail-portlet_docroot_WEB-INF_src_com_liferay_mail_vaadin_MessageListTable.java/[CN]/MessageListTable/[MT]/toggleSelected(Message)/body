{
  HashSet<Message> select=new HashSet<Message>();
  Set<Message> selected=(Set<Message>)getValue();
  if (selected != null) {
    for (    Message i : selected) {
      if (i != message) {
        select.add(i);
      }
    }
  }
  boolean retval=false;
  if (!isSelected(message)) {
    select.add(message);
    retval=true;
  }
  setValue(select);
  return retval;
}
