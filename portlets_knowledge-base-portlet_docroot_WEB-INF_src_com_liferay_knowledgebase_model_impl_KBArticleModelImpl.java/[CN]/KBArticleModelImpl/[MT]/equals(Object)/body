{
  if (obj == null) {
    return false;
  }
  KBArticleImpl kbArticle=null;
  try {
    kbArticle=(KBArticleImpl)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=kbArticle.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
