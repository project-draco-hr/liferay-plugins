{
  JSONObject jsonObject=(JSONObject)message.getPayload();
  JSONObject toUser=jsonObject.getJSONObject(PushNotificationsConstants.TO_USER);
  long toUserId=toUser.getLong(PushNotificationsConstants.USER_ID);
  if (_log.isDebugEnabled()) {
    _log.debug("Sending message " + jsonObject + " to user "+ toUserId);
  }
  try {
    PushNotificationsDeviceLocalServiceUtil.sendPushNotification(toUserId,jsonObject,QueryUtil.ALL_POS,QueryUtil.ALL_POS);
  }
 catch (  Exception e) {
    _log.error("Unable to send notification",e);
  }
}
