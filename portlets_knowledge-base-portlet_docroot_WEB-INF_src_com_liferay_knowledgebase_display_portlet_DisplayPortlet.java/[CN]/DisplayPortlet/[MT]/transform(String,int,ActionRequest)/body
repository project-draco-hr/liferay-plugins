{
  KBTemplateParser kbTemplateParser=AdminUtil.getKBTemplateParser(engineType);
  try {
    kbTemplateParser.transform(KBTemplate.class.getName(),content,null,PortalUtil.getHttpServletRequest(actionRequest));
  }
 catch (  Exception e) {
    throw new KBTemplateContentException(e.getMessage());
  }
}
