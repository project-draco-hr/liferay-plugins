{
  if (isPRPRedirect(renderRequest,renderResponse)) {
    include(jspPath + "view.jsp",renderRequest,renderResponse);
  }
 else   if (SessionErrors.contains(renderRequest,NoSuchArticleException.class.getName()) || SessionErrors.contains(renderRequest,NoSuchCommentException.class.getName()) || SessionErrors.contains(renderRequest,PrincipalException.class.getName())) {
    include(jspPath + "error.jsp",renderRequest,renderResponse);
  }
 else {
    super.doDispatch(renderRequest,renderResponse);
  }
}
