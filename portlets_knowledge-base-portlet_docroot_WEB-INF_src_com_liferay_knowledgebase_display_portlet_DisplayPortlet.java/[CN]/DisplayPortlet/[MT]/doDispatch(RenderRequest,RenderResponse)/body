{
  if (SessionErrors.contains(renderRequest,NoSuchArticleException.class.getName()) || SessionErrors.contains(renderRequest,NoSuchMessageException.class.getName()) || SessionErrors.contains(renderRequest,PrincipalException.class.getName())) {
    include("/display/error.jsp",renderRequest,renderResponse);
  }
 else {
    super.doDispatch(renderRequest,renderResponse);
  }
}
