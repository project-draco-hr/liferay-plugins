{
  PortletURL viewFeedbackByStatusURL=renderResponse.createRenderURL();
  viewFeedbackByStatusURL.setParameter("navItem",navItem);
  viewFeedbackByStatusURL.setParameter("expanded",Boolean.TRUE.toString());
  if (_kbArticle == null) {
    viewFeedbackByStatusURL.setParameter("mvcPath","/admin/view_feedback.jsp");
  }
 else {
    viewFeedbackByStatusURL.setParameter("resourcePrimKey",String.valueOf(_kbArticle.getResourcePrimKey()));
  }
  return viewFeedbackByStatusURL.toString();
}
