{
  Set<NotificationRecipient> notificationRecipientsSet=notificationRecipients.get(notificationReceptionType);
  if (notificationRecipientsSet == null) {
    notificationRecipientsSet=new HashSet<>();
    notificationRecipients.put(notificationReceptionType,notificationRecipientsSet);
  }
  return notificationRecipientsSet;
}
