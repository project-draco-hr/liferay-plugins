{
  try {
    Set<NotificationRecipient> notificationRecipients=getNotificationRecipients(kaleoNotificationRecipients,executionContext);
    if (notificationRecipients.isEmpty()) {
      return;
    }
    doSendNotification(notificationRecipients,defaultSubject,notificationMessage,executionContext);
  }
 catch (  Exception e) {
    throw new NotificationMessageSenderException("Unable to send notification message",e);
  }
}
