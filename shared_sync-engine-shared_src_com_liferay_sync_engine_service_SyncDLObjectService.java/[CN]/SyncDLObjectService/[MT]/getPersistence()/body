{
  if (_syncDLObjectPersistence != null) {
    return _syncDLObjectPersistence;
  }
  try {
    _syncDLObjectPersistence=new SyncDLObjectPersistence();
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
  }
  return _syncDLObjectPersistence;
}
