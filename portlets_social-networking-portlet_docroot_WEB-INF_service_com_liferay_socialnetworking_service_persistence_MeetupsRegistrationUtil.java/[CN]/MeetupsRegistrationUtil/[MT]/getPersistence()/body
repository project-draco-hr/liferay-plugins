{
  if (_persistence == null) {
    _persistence=(MeetupsRegistrationPersistence)PortletBeanLocatorUtil.locate(com.liferay.socialnetworking.service.ClpSerializer.getServletContextName(),MeetupsRegistrationPersistence.class.getName());
    ReferenceRegistry.registerReference(MeetupsRegistrationUtil.class,"_persistence");
  }
  return _persistence;
}
