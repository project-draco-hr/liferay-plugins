{
  Session session=null;
  try {
    session=openSession();
    Checkout checkout=(Checkout)session.get(CheckoutImpl.class,primaryKey);
    if (checkout == null) {
      if (_log.isDebugEnabled()) {
        _log.debug(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchCheckoutException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(checkout);
  }
 catch (  NoSuchCheckoutException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
