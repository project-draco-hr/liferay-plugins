{
  PrintWriter writer=response.getWriter();
  if (runTest) {
    writer.write("Click 'Process Event' to run test");
  }
 else {
    try {
      if (passed) {
        writer.write("PASSED");
      }
 else {
        writer.write("FAILED");
      }
    }
  finally {
      writer.close();
      runTest=true;
    }
  }
}
