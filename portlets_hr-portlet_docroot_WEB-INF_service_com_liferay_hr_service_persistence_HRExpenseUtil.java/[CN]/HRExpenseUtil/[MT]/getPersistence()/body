{
  if (_persistence == null) {
    _persistence=(HRExpensePersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRExpensePersistence.class.getName());
    ReferenceRegistry.registerReference(HRExpenseUtil.class,"_persistence");
  }
  return _persistence;
}
