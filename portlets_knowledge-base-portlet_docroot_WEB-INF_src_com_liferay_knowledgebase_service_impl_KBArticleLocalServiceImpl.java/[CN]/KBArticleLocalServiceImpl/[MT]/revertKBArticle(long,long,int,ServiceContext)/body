{
  KBArticle sourceKBArticle=KBArticleServiceUtil.getKBArticle(resourcePrimKey,version);
  return updateKBArticle(userId,resourcePrimKey,sourceKBArticle.getTitle(),sourceKBArticle.getContent(),sourceKBArticle.getDescription(),sourceKBArticle.getSourceURL(),StringUtil.split(sourceKBArticle.getSections()),null,null,serviceContext);
}
