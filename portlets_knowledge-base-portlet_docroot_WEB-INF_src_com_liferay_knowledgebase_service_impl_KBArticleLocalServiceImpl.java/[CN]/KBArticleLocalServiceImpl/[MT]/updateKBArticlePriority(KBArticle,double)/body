{
  List<KBArticle> kbArticleVersions=getKBArticleVersions(kbArticle.getResourcePrimKey(),WorkflowConstants.STATUS_ANY,QueryUtil.ALL_POS,QueryUtil.ALL_POS,null);
  for (  KBArticle kbArticleVersion : kbArticleVersions) {
    kbArticleVersion.setPriority(priority);
    kbArticlePersistence.update(kbArticleVersion);
  }
}
