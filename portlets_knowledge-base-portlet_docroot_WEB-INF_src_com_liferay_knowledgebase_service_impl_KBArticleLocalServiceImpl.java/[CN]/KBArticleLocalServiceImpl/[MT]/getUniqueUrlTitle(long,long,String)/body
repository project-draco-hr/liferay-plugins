{
  String urlTitle=KnowledgeBaseUtil.getUrlTitle(kbArticleId,title);
  for (int i=1; ; i++) {
    KBArticle kbArticle=null;
    try {
      kbArticle=getKBArticleByUrlTitle(groupId,KBFolderConstants.DEFAULT_PARENT_FOLDER_ID,urlTitle);
    }
 catch (    NoSuchArticleException nsae) {
    }
    if ((kbArticle == null) || (kbArticleId == kbArticle.getKbArticleId())) {
      break;
    }
 else {
      String suffix=StringPool.DASH + i;
      String prefix=urlTitle;
      if (urlTitle.length() > suffix.length()) {
        prefix=urlTitle.substring(0,urlTitle.length() - suffix.length());
      }
      urlTitle=prefix + suffix;
    }
  }
  return urlTitle;
}
