{
  KBArticle kbArticle=fetchKBArticleByUrlTitle(groupId,kbFolderUrlTitle,urlTitle);
  if (kbArticle == null) {
    throw new NoSuchArticleException("No KBArticle with the key {groupId=" + groupId + ", urlTitle="+ urlTitle+ "} found within a folder "+ "with urlTitle "+ kbFolderUrlTitle);
  }
  return kbArticle;
}
