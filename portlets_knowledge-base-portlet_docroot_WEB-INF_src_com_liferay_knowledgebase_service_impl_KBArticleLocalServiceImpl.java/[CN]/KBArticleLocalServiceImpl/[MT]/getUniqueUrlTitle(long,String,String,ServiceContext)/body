{
  if (Validator.isNotNull(urlTitle)) {
    urlTitle=KnowledgeBaseUtil.getUrlTitle(kbArticleId,urlTitle);
  }
 else {
    urlTitle=getUniqueUrlTitle(serviceContext.getScopeGroupId(),kbArticleId,title);
  }
  KBArticle kbArticle=null;
  try {
    kbArticle=getKBArticleByUrlTitle(serviceContext.getScopeGroupId(),urlTitle);
  }
 catch (  NoSuchArticleException nsae) {
  }
  if ((kbArticle != null) && (kbArticleId != kbArticle.getKbArticleId())) {
    urlTitle=getUniqueUrlTitle(serviceContext.getScopeGroupId(),kbArticleId,urlTitle);
  }
  return urlTitle;
}
