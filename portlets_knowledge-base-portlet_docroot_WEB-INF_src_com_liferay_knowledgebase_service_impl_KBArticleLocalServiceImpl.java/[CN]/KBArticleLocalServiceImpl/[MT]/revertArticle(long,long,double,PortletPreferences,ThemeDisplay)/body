{
  KBArticle article=getArticle(resourcePrimKey);
  KBArticle oldArticle=getArticle(resourcePrimKey,version);
  return updateArticle(userId,resourcePrimKey,0,oldArticle.getTitle(),oldArticle.getContent(),oldArticle.getDescription(),article.getDraft(),false,oldArticle.isTemplate(),oldArticle.getParentResourcePrimKey(),null,prefs,themeDisplay);
}
