{
  List<KBArticle> kbArticles=getSiblingKBArticles(groupId,parentResourcePrimKey,WorkflowConstants.STATUS_ANY,0,1,new KBArticlePriorityComparator());
  if (kbArticles.isEmpty()) {
    return KBArticleConstants.DEFAULT_PRIORITY;
  }
  KBArticle kbArticle=kbArticles.get(0);
  return Math.floor(kbArticle.getPriority()) + 1;
}
