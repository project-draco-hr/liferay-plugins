{
  if (obj == null) {
    return false;
  }
  GadgetClp gadget=null;
  try {
    gadget=(GadgetClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=gadget.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
