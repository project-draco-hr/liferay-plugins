{
  Session session=null;
  try {
    session=openSession();
    session.delete(kbArticleResource);
    session.flush();
    return kbArticleResource;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(KBArticleResource.class.getName());
  }
}
