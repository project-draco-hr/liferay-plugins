{
  bbbMeetingPersistence.remove(bbbMeeting);
  resourceLocalService.deleteResource(bbbMeeting,ResourceConstants.SCOPE_INDIVIDUAL);
  List<BBBParticipant> bbbParticipants=bbbParticipantLocalService.getBBBParticipants(bbbMeeting.getBbbMeetingId());
  for (  BBBParticipant bbbParticipant : bbbParticipants) {
    bbbParticipantLocalService.deleteBBBParticipant(bbbParticipant);
  }
  return bbbMeeting;
}
