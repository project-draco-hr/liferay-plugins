{
  if (obj == null) {
    return false;
  }
  KBComment kbComment=null;
  try {
    kbComment=(KBComment)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=kbComment.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
