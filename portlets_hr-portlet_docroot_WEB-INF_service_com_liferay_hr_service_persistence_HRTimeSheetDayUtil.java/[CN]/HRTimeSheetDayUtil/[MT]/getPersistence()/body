{
  if (_persistence == null) {
    _persistence=(HRTimeSheetDayPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRTimeSheetDayPersistence.class.getName());
    ReferenceRegistry.registerReference(HRTimeSheetDayUtil.class,"_persistence");
  }
