{
  String articleURL=PortalUtil.getControlPanelFullURL(groupId,PortletKeys.KNOWLEDGE_BASE_ADMIN,null);
  articleURL=HttpUtil.setParameter(articleURL,"p_p_id",PortletKeys.KNOWLEDGE_BASE_ADMIN);
  String namespace=PortalUtil.getPortletNamespace(PortletKeys.KNOWLEDGE_BASE_ADMIN);
  articleURL=HttpUtil.setParameter(articleURL,namespace + "jspPage","/admin/view_article.jsp");
  articleURL=HttpUtil.setParameter(articleURL,namespace + "resourcePrimKey",resourcePrimKey);
  return articleURL;
}
