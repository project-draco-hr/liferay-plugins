{
  String emailKBArticleFeedbackReceivedBody=preferences.getValue("emailKBArticleFeedbackReceivedBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleFeedbackReceivedBody)) {
    return emailKBArticleFeedbackReceivedBody;
  }
  return ContentUtil.get(PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_RECEIVED_BODY);
}
