{
  String emailKBArticleFeedbackInProgressSubject=preferences.getValue("emailKBArticleFeedbackInProgressSubject",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleFeedbackInProgressSubject)) {
    return emailKBArticleFeedbackInProgressSubject;
  }
  return ContentUtil.get(PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_IN_PROGRESS_SUBJECT);
}
