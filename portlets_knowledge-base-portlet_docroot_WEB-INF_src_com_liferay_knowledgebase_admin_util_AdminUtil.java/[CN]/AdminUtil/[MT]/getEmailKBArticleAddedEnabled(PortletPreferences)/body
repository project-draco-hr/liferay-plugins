{
  String emailKBArticleAddedEnabled=preferences.getValue("emailKBArticleAddedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleAddedEnabled)) {
    return GetterUtil.getBoolean(emailKBArticleAddedEnabled);
  }
  return GetterUtil.getBoolean(PortletProps.get("admin.email.kb.article.added.enabled"));
}
