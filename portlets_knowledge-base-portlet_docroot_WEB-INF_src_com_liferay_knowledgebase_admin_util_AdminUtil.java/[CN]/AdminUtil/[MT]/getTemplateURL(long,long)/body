{
  String templateURL=PortalUtil.getControlPanelFullURL(groupId,PortletKeys.KNOWLEDGE_BASE_ADMIN,null);
  templateURL=HttpUtil.setParameter(templateURL,"p_p_id",PortletKeys.KNOWLEDGE_BASE_ADMIN);
  String namespace=PortalUtil.getPortletNamespace(PortletKeys.KNOWLEDGE_BASE_ADMIN);
  templateURL=HttpUtil.setParameter(templateURL,namespace + "jspPage","/admin/view_template.jsp");
  templateURL=HttpUtil.setParameter(templateURL,namespace + "templateId",templateId);
  return templateURL;
}
