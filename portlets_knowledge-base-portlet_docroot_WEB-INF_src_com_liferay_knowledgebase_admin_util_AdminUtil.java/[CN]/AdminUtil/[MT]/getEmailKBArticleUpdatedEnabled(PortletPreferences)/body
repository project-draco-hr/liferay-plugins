{
  String emailKBArticleUpdatedEnabled=preferences.getValue("emailKBArticleUpdatedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleUpdatedEnabled)) {
    return GetterUtil.getBoolean(emailKBArticleUpdatedEnabled);
  }
  return GetterUtil.getBoolean(PortletProps.get("admin.email.kb.article.updated.enabled"));
}
