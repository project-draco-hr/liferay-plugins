{
  String emailKBArticleFeedbackResolvedBody=preferences.getValue("emailKBArticleFeedbackResolvedBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleFeedbackResolvedBody)) {
    return emailKBArticleFeedbackResolvedBody;
  }
  return ContentUtil.get(PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_RESOLVED_BODY);
}
