{
  String emailKBArticleFeedbackInProgressBody=preferences.getValue("emailKBArticleFeedbackInProgressBody",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleFeedbackInProgressBody)) {
    return emailKBArticleFeedbackInProgressBody;
  }
  return ContentUtil.get(PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_FEEDBACK_IN_PROGRESS_BODY);
}
