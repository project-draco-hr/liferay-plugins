{
  if (engineType == KBTemplateConstants.ENGINE_TYPE_FREEMARKER) {
    return (KBTemplateParser)InstancePool.get(KBFreeMarkerTemplateParserImpl.class.getName(),false);
  }
 else   if (engineType == KBTemplateConstants.ENGINE_TYPE_VELOCITY) {
    return (KBTemplateParser)InstancePool.get(KBVelocityTemplateParserImpl.class.getName(),false);
  }
  return null;
}
