{
  String emailKBArticleFeedbackInProgressEnabled=preferences.getValue("emailKBArticleFeedbackInProgressEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleFeedbackInProgressEnabled)) {
    return GetterUtil.getBoolean(emailKBArticleFeedbackInProgressEnabled);
  }
  return PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_FEEDBACK_IN_PROGRESS_ENABLED;
}
