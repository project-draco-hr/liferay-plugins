{
  if (engineType == KBTemplateConstants.ENGINE_TYPE_FREEMARKER) {
    return ContentUtil.get(PortletPropsValues.ADMIN_KB_TEMPLATE_CONTENT_FREEMARKER);
  }
 else   if (engineType == KBTemplateConstants.ENGINE_TYPE_VELOCITY) {
    return ContentUtil.get(PortletPropsValues.ADMIN_KB_TEMPLATE_CONTENT_VELOCITY);
  }
  return StringPool.BLANK;
}
