{
  String emailKBArticleFeedbackReceivedEnabled=preferences.getValue("emailKBArticleFeedbackReceivedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailKBArticleFeedbackReceivedEnabled)) {
    return GetterUtil.getBoolean(emailKBArticleFeedbackReceivedEnabled);
  }
  return PortletPropsValues.ADMIN_EMAIL_KB_ARTICLE_SUGGESTION_RECEIVED_ENABLED;
}
