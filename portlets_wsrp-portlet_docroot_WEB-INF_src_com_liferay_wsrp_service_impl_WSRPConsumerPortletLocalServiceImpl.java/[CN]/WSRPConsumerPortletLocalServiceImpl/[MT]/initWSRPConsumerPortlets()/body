{
  for (  WSRPConsumerPortlet wsrpConsumerPortlet : wsrpConsumerPortletPersistence.findAll()) {
    try {
      initWSRPConsumerPortlet(wsrpConsumerPortlet.getCompanyId(),wsrpConsumerPortlet.getWsrpConsumerId(),wsrpConsumerPortlet.getWsrpConsumerPortletId(),wsrpConsumerPortlet.getUuid(),wsrpConsumerPortlet.getName(),wsrpConsumerPortlet.getPortletHandle(),null);
    }
 catch (    Exception e) {
      if (_log.isErrorEnabled()) {
        _log.error("Unable to initialize portlet: " + wsrpConsumerPortlet.getUuid(),e);
      }
    }
  }
}
