{
  for (  WSRPConsumerPortlet wsrpConsumerPortlet : wsrpConsumerPortletPersistence.findAll()) {
    initWSRPConsumerPortlet(wsrpConsumerPortlet.getCompanyId(),wsrpConsumerPortlet.getWsrpConsumerId(),wsrpConsumerPortlet.getWsrpConsumerPortletId(),wsrpConsumerPortlet.getName(),wsrpConsumerPortlet.getPortletHandle(),null);
  }
}
