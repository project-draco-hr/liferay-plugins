{
  try {
    Portlet portlet=getPortlet(companyId,wsrpConsumerId,wsrpConsumerPortletId,name,portletHandle,userToken);
    PortletLocalServiceUtil.deployRemotePortlet(portlet,_WSRP_CATEGORY);
  }
 catch (  PortalException pe) {
    throw pe;
  }
catch (  SystemException se) {
    throw se;
  }
catch (  Exception e) {
    throw new SystemException(e);
  }
}
