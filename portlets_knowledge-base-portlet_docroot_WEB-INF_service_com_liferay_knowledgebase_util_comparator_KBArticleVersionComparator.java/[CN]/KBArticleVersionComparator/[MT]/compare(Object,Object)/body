{
  KBArticle kbArticle1=(KBArticle)obj1;
  KBArticle kbArticle2=(KBArticle)obj2;
  int value=0;
  if (kbArticle1.getVersion() < kbArticle2.getVersion()) {
    value=-1;
  }
 else   if (kbArticle1.getVersion() > kbArticle2.getVersion()) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
