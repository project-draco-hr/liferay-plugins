{
  _active=active;
  if (_assetRemoteModel != null) {
    try {
      Class<?> clazz=_assetRemoteModel.getClass();
      Method method=clazz.getMethod("setActive",boolean.class);
      method.invoke(_assetRemoteModel,active);
    }
 catch (    Exception e) {
      throw new UnsupportedOperationException(e);
    }
  }
}
