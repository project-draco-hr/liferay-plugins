{
  if (obj == null) {
    return false;
  }
  HRAssetDefinition hrAssetDefinition=null;
  try {
    hrAssetDefinition=(HRAssetDefinition)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrAssetDefinition.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
