{
  if (_syncAccountPersistence != null) {
    return _syncAccountPersistence;
  }
  try {
    _syncAccountPersistence=new SyncAccountPersistence();
  }
 catch (  SQLException sqle) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(sqle.getMessage(),sqle);
    }
  }
  return _syncAccountPersistence;
}
