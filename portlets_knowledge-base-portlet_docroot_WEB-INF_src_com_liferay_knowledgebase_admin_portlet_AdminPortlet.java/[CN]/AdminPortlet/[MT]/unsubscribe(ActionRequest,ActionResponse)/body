{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long resourcePrimKey=ParamUtil.getLong(actionRequest,"resourcePrimKey");
  if (resourcePrimKey <= 0) {
    ArticleServiceUtil.unsubscribe(themeDisplay.getScopeGroupId());
  }
 else {
    ArticleServiceUtil.unsubscribeArticle(themeDisplay.getCompanyId(),resourcePrimKey);
  }
}
