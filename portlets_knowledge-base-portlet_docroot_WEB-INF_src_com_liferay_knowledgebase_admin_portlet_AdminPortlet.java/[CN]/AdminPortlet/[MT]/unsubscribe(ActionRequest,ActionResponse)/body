{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String portletId=PortalUtil.getPortletId(actionRequest);
  ArticleServiceUtil.unsubscribe(themeDisplay.getCompanyId(),themeDisplay.getScopeGroupId(),themeDisplay.getPlid(),portletId,themeDisplay.getScopeGroupId());
}
