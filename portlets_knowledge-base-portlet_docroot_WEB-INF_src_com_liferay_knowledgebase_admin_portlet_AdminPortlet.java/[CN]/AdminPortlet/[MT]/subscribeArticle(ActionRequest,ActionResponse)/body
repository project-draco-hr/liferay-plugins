{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  long resourcePrimKey=ParamUtil.getLong(actionRequest,"resourcePrimKey");
  String portletId=PortalUtil.getPortletId(actionRequest);
  ArticleServiceUtil.subscribeArticle(themeDisplay.getCompanyId(),themeDisplay.getScopeGroupId(),themeDisplay.getPlid(),portletId,resourcePrimKey);
}
