{
  if (cause instanceof ArticleContentException || cause instanceof ArticleTitleException || cause instanceof DuplicateFileException|| cause instanceof FileSizeException|| cause instanceof NoSuchArticleException|| cause instanceof NoSuchFileException|| cause instanceof NoSuchTemplateException|| cause instanceof PrincipalException|| cause instanceof TemplateContentException|| cause instanceof TemplateTitleException) {
    return true;
  }
  return false;
}
