{
  setPostResponse("dependencies/watcher_test_move_file.json");
  Path sourceFilePath=Paths.get(_syncSite.getFilePathName() + "/test.txt");
  Files.createFile(sourceFilePath);
  Path destinationFilePath=Paths.get(_syncSite.getFilePathName() + "/test");
  Files.createDirectory(destinationFilePath);
  sleep();
  Files.move(sourceFilePath,destinationFilePath.resolve(sourceFilePath.getFileName()));
  sleep();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(4,_syncFiles.size());
  Assert.assertNotNull(SyncFileService.fetchSyncFile(FilePathNameUtil.getFilePathName(destinationFilePath),syncAccount.getSyncAccountId()));
}
