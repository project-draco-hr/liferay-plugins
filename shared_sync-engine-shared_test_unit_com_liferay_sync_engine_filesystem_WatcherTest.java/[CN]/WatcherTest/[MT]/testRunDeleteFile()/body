{
  setMockPostResponse("dependencies/watcher_test_delete_file.json");
  Path filePath=Paths.get(_syncSite.getFilePathName() + "/test.txt");
  Files.createFile(filePath);
  Thread.sleep(1000);
  Files.delete(filePath);
  Thread.sleep(1000);
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(2,_syncFiles.size());
  Assert.assertNull(SyncFileService.fetchSyncFile(FilePathNameUtil.getFilePathName(filePath),syncAccount.getSyncAccountId()));
}
