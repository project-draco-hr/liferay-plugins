{
  if (obj == null) {
    return false;
  }
  HRHolidayClp hrHoliday=null;
  try {
    hrHoliday=(HRHolidayClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrHoliday.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
