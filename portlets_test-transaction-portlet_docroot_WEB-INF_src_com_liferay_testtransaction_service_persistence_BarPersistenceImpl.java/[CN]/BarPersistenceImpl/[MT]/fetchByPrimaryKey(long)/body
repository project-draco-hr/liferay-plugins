{
  Bar bar=(Bar)EntityCacheUtil.getResult(BarModelImpl.ENTITY_CACHE_ENABLED,BarImpl.class,barId,this);
  if (bar == null) {
    Session session=null;
    try {
      session=openSession();
      bar=(Bar)session.get(BarImpl.class,Long.valueOf(barId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (bar != null) {
        cacheResult(bar);
      }
      closeSession(session);
    }
  }
  return bar;
}
