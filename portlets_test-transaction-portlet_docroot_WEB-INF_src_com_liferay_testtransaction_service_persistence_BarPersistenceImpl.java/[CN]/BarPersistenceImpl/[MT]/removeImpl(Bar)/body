{
  bar=toUnwrappedModel(bar);
  Session session=null;
  try {
    session=openSession();
    if (bar.isCachedModel()) {
      bar=(Bar)session.get(BarImpl.class,bar.getPrimaryKeyObj());
    }
    if (bar != null) {
      session.delete(bar);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (bar != null) {
    clearCache(bar);
  }
  return bar;
}
