{
  bar=toUnwrappedModel(bar);
  Session session=null;
  try {
    session=openSession();
    if (bar.isCachedModel()) {
      bar=(Bar)session.get(BarImpl.class,bar.getPrimaryKeyObj());
    }
    session.delete(bar);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(bar);
  return bar;
}
