{
  String fileName=null;
  if (Validator.isNull(title)) {
    fileName=sourceFileName;
  }
 else {
    fileName=title;
  }
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  ExtRepositoryFileEntry extRepositoryFileEntry=_extRepository.addFile(repositoryMappedFolderId,mimeType,fileName,description,changeLog,is);
  return _toExtRepositoryObjectAdapter(ExtRepositoryObjectAdapterType.FILE,extRepositoryFileEntry);
}
