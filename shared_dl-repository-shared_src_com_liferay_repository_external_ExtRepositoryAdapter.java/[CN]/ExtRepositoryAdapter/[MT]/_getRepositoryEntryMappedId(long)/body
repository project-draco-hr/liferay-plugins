{
  RepositoryEntry repositoryEntry=RepositoryEntryUtil.fetchByPrimaryKey(entryId);
  if (repositoryEntry != null) {
    return repositoryEntry.getMappedId();
  }
  DLFolder rootMountFolder=DLFolderLocalServiceUtil.getDLFolder(entryId);
  repositoryEntry=_getRootRepositoryEntry(rootMountFolder);
  return repositoryEntry.getMappedId();
}
