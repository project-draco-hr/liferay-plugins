{
  try {
    String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
    List<? extends ExtRepositoryEntry> extRepositoryEntries=_extRepository.getExtRepositoryEntries(ExtRepositoryModelType.ENTRY,repositoryMappedFolderId);
    List<ExtRepositoryEntryAdapter<?>> entries=_toExtRepositoryEntryAdapters(ExtRepositoryModelAdapterType.ENTRY,extRepositoryEntries);
    return _subList(entries,start,end,obc);
  }
 catch (  Exception e) {
    throw new RepositoryException(e);
  }
}
