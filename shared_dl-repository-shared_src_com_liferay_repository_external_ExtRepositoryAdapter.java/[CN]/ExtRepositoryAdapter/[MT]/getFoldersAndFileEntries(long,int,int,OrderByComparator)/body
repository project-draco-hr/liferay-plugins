{
  try {
    String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
    List<? extends ExtRepositoryObject> extRepositoryObjects=_extRepository.getExtRepositoryObjects(ExtRepositoryObjectType.ENTRY,repositoryMappedFolderId);
    List<ExtRepositoryObjectAdapter<?>> entries=_toExtRepositoryObjectAdapters(ExtRepositoryObjectAdapterType.OBJECT,extRepositoryObjects);
    return _subList(entries,start,end,obc);
  }
 catch (  Exception e) {
    throw new RepositoryException(e);
  }
}
