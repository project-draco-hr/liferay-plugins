{
  String repositoryMappedParentFolderId=_getRepositoryEntryMappedId(parentFolderId);
  List<ExtRepositoryFolder> extRepositoryFolders=_extRepository.getEntries(ExtRepositoryModelType.FOLDER,repositoryMappedParentFolderId);
  List<ExtRepositoryFolderAdapter> folders=_toExtRepositoryEntryAdapters(ExtRepositoryModelAdapterType.FOLDER,extRepositoryFolders);
  return (List)_subList(folders,start,end,obc);
}
