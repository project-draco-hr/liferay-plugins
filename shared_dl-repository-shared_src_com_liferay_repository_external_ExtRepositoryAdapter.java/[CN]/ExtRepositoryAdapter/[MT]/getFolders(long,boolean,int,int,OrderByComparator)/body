{
  String repositoryMappedParentFolderId=_getRepositoryEntryMappedId(parentFolderId);
  List<ExtRepositoryFolder> extRepositoryFolders=_extRepository.getExtRepositoryObjects(ExtRepositoryObjectType.FOLDER,repositoryMappedParentFolderId);
  List<ExtRepositoryFolderAdapter> folders=_toExtRepositoryObjectAdapters(ExtRepositoryObjectAdapterType.FOLDER,extRepositoryFolders);
  return (List)_subList(folders,start,end,obc);
}
