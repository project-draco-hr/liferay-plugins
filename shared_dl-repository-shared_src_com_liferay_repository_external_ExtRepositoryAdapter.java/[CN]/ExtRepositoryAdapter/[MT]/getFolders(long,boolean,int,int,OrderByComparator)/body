{
  String repositoryMappedParentFolderId=_getRepositoryEntryMappedId(parentFolderId);
  List<ExtRepositoryFolder> extRepositoryFolders=_extRepository.getExtRepositoryEntries(ExtRepositoryModelType.FOLDER,repositoryMappedParentFolderId);
  List<ExtRepositoryFolderAdapter> entries=_toExtRepositoryEntryAdapters(ExtRepositoryModelAdapterType.FOLDER,extRepositoryFolders);
  return (List)_subList(entries,start,end,obc);
}
