{
  String extRepositoryFolderKey=_getExtRepositoryObjectKey(folderId);
  List<ExtRepositoryFileEntry> extRepositoryFileEntries=_extRepository.getExtRepositoryObjects(ExtRepositoryObjectType.FILE,extRepositoryFolderKey);
  List<ExtRepositoryFileEntryAdapter> fileEntries=_toExtRepositoryObjectAdapters(ExtRepositoryObjectAdapterType.FILE,extRepositoryFileEntries);
  fileEntries=_filterByMimeType(fileEntries,mimeTypes);
  return (List)_subList(fileEntries,start,end,obc);
}
