{
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  List<ExtRepositoryFileEntry> extRepositoryFileEntries=_extRepository.getEntries(ExtRepositoryModelType.FILE,repositoryMappedFolderId);
  List<ExtRepositoryFileEntryAdapter> fileEntries=_toExtRepositoryEntryAdapters(ExtRepositoryModelAdapterType.FILE,extRepositoryFileEntries);
  fileEntries=_filterByMimeType(fileEntries,mimeTypes);
  return (List)_subList(fileEntries,start,end,obc);
}
