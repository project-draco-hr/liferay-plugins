{
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  List<ExtRepositoryFileEntry> extRepositoryFileEntries=_extRepository.getExtRepositoryObjects(ExtRepositoryObjectType.FILE,repositoryMappedFolderId);
  List<ExtRepositoryFileEntryAdapter> fileEntries=_toExtRepositoryObjectAdapters(ExtRepositoryObjectAdapterType.FILE,extRepositoryFileEntries);
  return (List)_subList(fileEntries,start,end,obc);
}
