{
  String repositoryMappedParentFolderId=_getRepositoryEntryMappedId(parentFolderId);
  ExtRepositoryFolder extRepositoryFolder=_extRepository.getEntry(ExtRepositoryModelType.FOLDER,repositoryMappedParentFolderId,title);
  return _toExtRepositoryEntryAdapter(ExtRepositoryModelAdapterType.FOLDER,extRepositoryFolder);
}
