{
  String repositoryMappedParentFolderId=_getRepositoryEntryMappedId(parentFolderId);
  ExtRepositoryEntry folder=_extRepository.getExtRepositoryEntry(ExtRepositoryModelType.FOLDER,repositoryMappedParentFolderId,title);
  return _toExtRepositoryEntryAdapter(ExtRepositoryModelAdapterType.FOLDER,folder);
}
