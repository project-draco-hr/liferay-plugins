{
  String extRepositoryParentFolderKey=_getExtRepositoryObjectKey(parentFolderId);
  ExtRepositoryFolder extRepositoryFolder=_extRepository.getExtRepositoryObject(ExtRepositoryObjectType.FOLDER,extRepositoryParentFolderKey,title);
  return _toExtRepositoryObjectAdapter(ExtRepositoryObjectAdapterType.FOLDER,extRepositoryFolder);
}
