{
  if (fileEntryTypeId == DLFileEntryTypeConstants.FILE_ENTRY_TYPE_ID_BASIC_DOCUMENT) {
    String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
    return _extRepository.getExtRepositoryObjectsCount(ExtRepositoryObjectType.FILE,repositoryMappedFolderId);
  }
 else {
    return 0;
  }
}
