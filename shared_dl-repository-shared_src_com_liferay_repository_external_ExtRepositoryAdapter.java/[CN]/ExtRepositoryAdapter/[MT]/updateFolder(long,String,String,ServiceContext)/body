{
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  ExtRepositoryFolder extRepositoryFolder=_extRepository.getEntry(ExtRepositoryModelType.FOLDER,repositoryMappedFolderId);
  ExtRepositoryFolder folder=_extRepository.getParentFolder(extRepositoryFolder);
  ExtRepositoryFolder moveExtRepositoryFolder=_extRepository.moveEntry(ExtRepositoryModelType.FOLDER,repositoryMappedFolderId,folder.getId(),title);
  return _toExtRepositoryEntryAdapter(ExtRepositoryModelAdapterType.FOLDER,moveExtRepositoryFolder);
}
