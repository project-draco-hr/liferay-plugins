{
  String extRepositoryFolderKey=getExtRepositoryObjectKey(folderId);
  ExtRepositoryFolder extRepositoryFolder=_extRepository.getExtRepositoryObject(ExtRepositoryObjectType.FOLDER,extRepositoryFolderKey);
  ExtRepositoryFolder parentExtRepositoryFolder=_extRepository.getExtRepositoryParentFolder(extRepositoryFolder);
  ExtRepositoryFolder newExtRepositoryFolder=_extRepository.moveExtRepositoryObject(ExtRepositoryObjectType.FOLDER,extRepositoryFolderKey,parentExtRepositoryFolder.getExtRepositoryModelKey(),name);
  return _toExtRepositoryObjectAdapter(ExtRepositoryObjectAdapterType.FOLDER,newExtRepositoryFolder);
}
