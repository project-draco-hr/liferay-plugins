{
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  ExtRepositoryFolder extRepositoryFolder=_extRepository.getExtRepositoryObject(ExtRepositoryObjectType.FOLDER,repositoryMappedFolderId);
  ExtRepositoryFolder folder=_extRepository.getExtRepositoryParentFolder(extRepositoryFolder);
  ExtRepositoryFolder moveExtRepositoryFolder=_extRepository.moveExtRepositoryObject(ExtRepositoryObjectType.FOLDER,repositoryMappedFolderId,folder.getId(),title);
  return _toExtRepositoryObjectAdapter(ExtRepositoryObjectAdapterType.FOLDER,moveExtRepositoryFolder);
}
