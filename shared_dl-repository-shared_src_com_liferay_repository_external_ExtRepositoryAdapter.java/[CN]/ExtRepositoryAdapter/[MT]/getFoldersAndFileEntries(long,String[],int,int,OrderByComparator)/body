{
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  List<ExtRepositoryEntry> extRepositoryEntries=_extRepository.getEntries(ExtRepositoryModelType.ENTRY,repositoryMappedFolderId);
  List<ExtRepositoryEntryAdapter<?>> entries=_toExtRepositoryEntryAdapters(ExtRepositoryModelAdapterType.ENTRY,extRepositoryEntries);
  entries=_filterByMimeType(entries,mimeTypes);
  return (List)_subList(entries,start,end,obc);
}
