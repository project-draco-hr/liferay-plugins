{
  String repositoryMappedFolderId=_getRepositoryEntryMappedId(folderId);
  List<ExtRepositoryObject> extRepositoryObjects=_extRepository.getExtRepositoryObjects(ExtRepositoryObjectType.ENTRY,repositoryMappedFolderId);
  List<ExtRepositoryObjectAdapter<?>> extRepositoryObjectAdapters=_toExtRepositoryObjectAdapters(ExtRepositoryObjectAdapterType.OBJECT,extRepositoryObjects);
  extRepositoryObjectAdapters=_filterByMimeType(extRepositoryObjectAdapters,mimeTypes);
  return (List)_subList(extRepositoryObjectAdapters,start,end,obc);
}
