{
  List<ProcessModelDefinition> processModelDefinitions=new ArrayList<ProcessModelDefinition>();
  if (workflowDefinitions == null) {
    return processModelDefinitions;
  }
  for (  WorkflowDefinition workflowDefinition : workflowDefinitions) {
    processModelDefinitions.add(new WorkflowDefinitionBridge(workflowDefinition));
  }
  return processModelDefinitions;
}
