{
  pollsQuestionPersistence.remove(pollsQuestion);
  resourceLocalService.deleteResource(pollsQuestion.getCompanyId(),PollsQuestion.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,pollsQuestion.getPollsQuestionId());
  pollsChoicePersistence.removeByPollsQuestionId(pollsQuestion.getPollsQuestionId());
  pollsVotePersistence.removeByPollsQuestionId(pollsQuestion.getPollsQuestionId());
  return pollsQuestion;
}
