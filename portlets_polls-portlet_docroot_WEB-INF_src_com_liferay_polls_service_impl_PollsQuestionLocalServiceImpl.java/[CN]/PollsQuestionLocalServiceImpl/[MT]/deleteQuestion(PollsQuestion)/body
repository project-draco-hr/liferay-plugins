{
  pollsQuestionPersistence.remove(question);
  resourceLocalService.deleteResource(question.getCompanyId(),PollsQuestion.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,question.getPollsQuestionId());
  pollsChoicePersistence.removeByPollsQuestionId(question.getPollsQuestionId());
  pollsVotePersistence.removeByPollsQuestionId(question.getPollsQuestionId());
}
