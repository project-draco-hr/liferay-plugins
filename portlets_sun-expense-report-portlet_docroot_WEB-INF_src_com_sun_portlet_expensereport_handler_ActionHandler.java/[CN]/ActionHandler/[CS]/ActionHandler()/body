{
  if (dbHelper == null) {
    String db;
    try {
      db=getProperty("DB");
      if (db != null && db.equals("MySQL")) {
        dbHelper=new DBHelper();
      }
 else {
        dbHelper=new HSQLDBHelper();
      }
    }
 catch (    IOException ex) {
      ex.printStackTrace();
    }
  }
  if (wfHelper == null) {
    wfHelper=new WFHelper();
  }
}
