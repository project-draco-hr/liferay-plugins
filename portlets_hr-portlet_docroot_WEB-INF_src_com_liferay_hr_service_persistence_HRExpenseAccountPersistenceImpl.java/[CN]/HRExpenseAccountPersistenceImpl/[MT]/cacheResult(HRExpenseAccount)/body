{
  EntityCacheUtil.putResult(HRExpenseAccountModelImpl.ENTITY_CACHE_ENABLED,HRExpenseAccountImpl.class,hrExpenseAccount.getPrimaryKey(),hrExpenseAccount);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_G_N,new Object[]{Long.valueOf(hrExpenseAccount.getGroupId()),hrExpenseAccount.getName()},hrExpenseAccount);
  hrExpenseAccount.resetOriginalValues();
}
