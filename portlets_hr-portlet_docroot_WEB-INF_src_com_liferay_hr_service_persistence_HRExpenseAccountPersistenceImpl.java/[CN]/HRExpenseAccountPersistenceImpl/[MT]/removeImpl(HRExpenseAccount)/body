{
  hrExpenseAccount=toUnwrappedModel(hrExpenseAccount);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,hrExpenseAccount);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  HRExpenseAccountModelImpl hrExpenseAccountModelImpl=(HRExpenseAccountModelImpl)hrExpenseAccount;
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_G_N,new Object[]{Long.valueOf(hrExpenseAccountModelImpl.getGroupId()),hrExpenseAccountModelImpl.getName()});
  EntityCacheUtil.removeResult(HRExpenseAccountModelImpl.ENTITY_CACHE_ENABLED,HRExpenseAccountImpl.class,hrExpenseAccount.getPrimaryKey());
  return hrExpenseAccount;
}
