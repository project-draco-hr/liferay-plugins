{
  HRExpenseAccount hrExpenseAccount=(HRExpenseAccount)EntityCacheUtil.getResult(HRExpenseAccountModelImpl.ENTITY_CACHE_ENABLED,HRExpenseAccountImpl.class,hrExpenseAccountId,this);
  if (hrExpenseAccount == null) {
    Session session=null;
    try {
      session=openSession();
      hrExpenseAccount=(HRExpenseAccount)session.get(HRExpenseAccountImpl.class,Long.valueOf(hrExpenseAccountId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrExpenseAccount != null) {
        cacheResult(hrExpenseAccount);
      }
      closeSession(session);
    }
  }
  return hrExpenseAccount;
}
