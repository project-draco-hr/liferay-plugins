{
  hrExpenseAccount=toUnwrappedModel(hrExpenseAccount);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,hrExpenseAccount,merge);
    hrExpenseAccount.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(HRExpenseAccountModelImpl.ENTITY_CACHE_ENABLED,HRExpenseAccountImpl.class,hrExpenseAccount.getPrimaryKey(),hrExpenseAccount);
  return hrExpenseAccount;
}
