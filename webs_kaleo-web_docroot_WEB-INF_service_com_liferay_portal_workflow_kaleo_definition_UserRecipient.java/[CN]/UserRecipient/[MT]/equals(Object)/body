{
  if (obj == null) {
    return false;
  }
  if (!(obj instanceof UserRecipient)) {
    return false;
  }
  UserRecipient userRecipient=(UserRecipient)obj;
  if (Validator.equals(_emailAddress,userRecipient._emailAddress) && Validator.equals(_screenName,userRecipient._screenName) && (_userId == userRecipient._userId)) {
    return true;
  }
  return true;
}
