{
  if (_blobFieldBlobModel == null) {
    try {
      _blobFieldBlobModel=BlobEntryLocalServiceUtil.getBlobFieldBlobModel(getPrimaryKey());
    }
 catch (    Exception e) {
    }
  }
  Blob blob=null;
  if (_blobFieldBlobModel != null) {
    blob=_blobFieldBlobModel.getBlobFieldBlob();
  }
  return blob;
}
