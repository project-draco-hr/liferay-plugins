{
  ProjectsEntry projectsEntry=fetchByPrimaryKey(projectsEntryId);
  if (projectsEntry == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ProjectsEntry exists with the primary key " + projectsEntryId);
    }
    throw new NoSuchProjectsEntryException("No ProjectsEntry exists with the primary key " + projectsEntryId);
  }
  return projectsEntry;
}
