{
  ProjectsEntry projectsEntry=(ProjectsEntry)EntityCacheUtil.getResult(ProjectsEntryModelImpl.ENTITY_CACHE_ENABLED,ProjectsEntryImpl.class,projectsEntryId,this);
  if (projectsEntry == null) {
    Session session=null;
    try {
      session=openSession();
      projectsEntry=(ProjectsEntry)session.get(ProjectsEntryImpl.class,new Long(projectsEntryId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (projectsEntry != null) {
        cacheResult(projectsEntry);
      }
      closeSession(session);
    }
  }
  return projectsEntry;
}
