{
  SemanticsElement semanticsElement=fetchByPrimaryKey(elementURI);
  if (semanticsElement == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No SemanticsElement exists with the primary key " + elementURI);
    }
    throw new NoSuchSemanticsElementException("No SemanticsElement exists with the primary key " + elementURI);
  }
  return semanticsElement;
}
