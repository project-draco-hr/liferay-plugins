{
  if (_persistence == null) {
    _persistence=(KaleoNodePersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoNodePersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoNodeUtil.class,"_persistence");
  }
  return _persistence;
}
