{
  Session session=null;
  try {
    session=openSession();
    BlobEntry blobEntry=(BlobEntry)session.get(BlobEntryImpl.class,primaryKey);
    if (blobEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
      }
      throw new NoSuchBlobEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    return remove(blobEntry);
  }
 catch (  NoSuchBlobEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
