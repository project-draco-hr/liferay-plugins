{
  blobEntry=toUnwrappedModel(blobEntry);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(blobEntry)) {
      blobEntry=(BlobEntry)session.get(BlobEntryImpl.class,blobEntry.getPrimaryKeyObj());
    }
    if (blobEntry != null) {
      session.delete(blobEntry);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (blobEntry != null) {
    clearCache(blobEntry);
  }
  return blobEntry;
}
