{
  BlobEntry blobEntry=findByPrimaryKey(blobEntryId);
  Session session=null;
  try {
    session=openSession();
    BlobEntry[] array=new BlobEntryImpl[3];
    array[0]=getByUuid_PrevAndNext(session,blobEntry,uuid,orderByComparator,true);
    array[1]=blobEntry;
    array[2]=getByUuid_PrevAndNext(session,blobEntry,uuid,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
