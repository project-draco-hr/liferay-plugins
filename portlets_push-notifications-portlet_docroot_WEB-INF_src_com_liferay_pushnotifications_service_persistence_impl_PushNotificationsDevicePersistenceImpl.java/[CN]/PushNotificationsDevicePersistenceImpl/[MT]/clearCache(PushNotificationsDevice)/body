{
  EntityCacheUtil.removeResult(PushNotificationsDeviceModelImpl.ENTITY_CACHE_ENABLED,PushNotificationsDeviceImpl.class,pushNotificationsDevice.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  clearUniqueFindersCache(pushNotificationsDevice);
}
