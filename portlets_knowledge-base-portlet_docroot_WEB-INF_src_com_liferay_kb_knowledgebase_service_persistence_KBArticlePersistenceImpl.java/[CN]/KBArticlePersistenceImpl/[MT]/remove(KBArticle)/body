{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(kbArticle);
    }
  }
  kbArticle=removeImpl(kbArticle);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(kbArticle);
    }
  }
  return kbArticle;
}
