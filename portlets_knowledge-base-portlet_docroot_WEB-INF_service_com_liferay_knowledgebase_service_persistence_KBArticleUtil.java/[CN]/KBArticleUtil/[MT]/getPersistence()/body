{
  if (_persistence == null) {
    _persistence=(KBArticlePersistence)PortletBeanLocatorUtil.locate(com.liferay.knowledgebase.service.ClpSerializer.getServletContextName(),KBArticlePersistence.class.getName());
    ReferenceRegistry.registerReference(KBArticleUtil.class,"_persistence");
  }
  return _persistence;
}
