{
  if (_persistence == null) {
    _persistence=(KBStructurePersistence)PortletBeanLocatorUtil.locate(com.liferay.knowledgebase.service.ClpSerializer.getServletContextName(),KBStructurePersistence.class.getName());
    ReferenceRegistry.registerReference(KBStructureUtil.class,"_persistence");
  }
  return _persistence;
}
