{
  JIRAProject jiraProject=(JIRAProject)EntityCacheUtil.getResult(JIRAProjectModelImpl.ENTITY_CACHE_ENABLED,JIRAProjectImpl.class,primaryKey);
  if (jiraProject == _nullJIRAProject) {
    return null;
  }
  if (jiraProject == null) {
    Session session=null;
    try {
      session=openSession();
      jiraProject=(JIRAProject)session.get(JIRAProjectImpl.class,primaryKey);
      if (jiraProject != null) {
        cacheResult(jiraProject);
