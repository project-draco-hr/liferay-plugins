{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  JIRAProject jiraProject : jiraProjects) {
    EntityCacheUtil.removeResult(JIRAProjectModelImpl.ENTITY_CACHE_ENABLED,JIRAProjectImpl.class,jiraProject.getPrimaryKey());
    clearUniqueFindersCache((JIRAProjectModelImpl)jiraProject);
  }
}
