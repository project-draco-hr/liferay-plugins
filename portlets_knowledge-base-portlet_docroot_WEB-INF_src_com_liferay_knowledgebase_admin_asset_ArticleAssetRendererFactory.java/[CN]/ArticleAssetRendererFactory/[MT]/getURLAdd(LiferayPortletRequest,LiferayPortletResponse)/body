{
  ThemeDisplay themeDisplay=(ThemeDisplay)liferayPortletRequest.getAttribute(WebKeys.THEME_DISPLAY);
  PortletURL addAssetURL=null;
  if (AdminPermission.contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),ActionKeys.ADD_ARTICLE)) {
    addAssetURL=liferayPortletResponse.createRenderURL(PortletKeys.KNOWLEDGE_BASE_ADMIN);
    addAssetURL.setParameter("jspPage","/admin/edit_article.jsp");
  }
  return addAssetURL;
}
