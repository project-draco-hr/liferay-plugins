{
  if (_persistence == null) {
    _persistence=(HRProjectStatusPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRProjectStatusPersistence.class.getName());
    ReferenceRegistry.registerReference(HRProjectStatusUtil.class,"_persistence");
  }
  return _persistence;
}
