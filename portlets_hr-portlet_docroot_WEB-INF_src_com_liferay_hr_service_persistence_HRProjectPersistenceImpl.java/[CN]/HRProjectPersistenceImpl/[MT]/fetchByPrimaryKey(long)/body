{
  HRProject hrProject=(HRProject)EntityCacheUtil.getResult(HRProjectModelImpl.ENTITY_CACHE_ENABLED,HRProjectImpl.class,hrProjectId,this);
  if (hrProject == null) {
    Session session=null;
    try {
      session=openSession();
      hrProject=(HRProject)session.get(HRProjectImpl.class,Long.valueOf(hrProjectId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrProject != null) {
        cacheResult(hrProject);
      }
      closeSession(session);
    }
  }
  return hrProject;
}
