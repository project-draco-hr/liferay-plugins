{
  searchString=searchString.trim();
  String[] messageUids=null;
  if (searchString.equals(StringPool.BLANK)) {
    messageUids=_getMessageUidsByFolder(user,mailAccount,folderName);
  }
 else {
    messageUids=_getMessageUidsBySearch(user,mailAccount,folderName,searchString);
  }
  for (int i=0; i < messageUids.length; i++) {
    long tempMessageUid=GetterUtil.getLong(messageUids[i]);
    if (messageUid == tempMessageUid) {
      return getJSONMessageByUid(user,mailAccount,folderName,messageUids[i + offset]);
    }
  }
  return null;
}
