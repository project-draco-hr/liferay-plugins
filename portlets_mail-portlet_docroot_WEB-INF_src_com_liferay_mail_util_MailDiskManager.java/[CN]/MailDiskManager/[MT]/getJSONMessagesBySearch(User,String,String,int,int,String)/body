{
  if (Validator.isNull(user) || Validator.isNull(emailAddress) || Validator.isNull(folderName)) {
    return null;
  }
  searchString=searchString.trim();
  long[] messageUids=getMessageUidsBySearch(user,emailAddress,folderName,searchString);
  int messageCount=messageUids.length;
  return getJSONPaginatedMessages(user,emailAddress,folderName,messageUids,pageNumber,messagesPerPage,messageCount,messageCount);
}
