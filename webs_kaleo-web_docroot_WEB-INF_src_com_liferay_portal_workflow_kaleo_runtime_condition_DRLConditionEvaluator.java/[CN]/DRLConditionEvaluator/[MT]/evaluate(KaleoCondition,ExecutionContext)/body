{
  List<Fact<?>> facts=RulesContextBuilder.buildRulesContext(executionContext);
  RulesResourceRetriever rulesResourceRetriever=new RulesResourceRetriever(new StringResourceRetriever(kaleoCondition.getScript()));
  Query query=Query.createStandardQuery();
  Map<String,?> results=RulesEngineUtil.execute(rulesResourceRetriever,facts,query,PortalClassLoaderUtil.getClassLoader());
  Boolean returnValue=(Boolean)results.get(_RETURN_VALUE);
  if (returnValue != null) {
    return returnValue;
  }
  if (_log.isWarnEnabled()) {
    _log.warn("Conditional did not return value for script " + kaleoCondition.getScript());
  }
  return false;
}
