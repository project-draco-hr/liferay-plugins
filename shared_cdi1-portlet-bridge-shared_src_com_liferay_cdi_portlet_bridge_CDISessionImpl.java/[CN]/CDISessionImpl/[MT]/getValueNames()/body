{
  String[] valueNames=null;
  Enumeration<String> attributeNames=getPortletSession().getAttributeNames();
  if (attributeNames != null) {
    List<String> valueNameArray=new ArrayList<String>();
    while (attributeNames.hasMoreElements()) {
      String attributeName=attributeNames.nextElement();
      valueNameArray.add(attributeName);
    }
    valueNames=valueNameArray.toArray(new String[valueNameArray.size()]);
  }
  return valueNames;
}
