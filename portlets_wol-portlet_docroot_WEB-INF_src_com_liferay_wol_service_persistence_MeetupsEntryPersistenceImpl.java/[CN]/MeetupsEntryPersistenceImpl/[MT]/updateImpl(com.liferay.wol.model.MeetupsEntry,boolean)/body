{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,meetupsEntry,merge);
    meetupsEntry.setNew(false);
    return meetupsEntry;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(MeetupsEntry.class.getName());
  }
}
