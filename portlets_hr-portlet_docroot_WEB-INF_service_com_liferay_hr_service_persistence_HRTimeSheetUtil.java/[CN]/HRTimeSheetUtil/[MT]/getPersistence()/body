{
  if (_persistence == null) {
    _persistence=(HRTimeSheetPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRTimeSheetPersistence.class.getName());
    ReferenceRegistry.registerReference(HRTimeSheetUtil.class,"_persistence");
  }
  return _persistence;
}
