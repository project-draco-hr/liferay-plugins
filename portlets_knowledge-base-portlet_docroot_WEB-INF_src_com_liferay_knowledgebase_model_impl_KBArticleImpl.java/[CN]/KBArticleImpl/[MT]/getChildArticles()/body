{
  List<KBArticle> articles=null;
  if (getParentResourcePrimKey() <= 0) {
    return Collections.EMPTY_LIST;
  }
  try {
    articles=KBArticleLocalServiceUtil.getChildren(getParentResourcePrimKey(),true);
  }
 catch (  Exception e) {
    articles=new ArrayList<KBArticle>();
    _log.error(e);
  }
  return articles;
}
