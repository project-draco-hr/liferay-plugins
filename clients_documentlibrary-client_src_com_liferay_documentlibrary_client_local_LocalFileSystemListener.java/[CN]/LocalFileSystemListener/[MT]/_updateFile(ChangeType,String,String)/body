{
  FileData data=FileSystemManagerUtil.getFileData(_getFullName(rootPath,name));
  if (data == null) {
    _createLocalFile(rootPath,name);
  }
 else   if (FileSystemEventManagerUtil.isLocalFileEvent(data.getId(),changeType)) {
    if (_log.isInfoEnabled()) {
      _log.info("Ignoring file event " + rootPath + StringPool.SPACE+ name);
    }
  }
 else {
    _updateLocalFile(changeType,rootPath,name);
  }
}
