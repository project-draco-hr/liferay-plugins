{
  ThemeDisplay themeDisplay=(ThemeDisplay)resourceRequest.getAttribute(WebKeys.THEME_DISPLAY);
  String keywords=ParamUtil.getString(resourceRequest,"keywords");
  JSONObject jsonObject=PrivateMessagingUtil.getJSONRecipients(themeDisplay.getUserId(),PortletPropsValues.AUTOCOMPLETE_RECIPIENT_TYPE,keywords,0,PortletPropsValues.AUTOCOMPLETE_RECIPIENT_MAX);
  JSONObject results=JSONFactoryUtil.createJSONObject();
  results.put("results",jsonObject);
  writeJSON(resourceRequest,resourceResponse,results);
}
