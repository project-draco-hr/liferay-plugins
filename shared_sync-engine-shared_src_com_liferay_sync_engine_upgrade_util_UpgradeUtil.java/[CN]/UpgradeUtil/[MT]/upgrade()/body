{
  int buildNumber=SyncPropService.getInteger("buildNumber");
  if (buildNumber == 0) {
    createTables();
    Path filePath=Paths.get(PropsValues.SYNC_CONFIGURATION_DIRECTORY + "/files");
    Files.createDirectories(filePath);
  }
 else   if (buildNumber == ReleaseInfo.getBuildNumber()) {
    return;
  }
  SyncPropService.updateSyncProp("buildNumber",ReleaseInfo.getBuildNumber());
}
