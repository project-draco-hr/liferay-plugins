{
  int buildNumber=SyncPropService.getInteger("buildNumber");
  if (buildNumber == 0) {
    createTables();
    Path configurationFilePath=Paths.get(FileUtil.getFilePathName(PropsValues.SYNC_CONFIGURATION_DIRECTORY));
    Files.createDirectories(configurationFilePath.resolve("files"));
    ClassLoader classLoader=LoggerUtil.class.getClassLoader();
    URL url=classLoader.getResource(PropsValues.SYNC_LOGGER_CONFIGURATION_FILE);
    Path sourceLoggerConfigurationFilePath=Paths.get(url.getPath());
    Path destinationLoggerConfigurationFilePath=configurationFilePath.resolve(sourceLoggerConfigurationFilePath.getFileName());
    if (!Files.exists(destinationLoggerConfigurationFilePath)) {
      Files.copy(sourceLoggerConfigurationFilePath,destinationLoggerConfigurationFilePath);
    }
  }
 else   if (buildNumber == ReleaseInfo.getBuildNumber()) {
    return;
  }
  SyncPropService.updateSyncProp("buildNumber",ReleaseInfo.getBuildNumber());
}
