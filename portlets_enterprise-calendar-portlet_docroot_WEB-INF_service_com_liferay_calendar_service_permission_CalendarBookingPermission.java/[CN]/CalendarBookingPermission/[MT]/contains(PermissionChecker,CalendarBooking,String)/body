{
  if (permissionChecker.hasOwnerPermission(calendarBooking.getCompanyId(),CalendarBooking.class.getName(),calendarBooking.getCalendarBookingId(),calendarBooking.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(calendarBooking.getGroupId(),CalendarBooking.class.getName(),calendarBooking.getCalendarBookingId(),actionId);
}
