{
  BBBParticipant bbbParticipant=findByPrimaryKey(bbbParticipantId);
  Session session=null;
  try {
    session=openSession();
    BBBParticipant[] array=new BBBParticipantImpl[3];
    array[0]=getByBbbMeetingId_PrevAndNext(session,bbbParticipant,bbbMeetingId,orderByComparator,true);
    array[1]=bbbParticipant;
    array[2]=getByBbbMeetingId_PrevAndNext(session,bbbParticipant,bbbMeetingId,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
