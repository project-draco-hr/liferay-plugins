{
  BBBMeeting bbbMeeting=findByPrimaryKey(bbbMeetingId);
  Session session=null;
  try {
    session=openSession();
    BBBMeeting[] array=new BBBMeetingImpl[3];
    array[0]=getByStatus_PrevAndNext(session,bbbMeeting,status,orderByComparator,true);
    array[1]=bbbMeeting;
    array[2]=getByStatus_PrevAndNext(session,bbbMeeting,status,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
