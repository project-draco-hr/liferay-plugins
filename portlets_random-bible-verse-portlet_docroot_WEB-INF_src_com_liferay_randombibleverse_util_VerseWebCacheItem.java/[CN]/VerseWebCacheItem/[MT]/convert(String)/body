{
  try {
    Verse verse=null;
    if (_language.equalsIgnoreCase("fi")) {
      verse=_getUskonkirjat(verse);
    }
 else {
      verse=_getBiblegateway(verse);
    }
    return verse;
  }
 catch (  Exception e) {
    throw new WebCacheException(_location + " " + _versionId+ " "+ e.toString());
  }
}
