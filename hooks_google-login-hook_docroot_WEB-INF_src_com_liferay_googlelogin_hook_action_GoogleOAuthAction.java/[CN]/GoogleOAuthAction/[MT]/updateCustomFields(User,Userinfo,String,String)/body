{
  ExpandoValueLocalServiceUtil.addValue(user.getCompanyId(),User.class.getName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,GOOGLE_ACCESS_TOKEN,user.getUserId(),accessToken);
  ExpandoValueLocalServiceUtil.addValue(user.getCompanyId(),User.class.getName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,GOOGLE_REFRESH_TOKEN,user.getUserId(),refreshToken);
  ExpandoValueLocalServiceUtil.addValue(user.getCompanyId(),User.class.getName(),ExpandoTableConstants.DEFAULT_TABLE_NAME,GOOGLE_USER_ID,user.getUserId(),userinfo.getId());
}
