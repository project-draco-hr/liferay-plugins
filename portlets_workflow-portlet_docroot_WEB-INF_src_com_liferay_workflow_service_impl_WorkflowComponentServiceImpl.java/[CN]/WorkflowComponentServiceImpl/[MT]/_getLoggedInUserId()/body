{
  User user=null;
  String userId=null;
  try {
    user=getUser();
  }
 catch (  PortalException e1) {
    user=null;
  }
catch (  SystemException e1) {
    throw new WorkflowComponentException(e1);
  }
  if (user != null) {
    userId=String.valueOf(user.getUserId());
    ;
  }
 else {
    userId="%";
  }
  return userId;
}
