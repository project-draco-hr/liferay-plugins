{
  if (_persistence == null) {
    _persistence=(HRTaskStatusPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRTaskStatusPersistence.class.getName());
    ReferenceRegistry.registerReference(HRTaskStatusUtil.class,"_persistence");
  }
  return _persistence;
}
