{
  kbStructurePersistence.remove(kbStructure);
  resourceLocalService.deleteResource(kbStructure.getCompanyId(),KBStructure.class.getName(),ResourceConstants.SCOPE_INDIVIDUAL,kbStructure.getKbStructureId());
  kbCommentLocalService.deleteKBComments(KBStructure.class.getName(),kbStructure.getKbStructureId());
  socialActivityLocalService.deleteActivities(KBStructure.class.getName(),kbStructure.getKbStructureId());
}
