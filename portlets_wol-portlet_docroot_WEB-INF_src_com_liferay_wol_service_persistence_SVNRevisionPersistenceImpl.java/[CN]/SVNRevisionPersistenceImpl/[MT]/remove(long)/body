{
  Session session=null;
  try {
    session=openSession();
    SVNRevision svnRevision=(SVNRevision)session.get(SVNRevisionImpl.class,new Long(svnRevisionId));
    if (svnRevision == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No SVNRevision exists with the primary key " + svnRevisionId);
      }
      throw new NoSuchSVNRevisionException("No SVNRevision exists with the primary key " + svnRevisionId);
    }
    return remove(svnRevision);
  }
 catch (  NoSuchSVNRevisionException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
