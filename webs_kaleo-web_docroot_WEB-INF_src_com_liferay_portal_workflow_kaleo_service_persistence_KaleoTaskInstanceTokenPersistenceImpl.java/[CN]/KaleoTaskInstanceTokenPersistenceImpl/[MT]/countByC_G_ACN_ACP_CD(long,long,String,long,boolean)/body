{
  Object[] finderArgs=new Object[]{new Long(companyId),new Long(groupId),assigneeClassName,new Long(assigneeClassPK),Boolean.valueOf(completed)};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_C_G_ACN_ACP_CD,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler(6);
      query.append(_SQL_COUNT_KALEOTASKINSTANCETOKEN_WHERE);
      query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_COMPANYID_2);
      query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_GROUPID_2);
      if (assigneeClassName == null) {
        query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_ASSIGNEECLASSNAME_1);
      }
 else {
        if (assigneeClassName.equals(StringPool.BLANK)) {
          query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_ASSIGNEECLASSNAME_3);
        }
 else {
          query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_ASSIGNEECLASSNAME_2);
        }
      }
      query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_ASSIGNEECLASSPK_2);
      query.append(_FINDER_COLUMN_C_G_ACN_ACP_CD_COMPLETED_2);
      String sql=query.toString();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(companyId);
      qPos.add(groupId);
      if (assigneeClassName != null) {
        qPos.add(assigneeClassName);
      }
      qPos.add(assigneeClassPK);
      qPos.add(completed);
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_C_G_ACN_ACP_CD,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
