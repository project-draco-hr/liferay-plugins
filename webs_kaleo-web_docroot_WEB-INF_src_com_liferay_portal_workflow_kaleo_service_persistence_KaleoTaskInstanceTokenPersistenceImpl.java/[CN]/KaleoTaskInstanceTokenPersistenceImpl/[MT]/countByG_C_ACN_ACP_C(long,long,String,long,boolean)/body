{
  Object[] finderArgs=new Object[]{new Long(groupId),new Long(companyId),assigneeClassName,new Long(assigneeClassPK),Boolean.valueOf(completed)};
  Long count=(Long)FinderCacheUtil.getResult(FINDER_PATH_COUNT_BY_G_C_ACN_ACP_C,finderArgs,this);
  if (count == null) {
    Session session=null;
    try {
      session=openSession();
      StringBundler query=new StringBundler(6);
      query.append(_SQL_COUNT_KALEOTASKINSTANCETOKEN_WHERE);
      query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_GROUPID_2);
      query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_COMPANYID_2);
      if (assigneeClassName == null) {
        query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_ASSIGNEECLASSNAME_1);
      }
 else {
        if (assigneeClassName.equals(StringPool.BLANK)) {
          query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_ASSIGNEECLASSNAME_3);
        }
 else {
          query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_ASSIGNEECLASSNAME_2);
        }
      }
      query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_ASSIGNEECLASSPK_2);
      query.append(_FINDER_COLUMN_G_C_ACN_ACP_C_COMPLETED_2);
      String sql=query.toString();
      Query q=session.createQuery(sql);
      QueryPos qPos=QueryPos.getInstance(q);
      qPos.add(groupId);
      qPos.add(companyId);
      if (assigneeClassName != null) {
        qPos.add(assigneeClassName);
      }
      qPos.add(assigneeClassPK);
      qPos.add(completed);
      count=(Long)q.uniqueResult();
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (count == null) {
        count=Long.valueOf(0);
      }
      FinderCacheUtil.putResult(FINDER_PATH_COUNT_BY_G_C_ACN_ACP_C,finderArgs,count);
      closeSession(session);
    }
  }
  return count.intValue();
}
