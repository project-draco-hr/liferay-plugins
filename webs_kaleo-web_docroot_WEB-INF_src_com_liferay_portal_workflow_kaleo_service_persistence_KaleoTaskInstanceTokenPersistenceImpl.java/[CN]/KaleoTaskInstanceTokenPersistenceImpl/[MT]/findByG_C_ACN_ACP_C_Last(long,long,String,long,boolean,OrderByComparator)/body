{
  int count=countByG_C_ACN_ACP_C(groupId,companyId,assigneeClassName,assigneeClassPK,completed);
  List<KaleoTaskInstanceToken> list=findByG_C_ACN_ACP_C(groupId,companyId,assigneeClassName,assigneeClassPK,completed,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(12);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", companyId=");
    msg.append(companyId);
    msg.append(", assigneeClassName=");
    msg.append(assigneeClassName);
    msg.append(", assigneeClassPK=");
    msg.append(assigneeClassPK);
    msg.append(", completed=");
    msg.append(completed);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchTaskInstanceTokenException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
