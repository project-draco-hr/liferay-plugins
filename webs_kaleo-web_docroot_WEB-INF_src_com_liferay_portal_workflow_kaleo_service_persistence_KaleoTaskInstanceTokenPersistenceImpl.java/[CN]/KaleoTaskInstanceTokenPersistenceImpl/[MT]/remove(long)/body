{
  Session session=null;
  try {
    session=openSession();
    KaleoTaskInstanceToken kaleoTaskInstanceToken=(KaleoTaskInstanceToken)session.get(KaleoTaskInstanceTokenImpl.class,new Long(kaleoTaskInstanceTokenId));
    if (kaleoTaskInstanceToken == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskInstanceTokenId);
      }
      throw new NoSuchTaskInstanceTokenException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskInstanceTokenId);
    }
    return remove(kaleoTaskInstanceToken);
  }
 catch (  NoSuchTaskInstanceTokenException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
