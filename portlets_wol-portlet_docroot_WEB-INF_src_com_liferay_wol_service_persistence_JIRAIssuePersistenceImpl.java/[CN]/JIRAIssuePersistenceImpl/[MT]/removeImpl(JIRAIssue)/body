{
  Session session=null;
  try {
    session=openSession();
    session.delete(jiraIssue);
    session.flush();
    return jiraIssue;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(JIRAIssue.class.getName());
  }
}
