{
  JIRAIssue jiraIssue=(JIRAIssue)EntityCacheUtil.getResult(JIRAIssueModelImpl.ENTITY_CACHE_ENABLED,JIRAIssueImpl.class,jiraIssueId,this);
  if (jiraIssue == null) {
    Session session=null;
    try {
      session=openSession();
      jiraIssue=(JIRAIssue)session.get(JIRAIssueImpl.class,new Long(jiraIssueId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (jiraIssue != null) {
        cacheResult(jiraIssue);
      }
      closeSession(session);
    }
  }
  return jiraIssue;
}
