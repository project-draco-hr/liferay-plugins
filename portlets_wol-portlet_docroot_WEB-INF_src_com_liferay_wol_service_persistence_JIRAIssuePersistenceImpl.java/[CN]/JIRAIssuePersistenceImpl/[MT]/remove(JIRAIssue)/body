{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(jiraIssue);
    }
  }
  jiraIssue=removeImpl(jiraIssue);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(jiraIssue);
    }
  }
  return jiraIssue;
}
