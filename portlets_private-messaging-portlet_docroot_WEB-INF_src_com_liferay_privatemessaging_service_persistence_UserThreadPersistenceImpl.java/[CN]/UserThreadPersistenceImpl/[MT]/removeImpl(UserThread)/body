{
  userThread=toUnwrappedModel(userThread);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,userThread);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(userThread);
  return userThread;
}
