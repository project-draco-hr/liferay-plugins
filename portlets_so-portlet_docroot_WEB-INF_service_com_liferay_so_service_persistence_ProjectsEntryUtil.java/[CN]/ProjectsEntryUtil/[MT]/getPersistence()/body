{
  if (_persistence == null) {
    _persistence=(ProjectsEntryPersistence)PortletBeanLocatorUtil.locate(com.liferay.so.service.ClpSerializer.SERVLET_CONTEXT_NAME,ProjectsEntryPersistence.class.getName());
    ReferenceRegistry.registerReference(ProjectsEntryUtil.class,"_persistence");
  }
  return _persistence;
}
