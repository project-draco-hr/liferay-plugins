{
  Weather weather=null;
  try {
    String xml=HttpUtil.URLtoString("http://www.google.com/ig/api?weather=" + HttpUtil.encodeURL(_zip));
    Document doc=SAXReaderUtil.read(xml);
    Element root=doc.getRootElement();
    Element weatherEl=root.element("weather");
    Element currentConditionsEl=weatherEl.element("current_conditions");
    Element temperatureEl=currentConditionsEl.element("temp_f");
    float temperature=GetterUtil.getFloat(temperatureEl.attributeValue("data"));
    Element iconEl=currentConditionsEl.element("icon");
    String iconURL=iconEl.attributeValue("data");
    weather=new Weather(_zip,iconURL,temperature);
  }
 catch (  Exception e) {
    throw new WebCacheException(_zip);
  }
  return weather;
}
