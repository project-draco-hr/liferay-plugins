{
  Weather weather=null;
  try {
    String xml=HttpUtil.URLtoString("http://www.google.com/ig/api?weather=" + HttpUtil.encodeURL(_zip));
    Document doc=SAXReaderUtil.read(xml);
    Element root=doc.getRootElement();
    Element weatherElement=root.element("weather");
    Element currentConditions=weatherElement.element("current_conditions");
    Element tempF=currentConditions.element("temp_f");
    float temperature=GetterUtil.getFloat(tempF.attributeValue("data"));
    Element icon=currentConditions.element("icon");
    String iconURL="http://www.google.com" + icon.attributeValue("data");
    weather=new Weather(_zip,iconURL,temperature);
  }
 catch (  Exception e) {
    throw new WebCacheException(_zip);
  }
  return weather;
}
