{
  try {
    List<Group> groups=getGroups(associationClassName,associationClassPK);
    User user=UserLocalServiceUtil.getUser((Long)classPK);
    MessageBusUtil.sendMessage(DestinationNames.ASYNC_SERVICE,new OnAssociationProcessCallable(groups,user,"deleteGroupMembers"));
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
