{
  try {
    final List<Group> groups=getGroups(associationClassName,associationClassPK);
    final User user=UserLocalServiceUtil.getUser((Long)classPK);
    Callable<Void> callable=new Callable<Void>(){
      @Override public Void call() throws Exception {
        MessageBusUtil.sendMessage(DestinationNames.ASYNC_SERVICE,new OnAssociationProcessCallable(groups,user,"deleteGroupMembers"));
        return null;
      }
    }
;
    TransactionCommitCallbackUtil.registerCallback(callable);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
