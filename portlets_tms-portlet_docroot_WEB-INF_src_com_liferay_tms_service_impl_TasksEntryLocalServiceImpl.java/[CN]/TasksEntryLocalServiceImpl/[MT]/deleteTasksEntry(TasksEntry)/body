{
  TagsAssetLocalServiceUtil.deleteAsset(TasksEntry.class.getName(),tasksEntry.getTasksEntryId());
  SocialActivityLocalServiceUtil.deleteActivities(TasksEntry.class.getName(),tasksEntry.getTasksEntryId());
  MBMessageLocalServiceUtil.deleteDiscussionMessages(TasksEntry.class.getName(),tasksEntry.getTasksEntryId());
  tasksEntryPersistence.remove(tasksEntry);
}
