{
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  for (  AkismetData akismetData : akismetDatas) {
    EntityCacheUtil.removeResult(AkismetDataModelImpl.ENTITY_CACHE_ENABLED,AkismetDataImpl.class,akismetData.getPrimaryKey());
    clearUniqueFindersCache((AkismetDataModelImpl)akismetData);
  }
}
