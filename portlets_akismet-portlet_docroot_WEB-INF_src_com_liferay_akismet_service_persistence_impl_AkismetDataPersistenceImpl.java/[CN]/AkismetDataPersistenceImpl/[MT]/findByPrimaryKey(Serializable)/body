{
  AkismetData akismetData=fetchByPrimaryKey(primaryKey);
  if (akismetData == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
    }
    throw new NoSuchDataException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + primaryKey);
  }
  return akismetData;
}
