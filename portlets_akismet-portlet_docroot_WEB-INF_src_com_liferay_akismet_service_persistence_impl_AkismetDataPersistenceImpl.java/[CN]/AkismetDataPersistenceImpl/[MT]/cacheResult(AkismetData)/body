{
  entityCache.putResult(AkismetDataModelImpl.ENTITY_CACHE_ENABLED,AkismetDataImpl.class,akismetData.getPrimaryKey(),akismetData);
  finderCache.putResult(FINDER_PATH_FETCH_BY_C_C,new Object[]{akismetData.getClassNameId(),akismetData.getClassPK()},akismetData);
  akismetData.resetOriginalValues();
}
