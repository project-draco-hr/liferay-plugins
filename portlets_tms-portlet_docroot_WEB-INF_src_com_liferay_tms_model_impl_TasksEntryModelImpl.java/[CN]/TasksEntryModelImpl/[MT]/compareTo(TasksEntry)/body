{
  int value=0;
  if (getPriority() < tasksEntry.getPriority()) {
    value=-1;
  }
 else   if (getPriority() > tasksEntry.getPriority()) {
    value=1;
  }
 else {
    value=0;
  }
  value=value * -1;
  if (value != 0) {
    return value;
  }
  value=DateUtil.compareTo(getCreateDate(),tasksEntry.getCreateDate());
  value=value * -1;
  if (value != 0) {
    return value;
  }
  return 0;
}
