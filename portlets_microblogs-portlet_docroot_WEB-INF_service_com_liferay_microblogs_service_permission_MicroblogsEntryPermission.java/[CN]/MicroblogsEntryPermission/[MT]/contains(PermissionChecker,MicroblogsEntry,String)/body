{
  if (permissionChecker.getUserId() == microblogsEntry.getUserId()) {
    return true;
  }
  if (permissionChecker.hasOwnerPermission(microblogsEntry.getCompanyId(),MicroblogsEntry.class.getName(),microblogsEntry.getMicroblogsEntryId(),microblogsEntry.getUserId(),actionId)) {
    return true;
  }
  if ((microblogsEntry.getSocialRelationType() != 0) && (microblogsEntry.getUserId() != permissionChecker.getUserId()) && !SocialRelationLocalServiceUtil.hasRelation(permissionChecker.getUserId(),microblogsEntry.getUserId(),microblogsEntry.getSocialRelationType())) {
    return false;
  }
  return permissionChecker.hasPermission(0,MicroblogsEntry.class.getName(),microblogsEntry.getMicroblogsEntryId(),actionId);
}
