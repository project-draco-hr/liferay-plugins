{
  try {
    com.liferay.knowledgebase.model.KBStructure returnValue=KBStructureServiceUtil.updateKBStructure(kbStructureId,localizedLanguageId,title,kbStructureFields,serviceContext);
    return com.liferay.knowledgebase.model.KBStructureSoap.toSoapModel(returnValue);
