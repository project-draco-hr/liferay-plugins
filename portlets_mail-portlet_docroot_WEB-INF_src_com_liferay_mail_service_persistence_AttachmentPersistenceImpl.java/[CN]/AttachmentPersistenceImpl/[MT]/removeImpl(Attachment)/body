{
  attachment=toUnwrappedModel(attachment);
  Session session=null;
  try {
    session=openSession();
    if (attachment.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(AttachmentImpl.class,attachment.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(attachment);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(AttachmentModelImpl.ENTITY_CACHE_ENABLED,AttachmentImpl.class,attachment.getPrimaryKey());
  return attachment;
}
