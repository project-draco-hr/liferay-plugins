{
  Attachment attachment=(Attachment)EntityCacheUtil.getResult(AttachmentModelImpl.ENTITY_CACHE_ENABLED,AttachmentImpl.class,attachmentId,this);
  if (attachment == null) {
    Session session=null;
    try {
      session=openSession();
      attachment=(Attachment)session.get(AttachmentImpl.class,new Long(attachmentId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (attachment != null) {
        cacheResult(attachment);
      }
      closeSession(session);
    }
  }
  return attachment;
}
