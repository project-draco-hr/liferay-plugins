{
  long companyId=PortalUtil.getCompanyId(request);
  try {
    boolean enabled=GetterUtil.getBoolean(PrefsPropsUtil.getString(companyId,PropsKeys.SAML_IDP_ENABLED));
    if (enabled) {
      SAMLUtil.performLogout(request,response);
    }
  }
 catch (  Exception e) {
    throw new ActionException(e);
  }
}
