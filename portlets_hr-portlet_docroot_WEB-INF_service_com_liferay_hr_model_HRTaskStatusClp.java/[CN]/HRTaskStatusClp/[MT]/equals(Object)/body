{
  if (obj == null) {
    return false;
  }
  HRTaskStatusClp hrTaskStatus=null;
  try {
    hrTaskStatus=(HRTaskStatusClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrTaskStatus.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
