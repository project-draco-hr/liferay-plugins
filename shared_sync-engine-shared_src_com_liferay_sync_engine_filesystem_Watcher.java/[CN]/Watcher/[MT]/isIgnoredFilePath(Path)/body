{
  try {
    if (FileUtil.isIgnoredFilePath(filePath) || !FileUtil.isValidFileName(String.valueOf(filePath.getFileName()))) {
      return true;
    }
    return false;
  }
 catch (  Exception e) {
    if (_logger.isDebugEnabled()) {
      _logger.debug(e.getMessage(),e);
    }
    return false;
  }
}
