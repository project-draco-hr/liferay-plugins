{
  SyncFile syncFile=SyncFileService.fetchSyncFile(FilePathNameUtil.getFilePathName(filePath),_watchEventListener.getSyncAccountId());
  if (syncFile != null) {
    syncFile.setLocalSyncTime(System.currentTimeMillis());
    SyncFileService.update(syncFile);
    if (syncFile.getState() == SyncFile.STATE_ERROR) {
      return;
    }
  }
  _watchEventListener.watchEvent(eventType,filePath);
}
