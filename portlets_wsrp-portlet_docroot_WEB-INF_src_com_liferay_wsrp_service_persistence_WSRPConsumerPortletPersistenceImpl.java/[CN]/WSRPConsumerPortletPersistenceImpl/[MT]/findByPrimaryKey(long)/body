{
  WSRPConsumerPortlet wsrpConsumerPortlet=fetchByPrimaryKey(wsrpConsumerPortletId);
  if (wsrpConsumerPortlet == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No WSRPConsumerPortlet exists with the primary key " + wsrpConsumerPortletId);
    }
    throw new NoSuchConsumerPortletException("No WSRPConsumerPortlet exists with the primary key " + wsrpConsumerPortletId);
  }
  return wsrpConsumerPortlet;
}
