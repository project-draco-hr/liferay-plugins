{
  WSRPConsumerPortlet wsrpConsumerPortlet=fetchByPrimaryKey(wsrpConsumerPortletId);
  if (wsrpConsumerPortlet == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + wsrpConsumerPortletId);
    }
    throw new NoSuchConsumerPortletException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + wsrpConsumerPortletId);
  }
  return wsrpConsumerPortlet;
}
