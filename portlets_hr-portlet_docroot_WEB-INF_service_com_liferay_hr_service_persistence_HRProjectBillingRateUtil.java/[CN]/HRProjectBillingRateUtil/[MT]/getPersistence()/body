{
  if (_persistence == null) {
    _persistence=(HRProjectBillingRatePersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRProjectBillingRatePersistence.class.getName());
    ReferenceRegistry.registerReference(HRProjectBillingRateUtil.class,"_persistence");
