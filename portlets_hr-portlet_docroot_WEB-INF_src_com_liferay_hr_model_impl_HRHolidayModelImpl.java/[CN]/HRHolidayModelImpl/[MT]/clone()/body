{
  HRHolidayImpl hrHolidayImpl=new HRHolidayImpl();
  hrHolidayImpl.setHrHolidayId(getHrHolidayId());
  hrHolidayImpl.setGroupId(getGroupId());
  hrHolidayImpl.setCompanyId(getCompanyId());
  hrHolidayImpl.setUserId(getUserId());
  hrHolidayImpl.setUserName(getUserName());
  hrHolidayImpl.setCreateDate(getCreateDate());
  hrHolidayImpl.setModifiedDate(getModifiedDate());
  hrHolidayImpl.setName(getName());
  hrHolidayImpl.setDescription(getDescription());
  hrHolidayImpl.setDayOfYear(getDayOfYear());
  hrHolidayImpl.setYear(getYear());
  hrHolidayImpl.setPaid(getPaid());
  hrHolidayImpl.resetOriginalValues();
  return hrHolidayImpl;
}
