{
  KaleoTaskAssignment kaleoTaskAssignment=fetchByPrimaryKey(kaleoTaskAssignmentId);
  if (kaleoTaskAssignment == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskAssignmentId);
    }
    throw new NoSuchTaskAssignmentException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskAssignmentId);
  }
  return kaleoTaskAssignment;
}
