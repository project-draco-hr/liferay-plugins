{
  kaleoTaskAssignment=toUnwrappedModel(kaleoTaskAssignment);
  Session session=null;
  try {
    session=openSession();
    if (kaleoTaskAssignment.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(KaleoTaskAssignmentImpl.class,kaleoTaskAssignment.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(kaleoTaskAssignment);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(KaleoTaskAssignmentModelImpl.ENTITY_CACHE_ENABLED,KaleoTaskAssignmentImpl.class,kaleoTaskAssignment.getPrimaryKey());
  return kaleoTaskAssignment;
}
