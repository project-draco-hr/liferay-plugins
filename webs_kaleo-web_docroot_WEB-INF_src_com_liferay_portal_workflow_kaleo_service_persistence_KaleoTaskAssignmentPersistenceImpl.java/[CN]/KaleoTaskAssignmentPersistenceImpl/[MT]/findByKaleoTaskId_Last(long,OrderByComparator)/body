{
  int count=countByKaleoTaskId(kaleoTaskId);
  List<KaleoTaskAssignment> list=findByKaleoTaskId(kaleoTaskId,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(4);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("kaleoTaskId=");
    msg.append(kaleoTaskId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchTaskAssignmentException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
