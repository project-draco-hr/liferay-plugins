{
  WorkflowTask workflowTask=(WorkflowTask)EntityCacheUtil.getResult(WorkflowTaskModelImpl.ENTITY_CACHE_ENABLED,WorkflowTaskImpl.class,workflowTaskId,this);
  if (workflowTask == null) {
    Session session=null;
    try {
      session=openSession();
      workflowTask=(WorkflowTask)session.get(WorkflowTaskImpl.class,new Long(workflowTaskId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (workflowTask != null) {
        cacheResult(workflowTask);
      }
      closeSession(session);
    }
  }
  return workflowTask;
}
