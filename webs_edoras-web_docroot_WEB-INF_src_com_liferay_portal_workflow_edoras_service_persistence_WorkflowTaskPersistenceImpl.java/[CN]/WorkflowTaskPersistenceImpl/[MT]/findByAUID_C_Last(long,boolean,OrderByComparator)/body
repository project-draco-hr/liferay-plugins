{
  int count=countByAUID_C(assigneeUserId,completed);
  List<WorkflowTask> list=findByAUID_C(assigneeUserId,completed,count - 1,count,obc);
  if (list.isEmpty()) {
    StringBuilder msg=new StringBuilder();
    msg.append("No WorkflowTask exists with the key {");
    msg.append("assigneeUserId=" + assigneeUserId);
    msg.append(", ");
    msg.append("completed=" + completed);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchWorkflowTaskException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
