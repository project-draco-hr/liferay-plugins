{
  WorkflowTask workflowTask=fetchByPrimaryKey(workflowTaskId);
  if (workflowTask == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No WorkflowTask exists with the primary key " + workflowTaskId);
    }
    throw new NoSuchWorkflowTaskException("No WorkflowTask exists with the primary key " + workflowTaskId);
  }
  return workflowTask;
}
