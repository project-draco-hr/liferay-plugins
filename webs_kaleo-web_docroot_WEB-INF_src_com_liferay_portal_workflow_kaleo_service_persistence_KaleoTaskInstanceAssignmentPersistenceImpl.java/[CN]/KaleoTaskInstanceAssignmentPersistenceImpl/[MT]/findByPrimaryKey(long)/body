{
  KaleoTaskInstanceAssignment kaleoTaskInstanceAssignment=fetchByPrimaryKey(kaleoTaskInstanceAssignmentId);
  if (kaleoTaskInstanceAssignment == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskInstanceAssignmentId);
    }
    throw new NoSuchTaskInstanceAssignmentException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskInstanceAssignmentId);
  }
  return kaleoTaskInstanceAssignment;
}
