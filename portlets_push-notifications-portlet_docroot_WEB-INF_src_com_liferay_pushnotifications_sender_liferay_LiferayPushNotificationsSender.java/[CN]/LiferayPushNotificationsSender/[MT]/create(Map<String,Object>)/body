{
  String username=MapUtil.getString(configuration,PortletPropsKeys.LIFERAY_USERNAME,null);
  String password=MapUtil.getString(configuration,PortletPropsKeys.LIFERAY_PASSWORD,null);
  String server=MapUtil.getString(configuration,PortletPropsKeys.LIFERAY_SERVER,null);
  Integer timeout=(Integer)configuration.get(PortletPropsKeys.LIFERAY_TIMEOUT);
  return new LiferayPushNotificationsSender(username,password,server,timeout);
}
