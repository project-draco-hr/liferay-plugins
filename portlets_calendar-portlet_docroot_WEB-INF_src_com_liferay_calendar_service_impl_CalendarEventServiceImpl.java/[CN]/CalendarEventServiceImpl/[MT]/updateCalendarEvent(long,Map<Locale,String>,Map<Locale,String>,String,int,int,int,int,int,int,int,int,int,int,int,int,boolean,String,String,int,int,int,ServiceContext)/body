{
  CalendarEventPermission.check(getPermissionChecker(),calendarEventId,ActionKeys.UPDATE);
  return calendarEventLocalService.updateCalendarEvent(calendarEventId,titleMap,descriptionMap,location,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,endDateHour,endDateMinute,durationHour,durationMinute,allDay,recurrence,type,remindBy,firstReminder,secondReminder,serviceContext);
}
