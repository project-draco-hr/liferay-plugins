{
  String sql=CustomSQLUtil.get(FIND_BY_USERS_GROUPS);
  if (groupNames.length == 0) {
    sql=StringUtil.replace(sql,new String[]{"[$USERS_GROUPS_JOIN$]","[$USERS_GROUPS_WHERE$]"},new String[]{StringPool.BLANK,StringPool.BLANK});
  }
  return StringUtil.replace(sql,new String[]{"[$USERS_GROUPS_JOIN$]","[$USERS_GROUPS_WHERE$]"},new String[]{"INNER JOIN Group_ ON Group_.groupId = Users_Groups.groupId","AND Group_.name NOT IN (?)"});
}
