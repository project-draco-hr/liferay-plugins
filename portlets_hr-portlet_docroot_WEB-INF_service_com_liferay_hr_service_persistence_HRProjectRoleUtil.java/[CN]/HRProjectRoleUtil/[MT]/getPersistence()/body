{
  if (_persistence == null) {
    _persistence=(HRProjectRolePersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRProjectRolePersistence.class.getName());
    ReferenceRegistry.registerReference(HRProjectRoleUtil.class,"_persistence");
  }
  return _persistence;
}
