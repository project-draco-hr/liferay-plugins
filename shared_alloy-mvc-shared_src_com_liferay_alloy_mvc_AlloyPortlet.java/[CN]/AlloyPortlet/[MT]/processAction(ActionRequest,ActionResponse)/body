{
  String actionName=ParamUtil.getString(actionRequest,ActionRequest.ACTION_NAME);
  if (actionName.equals("alloyDataRequest")) {
    try {
      AlloyDataRequestProcessor.process(actionRequest,actionResponse,_alloyControllers);
    }
 catch (    Exception e) {
      throw new IOException(e);
    }
    return;
  }
  String format=ParamUtil.getString(actionRequest,"format");
  if (format.equals("json")) {
    try {
      AlloyAPIRequestProcessor.processAction(actionRequest,actionResponse,_alloyControllers);
    }
 catch (    Exception e) {
      throw new IOException(e);
    }
    return;
  }
  String path=getPath(actionRequest);
  include(path,actionRequest,actionResponse);
}
