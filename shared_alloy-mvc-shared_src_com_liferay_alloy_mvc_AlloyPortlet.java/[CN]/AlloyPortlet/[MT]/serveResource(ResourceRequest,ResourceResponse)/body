{
  String format=ParamUtil.getString(resourceRequest,"format");
  if (format.equals("json")) {
    BaseAlloyControllerImpl alloyController=_alloyControllers.get(getControllerPath(resourceRequest));
    try {
      alloyController.afterPropertiesSet(resourceRequest,resourceResponse);
      alloyController.execute();
    }
 catch (    Exception e) {
      throw new IOException(e);
    }
    return;
  }
  String path=getPath(resourceRequest);
  include(path,resourceRequest,resourceResponse);
}
