{
  try {
    String customJspServletContextName=group.getTypeSettingsProperty("customJspServletContextName");
    if (Validator.isNotNull(customJspServletContextName) && customJspServletContextName.equals("so-hook")) {
      setEnableSocialOffice(group);
    }
  }
 catch (  Exception e) {
    throw new ModelListenerException(e);
  }
}
