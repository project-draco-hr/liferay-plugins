{
  try {
    java.util.List<com.liferay.microblogs.model.MicroblogsEntry> returnValue=MicroblogsEntryServiceUtil.getUserMicroblogsEntries(microblogsEntryUserId,start,end);
    return returnValue.toArray(new com.liferay.microblogs.model.MicroblogsEntry[returnValue.size()]);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
