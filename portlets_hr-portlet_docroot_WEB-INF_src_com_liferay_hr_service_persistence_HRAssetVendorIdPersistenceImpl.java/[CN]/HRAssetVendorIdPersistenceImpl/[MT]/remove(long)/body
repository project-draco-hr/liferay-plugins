{
  Session session=null;
  try {
    session=openSession();
    HRAssetVendorId hrAssetVendorId=(HRAssetVendorId)session.get(HRAssetVendorIdImpl.class,Long.valueOf(hrAssetVendorId));
    if (hrAssetVendorId == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrAssetVendorId);
      }
      throw new NoSuchAssetVendorIdException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrAssetVendorId);
    }
    return hrAssetVendorIdPersistence.remove(hrAssetVendorId);
  }
 catch (  NoSuchAssetVendorIdException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
