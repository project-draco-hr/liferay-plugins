{
  Session session=null;
  try {
    session=openSession();
    AMSCheckout amsCheckout=(AMSCheckout)session.get(AMSCheckoutImpl.class,new Long(amsCheckoutId));
    if (amsCheckout == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + amsCheckoutId);
      }
      throw new NoSuchCheckoutException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + amsCheckoutId);
    }
    return remove(amsCheckout);
  }
 catch (  NoSuchCheckoutException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
