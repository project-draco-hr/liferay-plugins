{
  if (_sender == null) {
    String key=PrefsPropsUtil.getString(PortletPropsKeys.ANDROID_API_KEY,PortletPropsValues.ANDROID_API_KEY);
    if (Validator.isNull(key)) {
      throw new PushNotificationsException("The property \"android.api.key\" is not set in " + "portlet.properties");
    }
    _sender=new Sender(key);
  }
  return _sender;
}
