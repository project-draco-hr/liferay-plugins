{
  if (queryConfig.isAllFieldsSelected()) {
    return;
  }
  Set<String> selectedFieldNamesSet=SetUtil.fromArray(queryConfig.getSelectedFieldNames());
  if (!selectedFieldNamesSet.contains(Field.UID)) {
    selectedFieldNamesSet.add(Field.UID);
  }
  solrQuery.setFields(selectedFieldNamesSet.toArray(new String[selectedFieldNamesSet.size()]));
}
