{
  Class<?> clazz=baseModel.getClass();
  String modelClassName=StringUtil.replace(clazz.getName(),"impl.",StringPool.BLANK);
  modelClassName=modelClassName.substring(0,modelClassName.length() - 4);
  return contains(themeDisplay.getPermissionChecker(),themeDisplay.getScopeGroupId(),modelClassName,(Long)baseModel.getPrimaryKeyObj(),StringUtil.toUpperCase(action));
}
