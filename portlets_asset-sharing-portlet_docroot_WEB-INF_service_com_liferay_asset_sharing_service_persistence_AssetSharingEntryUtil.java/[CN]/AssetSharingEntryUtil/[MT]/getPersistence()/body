{
  if (_persistence == null) {
    _persistence=(AssetSharingEntryPersistence)PortletBeanLocatorUtil.locate(com.liferay.asset.sharing.service.ClpSerializer.getServletContextName(),AssetSharingEntryPersistence.class.getName());
    ReferenceRegistry.registerReference(AssetSharingEntryUtil.class,"_persistence");
  }
  return _persistence;
}
