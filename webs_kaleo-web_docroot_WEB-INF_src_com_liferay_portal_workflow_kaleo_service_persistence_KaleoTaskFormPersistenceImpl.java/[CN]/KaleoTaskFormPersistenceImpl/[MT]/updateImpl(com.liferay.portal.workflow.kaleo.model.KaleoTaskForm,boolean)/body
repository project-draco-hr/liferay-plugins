{
  kaleoTaskForm=toUnwrappedModel(kaleoTaskForm);
  boolean isNew=kaleoTaskForm.isNew();
  KaleoTaskFormModelImpl kaleoTaskFormModelImpl=(KaleoTaskFormModelImpl)kaleoTaskForm;
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,kaleoTaskForm,merge);
    kaleoTaskForm.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  if (isNew) {
    FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  }
 else {
    if (kaleoTaskForm.getCompanyId() != kaleoTaskFormModelImpl.getOriginalCompanyId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_COMPANYID,new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getOriginalCompanyId())});
    }
    if (kaleoTaskForm.getKaleoDefinitionId() != kaleoTaskFormModelImpl.getOriginalKaleoDefinitionId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEODEFINITIONID,new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getOriginalKaleoDefinitionId())});
    }
    if (kaleoTaskForm.getKaleoTaskId() != kaleoTaskFormModelImpl.getOriginalKaleoTaskId()) {
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEOTASKID,new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getOriginalKaleoTaskId())});
    }
  }
  EntityCacheUtil.putResult(KaleoTaskFormModelImpl.ENTITY_CACHE_ENABLED,KaleoTaskFormImpl.class,kaleoTaskForm.getPrimaryKey(),kaleoTaskForm);
  return kaleoTaskForm;
}
