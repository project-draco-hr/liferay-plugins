{
  kaleoTaskForm=toUnwrappedModel(kaleoTaskForm);
  boolean isNew=kaleoTaskForm.isNew();
  KaleoTaskFormModelImpl kaleoTaskFormModelImpl=(KaleoTaskFormModelImpl)kaleoTaskForm;
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,kaleoTaskForm,merge);
    kaleoTaskForm.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  if (isNew || !KaleoTaskFormModelImpl.COLUMN_BITMASK_ENABLED) {
    FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  }
 else {
    if ((kaleoTaskFormModelImpl.getColumnBitmask() & FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_COMPANYID.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getOriginalCompanyId())};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_COMPANYID,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_COMPANYID,args);
      args=new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getCompanyId())};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_COMPANYID,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_COMPANYID,args);
    }
    if ((kaleoTaskFormModelImpl.getColumnBitmask() & FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEODEFINITIONID.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getOriginalKaleoDefinitionId())};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_KALEODEFINITIONID,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEODEFINITIONID,args);
      args=new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getKaleoDefinitionId())};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_KALEODEFINITIONID,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEODEFINITIONID,args);
    }
    if ((kaleoTaskFormModelImpl.getColumnBitmask() & FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEOTASKID.getColumnBitmask()) != 0) {
      Object[] args=new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getOriginalKaleoTaskId())};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_KALEOTASKID,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEOTASKID,args);
      args=new Object[]{Long.valueOf(kaleoTaskFormModelImpl.getKaleoTaskId())};
      FinderCacheUtil.removeResult(FINDER_PATH_COUNT_BY_KALEOTASKID,args);
      FinderCacheUtil.removeResult(FINDER_PATH_WITHOUT_PAGINATION_FIND_BY_KALEOTASKID,args);
    }
  }
  EntityCacheUtil.putResult(KaleoTaskFormModelImpl.ENTITY_CACHE_ENABLED,KaleoTaskFormImpl.class,kaleoTaskForm.getPrimaryKey(),kaleoTaskForm);
  return kaleoTaskForm;
}
