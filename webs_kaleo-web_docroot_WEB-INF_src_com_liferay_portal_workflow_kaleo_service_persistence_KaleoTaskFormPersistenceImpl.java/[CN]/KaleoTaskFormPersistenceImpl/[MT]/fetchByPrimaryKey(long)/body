{
  KaleoTaskForm kaleoTaskForm=(KaleoTaskForm)EntityCacheUtil.getResult(KaleoTaskFormModelImpl.ENTITY_CACHE_ENABLED,KaleoTaskFormImpl.class,kaleoTaskFormId);
  if (kaleoTaskForm == _nullKaleoTaskForm) {
    return null;
  }
  if (kaleoTaskForm == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      kaleoTaskForm=(KaleoTaskForm)session.get(KaleoTaskFormImpl.class,Long.valueOf(kaleoTaskFormId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (kaleoTaskForm != null) {
        cacheResult(kaleoTaskForm);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(KaleoTaskFormModelImpl.ENTITY_CACHE_ENABLED,KaleoTaskFormImpl.class,kaleoTaskFormId,_nullKaleoTaskForm);
      }
      closeSession(session);
    }
  }
  return kaleoTaskForm;
}
