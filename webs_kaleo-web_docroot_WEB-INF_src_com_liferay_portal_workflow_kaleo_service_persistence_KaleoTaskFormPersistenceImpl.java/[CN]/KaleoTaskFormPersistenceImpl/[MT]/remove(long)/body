{
  Session session=null;
  try {
    session=openSession();
    KaleoTaskForm kaleoTaskForm=(KaleoTaskForm)session.get(KaleoTaskFormImpl.class,Long.valueOf(kaleoTaskFormId));
    if (kaleoTaskForm == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskFormId);
      }
      throw new NoSuchTaskFormException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoTaskFormId);
    }
    return kaleoTaskFormPersistence.remove(kaleoTaskForm);
  }
 catch (  NoSuchTaskFormException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
