{
  try {
    if (presenceStatusPersistence.countAll() == 0) {
      PresenceStatus pstatusesOnline=new PresenceStatusImpl();
      pstatusesOnline.setPresenceStatusId(1);
      pstatusesOnline.setPresenceStatusMessage("Online");
      addPresenceStatus(pstatusesOnline);
      PresenceStatus pstatusesoffline=new PresenceStatusImpl();
      pstatusesoffline.setPresenceStatusId(2);
      pstatusesoffline.setPresenceStatusMessage("Offline");
      addPresenceStatus(pstatusesoffline);
    }
  }
 catch (  Exception e) {
    throw new RUONException(e);
  }
}
