{
  HRExpenseType hrExpenseType=(HRExpenseType)EntityCacheUtil.getResult(HRExpenseTypeModelImpl.ENTITY_CACHE_ENABLED,HRExpenseTypeImpl.class,hrExpenseTypeId);
  if (hrExpenseType == _nullHRExpenseType) {
    return null;
  }
  if (hrExpenseType == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrExpenseType=(HRExpenseType)session.get(HRExpenseTypeImpl.class,Long.valueOf(hrExpenseTypeId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrExpenseType != null) {
        cacheResult(hrExpenseType);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRExpenseTypeModelImpl.ENTITY_CACHE_ENABLED,HRExpenseTypeImpl.class,hrExpenseTypeId,_nullHRExpenseType);
      }
      closeSession(session);
    }
  }
  return hrExpenseType;
}
