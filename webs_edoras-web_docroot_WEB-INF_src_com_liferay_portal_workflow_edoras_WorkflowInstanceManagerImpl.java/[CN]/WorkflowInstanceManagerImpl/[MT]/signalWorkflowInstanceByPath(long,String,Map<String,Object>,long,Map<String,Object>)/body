{
  ProcessSession processSession=ProcessSystemUtil.getCurrentSession();
  ProcessInstance processInstance=processSession.getProcessDao().loadProcessInstance(workflowInstanceId,true);
  if (attributes != null) {
    processInstance.getAttributeMap().copyFrom(attributes);
  }
  processSession.getEngine().executeActivityByPath(pathName,processInstance);
  return new WorkflowInstanceInfoImpl(processInstance,true);
}
