{
  ProcessSetup processSetup=getSetup();
  ProcessDao processDao=processSetup.getProcessDao();
  List<? extends MutableProcessInstance> mutableProcessInstances=processDao.loadProcessInstances(relationType,relationId,retrieveChildrenInfo);
  if (!mutableProcessInstances.isEmpty()) {
    return new WorkflowInstanceInfoImpl(mutableProcessInstances.get(0),retrieveChildrenInfo);
  }
  return null;
}
