{
  try {
    com.liferay.knowledgebase.model.Template returnValue=TemplateServiceUtil.updateTemplate(templateId,title,content,description,serviceContext);
    return com.liferay.knowledgebase.model.TemplateSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
