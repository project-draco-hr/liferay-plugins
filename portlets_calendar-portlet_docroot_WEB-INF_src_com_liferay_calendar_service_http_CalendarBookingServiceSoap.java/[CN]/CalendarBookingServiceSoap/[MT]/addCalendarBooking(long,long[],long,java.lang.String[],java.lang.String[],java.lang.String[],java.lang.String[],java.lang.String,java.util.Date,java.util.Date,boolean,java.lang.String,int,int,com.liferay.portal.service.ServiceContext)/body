{
  try {
    Map<Locale,String> titleMap=LocalizationUtil.getLocalizationMap(titleMapLanguageIds,titleMapValues);
    Map<Locale,String> descriptionMap=LocalizationUtil.getLocalizationMap(descriptionMapLanguageIds,descriptionMapValues);
    com.liferay.calendar.model.CalendarBooking returnValue=CalendarBookingServiceUtil.addCalendarBooking(calendarId,childCalendarIds,parentCalendarBookingId,titleMap,descriptionMap,location,startDate,endDate,allDay,recurrence,firstReminder,secondReminder,serviceContext);
    return com.liferay.calendar.model.CalendarBookingSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
