{
  List<CalendarBooking> calendarBookings=CalendarBookingLocalServiceUtil.getCalendarBookingsByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<CalendarBooking>());
  if (ListUtil.isEmpty(calendarBookings)) {
    return null;
  }
  return calendarBookings.get(0);
}
