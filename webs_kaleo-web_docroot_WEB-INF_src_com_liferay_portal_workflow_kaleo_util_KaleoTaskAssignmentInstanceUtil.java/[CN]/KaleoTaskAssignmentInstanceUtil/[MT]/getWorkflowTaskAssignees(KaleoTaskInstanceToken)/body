{
  List<KaleoTaskAssignmentInstance> kaleoTaskAssignmentInstances=kaleoTaskInstanceToken.getKaleoTaskAssignmentInstances();
  List<WorkflowTaskAssignee> workflowTaskAssignees=new ArrayList<WorkflowTaskAssignee>(kaleoTaskAssignmentInstances.size());
  for (  KaleoTaskAssignmentInstance kaleoTaskAssignmentInstance : kaleoTaskAssignmentInstances) {
    workflowTaskAssignees.add(new WorkflowTaskAssignee(kaleoTaskAssignmentInstance.getAssigneeClassName(),kaleoTaskAssignmentInstance.getAssigneeClassPK()));
  }
  return workflowTaskAssignees;
}
