{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("select * from WSRP_WSRPPortlet");
    rs=ps.executeQuery();
  }
 catch (  Exception e) {
    return true;
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return false;
}
