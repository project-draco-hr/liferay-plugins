{
  String cmd=ParamUtil.getString(req,Constants.CMD);
  if (!cmd.equals(Constants.UPDATE)) {
    return;
  }
  String license=ParamUtil.getString(req,"license");
  String portletResource=ParamUtil.getString(req,"portletResource");
  PortletPreferences preferences=PortletPreferencesFactoryUtil.getPortletSetup(req,portletResource);
  preferences.setValue("license",license);
  preferences.store();
  SessionMessages.add(req,portletConfig.getPortletName() + ".doConfigure");
}
