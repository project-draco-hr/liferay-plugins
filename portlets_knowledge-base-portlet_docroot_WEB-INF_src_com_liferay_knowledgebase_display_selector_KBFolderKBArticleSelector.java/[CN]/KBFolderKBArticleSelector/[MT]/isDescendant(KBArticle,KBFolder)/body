{
  KBFolder parentKBFolder=KBFolderLocalServiceUtil.fetchKBFolder(kbArticle.getKbFolderId());
  while ((parentKBFolder != null) && !parentKBFolder.equals(ancestorKBFolder)) {
    parentKBFolder=KBFolderLocalServiceUtil.fetchKBFolder(parentKBFolder.getParentKBFolderId());
  }
  return parentKBFolder != null;
}
