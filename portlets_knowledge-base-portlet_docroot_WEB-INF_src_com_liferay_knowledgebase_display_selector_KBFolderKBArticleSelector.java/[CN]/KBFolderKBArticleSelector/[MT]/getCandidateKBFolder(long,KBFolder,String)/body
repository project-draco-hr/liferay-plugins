{
  KBFolder kbFolder=KBFolderLocalServiceUtil.fetchKBFolderByUrlTitle(groupId,ancestorKBFolder.getKbFolderId(),kbFolderUrlTitle);
  if (kbFolder != null) {
    return kbFolder;
  }
  List<KBFolder> kbFolders=KBFolderLocalServiceUtil.getKBFolders(groupId,ancestorKBFolder.getKbFolderId(),0,1);
  if (!kbFolders.isEmpty()) {
    return kbFolders.get(0);
  }
  return ancestorKBFolder;
}
