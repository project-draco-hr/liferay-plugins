{
  int count=countByG_A(groupId,assigneeUserId);
  List<TasksEntry> list=findByG_A(groupId,assigneeUserId,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("groupId=");
    msg.append(groupId);
    msg.append(", assigneeUserId=");
    msg.append(assigneeUserId);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchTasksEntryException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
