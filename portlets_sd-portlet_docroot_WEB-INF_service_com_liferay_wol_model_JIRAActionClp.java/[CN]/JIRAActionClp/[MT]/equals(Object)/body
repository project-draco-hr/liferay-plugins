{
  if (obj == null) {
    return false;
  }
  JIRAActionClp jiraAction=null;
  try {
    jiraAction=(JIRAActionClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=jiraAction.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
