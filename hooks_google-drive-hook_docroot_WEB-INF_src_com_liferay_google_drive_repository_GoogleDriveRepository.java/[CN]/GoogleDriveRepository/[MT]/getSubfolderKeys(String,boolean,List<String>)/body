{
  List<ExtRepositoryFolder> extRepositoryFolders=getExtRepositoryObjects(ExtRepositoryObjectType.FOLDER,extRepositoryFolderKey);
  for (  ExtRepositoryFolder extRepositoryFolder : extRepositoryFolders) {
    subfolderKeys.add(extRepositoryFolder.getExtRepositoryModelKey());
    if (recurse) {
      getSubfolderKeys(extRepositoryFolder.getExtRepositoryModelKey(),recurse,subfolderKeys);
    }
  }
  return subfolderKeys;
}
