{
  SVNRevision svnRevision=(SVNRevision)EntityCacheUtil.getResult(SVNRevisionModelImpl.ENTITY_CACHE_ENABLED,SVNRevisionImpl.class,svnRevisionId,this);
  if (svnRevision == null) {
    Session session=null;
    try {
      session=openSession();
      svnRevision=(SVNRevision)session.get(SVNRevisionImpl.class,new Long(svnRevisionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (svnRevision != null) {
        cacheResult(svnRevision);
      }
      closeSession(session);
    }
  }
  return svnRevision;
}
