{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),PollsVoteLocalService.class.getName());
    if (invokableLocalService instanceof PollsVoteLocalService) {
      _service=(PollsVoteLocalService)invokableLocalService;
    }
 else {
      _service=new PollsVoteLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(PollsVoteLocalServiceUtil.class,"_service");
  }
  return _service;
}
