{
  if (_persistence == null) {
    _persistence=(JIRAChangeItemPersistence)PortletBeanLocatorUtil.locate(com.liferay.socialcoding.service.ClpSerializer.getServletContextName(),JIRAChangeItemPersistence.class.getName());
