{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof NotificationRecipient)) {
    return false;
  }
  NotificationRecipient notificationRecipient=(NotificationRecipient)obj;
  if (Validator.equals(_companyId,notificationRecipient._companyId) && Validator.equals(_userId,notificationRecipient._userId) && Validator.equals(_emailAddress,notificationRecipient._emailAddress)) {
    return true;
  }
  return false;
}
