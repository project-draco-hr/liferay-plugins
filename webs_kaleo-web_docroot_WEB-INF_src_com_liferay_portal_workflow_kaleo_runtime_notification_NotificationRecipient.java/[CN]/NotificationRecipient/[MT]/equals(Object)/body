{
  if (this == object) {
    return true;
  }
  if ((object == null) || (getClass() != object.getClass())) {
    return false;
  }
  NotificationRecipient notificationRecipient=(NotificationRecipient)object;
  if (_companyId != notificationRecipient.getCompanyId()) {
    return false;
  }
  if (_userId != notificationRecipient.getUserId()) {
    return false;
  }
  return true;
}
