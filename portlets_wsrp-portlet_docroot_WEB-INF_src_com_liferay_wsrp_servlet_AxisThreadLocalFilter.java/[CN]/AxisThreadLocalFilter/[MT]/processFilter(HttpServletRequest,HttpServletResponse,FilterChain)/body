{
  try {
    processFilter(AxisThreadLocalFilter.class,request,response,filterChain);
  }
  finally {
    try {
      ThreadLocal cache=(ThreadLocal)_cacheField.get(null);
      if (cache != null) {
        cache.remove();
      }
    }
 catch (    Exception e) {
      _log.error("Fail to remove " + MethodCache.class + "'s ThreadLocal",e);
    }
  }
}
