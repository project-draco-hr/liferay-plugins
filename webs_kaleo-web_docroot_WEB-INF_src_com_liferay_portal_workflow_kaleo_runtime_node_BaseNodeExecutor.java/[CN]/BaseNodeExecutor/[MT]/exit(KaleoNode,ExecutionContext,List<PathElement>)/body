{
  doExit(currentKaleoNode,executionContext,remainingPathElement);
  ActionExecutorUtil.executeKaleoActions(currentKaleoNode.getKaleoNodeId(),ExecutionType.ON_EXIT,executionContext);
  NotificationUtil.sendKaleoNotifications(currentKaleoNode.getKaleoNodeId(),ExecutionType.ON_EXIT,executionContext);
}
