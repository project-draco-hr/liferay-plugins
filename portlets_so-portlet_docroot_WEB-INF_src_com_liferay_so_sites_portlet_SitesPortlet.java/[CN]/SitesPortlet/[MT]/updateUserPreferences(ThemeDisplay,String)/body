{
  PortletPreferences portletPreferences=PortletPreferencesLocalServiceUtil.getPreferences(themeDisplay.getCompanyId(),themeDisplay.getUserId(),PortletKeys.PREFS_OWNER_TYPE_USER,LayoutConstants.DEFAULT_PLID,PortletKeys.SO_SITES);
  portletPreferences.setValue("defaultSearchTab",searchTab);
  portletPreferences.store();
}
