{
  long companyId=getCompanyId(securityToken);
  long userIdLong=GetterUtil.getLong(userId.getUserId(securityToken));
  for (  String field : fields) {
    ExpandoColumn expandoColumn=getExpandoColumn(companyId,appId,field);
    ExpandoValueLocalServiceUtil.deleteValue(companyId,User.class.getName(),getTableName(appId),expandoColumn.getName(),userIdLong);
  }
  return;
}
