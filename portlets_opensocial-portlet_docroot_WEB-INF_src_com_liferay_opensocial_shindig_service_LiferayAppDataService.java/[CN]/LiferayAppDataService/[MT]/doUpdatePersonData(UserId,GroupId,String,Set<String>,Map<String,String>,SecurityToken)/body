{
  long companyId=getCompanyId(securityToken);
  long userIdLong=GetterUtil.getLong(userId.getUserId(securityToken));
  for (  String field : fields) {
    ExpandoColumn expandoColumn=getExpandoColumn(companyId,field);
    ExpandoValueLocalServiceUtil.addValue(companyId,User.class.getName(),OPEN_SOCIAL_DATA,expandoColumn.getName(),userIdLong,values.get(field));
  }
  return;
}
