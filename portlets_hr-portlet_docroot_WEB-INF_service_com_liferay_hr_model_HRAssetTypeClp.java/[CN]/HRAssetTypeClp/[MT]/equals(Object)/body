{
  if (obj == null) {
    return false;
  }
  HRAssetTypeClp hrAssetType=null;
  try {
    hrAssetType=(HRAssetTypeClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrAssetType.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
