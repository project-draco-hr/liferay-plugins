{
  if (obj == null) {
    return false;
  }
  HRUserClp hrUser=null;
  try {
    hrUser=(HRUserClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrUser.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
