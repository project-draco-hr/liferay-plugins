{
  calendarResource=toUnwrappedModel(calendarResource);
  Session session=null;
  try {
    session=openSession();
    if (calendarResource.isCachedModel()) {
      calendarResource=(CalendarResource)session.get(CalendarResourceImpl.class,calendarResource.getPrimaryKeyObj());
    }
    if (calendarResource != null) {
      session.delete(calendarResource);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (calendarResource != null) {
    clearCache(calendarResource);
  }
  return calendarResource;
}
