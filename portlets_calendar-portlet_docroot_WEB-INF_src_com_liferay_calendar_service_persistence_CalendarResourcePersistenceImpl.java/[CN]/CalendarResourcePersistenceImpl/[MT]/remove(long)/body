{
  Session session=null;
  try {
    session=openSession();
    CalendarResource calendarResource=(CalendarResource)session.get(CalendarResourceImpl.class,Long.valueOf(calendarResourceId));
    if (calendarResource == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + calendarResourceId);
      }
      throw new NoSuchResourceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + calendarResourceId);
    }
    return calendarResourcePersistence.remove(calendarResource);
  }
 catch (  NoSuchResourceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
