{
  List<CalendarResource> list=findByC_C_A(companyId,code,active,0,1,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(8);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("companyId=");
    msg.append(companyId);
    msg.append(", code=");
    msg.append(code);
    msg.append(", active=");
    msg.append(active);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchResourceException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
