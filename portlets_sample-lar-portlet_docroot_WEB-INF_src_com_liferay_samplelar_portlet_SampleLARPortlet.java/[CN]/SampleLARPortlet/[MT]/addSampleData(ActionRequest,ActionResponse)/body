{
  long userId=UserLocalServiceUtil.getDefaultUserId(CompanyThreadLocal.getCompanyId());
  long groupId=ParamUtil.getLong(actionRequest,"groupId");
  for (int i=0; i <= 1; i++) {
    String bookingNumber="LR" + GetterUtil.getInteger(Math.random() * 100);
    SampleLARBookingLocalServiceUtil.addSampleLARBooking(userId,groupId,bookingNumber,new ServiceContext());
  }
}
