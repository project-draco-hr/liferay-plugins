{
  RequestedReport requestedReport=fetchByPrimaryKey(requestId);
  if (requestedReport == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No RequestedReport exists with the primary key " + requestId);
    }
    throw new NoSuchRequestedReportException("No RequestedReport exists with the primary key " + requestId);
  }
  return requestedReport;
}
