{
  Session session=null;
  try {
    session=openSession();
    RequestedReport requestedReport=(RequestedReport)session.get(RequestedReportImpl.class,new Long(requestId));
    if (requestedReport == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No RequestedReport exists with the primary key " + requestId);
      }
      throw new NoSuchRequestedReportException("No RequestedReport exists with the primary key " + requestId);
    }
    return remove(requestedReport);
  }
 catch (  NoSuchRequestedReportException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
