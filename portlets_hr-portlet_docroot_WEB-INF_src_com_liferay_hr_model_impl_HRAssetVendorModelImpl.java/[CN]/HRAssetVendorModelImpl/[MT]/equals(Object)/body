{
  if (obj == null) {
    return false;
  }
  HRAssetVendor hrAssetVendor=null;
  try {
    hrAssetVendor=(HRAssetVendor)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrAssetVendor.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
