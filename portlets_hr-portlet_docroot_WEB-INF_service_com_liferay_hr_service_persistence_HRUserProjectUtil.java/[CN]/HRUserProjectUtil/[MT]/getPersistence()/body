{
  if (_persistence == null) {
    _persistence=(HRUserProjectPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRUserProjectPersistence.class.getName());
    ReferenceRegistry.registerReference(HRUserProjectUtil.class,"_persistence");
  }
  return _persistence;
}
