{
  validate(0,version);
  App app=appPersistence.findByPrimaryKey(appId);
  app.setModifiedDate(new Date());
  app.setVersion(version);
  appPersistence.update(app,false);
  if (inputStream != null) {
    try {
      DLStoreUtil.deleteFile(app.getCompanyId(),CompanyConstants.SYSTEM,app.getFilePath());
    }
 catch (    Exception e) {
    }
    DLStoreUtil.addFile(app.getCompanyId(),CompanyConstants.SYSTEM,app.getFilePath(),false,inputStream);
  }
  return app;
}
