{
  int count=countByC_S(calendarResourceId,status);
  List<CalendarBooking> list=findByC_S(calendarResourceId,status,count - 1,count,orderByComparator);
  if (list.isEmpty()) {
    StringBundler msg=new StringBundler(6);
    msg.append(_NO_SUCH_ENTITY_WITH_KEY);
    msg.append("calendarResourceId=");
    msg.append(calendarResourceId);
    msg.append(", status=");
    msg.append(status);
    msg.append(StringPool.CLOSE_CURLY_BRACE);
    throw new NoSuchBookingException(msg.toString());
  }
 else {
    return list.get(0);
  }
}
