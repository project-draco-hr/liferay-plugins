{
  CalendarBooking calendarBooking=findByPrimaryKey(calendarBookingId);
  Session session=null;
  try {
    session=openSession();
    CalendarBooking[] array=new CalendarBookingImpl[3];
    array[0]=getByC_R_PrevAndNext(session,calendarBooking,calendarId,remindBy,orderByComparator,true);
    array[1]=calendarBooking;
    array[2]=getByC_R_PrevAndNext(session,calendarBooking,calendarId,remindBy,orderByComparator,false);
    return array;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
