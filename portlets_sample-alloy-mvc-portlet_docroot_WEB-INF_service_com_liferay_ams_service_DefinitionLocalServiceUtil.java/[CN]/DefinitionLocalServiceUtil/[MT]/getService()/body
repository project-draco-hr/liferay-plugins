{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),DefinitionLocalService.class.getName());
    if (invokableLocalService instanceof DefinitionLocalService) {
      _service=(DefinitionLocalService)invokableLocalService;
    }
 else {
      _service=new DefinitionLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(DefinitionLocalServiceUtil.class,"_service");
  }
  return _service;
}
