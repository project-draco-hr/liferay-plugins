{
  String command=message.getString("command");
  String servletContextName=message.getString("servletContextName");
  if (command.equals("undeploy") && servletContextName.equals(SERVLET_CONTEXT_NAME)) {
    WorkflowComponentServiceUtil.clearService();
    WorkflowDefinitionServiceUtil.clearService();
    WorkflowInstanceServiceUtil.clearService();
    WorkflowTaskServiceUtil.clearService();
  }
}
