{
  Map<String,String[]> mappings=(Map<String,String[]>)servletContext.getAttribute(Constants.JSP_TLD_URI_TO_LOCATION_MAP);
  if (mappings != null) {
    return;
  }
  mappings=new HashMap<String,String[]>();
  mappings.put("http://java.sun.com/jsp/jstl/core",new String[]{"/WEB-INF/tld/c.tld",null});
  servletContext.setAttribute(Constants.JSP_TLD_URI_TO_LOCATION_MAP,mappings);
}
