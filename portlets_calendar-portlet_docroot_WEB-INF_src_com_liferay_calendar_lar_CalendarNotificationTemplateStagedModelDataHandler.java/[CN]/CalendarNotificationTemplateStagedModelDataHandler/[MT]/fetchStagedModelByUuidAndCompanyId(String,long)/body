{
  List<CalendarNotificationTemplate> calendarNotificationTemplates=CalendarNotificationTemplateLocalServiceUtil.getCalendarNotificationTemplatesByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<CalendarNotificationTemplate>());
  if (ListUtil.isEmpty(calendarNotificationTemplates)) {
    return null;
  }
  return calendarNotificationTemplates.get(0);
}
