{
  try {
    if ((PortletPropsValues.EMAIL_LARGE_GROUP_SIZE >= 0) && !Validator.isNull(PortletPropsValues.EMAIL_LARGE_GROUP_ROLE)) {
      for (      String id : ids) {
        long companyId=Long.valueOf(id);
        setupExpando(companyId);
        setupRole(companyId);
      }
    }
    if (!PortletPropsValues.SYNC_ON_STARTUP) {
      return;
    }
    GoogleMailGroupsUtil.syncGroups();
  }
 catch (  Exception e) {
    throw new ActionException(e);
  }
}
