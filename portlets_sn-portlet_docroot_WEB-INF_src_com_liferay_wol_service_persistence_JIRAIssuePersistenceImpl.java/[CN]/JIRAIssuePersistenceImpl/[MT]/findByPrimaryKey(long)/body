{
  JIRAIssue jiraIssue=fetchByPrimaryKey(jiraIssueId);
  if (jiraIssue == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No JIRAIssue exists with the primary key " + jiraIssueId);
    }
    throw new NoSuchJIRAIssueException("No JIRAIssue exists with the primary key " + jiraIssueId);
  }
  return jiraIssue;
}
