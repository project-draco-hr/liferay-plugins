{
  Session session=null;
  try {
    session=openSession();
    HRTimeOffFrequencyType hrTimeOffFrequencyType=(HRTimeOffFrequencyType)session.get(HRTimeOffFrequencyTypeImpl.class,Long.valueOf(hrTimeOffFrequencyTypeId));
    if (hrTimeOffFrequencyType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeOffFrequencyTypeId);
      }
      throw new NoSuchTimeOffFrequencyTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeOffFrequencyTypeId);
    }
    return hrTimeOffFrequencyTypePersistence.remove(hrTimeOffFrequencyType);
  }
 catch (  NoSuchTimeOffFrequencyTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
