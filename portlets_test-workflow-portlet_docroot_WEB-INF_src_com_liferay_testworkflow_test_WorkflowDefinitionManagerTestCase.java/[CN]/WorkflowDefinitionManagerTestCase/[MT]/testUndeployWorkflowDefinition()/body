{
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,defaultUserId,_workflowDefinition1_1.getName(),_workflowDefinition1_1.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,defaultUserId,_workflowDefinition1_2.getName(),_workflowDefinition1_2.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,defaultUserId,_workflowDefinition1_3.getName(),_workflowDefinition1_3.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,defaultUserId,_workflowDefinition2_1.getName(),_workflowDefinition2_1.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,defaultUserId,_workflowDefinition2_2.getName(),_workflowDefinition2_2.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(companyId,defaultUserId,_workflowDefinition3_1.getName(),_workflowDefinition3_1.getVersion());
  int count=WorkflowDefinitionManagerUtil.getWorkflowDefinitionCount(companyId);
  assertEquals(0,count);
}
