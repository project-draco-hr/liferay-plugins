{
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(_user.getUserId(),_NAME_1,1);
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(_user.getUserId(),_NAME_1,2);
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(_user.getUserId(),_NAME_1,3);
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(_user.getUserId(),_NAME_2,1);
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(_user.getUserId(),_NAME_2,2);
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(_user.getUserId(),_NAME_3,1);
  int count=WorkflowDefinitionManagerUtil.getWorkflowDefinitionCount();
  assertEquals(0,count);
}
