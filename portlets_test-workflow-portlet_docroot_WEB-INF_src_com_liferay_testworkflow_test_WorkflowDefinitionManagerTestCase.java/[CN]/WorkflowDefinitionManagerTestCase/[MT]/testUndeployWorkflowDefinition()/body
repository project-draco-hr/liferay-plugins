{
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(USER.getUserId(),_workflowDefinition1_1.getName(),_workflowDefinition1_1.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(USER.getUserId(),_workflowDefinition1_2.getName(),_workflowDefinition1_2.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(USER.getUserId(),_workflowDefinition1_3.getName(),_workflowDefinition1_3.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(USER.getUserId(),_workflowDefinition2_1.getName(),_workflowDefinition2_1.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(USER.getUserId(),_workflowDefinition2_2.getName(),_workflowDefinition2_2.getVersion());
  WorkflowDefinitionManagerUtil.undeployWorkflowDefinition(USER.getUserId(),_workflowDefinition3_1.getName(),_workflowDefinition3_1.getVersion());
  int count=WorkflowDefinitionManagerUtil.getWorkflowDefinitionCount();
  assertEquals(0,count);
}
