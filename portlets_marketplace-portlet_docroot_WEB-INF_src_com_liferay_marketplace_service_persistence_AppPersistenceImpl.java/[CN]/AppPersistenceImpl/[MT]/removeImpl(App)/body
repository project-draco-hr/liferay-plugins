{
  app=toUnwrappedModel(app);
  Session session=null;
  try {
    session=openSession();
    if (app.isCachedModel()) {
      app=(App)session.get(AppImpl.class,app.getPrimaryKeyObj());
    }
    session.delete(app);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(app);
  return app;
}
