{
  String deployDirName=DeployManagerUtil.getDeployDir();
  File deployDir=new File(deployDirName,context);
  if (deployDir.exists()) {
    return deployDir;
  }
  File deployWarDir=new File(deployDirName,context + ".war");
  if (deployWarDir.exists()) {
    return deployWarDir;
  }
  return deployDir;
}
