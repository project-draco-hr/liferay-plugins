{
  try {
    if (eventType.equals(SyncWatchEvent.EVENT_TYPE_CREATE) && FileUtil.isIgnoredFilePath(filePath)) {
      return;
    }
    SyncWatchEventService.addSyncWatchEvent(eventType,FilePathNameUtil.getFilePathName(filePath),getFileType(eventType,filePath),getSyncAccountId());
  }
 catch (  Exception e) {
    _logger.error(e.getMessage(),e);
  }
}
