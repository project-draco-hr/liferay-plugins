{
  while (true) {
    filePath=filePath.getParent();
    if (filePath == null) {
      return 0;
    }
    SyncFile syncFile=SyncFileService.fetchSyncFile(FilePathNameUtil.getFilePathName(filePath),getSyncAccountId());
    if (syncFile != null) {
      return syncFile.getRepositoryId();
    }
  }
}
