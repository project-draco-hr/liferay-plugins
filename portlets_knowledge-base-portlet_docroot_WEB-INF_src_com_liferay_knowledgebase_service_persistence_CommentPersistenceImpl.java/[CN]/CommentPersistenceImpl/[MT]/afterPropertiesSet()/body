{
  String[] listenerClassNames=StringUtil.split(GetterUtil.getString(com.liferay.util.service.ServiceProps.get("value.object.listener.com.liferay.knowledgebase.model.Comment")));
  if (listenerClassNames.length > 0) {
    try {
      List<ModelListener<Comment>> listenersList=new ArrayList<ModelListener<Comment>>();
      for (      String listenerClassName : listenerClassNames) {
        listenersList.add((ModelListener<Comment>)InstanceFactory.newInstance(listenerClassName));
      }
      listeners=listenersList.toArray(new ModelListener[listenersList.size()]);
    }
 catch (    Exception e) {
      _log.error(e);
    }
  }
}
