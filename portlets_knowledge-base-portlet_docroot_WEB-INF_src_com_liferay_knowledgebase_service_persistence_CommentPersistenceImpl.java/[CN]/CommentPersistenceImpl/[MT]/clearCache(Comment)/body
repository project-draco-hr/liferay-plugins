{
  EntityCacheUtil.removeResult(CommentModelImpl.ENTITY_CACHE_ENABLED,CommentImpl.class,comment.getPrimaryKey());
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{comment.getUuid(),Long.valueOf(comment.getGroupId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_U_C_C,new Object[]{Long.valueOf(comment.getUserId()),Long.valueOf(comment.getClassNameId()),Long.valueOf(comment.getClassPK())});
}
