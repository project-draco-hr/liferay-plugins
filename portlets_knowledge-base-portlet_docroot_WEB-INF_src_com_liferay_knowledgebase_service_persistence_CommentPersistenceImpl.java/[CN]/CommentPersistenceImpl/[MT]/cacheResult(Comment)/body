{
  EntityCacheUtil.putResult(CommentModelImpl.ENTITY_CACHE_ENABLED,CommentImpl.class,comment.getPrimaryKey(),comment);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{comment.getUuid(),Long.valueOf(comment.getGroupId())},comment);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_C_C,new Object[]{Long.valueOf(comment.getUserId()),Long.valueOf(comment.getClassNameId()),Long.valueOf(comment.getClassPK())},comment);
  comment.resetOriginalValues();
}
