{
  EntityCacheUtil.putResult(CommentModelImpl.ENTITY_CACHE_ENABLED,CommentImpl.class,comment.getPrimaryKey(),comment);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{comment.getUuid(),new Long(comment.getGroupId())},comment);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_U_C_C,new Object[]{new Long(comment.getUserId()),new Long(comment.getClassNameId()),new Long(comment.getClassPK())},comment);
}
