{
  SimpleDateFormat sdf=new SimpleDateFormat("MMM dd yyyy HH:mm");
  Message message=new MimeMessage(getSession());
  message.setFrom(new InternetAddress(_mailAccount.getEmailAddress()));
  message.setRecipient(Message.RecipientType.TO,new InternetAddress(_mailAccount.getEmailAddress()));
  message.setSubject("[" + sdf.format(new Date()) + "] testing outgoing email");
  message.setContent("testing outgoing email","text/plain");
  return message;
}
