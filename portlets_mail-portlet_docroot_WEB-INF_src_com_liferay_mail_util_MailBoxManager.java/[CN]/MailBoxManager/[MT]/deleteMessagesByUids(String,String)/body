{
  long[] messageUidsArray=GetterUtil.getLongValues(messageUids.split("\\s*,\\s*"));
  if (Validator.isNull(messageUidsArray)) {
    return _failureJSON;
  }
  try {
    Folder folder=openFolder(folderName);
    for (    long messageUid : messageUidsArray) {
      deleteMessage(folder,messageUid,false);
    }
    folder.close(true);
    return _successJSON;
  }
 catch (  MessagingException me) {
    _log.error(me,me);
    return _failureJSON;
  }
}
