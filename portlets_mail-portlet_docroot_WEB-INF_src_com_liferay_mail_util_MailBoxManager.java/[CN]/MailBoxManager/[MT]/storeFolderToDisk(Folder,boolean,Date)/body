{
  try {
    JSONObject jsonObj=getJSONFolder(folder);
    String filePath=MailDiskManager.getFolderFilePath(_user,_mailAccount.getEmailAddress(),folder.getFullName());
    JSONUtil.put(jsonObj,"initialized",initialized);
    JSONUtil.put(jsonObj,"lastUpdated",date);
    FileUtil.write(filePath,jsonObj.toString());
    return jsonObj;
  }
 catch (  IOException ioe) {
    _log.error(ioe,ioe);
  }
catch (  MessagingException me) {
    _log.error(me,me);
  }
  return null;
}
