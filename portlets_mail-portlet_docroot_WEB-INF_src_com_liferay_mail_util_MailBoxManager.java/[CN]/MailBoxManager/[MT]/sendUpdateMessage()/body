{
  JSONObject mailRequestJSON=new JSONObject();
  JSONUtil.put(mailRequestJSON,"emailAddress",_mailAccount.getEmailAddress());
  JSONUtil.put(mailRequestJSON,"userId",_user.getUserId());
  MessageBusUtil.sendMessage(DestinationNames.MAIL,mailRequestJSON.toString());
  JSONObject jsonObj=new JSONObject();
  JSONUtil.put(jsonObj,"success",true);
  return jsonObj;
}
