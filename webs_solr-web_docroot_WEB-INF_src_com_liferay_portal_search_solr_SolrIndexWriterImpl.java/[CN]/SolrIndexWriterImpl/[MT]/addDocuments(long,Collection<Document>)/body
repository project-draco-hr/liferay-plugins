{
  try {
    Collection<SolrInputDocument> solrDocuments=getSolrDocuments(documents);
    if (solrDocuments.isEmpty()) {
      return;
    }
    _solrServer.add(solrDocuments);
    if (_commit) {
      _solrServer.commit();
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new SearchException(e.getMessage());
  }
}
