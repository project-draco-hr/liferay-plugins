{
  String[] syncNotificationFields=_syncNotificationFieldsCache.get(className);
  if (syncNotificationFields != null) {
    return syncNotificationFields;
  }
  syncNotificationFields=PropsUtil.getArray(PropsKeys.SYNC_NOTIFICATION_FIELDS_PREFIX + "." + className);
  _syncNotificationFieldsCache.put(className,syncNotificationFields);
  return syncNotificationFields;
}
