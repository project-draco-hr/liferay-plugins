{
  String[] syncNotificationFieldsArray=_syncNotificationFieldsCache.get(className);
  if (syncNotificationFieldsArray != null) {
    return syncNotificationFieldsArray;
  }
  String syncNotificationFields=PropsUtil.get(PropsKeys.SYNC_NOTIFICATION_FIELDS_PREFIX + "." + className);
  syncNotificationFieldsArray=syncNotificationFields.split(",");
  _syncNotificationFieldsCache.put(className,syncNotificationFieldsArray);
  return syncNotificationFieldsArray;
}
