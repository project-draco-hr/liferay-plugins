{
  if (isEscapedModel()) {
    return (GadgetsEntry)this;
  }
 else {
    GadgetsEntry model=new GadgetsEntryImpl();
    model.setNew(isNew());
    model.setEscapedModel(true);
    model.setGadgetsEntryId(getGadgetsEntryId());
    model.setCompanyId(getCompanyId());
    model.setCreateDate(getCreateDate());
    model.setModifiedDate(getModifiedDate());
    model.setName(HtmlUtil.escape(getName()));
    model.setUrl(HtmlUtil.escape(getUrl()));
    model.setXml(HtmlUtil.escape(getXml()));
    model=(GadgetsEntry)Proxy.newProxyInstance(GadgetsEntry.class.getClassLoader(),new Class[]{GadgetsEntry.class},new ReadOnlyBeanHandler(model));
    return model;
  }
}
