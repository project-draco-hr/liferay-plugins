{
  HRProjectBillingRate hrProjectBillingRate=fetchByPrimaryKey(hrProjectBillingRateId);
  if (hrProjectBillingRate == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrProjectBillingRateId);
    }
    throw new NoSuchProjectBillingRateException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrProjectBillingRateId);
  }
  return hrProjectBillingRate;
}
