{
  Session session=null;
  try {
    session=openSession();
    SQLQuery q=buildKaleoDefinitionQuerySQL(kaleoDefinitionQuery,false,session);
    List<KaleoDefinition> kaleoDefinitions=(List<KaleoDefinition>)QueryUtil.list(q,getDialect(),kaleoDefinitionQuery.getStart(),kaleoDefinitionQuery.getEnd());
    return kaleoDefinitions;
  }
 catch (  Exception e) {
    throw new SystemException(e);
  }
 finally {
    closeSession(session);
  }
}
