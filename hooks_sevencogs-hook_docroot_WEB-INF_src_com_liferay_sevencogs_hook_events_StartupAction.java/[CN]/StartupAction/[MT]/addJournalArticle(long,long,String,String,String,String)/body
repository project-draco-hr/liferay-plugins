{
  String content=getString("/journal/articles/" + fileName);
  PortletPreferences preferences=null;
  JournalArticle journalArticle=JournalArticleLocalServiceUtil.addArticle(userId,groupId,StringPool.BLANK,true,title,StringPool.BLANK,content,"general",structureId,templateId,1,1,2008,0,0,0,0,0,0,0,true,0,0,0,0,0,true,true,false,StringPool.BLANK,null,null,StringPool.BLANK,preferences,new String[0],new String[0],false,false);
  JournalArticleLocalServiceUtil.approveArticle(userId,groupId,journalArticle.getArticleId(),journalArticle.getVersion(),StringPool.BLANK,preferences);
  return journalArticle;
}
