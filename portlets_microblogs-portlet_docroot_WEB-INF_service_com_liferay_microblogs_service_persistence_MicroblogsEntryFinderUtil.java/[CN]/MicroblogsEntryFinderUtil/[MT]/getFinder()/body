{
  if (_finder == null) {
    _finder=(MicroblogsEntryFinder)PortletBeanLocatorUtil.locate(com.liferay.microblogs.service.ClpSerializer.getServletContextName(),MicroblogsEntryFinder.class.getName());
    ReferenceRegistry.registerReference(MicroblogsEntryFinderUtil.class,"_finder");
  }
  return _finder;
}
