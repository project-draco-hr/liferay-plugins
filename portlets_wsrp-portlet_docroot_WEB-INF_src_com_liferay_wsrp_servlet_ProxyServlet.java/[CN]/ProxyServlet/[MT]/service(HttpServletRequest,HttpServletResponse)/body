{
  try {
    String urlString=ParamUtil.getString(request,"url");
    URL url=new URL(urlString);
    if (isAllowedURL(url)) {
      proxyURL(request,response,url);
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
    response.sendError(HttpServletResponse.SC_INTERNAL_SERVER_ERROR,e.getMessage());
  }
}
