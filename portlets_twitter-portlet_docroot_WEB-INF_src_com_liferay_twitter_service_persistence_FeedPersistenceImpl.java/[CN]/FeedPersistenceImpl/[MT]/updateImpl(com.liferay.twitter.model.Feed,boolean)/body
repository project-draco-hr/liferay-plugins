{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,feed,merge);
    feed.setNew(false);
    return feed;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(Feed.class.getName());
  }
}
