{
  Feed feed=(Feed)EntityCacheUtil.getResult(FeedModelImpl.ENTITY_CACHE_ENABLED,FeedImpl.class,primaryKey);
  if (feed == _nullFeed) {
    return null;
  }
  if (feed == null) {
    Session session=null;
    try {
      session=openSession();
      feed=(Feed)session.get(FeedImpl.class,primaryKey);
      if (feed != null) {
        cacheResult(feed);
      }
 else {
        EntityCacheUtil.putResult(FeedModelImpl.ENTITY_CACHE_ENABLED,FeedImpl.class,primaryKey,_nullFeed);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(FeedModelImpl.ENTITY_CACHE_ENABLED,FeedImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return feed;
}
