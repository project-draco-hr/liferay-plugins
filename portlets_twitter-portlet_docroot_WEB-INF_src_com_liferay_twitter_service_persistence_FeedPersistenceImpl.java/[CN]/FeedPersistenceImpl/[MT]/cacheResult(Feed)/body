{
  EntityCacheUtil.putResult(FeedModelImpl.ENTITY_CACHE_ENABLED,FeedImpl.class,feed.getPrimaryKey(),feed);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_TWUI,new Object[]{feed.getCompanyId(),feed.getTwitterUserId()},feed);
  FinderCacheUtil.putResult(FINDER_PATH_FETCH_BY_C_TSN,new Object[]{feed.getCompanyId(),feed.getTwitterScreenName()},feed);
  feed.resetOriginalValues();
}
