{
  EntityCacheUtil.removeResult(FeedModelImpl.ENTITY_CACHE_ENABLED,FeedImpl.class,feed.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_TWUI,new Object[]{Long.valueOf(feed.getCompanyId()),Long.valueOf(feed.getTwitterUserId())});
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_C_TSN,new Object[]{Long.valueOf(feed.getCompanyId()),feed.getTwitterScreenName()});
}
