{
  HRBranch hrBranch=(HRBranch)EntityCacheUtil.getResult(HRBranchModelImpl.ENTITY_CACHE_ENABLED,HRBranchImpl.class,hrBranchId,this);
  if (hrBranch == null) {
    Session session=null;
    try {
      session=openSession();
      hrBranch=(HRBranch)session.get(HRBranchImpl.class,Long.valueOf(hrBranchId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrBranch != null) {
        cacheResult(hrBranch);
      }
      closeSession(session);
    }
  }
  return hrBranch;
}
