{
  try {
    Map<Locale,String> titleMap=LocalizationUtil.getLocalizationMap(titleMapLanguageIds,titleMapValues);
    Map<Locale,String> descriptionMap=LocalizationUtil.getLocalizationMap(descriptionMapLanguageIds,descriptionMapValues);
    com.liferay.calendar.model.CalendarEvent returnValue=CalendarEventServiceUtil.updateCalendarEvent(calendarEventId,titleMap,descriptionMap,location,startDateMonth,startDateDay,startDateYear,startDateHour,startDateMinute,endDateMonth,endDateDay,endDateYear,endDateHour,endDateMinute,durationHour,durationMinute,allDay,recurrence,type,remindBy,firstReminder,secondReminder,serviceContext);
    return com.liferay.calendar.model.CalendarEventSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
