{
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(postEntry);
    }
  }
  postEntry=removeImpl(postEntry);
  if (_listeners.length > 0) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(postEntry);
    }
  }
  return postEntry;
}
