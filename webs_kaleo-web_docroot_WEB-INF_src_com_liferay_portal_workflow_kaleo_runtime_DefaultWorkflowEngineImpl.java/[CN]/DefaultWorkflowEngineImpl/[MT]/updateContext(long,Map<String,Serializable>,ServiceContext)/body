{
  try {
    KaleoInstance kaleoInstance=doUpdateContext(workflowInstanceId,context,serviceContext);
    KaleoInstanceToken rootKaleoInstanceToken=kaleoInstance.getRootKaleoInstanceToken(serviceContext);
    return new WorkflowInstanceAdapter(kaleoInstance,rootKaleoInstanceToken);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
