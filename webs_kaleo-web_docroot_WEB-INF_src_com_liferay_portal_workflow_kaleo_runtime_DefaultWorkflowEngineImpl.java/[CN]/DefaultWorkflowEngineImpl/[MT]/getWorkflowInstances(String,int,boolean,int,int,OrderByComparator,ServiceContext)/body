{
  try {
    List<KaleoInstance> kaleoInstances=kaleoInstanceLocalService.getKaleoInstances(workflowDefinitionName,workflowDefinitionVersion,completed,start,end,orderByComparator,serviceContext);
    return toWorkflowInstances(kaleoInstances,serviceContext);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
