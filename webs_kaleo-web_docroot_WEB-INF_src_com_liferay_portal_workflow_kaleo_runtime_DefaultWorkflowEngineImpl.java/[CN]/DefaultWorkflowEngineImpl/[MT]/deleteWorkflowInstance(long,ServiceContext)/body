{
  try {
    kaleoTaskInstanceTokenLocalService.deleteKaleoTaskInstanceTokensByInstanceId(workflowInstanceId);
    kaleoInstanceTokenLocalService.deleteKaleoInstanceTokensByInstanceId(workflowInstanceId);
    kaleoLogLocalService.deleteKaleoLogsByInstanceId(workflowInstanceId);
    kaleoInstanceLocalService.deleteKaleoInstance(workflowInstanceId);
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
