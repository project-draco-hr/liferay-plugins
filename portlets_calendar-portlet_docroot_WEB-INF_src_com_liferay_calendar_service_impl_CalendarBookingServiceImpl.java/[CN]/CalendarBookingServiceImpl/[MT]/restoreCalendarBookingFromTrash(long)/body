{
  CalendarBooking calendarBooking=calendarBookingPersistence.findByPrimaryKey(calendarBookingId);
  CalendarPermission.check(getPermissionChecker(),calendarBooking.getCalendarId(),ActionKeys.MANAGE_BOOKINGS);
  calendarBookingLocalService.restoreCalendarBookingFromTrash(getUserId(),calendarBooking.getCalendarBookingId());
}
