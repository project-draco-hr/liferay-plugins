{
  long[] childCalendarIds=calendarBookingLocalService.getChildCalendarIds(calendarBookingId,calendarId);
  return updateCalendarBookingOffsetAndDuration(calendarBookingId,calendarId,childCalendarIds,titleMap,descriptionMap,location,offset,duration,allDay,recurrence,firstReminder,firstReminderType,secondReminder,secondReminderType,status,serviceContext);
}
