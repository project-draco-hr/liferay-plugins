{
  CalendarPermission.check(getPermissionChecker(),calendarId,ActionKeys.MANAGE_BOOKINGS);
  return calendarBookingLocalService.updateCalendarBookingInstance(getUserId(),calendarBookingId,calendarId,titleMap,descriptionMap,location,startDate,endDate,allDay,recurrence,allFollowing,firstReminder,firstReminderType,secondReminder,secondReminderType,status,serviceContext);
}
