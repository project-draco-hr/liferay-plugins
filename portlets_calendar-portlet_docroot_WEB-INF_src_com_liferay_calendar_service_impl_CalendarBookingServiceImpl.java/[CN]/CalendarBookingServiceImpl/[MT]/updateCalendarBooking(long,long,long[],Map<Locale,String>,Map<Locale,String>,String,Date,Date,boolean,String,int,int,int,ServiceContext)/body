{
  CalendarPermission.check(getPermissionChecker(),calendarId,ActionKeys.MANAGE_BOOKINGS);
  return calendarBookingLocalService.updateCalendarBooking(getUserId(),calendarBookingId,calendarId,childCalendarIds,titleMap,descriptionMap,location,startDate,endDate,allDay,recurrence,firstReminder,secondReminder,status,serviceContext);
}
