{
  List<CalendarBooking> calendarBookings=calendarBookingLocalService.search(companyId,groupIds,calendarIds,calendarResourceIds,parentCalendarBookingId,title,description,location,startDate,endDate,recurring,statuses,andOperator,start,end,orderByComparator);
  return filterCalendarBookings(calendarBookings,ActionKeys.VIEW);
}
