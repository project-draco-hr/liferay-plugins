{
  SPSession spSession=(SPSession)EntityCacheUtil.getResult(SPSessionModelImpl.ENTITY_CACHE_ENABLED,SPSessionImpl.class,spSessionId,this);
  if (spSession == null) {
    Session session=null;
    try {
      session=openSession();
      spSession=(SPSession)session.get(SPSessionImpl.class,new Long(spSessionId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (spSession != null) {
        cacheResult(spSession);
      }
      closeSession(session);
    }
  }
  return spSession;
}
