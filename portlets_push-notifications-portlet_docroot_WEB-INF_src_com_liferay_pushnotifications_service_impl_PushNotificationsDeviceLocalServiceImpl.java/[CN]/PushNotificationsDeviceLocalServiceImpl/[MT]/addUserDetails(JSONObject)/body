{
  long userId=jsonObject.getLong(PushNotificationsConstants.FROM_USER_ID);
  User user=userLocalService.getUser(userId);
  jsonObject.put(PushNotificationsConstants.FULL_NAME,user.getFullName());
  jsonObject.put(PushNotificationsConstants.PORTRAIT_ID,user.getPortraitId());
  jsonObject.put(PushNotificationsConstants.UUID,user.getUuid());
}
