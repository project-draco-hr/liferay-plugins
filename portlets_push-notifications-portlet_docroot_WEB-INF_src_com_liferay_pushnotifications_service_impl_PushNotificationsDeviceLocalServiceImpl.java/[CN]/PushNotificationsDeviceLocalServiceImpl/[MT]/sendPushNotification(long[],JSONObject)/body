{
  for (  Map.Entry<String,PushNotificationsSender> entry : _pushNotificationsSenders.entrySet()) {
    List<String> tokens=new ArrayList<>();
    List<PushNotificationsDevice> pushNotificationsDevices=pushNotificationsDevicePersistence.findByU_P(toUserIds,entry.getKey(),QueryUtil.ALL_POS,QueryUtil.ALL_POS);
    for (    PushNotificationsDevice pushNotificationsDevice : pushNotificationsDevices) {
      tokens.add(pushNotificationsDevice.getToken());
    }
    if (tokens.isEmpty()) {
      continue;
    }
    sendPushNotification(entry.getValue(),tokens,payloadJSONObject);
  }
}
