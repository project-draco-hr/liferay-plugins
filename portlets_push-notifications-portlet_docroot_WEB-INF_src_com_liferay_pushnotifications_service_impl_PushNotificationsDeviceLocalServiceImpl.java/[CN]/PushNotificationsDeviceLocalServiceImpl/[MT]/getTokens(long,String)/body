{
  List<String> tokens=new ArrayList<String>();
  List<PushNotificationsDevice> pushNotificationsDevices=pushNotificationsDevicePersistence.findByU_P(userId,platform);
  for (  PushNotificationsDevice pushNotificationsDevice : pushNotificationsDevices) {
    tokens.add(pushNotificationsDevice.getToken());
  }
  return tokens;
}
