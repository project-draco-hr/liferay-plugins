{
  asset=toUnwrappedModel(asset);
  Session session=null;
  try {
    session=openSession();
    if (asset.isCachedModel()) {
      asset=(Asset)session.get(AssetImpl.class,asset.getPrimaryKeyObj());
    }
    session.delete(asset);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(asset);
  return asset;
}
