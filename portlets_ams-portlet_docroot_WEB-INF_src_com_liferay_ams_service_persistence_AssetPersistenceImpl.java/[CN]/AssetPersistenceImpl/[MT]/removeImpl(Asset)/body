{
  asset=toUnwrappedModel(asset);
  Session session=null;
  try {
    session=openSession();
    if (asset.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(AssetImpl.class,asset.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(asset);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(AssetModelImpl.ENTITY_CACHE_ENABLED,AssetImpl.class,asset.getPrimaryKey());
  return asset;
}
