{
  asset=toUnwrappedModel(asset);
  Session session=null;
  try {
    session=openSession();
    if (!session.contains(asset)) {
      asset=(Asset)session.get(AssetImpl.class,asset.getPrimaryKeyObj());
    }
    if (asset != null) {
      session.delete(asset);
    }
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  if (asset != null) {
    clearCache(asset);
  }
  return asset;
}
