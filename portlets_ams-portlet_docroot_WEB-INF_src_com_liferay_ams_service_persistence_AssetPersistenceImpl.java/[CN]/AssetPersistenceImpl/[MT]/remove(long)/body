{
  Session session=null;
  try {
    session=openSession();
    Asset asset=(Asset)session.get(AssetImpl.class,new Long(assetId));
    if (asset == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + assetId);
      }
      throw new NoSuchAssetException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + assetId);
    }
    return remove(asset);
  }
 catch (  NoSuchAssetException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
