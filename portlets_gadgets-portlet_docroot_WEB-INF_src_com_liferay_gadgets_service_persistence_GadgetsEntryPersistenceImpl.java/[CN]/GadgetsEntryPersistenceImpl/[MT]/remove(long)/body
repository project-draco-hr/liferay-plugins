{
  Session session=null;
  try {
    session=openSession();
    GadgetsEntry gadgetsEntry=(GadgetsEntry)session.get(GadgetsEntryImpl.class,new Long(gadgetEntryId));
    if (gadgetsEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + gadgetEntryId);
      }
      throw new NoSuchEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + gadgetEntryId);
    }
    return remove(gadgetsEntry);
  }
 catch (  NoSuchEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
