{
  if (obj == null) {
    return false;
  }
  MicroblogsEntry microblogsEntry=null;
  try {
    microblogsEntry=(MicroblogsEntry)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=microblogsEntry.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
