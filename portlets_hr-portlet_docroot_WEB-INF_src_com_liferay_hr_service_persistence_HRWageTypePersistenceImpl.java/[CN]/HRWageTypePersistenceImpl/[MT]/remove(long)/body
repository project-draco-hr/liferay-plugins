{
  Session session=null;
  try {
    session=openSession();
    HRWageType hrWageType=(HRWageType)session.get(HRWageTypeImpl.class,Long.valueOf(hrWageTypeId));
    if (hrWageType == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrWageTypeId);
      }
      throw new NoSuchWageTypeException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrWageTypeId);
    }
    return hrWageTypePersistence.remove(hrWageType);
  }
 catch (  NoSuchWageTypeException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
