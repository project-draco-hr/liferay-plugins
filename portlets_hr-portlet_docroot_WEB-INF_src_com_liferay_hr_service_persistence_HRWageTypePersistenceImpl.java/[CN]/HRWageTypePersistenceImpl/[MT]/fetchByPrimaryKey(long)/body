{
  HRWageType hrWageType=(HRWageType)EntityCacheUtil.getResult(HRWageTypeModelImpl.ENTITY_CACHE_ENABLED,HRWageTypeImpl.class,hrWageTypeId,this);
  if (hrWageType == null) {
    Session session=null;
    try {
      session=openSession();
      hrWageType=(HRWageType)session.get(HRWageTypeImpl.class,Long.valueOf(hrWageTypeId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrWageType != null) {
        cacheResult(hrWageType);
      }
      closeSession(session);
    }
  }
  return hrWageType;
}
