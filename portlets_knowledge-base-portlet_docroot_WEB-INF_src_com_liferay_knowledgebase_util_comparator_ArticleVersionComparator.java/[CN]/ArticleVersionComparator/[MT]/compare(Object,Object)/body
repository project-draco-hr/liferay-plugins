{
  Article article1=(Article)obj1;
  Article article2=(Article)obj2;
  int value=0;
  if (article1.getVersion() < article2.getVersion()) {
    value=-1;
  }
 else   if (article1.getVersion() > article2.getVersion()) {
    value=1;
  }
  if (_ascending) {
    return value;
  }
 else {
    return -value;
  }
}
