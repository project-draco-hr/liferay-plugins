{
  Map<Locale,String> nameMap=new HashMap<Locale,String>();
  nameMap.put(Locale.ENGLISH,name);
  User user=UserLocalServiceUtil.getDefaultUser(companyId);
  LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.addLayoutSetPrototype(user.getUserId(),companyId,nameMap,StringPool.BLANK,true,true,new ServiceContext());
  Group group=layoutSetPrototype.getGroup();
  LayoutLocalServiceUtil.importLayouts(user.getUserId(),group.getGroupId(),false,getParameterMap(),lar);
}
