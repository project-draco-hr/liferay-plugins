{
  try {
    String token=_service.login(USER_NAME,PASSWORD);
    Collection projects=_proxy.getPriorities(token);
    System.out.println("===================================");
    System.out.println("this is testGetPriorities" + "P:testGetPriorities token");
    Iterator listProject=projects.iterator();
    while (listProject.hasNext()) {
      Priority priority=listProject.next();
      System.out.println("testGetPrioritiesName: " + priority.getPriorityName());
      System.out.println("testGetPrioritiesID:  " + priority.getPriorityId());
    }
    System.out.println("===================================");
  }
 catch (  RemoteException e) {
    fail("Caught remote error");
    e.printStackTrace();
  }
catch (  SystemException e) {
    fail("Caught system error");
    e.printStackTrace();
  }
catch (  Exception e) {
    fail("Caught security error");
    e.printStackTrace();
  }
}
