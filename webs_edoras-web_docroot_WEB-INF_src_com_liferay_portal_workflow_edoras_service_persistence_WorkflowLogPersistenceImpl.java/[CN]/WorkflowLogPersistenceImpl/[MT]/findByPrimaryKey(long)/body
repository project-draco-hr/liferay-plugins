{
  WorkflowLog workflowLog=fetchByPrimaryKey(workflowLogId);
  if (workflowLog == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No WorkflowLog exists with the primary key " + workflowLogId);
    }
    throw new NoSuchWorkflowLogException("No WorkflowLog exists with the primary key " + workflowLogId);
  }
  return workflowLog;
}
