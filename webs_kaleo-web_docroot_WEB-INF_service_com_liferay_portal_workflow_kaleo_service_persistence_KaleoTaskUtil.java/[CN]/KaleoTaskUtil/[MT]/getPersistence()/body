{
  if (_persistence == null) {
    _persistence=(KaleoTaskPersistence)PortletBeanLocatorUtil.locate(com.liferay.portal.workflow.kaleo.service.ClpSerializer.SERVLET_CONTEXT_NAME,KaleoTaskPersistence.class.getName());
    ReferenceRegistry.registerReference(KaleoTaskUtil.class,"_persistence");
  }
  return _persistence;
}
