{
  final String ticketId=request.getParameter(JiraPortletConstants.TICKET_ID_KEY);
  final ModelAndView view=new ModelAndView("createTicketFinish");
  view.addObject(JiraPortletConstants.TICKET_ID_KEY,ticketId);
  final PortletPreferences prefs=request.getPreferences();
  final String url=prefs.getValue(JiraPortletConstants.USER_NAME_PREFERENCE,StringUtil.EMPTY_STRING);
  view.addObject(JiraPortletConstants.URL_PREFERENCE,url);
  return view;
}
