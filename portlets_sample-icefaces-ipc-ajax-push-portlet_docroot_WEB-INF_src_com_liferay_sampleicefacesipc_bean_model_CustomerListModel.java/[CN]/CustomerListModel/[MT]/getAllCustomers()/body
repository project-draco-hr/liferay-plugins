{
  List<Customer> allCustomers=(List<Customer>)PortletSessionUtil.getSharedSessionAttribute(PortletSessionUtil.CUSTOMER_LIST);
  if (allCustomers == null) {
    allCustomers=_customerService.getAllCustomers();
    PortletSessionUtil.setSharedSessionAttribute(PortletSessionUtil.CUSTOMER_LIST,allCustomers);
  }
  return allCustomers;
}
