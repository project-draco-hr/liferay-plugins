{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,meetupsEntry,merge);
    meetupsEntry.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(MeetupsEntryModelImpl.ENTITY_CACHE_ENABLED,MeetupsEntryImpl.class,meetupsEntry.getPrimaryKey(),meetupsEntry);
  return meetupsEntry;
}
