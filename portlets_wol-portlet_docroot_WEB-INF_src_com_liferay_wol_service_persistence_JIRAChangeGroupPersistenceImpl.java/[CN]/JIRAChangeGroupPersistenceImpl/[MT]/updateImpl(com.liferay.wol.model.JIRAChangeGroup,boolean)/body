{
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,jiraChangeGroup,merge);
    jiraChangeGroup.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(JIRAChangeGroupModelImpl.ENTITY_CACHE_ENABLED,JIRAChangeGroupImpl.class,jiraChangeGroup.getPrimaryKey(),jiraChangeGroup);
  return jiraChangeGroup;
}
