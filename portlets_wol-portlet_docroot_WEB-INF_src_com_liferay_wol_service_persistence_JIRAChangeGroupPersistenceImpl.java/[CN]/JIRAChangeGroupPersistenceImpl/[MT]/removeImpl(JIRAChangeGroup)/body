{
  Session session=null;
  try {
    session=openSession();
    session.delete(jiraChangeGroup);
    session.flush();
    return jiraChangeGroup;
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
    FinderCacheUtil.clearCache(JIRAChangeGroup.class.getName());
  }
}
