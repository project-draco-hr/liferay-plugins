{
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onBeforeRemove(jiraChangeGroup);
    }
  }
  jiraChangeGroup=removeImpl(jiraChangeGroup);
  if (_listeners != null) {
    for (    ModelListener listener : _listeners) {
      listener.onAfterRemove(jiraChangeGroup);
    }
  }
  return jiraChangeGroup;
}
