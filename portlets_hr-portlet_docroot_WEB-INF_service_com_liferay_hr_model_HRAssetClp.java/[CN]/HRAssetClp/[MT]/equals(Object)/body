{
  if (obj == null) {
    return false;
  }
  HRAssetClp hrAsset=null;
  try {
    hrAsset=(HRAssetClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrAsset.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
