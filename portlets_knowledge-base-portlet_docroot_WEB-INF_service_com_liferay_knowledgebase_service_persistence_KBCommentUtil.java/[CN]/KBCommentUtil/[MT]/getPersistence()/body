{
  if (_persistence == null) {
    _persistence=(KBCommentPersistence)PortletBeanLocatorUtil.locate(com.liferay.knowledgebase.service.ClpSerializer.getServletContextName(),KBCommentPersistence.class.getName());
    ReferenceRegistry.registerReference(KBCommentUtil.class,"_persistence");
  }
  return _persistence;
}
