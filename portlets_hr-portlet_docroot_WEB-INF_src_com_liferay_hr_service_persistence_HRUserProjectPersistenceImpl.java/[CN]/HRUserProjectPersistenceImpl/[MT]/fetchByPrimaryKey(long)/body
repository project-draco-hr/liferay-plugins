{
  HRUserProject hrUserProject=(HRUserProject)EntityCacheUtil.getResult(HRUserProjectModelImpl.ENTITY_CACHE_ENABLED,HRUserProjectImpl.class,hrUserProjectId,this);
  if (hrUserProject == null) {
    Session session=null;
    try {
      session=openSession();
      hrUserProject=(HRUserProject)session.get(HRUserProjectImpl.class,Long.valueOf(hrUserProjectId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrUserProject != null) {
        cacheResult(hrUserProject);
      }
      closeSession(session);
    }
  }
  return hrUserProject;
}
