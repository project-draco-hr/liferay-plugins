{
  int defaultMaxConnectionsPerRoute=MapUtil.getInteger(properties,"default.max.connections.per.route",20);
  setDefaultMaxConnectionsPerRoute(defaultMaxConnectionsPerRoute);
  int maxTotalConnections=MapUtil.getInteger(properties,"max.total.connections",20);
  setMaxTotalConnections(maxTotalConnections);
  String password=MapUtil.getString(properties,"password",StringPool.BLANK);
  setPassword(password);
  String username=MapUtil.getString(properties,"username",StringPool.BLANK);
  setUsername(username);
}
