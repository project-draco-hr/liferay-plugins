{
  HRExpenseCurrency hrExpenseCurrency=(HRExpenseCurrency)EntityCacheUtil.getResult(HRExpenseCurrencyModelImpl.ENTITY_CACHE_ENABLED,HRExpenseCurrencyImpl.class,hrExpenseCurrencyId,this);
  if (hrExpenseCurrency == null) {
    Session session=null;
    try {
      session=openSession();
      hrExpenseCurrency=(HRExpenseCurrency)session.get(HRExpenseCurrencyImpl.class,Long.valueOf(hrExpenseCurrencyId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrExpenseCurrency != null) {
        cacheResult(hrExpenseCurrency);
      }
      closeSession(session);
    }
  }
  return hrExpenseCurrency;
}
