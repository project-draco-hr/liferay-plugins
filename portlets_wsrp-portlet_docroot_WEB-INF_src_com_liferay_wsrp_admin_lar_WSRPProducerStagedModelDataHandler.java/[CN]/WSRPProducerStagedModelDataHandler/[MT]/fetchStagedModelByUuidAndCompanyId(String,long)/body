{
  List<WSRPProducer> wsrpProducers=WSRPProducerLocalServiceUtil.getWSRPProducersByUuidAndCompanyId(uuid,companyId,QueryUtil.ALL_POS,QueryUtil.ALL_POS,new StagedModelModifiedDateComparator<WSRPProducer>());
  if (ListUtil.isEmpty(wsrpProducers)) {
    return null;
  }
  return wsrpProducers.get(0);
}
