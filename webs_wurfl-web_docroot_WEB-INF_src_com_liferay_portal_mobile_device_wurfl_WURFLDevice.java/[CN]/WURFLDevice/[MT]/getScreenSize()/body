{
  Capability widthCapability=_capabilities.get(WURFLConstants.RESOLUTION_WIDTH);
  float width=GetterUtil.getFloat(widthCapability.getValue());
  Capability heightCapability=_capabilities.get(WURFLConstants.RESOLUTION_HEIGHT);
  float height=GetterUtil.getFloat(heightCapability.getValue());
  return new Dimensions(width,height);
}
