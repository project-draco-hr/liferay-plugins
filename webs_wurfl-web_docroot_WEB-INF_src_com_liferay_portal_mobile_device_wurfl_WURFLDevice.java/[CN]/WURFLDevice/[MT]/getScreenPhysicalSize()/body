{
  Capability heightCapability=_capabilities.get(WURFLConstants.SCREEN_PHYSICAL_HEIGHT);
  Capability widthCapability=_capabilities.get(WURFLConstants.SCREEN_PHYSICAL_WIDTH);
  if ((heightCapability == null) || (widthCapability == null)) {
    return Dimensions.UNKNOWN;
  }
  int height=GetterUtil.getInteger(heightCapability.getValue());
  int width=GetterUtil.getInteger(widthCapability.getValue());
  return new Dimensions(height,width);
}
