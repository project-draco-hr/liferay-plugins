{
  _active=active;
  if (_bbbServerRemoteModel != null) {
    try {
      Class<?> clazz=_bbbServerRemoteModel.getClass();
      Method method=clazz.getMethod("setActive",boolean.class);
      method.invoke(_bbbServerRemoteModel,active);
    }
