{
  KBTemplate kbTemplate=kbTemplatePersistence.remove(kbTemplateId);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.delete(kbTemplate);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
}
