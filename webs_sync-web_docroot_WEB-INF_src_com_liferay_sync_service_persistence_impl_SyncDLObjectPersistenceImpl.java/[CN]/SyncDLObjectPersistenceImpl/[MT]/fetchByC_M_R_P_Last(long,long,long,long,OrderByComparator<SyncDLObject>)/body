{
  int count=countByC_M_R_P(companyId,modifiedTime,repositoryId,parentFolderId);
  if (count == 0) {
    return null;
  }
  List<SyncDLObject> list=findByC_M_R_P(companyId,modifiedTime,repositoryId,parentFolderId,count - 1,count,orderByComparator);
  if (!list.isEmpty()) {
    return list.get(0);
  }
  return null;
}
