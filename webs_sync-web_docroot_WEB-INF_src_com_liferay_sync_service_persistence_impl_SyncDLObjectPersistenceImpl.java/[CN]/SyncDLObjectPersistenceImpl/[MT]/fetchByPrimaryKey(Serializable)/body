{
  SyncDLObject syncDLObject=(SyncDLObject)EntityCacheUtil.getResult(SyncDLObjectModelImpl.ENTITY_CACHE_ENABLED,SyncDLObjectImpl.class,primaryKey);
  if (syncDLObject == _nullSyncDLObject) {
    return null;
  }
  if (syncDLObject == null) {
    Session session=null;
    try {
      session=openSession();
      syncDLObject=(SyncDLObject)session.get(SyncDLObjectImpl.class,primaryKey);
      if (syncDLObject != null) {
        cacheResult(syncDLObject);
      }
 else {
        EntityCacheUtil.putResult(SyncDLObjectModelImpl.ENTITY_CACHE_ENABLED,SyncDLObjectImpl.class,primaryKey,_nullSyncDLObject);
      }
    }
 catch (    Exception e) {
      EntityCacheUtil.removeResult(SyncDLObjectModelImpl.ENTITY_CACHE_ENABLED,SyncDLObjectImpl.class,primaryKey);
      throw processException(e);
    }
 finally {
      closeSession(session);
    }
  }
  return syncDLObject;
}
