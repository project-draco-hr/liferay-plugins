{
  HRJobTitle hrJobTitle=(HRJobTitle)EntityCacheUtil.getResult(HRJobTitleModelImpl.ENTITY_CACHE_ENABLED,HRJobTitleImpl.class,hrJobTitleId,this);
  if (hrJobTitle == null) {
    Session session=null;
    try {
      session=openSession();
      hrJobTitle=(HRJobTitle)session.get(HRJobTitleImpl.class,Long.valueOf(hrJobTitleId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrJobTitle != null) {
        cacheResult(hrJobTitle);
      }
      closeSession(session);
    }
  }
  return hrJobTitle;
}
