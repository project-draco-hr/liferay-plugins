{
  HRJobTitle hrJobTitle=(HRJobTitle)EntityCacheUtil.getResult(HRJobTitleModelImpl.ENTITY_CACHE_ENABLED,HRJobTitleImpl.class,hrJobTitleId);
  if (hrJobTitle == _nullHRJobTitle) {
    return null;
  }
  if (hrJobTitle == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrJobTitle=(HRJobTitle)session.get(HRJobTitleImpl.class,Long.valueOf(hrJobTitleId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrJobTitle != null) {
        cacheResult(hrJobTitle);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRJobTitleModelImpl.ENTITY_CACHE_ENABLED,HRJobTitleImpl.class,hrJobTitleId,_nullHRJobTitle);
      }
      closeSession(session);
    }
  }
  return hrJobTitle;
}
