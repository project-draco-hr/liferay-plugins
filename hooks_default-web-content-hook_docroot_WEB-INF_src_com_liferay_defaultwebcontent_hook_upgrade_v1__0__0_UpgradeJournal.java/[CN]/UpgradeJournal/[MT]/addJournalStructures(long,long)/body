{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setAddCommunityPermissions(true);
  serviceContext.setAddGuestPermissions(true);
  String structureId="ARTICLE";
  String name="Article";
  String description="This structure accommodates article title, " + "both main, and preview images, and the main article body.";
  String xsd=getFileAsString("/structures/article.xml");
  JournalStructureLocalServiceUtil.addStructure(userId,groupId,structureId,false,StringPool.BLANK,name,description,xsd,serviceContext);
  structureId="MULTIPLE-ITEM-CAROUSEL";
  name="Carousel";
  description="This is a simple carousel structure designed to " + "handle other necessary carousel configurations.";
  xsd=getFileAsString("/structures/multiple_item_carousel.xml");
  JournalStructureLocalServiceUtil.addStructure(userId,groupId,structureId,false,StringPool.BLANK,name,description,xsd,serviceContext);
  structureId="MULTIPLE-ITEM";
  name="Multiple Item";
  description="This is a simple structure with a single repeatable " + "element that includes an HTML field, and text-box for a title " + "and url designation.";
  xsd=getFileAsString("/structures/multiple_item.xml");
  JournalStructureLocalServiceUtil.addStructure(userId,groupId,structureId,false,StringPool.BLANK,name,description,xsd,serviceContext);
}
