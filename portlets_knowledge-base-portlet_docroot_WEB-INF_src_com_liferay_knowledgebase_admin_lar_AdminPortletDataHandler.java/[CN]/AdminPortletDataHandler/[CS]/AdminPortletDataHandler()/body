{
  setDataLevel(DataLevel.SITE);
  setDeletionSystemEventStagedModelTypes(new StagedModelType(KBArticle.class),new StagedModelType(KBComment.class),new StagedModelType(KBTemplate.class));
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"articles",true,true,null,KBArticle.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"templates",true,true,null,KBTemplate.class.getName()),new PortletDataHandlerBoolean(NAMESPACE,"comments",true,true,null,KBComment.class.getName()));
}
