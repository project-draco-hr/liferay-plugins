{
  portletDataContext.importPermissions("com.liferay.knowledgebase.admin",portletDataContext.getSourceGroupId(),portletDataContext.getScopeGroupId());
  Element rootElement=portletDataContext.getImportDataRootElement();
  importKBArticles(portletDataContext,rootElement);
  importKBTemplates(portletDataContext,rootElement);
  importKBComments(portletDataContext,rootElement);
  return null;
}
