{
  Element kbArticleElement=rootElement.addElement("kb-article");
  exportKBArticleVersions(portletDataContext,kbArticleElement,kbArticle.getResourcePrimKey());
  exportKBArticleAttachments(portletDataContext,kbArticleElement,kbArticle);
  portletDataContext.addClassedModel(kbArticleElement,path,kbArticle,NAMESPACE);
}
