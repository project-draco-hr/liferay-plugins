{
  if (obj == null) {
    return false;
  }
  JIRAIssueClp jiraIssue=null;
  try {
    jiraIssue=(JIRAIssueClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=jiraIssue.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
