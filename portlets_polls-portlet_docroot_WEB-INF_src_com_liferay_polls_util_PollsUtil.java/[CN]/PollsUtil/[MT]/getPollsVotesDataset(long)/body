{
  DefaultCategoryDataset defaultCategoryDataset=new DefaultCategoryDataset();
  String seriesName=StringPool.BLANK;
  for (  PollsChoice pollsChoice : PollsChoiceLocalServiceUtil.getPollsChoices(pollsQuestionId)) {
    Integer number=pollsChoice.getPollsVotesCount();
    defaultCategoryDataset.addValue(number,seriesName,pollsChoice.getName());
  }
  return defaultCategoryDataset;
}
