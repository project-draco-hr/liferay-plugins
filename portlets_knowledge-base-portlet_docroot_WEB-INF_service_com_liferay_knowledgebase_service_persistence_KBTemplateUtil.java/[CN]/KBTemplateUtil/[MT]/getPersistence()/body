{
  if (_persistence == null) {
    _persistence=(KBTemplatePersistence)PortletBeanLocatorUtil.locate(com.liferay.knowledgebase.service.ClpSerializer.getServletContextName(),KBTemplatePersistence.class.getName());
    ReferenceRegistry.registerReference(KBTemplateUtil.class,"_persistence");
  }
  return _persistence;
}
