{
  ProjectEntry projectEntry=fetchByPrimaryKey(projectEntryId);
  if (projectEntry == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ProjectEntry exists with the primary key " + projectEntryId);
    }
    throw new NoSuchProjectEntryException("No ProjectEntry exists with the primary key " + projectEntryId);
  }
  return projectEntry;
}
