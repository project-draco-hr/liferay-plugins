{
  Session session=null;
  try {
    session=openSession();
    ProjectEntry projectEntry=(ProjectEntry)session.get(ProjectEntryImpl.class,new Long(projectEntryId));
    if (projectEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No ProjectEntry exists with the primary key " + projectEntryId);
      }
      throw new NoSuchProjectEntryException("No ProjectEntry exists with the primary key " + projectEntryId);
    }
    return remove(projectEntry);
  }
 catch (  NoSuchProjectEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
