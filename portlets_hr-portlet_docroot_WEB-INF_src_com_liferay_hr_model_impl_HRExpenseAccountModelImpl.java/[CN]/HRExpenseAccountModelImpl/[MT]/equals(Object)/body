{
  if (obj == null) {
    return false;
  }
  HRExpenseAccount hrExpenseAccount=null;
  try {
    hrExpenseAccount=(HRExpenseAccount)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrExpenseAccount.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
