{
  WorkflowDefinition workflowDefinition=fetchByPrimaryKey(workflowDefinitionId);
  if (workflowDefinition == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No WorkflowDefinition exists with the primary key " + workflowDefinitionId);
    }
    throw new NoSuchWorkflowDefinitionException("No WorkflowDefinition exists with the primary key " + workflowDefinitionId);
  }
  return workflowDefinition;
}
