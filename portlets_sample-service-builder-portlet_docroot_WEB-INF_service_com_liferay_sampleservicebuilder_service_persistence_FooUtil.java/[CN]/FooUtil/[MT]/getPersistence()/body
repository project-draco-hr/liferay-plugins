{
  if (_persistence == null) {
    _persistence=(FooPersistence)PortletBeanLocatorUtil.locate(com.liferay.sampleservicebuilder.service.ClpSerializer.SERVLET_CONTEXT_NAME,FooPersistence.class.getName());
    ReferenceRegistry.registerReference(FooUtil.class,"_persistence");
  }
  return _persistence;
}
