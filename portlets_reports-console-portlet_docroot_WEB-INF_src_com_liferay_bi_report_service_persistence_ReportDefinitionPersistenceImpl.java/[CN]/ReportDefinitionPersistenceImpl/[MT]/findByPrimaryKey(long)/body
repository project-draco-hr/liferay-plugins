{
  ReportDefinition reportDefinition=fetchByPrimaryKey(definitionId);
  if (reportDefinition == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No ReportDefinition exists with the primary key " + definitionId);
    }
    throw new NoSuchDefinitionException("No ReportDefinition exists with the primary key " + definitionId);
  }
  return reportDefinition;
}
