{
  name=getName(name);
  Map<Locale,String> nameMap=getNameMap(name);
  String xsd=StringUtil.read(inputStream);
  JournalStructure journalStructure=JournalStructureLocalServiceUtil.addStructure(userId,groupId,StringPool.BLANK,true,StringPool.BLANK,nameMap,null,xsd,serviceContext);
  addJournalTemplates(journalStructure.getStructureId(),"/journal/templates/" + name);
}
