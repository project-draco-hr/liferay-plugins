{
  File[] files=directory.listFiles();
  if (Validator.isNull(files)) {
    return;
  }
  for (  File file : files) {
    if (file.isDirectory()) {
      long folderId=addDLFolder(parentFolderId,file.getName());
      recurseDLDirectory(folderId,file);
    }
 else {
      doAddDLFileEntry(parentFolderId,file);
    }
  }
}
