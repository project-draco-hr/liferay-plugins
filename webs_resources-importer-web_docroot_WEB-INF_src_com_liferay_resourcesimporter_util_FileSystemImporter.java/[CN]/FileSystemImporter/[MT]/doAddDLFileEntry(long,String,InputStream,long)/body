{
  String mimeType=MimeTypesUtil.getContentType(fileName);
  String title=FileUtil.stripExtension(fileName);
  setServiceContext(fileName);
  FileEntry fileEntry=DLAppLocalServiceUtil.addFileEntry(userId,groupId,parentFolderId,fileName,mimeType,title,StringPool.BLANK,StringPool.BLANK,inputStream,length,serviceContext);
  _fileEntries.put(fileName,fileEntry);
}
