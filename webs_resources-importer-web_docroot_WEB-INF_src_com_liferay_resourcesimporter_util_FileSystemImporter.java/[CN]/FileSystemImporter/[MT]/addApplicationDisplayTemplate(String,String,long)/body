{
  String appDisplayTemplateTypeDir=StringUtil.add(parentDirName,dirName,"/");
  File applicationDisplayTemplatesDir=new File(_resourcesDir,appDisplayTemplateTypeDir);
  if (!applicationDisplayTemplatesDir.isDirectory() || !applicationDisplayTemplatesDir.canRead()) {
    return;
  }
  File[] files=listFiles(applicationDisplayTemplatesDir);
  for (  File file : files) {
    String script=StringUtil.read(getInputStream(file));
    if (Validator.isNull(script)) {
      continue;
    }
    addApplicationDisplayTemplate(script,file,classNameId);
  }
}
