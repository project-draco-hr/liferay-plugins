{
  File dlDocumentsDir=new File(_resourcesDir,fileEntriesDirName);
  if (!dlDocumentsDir.isDirectory() || !dlDocumentsDir.canRead()) {
    return;
  }
  File[] files=dlDocumentsDir.listFiles();
  if (Validator.isNull(files)) {
    return;
  }
  for (  File file : files) {
    if (file.isDirectory()) {
      long folderId=addDLFolder(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,file.getName());
      recurseDLDirectory(folderId,file);
    }
 else {
      doAddDLFileEntry(DLFolderConstants.DEFAULT_PARENT_FOLDER_ID,file);
    }
  }
}
