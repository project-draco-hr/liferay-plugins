{
  String fileName=FileUtil.stripExtension(file.getName());
  String name=getName(fileName);
  DDMTemplate ddmTemplate=DDMTemplateLocalServiceUtil.fetchTemplate(groupId,classNameId,getKey(fileName));
  if (ddmTemplate != null) {
    if (!developerModeEnabled) {
      if (_log.isInfoEnabled()) {
        _log.info("DDM template with name " + name + " and version "+ version+ " already exists");
      }
      return;
    }
    DDMTemplateLocalServiceUtil.deleteTemplate(ddmTemplate);
  }
  DDMTemplateLocalServiceUtil.addTemplate(userId,groupId,classNameId,0,getKey(fileName),getMap(name),null,DDMTemplateConstants.TEMPLATE_TYPE_DISPLAY,StringPool.BLANK,getDDMTemplateLanguage(name),script,false,false,StringPool.BLANK,null,serviceContext);
}
