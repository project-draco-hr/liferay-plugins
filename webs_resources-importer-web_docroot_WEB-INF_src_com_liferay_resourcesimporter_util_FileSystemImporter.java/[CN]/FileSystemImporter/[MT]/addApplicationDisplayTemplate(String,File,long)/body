{
  String fileName=FileUtil.stripExtension(file.getName());
  Map<Locale,String> nameMap=getMap(fileName);
  String language=getDDMTemplateLanguage(fileName);
  boolean cacheable=false;
  boolean smallImage=false;
  String smallImageURL=StringPool.BLANK;
  DDMTemplateLocalServiceUtil.addTemplate(userId,groupId,classNameId,0,null,nameMap,null,DDMTemplateConstants.TEMPLATE_TYPE_DISPLAY,StringPool.BLANK,language,script,cacheable,smallImage,smallImageURL,null,serviceContext);
}
