{
  String name=FileUtil.stripExtension(file.getName());
  DDMTemplateLocalServiceUtil.addTemplate(userId,groupId,classNameId,0,null,getMap(name),null,DDMTemplateConstants.TEMPLATE_TYPE_DISPLAY,StringPool.BLANK,getDDMTemplateLanguage(name),script,false,false,StringPool.BLANK,null,serviceContext);
}
