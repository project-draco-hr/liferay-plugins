{
  long classNameId=PortalUtil.getClassNameId(DDMStructure.class.getName());
  String fullFileName=FileUtil.stripExtension(FileUtil.getShortFileName(fileName));
  Map<Locale,String> nameMap=getMap(fullFileName);
  boolean cacheable=false;
  boolean smallImage=false;
  String smallImageURL="";
  DDMTemplateLocalServiceUtil.addTemplate(userId,templateGroupId,classNameId,ddmStructureId,null,nameMap,null,type,mode,language,script,cacheable,smallImage,smallImageURL,null,serviceContext);
}
