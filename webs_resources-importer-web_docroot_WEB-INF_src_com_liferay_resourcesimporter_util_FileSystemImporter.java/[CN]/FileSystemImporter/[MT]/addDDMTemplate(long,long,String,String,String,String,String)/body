{
  fileName=FileUtil.stripExtension(FileUtil.getShortFileName(fileName));
  DDMTemplateLocalServiceUtil.addTemplate(userId,templateGroupId,PortalUtil.getClassNameId(DDMStructure.class),ddmStructureId,null,getMap(fileName),null,type,mode,language,script,false,false,StringPool.BLANK,null,serviceContext);
}
