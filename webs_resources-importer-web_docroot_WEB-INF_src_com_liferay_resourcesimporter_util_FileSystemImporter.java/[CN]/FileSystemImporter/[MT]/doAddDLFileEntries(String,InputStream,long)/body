{
  ServiceContext serviceContext=new ServiceContext();
  serviceContext.setScopeGroupId(groupId);
  String mimeType=MimeTypesUtil.getContentType(name);
  FileEntry fileEntry=DLAppLocalServiceUtil.addFileEntry(userId,groupId,0,name,mimeType,name,StringPool.BLANK,StringPool.BLANK,inputStream,length,serviceContext);
  _fileEntries.put(name,fileEntry);
}
