{
  UserPresence userPresence=fetchByPrimaryKey(presenceUserId);
  if (userPresence == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No UserPresence exists with the primary key " + presenceUserId);
    }
    throw new NoSuchUserPresenceException("No UserPresence exists with the primary key " + presenceUserId);
  }
  return userPresence;
}
