{
  Session session=null;
  try {
    session=openSession();
    UserPresence userPresence=(UserPresence)session.get(UserPresenceImpl.class,new Long(presenceUserId));
    if (userPresence == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No UserPresence exists with the primary key " + presenceUserId);
      }
      throw new NoSuchUserPresenceException("No UserPresence exists with the primary key " + presenceUserId);
    }
    return remove(userPresence);
  }
 catch (  NoSuchUserPresenceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
