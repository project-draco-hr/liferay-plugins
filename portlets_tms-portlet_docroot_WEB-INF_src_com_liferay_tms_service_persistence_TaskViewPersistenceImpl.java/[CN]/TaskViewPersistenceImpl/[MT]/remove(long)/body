{
  Session session=null;
  try {
    session=openSession();
    TaskView taskView=(TaskView)session.get(TaskViewImpl.class,new Long(taskViewId));
    if (taskView == null) {
      if (_log.isWarnEnabled()) {
        _log.warn("No TaskView exists with the primary key " + taskViewId);
      }
      throw new NoSuchTaskViewException("No TaskView exists with the primary key " + taskViewId);
    }
    return remove(taskView);
  }
 catch (  NoSuchTaskViewException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
