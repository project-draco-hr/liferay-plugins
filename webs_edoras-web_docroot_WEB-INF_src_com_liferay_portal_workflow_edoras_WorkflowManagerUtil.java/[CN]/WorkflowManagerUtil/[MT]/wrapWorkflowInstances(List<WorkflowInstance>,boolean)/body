{
  List<WorkflowInstanceInfo> workflowInstanceInfos=new ArrayList<WorkflowInstanceInfo>(workflowInstances.size());
  for (  WorkflowInstance workflowInstance : workflowInstances) {
    workflowInstanceInfos.add(new WorkflowInstanceInfoImpl(workflowInstance,includeChildren));
  }
  return workflowInstanceInfos;
}
