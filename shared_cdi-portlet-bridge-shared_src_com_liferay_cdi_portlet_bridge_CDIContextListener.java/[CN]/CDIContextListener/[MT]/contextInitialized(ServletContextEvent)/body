{
  ServletContext servletContext=servletContextEvent.getServletContext();
  BeanManager beanManager=(BeanManager)servletContext.getAttribute(BeanManager.class.getName());
  if (beanManager == null) {
    beanManager=(BeanManager)servletContext.getAttribute("org.jboss.weld.environment.servlet.javax.enterprise." + "inject.spi.BeanManager");
  }
  CDIBeanManagerUtil.setBeanManager(beanManager);
}
