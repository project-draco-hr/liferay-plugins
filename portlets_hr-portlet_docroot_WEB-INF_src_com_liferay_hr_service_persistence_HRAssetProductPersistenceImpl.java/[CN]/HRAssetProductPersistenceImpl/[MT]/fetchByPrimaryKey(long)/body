        cacheResult(hrAssetProduct);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRAssetProductModelImpl.ENTITY_CACHE_ENABLED,HRAssetProductImpl.class,hrAssetProductId,_nullHRAssetProduct);
