{
  HRAssetProduct hrAssetProduct=(HRAssetProduct)EntityCacheUtil.getResult(HRAssetProductModelImpl.ENTITY_CACHE_ENABLED,HRAssetProductImpl.class,hrAssetProductId,this);
  if (hrAssetProduct == null) {
    Session session=null;
    try {
      session=openSession();
      hrAssetProduct=(HRAssetProduct)session.get(HRAssetProductImpl.class,Long.valueOf(hrAssetProductId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrAssetProduct != null) {
        cacheResult(hrAssetProduct);
      }
      closeSession(session);
    }
  }
  return hrAssetProduct;
}
