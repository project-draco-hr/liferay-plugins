{
  String emailArticleAddedEnabled=jxPreferences.getValue("emailArticleAddedEnabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleAddedEnabled)) {
    return GetterUtil.getBoolean(emailArticleAddedEnabled);
  }
  return GetterUtil.getBoolean(PortletProps.get("admin.email.article.added.enabled"));
}
