{
  String emailArticleUpdatedEnabled=jxPreferences.getValue("email-article-updated-enabled",StringPool.BLANK);
  if (Validator.isNotNull(emailArticleUpdatedEnabled)) {
    return GetterUtil.getBoolean(emailArticleUpdatedEnabled);
  }
  return GetterUtil.getBoolean(PortletProps.get("admin.email.article.updated.enabled"));
}
