{
  Layout layout=LayoutLocalServiceUtil.getLayout(plid);
  String articleURL=portalURL + PortalUtil.getLayoutActualURL(layout);
  String namespace=ServicePreAction.NAMESPACE;
  articleURL=HttpUtil.setParameter(articleURL,namespace + Constants.CMD,Constants.FIND_ARTICLE);
  articleURL=HttpUtil.setParameter(articleURL,namespace + "resourcePrimKey",resourcePrimKey);
  return articleURL;
}
