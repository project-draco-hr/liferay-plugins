  addPortletId(layout,PortletKeys.TAGS_CATEGORIES_NAVIGATION,"column-1");
  addPortletId(layout,PortletKeys.SEARCH,"column-2");
  addPortletId(layout,PortletKeys.ASSET_PUBLISHER,"column-2");
}
