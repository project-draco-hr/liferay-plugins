{
  String[] classNameIds=GetterUtil.getStringValues(portletPreferences.getValues(preferenceName,null));
  for (  String classNameId : classNameIds) {
    if (classNameId.equals(calEventId)) {
      ArrayUtil.replace(classNameIds,String.valueOf(classNameId),calendarBookingId);
      portletPreferences.setValues(preferenceName,classNameIds);
      break;
    }
  }
}
