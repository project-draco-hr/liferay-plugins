{
  try {
    if (actorId.equals(String.valueOf(userId))) {
      return true;
    }
    if (Validator.isEmailAddress(actorId)) {
      User user=UserLocalServiceUtil.getUser(userId);
      if (actorId.equals(user.getEmailAddress())) {
        return true;
      }
    }
    return false;
  }
 catch (  Exception e) {
    throw new WorkflowException(e);
  }
}
