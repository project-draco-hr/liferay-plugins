{
  if (_persistence == null) {
    _persistence=(PushNotificationsDevicePersistence)PortletBeanLocatorUtil.locate(com.liferay.pushnotifications.service.ClpSerializer.getServletContextName(),PushNotificationsDevicePersistence.class.getName());
    ReferenceRegistry.registerReference(PushNotificationsDeviceUtil.class,"_persistence");
  }
