{
  _ipGeocoderRequestDestination=new ParallelDispatchedDestination(DestinationNames.IP_GEOCODER_REQUEST,5,10);
  MessageBusUtil.addDestination(_ipGeocoderRequestDestination);
  _ipGeocoderRequestMessageListener=new IPGeocoderRequestMessageListener();
  MessageBusUtil.registerMessageListener(_ipGeocoderRequestDestination.getName(),_ipGeocoderRequestMessageListener);
}
