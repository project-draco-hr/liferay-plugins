{
  ResponseBuilder responseBuilder=getResponseBuilder();
  Response response=responseBuilder.build();
  try {
    JSONObject folderJson=new JSONObject(response.getResponseBody());
    Folder folder=new Folder(folderJson);
    if (folder.getFolderId() == 0) {
      _log.info("Error adding new folder " + response.getResponseBody());
    }
 else {
      _folderData.setId(folder.getFolderId());
      _folderData.setParentFolderId(folder.getParentFolderId());
      _folderData.setRepositoryId(GetterUtil.getLong(AppPropsValues.REPOSITORY_ID));
      if (_log.isInfoEnabled()) {
        _log.info("Updated server with new folder " + _folderData.getTitle());
      }
    }
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
  return response;
}
