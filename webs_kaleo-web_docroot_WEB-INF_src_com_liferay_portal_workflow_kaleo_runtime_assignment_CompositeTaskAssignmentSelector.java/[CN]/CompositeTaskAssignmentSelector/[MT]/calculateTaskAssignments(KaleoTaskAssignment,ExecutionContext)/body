{
  String assigneeClassName=configuredKaleoTaskAssignment.getAssigneeClassName();
  TaskAssignmentSelector taskAssignmentSelector=_taskAssignmentSelectors.get(assigneeClassName);
  if (taskAssignmentSelector == null) {
    throw new IllegalArgumentException("Unrecognized task assignment type: " + assigneeClassName);
  }
  return taskAssignmentSelector.calculateTaskAssignments(configuredKaleoTaskAssignment,executionContext);
}
