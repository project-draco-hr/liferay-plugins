{
  try {
    com.liferay.contacts.model.Entry returnValue=EntryServiceUtil.updateEntry(entryId,fullName,emailAddress,comments);
    return com.liferay.contacts.model.EntrySoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
