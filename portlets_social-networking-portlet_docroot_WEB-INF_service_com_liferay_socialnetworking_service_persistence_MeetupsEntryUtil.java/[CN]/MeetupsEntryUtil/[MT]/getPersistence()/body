{
  if (_persistence == null) {
    _persistence=(MeetupsEntryPersistence)PortletBeanLocatorUtil.locate(com.liferay.socialnetworking.service.ClpSerializer.getServletContextName(),MeetupsEntryPersistence.class.getName());
    ReferenceRegistry.registerReference(MeetupsEntryUtil.class,"_persistence");
  }
  return _persistence;
}
