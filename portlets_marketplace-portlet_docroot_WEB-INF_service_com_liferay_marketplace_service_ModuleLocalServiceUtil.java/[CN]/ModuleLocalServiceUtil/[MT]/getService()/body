{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),ModuleLocalService.class.getName());
    if (invokableLocalService instanceof ModuleLocalService) {
      _service=(ModuleLocalService)invokableLocalService;
    }
 else {
      _service=new ModuleLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(ModuleLocalServiceUtil.class,"_service");
  }
  return _service;
}
