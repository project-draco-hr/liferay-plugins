{
  SVNRepository svnRepository=fetchByPrimaryKey(svnRepositoryId);
  if (svnRepository == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No SVNRepository exists with the primary key " + svnRepositoryId);
    }
    throw new NoSuchSVNRepositoryException("No SVNRepository exists with the primary key " + svnRepositoryId);
  }
  return svnRepository;
}
