{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoTaskLocalService.class.getName());
    _service=new KaleoTaskLocalServiceClp(invokableLocalService);
    ReferenceRegistry.registerReference(KaleoTaskLocalServiceUtil.class,"_service");
    MethodCache.remove(KaleoTaskLocalService.class);
  }
  return _service;
}
