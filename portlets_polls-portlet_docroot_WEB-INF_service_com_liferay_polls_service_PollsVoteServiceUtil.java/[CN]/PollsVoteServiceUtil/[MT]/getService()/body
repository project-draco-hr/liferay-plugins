{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),PollsVoteService.class.getName());
    if (invokableService instanceof PollsVoteService) {
      _service=(PollsVoteService)invokableService;
    }
 else {
      _service=new PollsVoteServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(PollsVoteServiceUtil.class,"_service");
  }
  return _service;
}
