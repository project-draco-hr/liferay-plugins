{
  if (attributeId.equalsIgnoreCase("cn") || attributeId.equalsIgnoreCase("description") || attributeId.equalsIgnoreCase("member")|| attributeId.equalsIgnoreCase("ou")) {
    return true;
  }
 else   if (attributeId.equalsIgnoreCase("objectclass")) {
    if (value.equalsIgnoreCase("groupOfNames") || value.equalsIgnoreCase("organizationalUnit") || value.equalsIgnoreCase("top")|| value.equalsIgnoreCase("*")) {
      return true;
    }
  }
  return false;
}
