{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),WSRPConsumerPortletLocalService.class.getName());
    if (invokableLocalService instanceof WSRPConsumerPortletLocalService) {
      _service=(WSRPConsumerPortletLocalService)invokableLocalService;
    }
 else {
      _service=new WSRPConsumerPortletLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(WSRPConsumerPortletLocalServiceUtil.class,"_service");
    MethodCache.remove(WSRPConsumerPortletLocalService.class);
  }
  return _service;
}
