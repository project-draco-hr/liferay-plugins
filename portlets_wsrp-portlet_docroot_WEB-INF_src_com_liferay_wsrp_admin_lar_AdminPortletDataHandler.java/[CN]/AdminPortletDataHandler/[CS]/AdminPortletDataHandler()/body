{
  setDataLevel(DataLevel.SITE);
  setExportControls(new PortletDataHandlerBoolean(NAMESPACE,"wsrp-producers",false),new PortletDataHandlerBoolean(NAMESPACE,"wsrp-consumers",true,new PortletDataHandlerControl[]{new PortletDataHandlerBoolean(NAMESPACE,"wsrp-consumer-portlets")}));
