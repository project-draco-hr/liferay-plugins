{
  Session session=null;
  try {
    session=openSession();
    WallEntry wallEntry=(WallEntry)session.get(WallEntryImpl.class,new Long(wallEntryId));
    if (wallEntry == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + wallEntryId);
      }
      throw new NoSuchWallEntryException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + wallEntryId);
    }
    return wallEntryPersistence.remove(wallEntry);
  }
 catch (  NoSuchWallEntryException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
