{
  Session session=null;
  try {
    session=openSession();
    if (wallEntry.isCachedModel() || BatchSessionUtil.isEnabled()) {
      Object staleObject=session.get(WallEntryImpl.class,wallEntry.getPrimaryKeyObj());
      if (staleObject != null) {
        session.evict(staleObject);
      }
    }
    session.delete(wallEntry);
    session.flush();
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.removeResult(WallEntryModelImpl.ENTITY_CACHE_ENABLED,WallEntryImpl.class,wallEntry.getPrimaryKey());
  return wallEntry;
}
