{
  wallEntry=toUnwrappedModel(wallEntry);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.delete(session,wallEntry);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(wallEntry);
  return wallEntry;
}
