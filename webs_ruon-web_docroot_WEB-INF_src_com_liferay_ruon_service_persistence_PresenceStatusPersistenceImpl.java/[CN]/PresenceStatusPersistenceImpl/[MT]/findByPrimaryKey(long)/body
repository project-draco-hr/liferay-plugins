{
  PresenceStatus presenceStatus=fetchByPrimaryKey(presenceStatusId);
  if (presenceStatus == null) {
    if (_log.isWarnEnabled()) {
      _log.warn("No PresenceStatus exists with the primary key " + presenceStatusId);
    }
    throw new NoSuchPresenceStatusException("No PresenceStatus exists with the primary key " + presenceStatusId);
  }
  return presenceStatus;
}
