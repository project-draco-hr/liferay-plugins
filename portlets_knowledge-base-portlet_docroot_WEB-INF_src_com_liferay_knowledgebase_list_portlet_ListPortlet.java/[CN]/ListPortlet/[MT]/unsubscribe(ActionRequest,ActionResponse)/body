{
  ThemeDisplay themeDisplay=(ThemeDisplay)actionRequest.getAttribute(WebKeys.THEME_DISPLAY);
  if (!themeDisplay.isSignedIn()) {
    return;
  }
  long resourcePrimKey=ParamUtil.getLong(actionRequest,"resourcePrimKey");
  String portletId=PortalUtil.getPortletId(actionRequest);
  if (resourcePrimKey <= 0) {
    ArticleLocalServiceUtil.unsubscribe(themeDisplay.getCompanyId(),themeDisplay.getUserId(),portletId,themeDisplay.getScopeGroupId());
  }
 else {
    ArticleServiceUtil.unsubscribeArticle(themeDisplay.getCompanyId(),portletId,resourcePrimKey);
  }
}
