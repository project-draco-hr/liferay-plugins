{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),BBBServerLocalService.class.getName());
    if (invokableLocalService instanceof BBBServerLocalService) {
      _service=(BBBServerLocalService)invokableLocalService;
    }
 else {
      _service=new BBBServerLocalServiceClp(invokableLocalService);
    }
    ReferenceRegistry.registerReference(BBBServerLocalServiceUtil.class,"_service");
  }
  return _service;
}
