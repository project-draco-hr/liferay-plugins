{
  if (_persistence == null) {
    _persistence=(HRExpenseCurrencyConversionPersistence)PortletBeanLocatorUtil.locate(com.liferay.hr.service.ClpSerializer.getServletContextName(),HRExpenseCurrencyConversionPersistence.class.getName());
