{
  try {
    LdapApiService ldapApiService=new StandaloneLdapApiService();
    LdapMessageContainer<MessageDecorator<? extends Message>> ldapMessageContainer=new LdapMessageContainer<MessageDecorator<? extends Message>>(ldapApiService);
    ioSession.setAttribute("messageContainer",ldapMessageContainer);
  }
 catch (  Exception e) {
    _log.error(e,e);
  }
}
