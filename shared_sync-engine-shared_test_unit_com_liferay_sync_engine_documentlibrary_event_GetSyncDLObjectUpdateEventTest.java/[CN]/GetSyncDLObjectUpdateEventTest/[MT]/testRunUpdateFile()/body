{
  setGetResponse("dependencies/get_sync_dl_object_update_event_update.json");
  setPostResponse("dependencies/get_sync_dl_object_update_event_update.json");
  Path fileFilePath=Paths.get(_syncSite.getFilePathName() + "/test.txt");
  Files.createFile(fileFilePath);
  SyncFileTestUtil.addFileSyncFile(FilePathNameUtil.getFilePathName(fileFilePath),0,_syncSite.getGroupId(),syncAccount.getSyncAccountId(),27382);
  run();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(3,_syncFiles.size());
  Path filePath=Paths.get(_syncSite.getFilePathName() + "/test2.txt");
  Assert.assertTrue(Files.exists(filePath));
  SyncFile syncFile=SyncFileService.fetchSyncFile(FilePathNameUtil.getFilePathName(filePath),syncAccount.getSyncAccountId());
  Assert.assertEquals("Updated Description",syncFile.getDescription());
}
