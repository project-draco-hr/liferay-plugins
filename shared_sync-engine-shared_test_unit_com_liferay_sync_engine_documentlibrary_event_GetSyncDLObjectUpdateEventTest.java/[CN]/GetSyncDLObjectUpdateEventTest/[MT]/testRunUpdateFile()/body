{
  setGetResponse("dependencies/get_sync_dl_object_update_event_update.json");
  setPostResponse("dependencies/get_sync_dl_object_update_event_update.json");
  Path sourceFilePath=Paths.get(_syncSite.getFilePathName() + "/test.txt");
  Files.createFile(sourceFilePath);
  SyncFileTestUtil.addFileSyncFile(FilePathNameUtil.getFilePathName(sourceFilePath),0,_syncSite.getGroupId(),syncAccount.getSyncAccountId(),27382);
  run();
  _syncFiles=SyncFileService.findSyncFiles(syncAccount.getSyncAccountId());
  Assert.assertEquals(3,_syncFiles.size());
  Path targetFilePath=Paths.get(_syncSite.getFilePathName() + "/test2.txt");
  Assert.assertTrue(Files.exists(targetFilePath));
  SyncFile syncFile=SyncFileService.fetchSyncFile(FilePathNameUtil.getFilePathName(targetFilePath),syncAccount.getSyncAccountId());
  Assert.assertEquals("Updated Description",syncFile.getDescription());
}
