{
  HRTimeSheet hrTimeSheet=(HRTimeSheet)EntityCacheUtil.getResult(HRTimeSheetModelImpl.ENTITY_CACHE_ENABLED,HRTimeSheetImpl.class,hrTimeSheetId,this);
  if (hrTimeSheet == null) {
    Session session=null;
    try {
      session=openSession();
      hrTimeSheet=(HRTimeSheet)session.get(HRTimeSheetImpl.class,Long.valueOf(hrTimeSheetId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (hrTimeSheet != null) {
        cacheResult(hrTimeSheet);
      }
      closeSession(session);
    }
  }
  return hrTimeSheet;
}
