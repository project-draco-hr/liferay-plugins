{
  HRTimeSheet hrTimeSheet=(HRTimeSheet)EntityCacheUtil.getResult(HRTimeSheetModelImpl.ENTITY_CACHE_ENABLED,HRTimeSheetImpl.class,hrTimeSheetId,this);
  if (hrTimeSheet == _nullHRTimeSheet) {
    return null;
  }
  if (hrTimeSheet == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      hrTimeSheet=(HRTimeSheet)session.get(HRTimeSheetImpl.class,Long.valueOf(hrTimeSheetId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (hrTimeSheet != null) {
        cacheResult(hrTimeSheet);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(HRTimeSheetModelImpl.ENTITY_CACHE_ENABLED,HRTimeSheetImpl.class,hrTimeSheetId,_nullHRTimeSheet);
      }
      closeSession(session);
    }
  }
  return hrTimeSheet;
}
