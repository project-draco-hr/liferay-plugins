{
  setMockPostResponse("dependencies/get_all_sync_dl_objects.json");
  GetAllSyncDLObjectsEvent getAllSyncDLObjectsEvent=new GetAllSyncDLObjectsEvent(_syncAccount.getSyncAccountId(),null);
  getAllSyncDLObjectsEvent.run();
