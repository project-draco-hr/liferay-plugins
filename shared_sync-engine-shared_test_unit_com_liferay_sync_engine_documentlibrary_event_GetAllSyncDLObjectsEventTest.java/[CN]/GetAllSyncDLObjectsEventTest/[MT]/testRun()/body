{
  setMockPostResponse("dependencies/get_all_sync_dl_objects.json");
  GetAllSyncDLObjectsEvent getAllSyncDLObjectsEvent=new GetAllSyncDLObjectsEvent(syncAccount.getSyncAccountId(),null);
  getAllSyncDLObjectsEvent.run();
