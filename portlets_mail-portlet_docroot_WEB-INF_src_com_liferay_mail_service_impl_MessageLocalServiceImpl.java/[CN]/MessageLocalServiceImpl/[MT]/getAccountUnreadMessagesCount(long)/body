{
  DynamicQuery dynamicQuery=DynamicQueryFactoryUtil.forClass(Message.class,PortletClassLoaderUtil.getClassLoader());
  dynamicQuery.add(RestrictionsFactoryUtil.eq("accountId",accountId));
  dynamicQuery.add(RestrictionsFactoryUtil.not(RestrictionsFactoryUtil.like("flags","%" + MailConstants.FLAG_SEEN + ",%")));
  return dynamicQueryCount(dynamicQuery);
}
