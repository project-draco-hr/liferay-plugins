{
  if (cause instanceof CalendarEventDurationException || cause instanceof CalendarEventEndDateException || cause instanceof CalendarEventStartDateException|| cause instanceof CalendarEventTitleException|| cause instanceof DuplicateCalendarResourceException|| cause instanceof NoSuchBookingException|| cause instanceof NoSuchEventException|| cause instanceof NoSuchResourceException|| cause instanceof PrincipalException) {
    return true;
  }
  return false;
}
