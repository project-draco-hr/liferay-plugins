{
  long firstReminder=ParamUtil.getInteger(actionRequest,"reminderValue0");
  long firstReminderDuration=ParamUtil.getInteger(actionRequest,"reminderDuration0");
  long secondReminder=ParamUtil.getInteger(actionRequest,"reminderValue1");
  long secondReminderDuration=ParamUtil.getInteger(actionRequest,"reminderDuration1");
  return new long[]{firstReminder * firstReminderDuration * Time.SECOND,secondReminder * secondReminderDuration * Time.SECOND};
}
