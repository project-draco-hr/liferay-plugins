{
  String rID=request.getResourceID();
  if ("functionality".equals(rID)) {
    PrintWriter writer=response.getWriter();
    writer.println("<br>action scope attribute set in" + "PA1() of portlet1" + request.getAttribute(("n1")));
    writer.println("<br>action scope attribute set in " + "PE1() of portlet1:" + request.getAttribute(("n2")));
    writer.println("<br>action scope attribute set in " + "PE2() of portlet1:" + request.getAttribute(("n3")));
    writer.println("<br>Event Received from  portlet2 :" + request.getParameter("key2"));
  }
 else {
    response.setContentType("text/html");
    String content=getContents("/WEB-INF/jsp/PCAutomationTC.html");
    PrintWriter writer=response.getWriter();
    writer.print(content);
  }
}
