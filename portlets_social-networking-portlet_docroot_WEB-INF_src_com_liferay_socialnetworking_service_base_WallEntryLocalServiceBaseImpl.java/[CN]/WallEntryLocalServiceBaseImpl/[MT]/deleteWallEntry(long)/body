{
  WallEntry wallEntry=wallEntryPersistence.remove(wallEntryId);
  Indexer indexer=IndexerRegistryUtil.getIndexer(getModelClassName());
  if (indexer != null) {
    try {
      indexer.delete(wallEntry);
    }
 catch (    SearchException se) {
      if (_log.isWarnEnabled()) {
        _log.warn(se,se);
      }
    }
  }
}
