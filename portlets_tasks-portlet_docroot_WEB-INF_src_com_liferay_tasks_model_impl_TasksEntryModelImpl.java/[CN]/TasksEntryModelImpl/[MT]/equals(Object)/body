{
  if (obj == null) {
    return false;
  }
  TasksEntry tasksEntry=null;
  try {
    tasksEntry=(TasksEntry)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=tasksEntry.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
