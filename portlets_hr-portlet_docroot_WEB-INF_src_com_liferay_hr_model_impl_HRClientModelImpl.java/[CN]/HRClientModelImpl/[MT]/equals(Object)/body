{
  if (obj == null) {
    return false;
  }
  HRClient hrClient=null;
  try {
    hrClient=(HRClient)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrClient.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
