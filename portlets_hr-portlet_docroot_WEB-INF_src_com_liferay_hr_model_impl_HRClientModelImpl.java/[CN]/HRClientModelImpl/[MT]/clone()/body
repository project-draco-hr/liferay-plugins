{
  HRClientImpl hrClientImpl=new HRClientImpl();
  hrClientImpl.setHrClientId(getHrClientId());
  hrClientImpl.setGroupId(getGroupId());
  hrClientImpl.setCompanyId(getCompanyId());
  hrClientImpl.setUserId(getUserId());
  hrClientImpl.setUserName(getUserName());
  hrClientImpl.setCreateDate(getCreateDate());
  hrClientImpl.setModifiedDate(getModifiedDate());
  hrClientImpl.setName(getName());
  hrClientImpl.setDescription(getDescription());
  hrClientImpl.resetOriginalValues();
  return hrClientImpl;
}
