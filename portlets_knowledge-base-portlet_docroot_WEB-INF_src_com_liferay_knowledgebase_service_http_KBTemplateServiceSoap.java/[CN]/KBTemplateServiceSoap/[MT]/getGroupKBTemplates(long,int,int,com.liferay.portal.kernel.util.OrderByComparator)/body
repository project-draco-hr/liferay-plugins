{
  try {
    java.util.List<com.liferay.knowledgebase.model.KBTemplate> returnValue=KBTemplateServiceUtil.getGroupKBTemplates(groupId,start,end,orderByComparator);
    return returnValue.toArray(new com.liferay.knowledgebase.model.KBTemplate[returnValue.size()]);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
