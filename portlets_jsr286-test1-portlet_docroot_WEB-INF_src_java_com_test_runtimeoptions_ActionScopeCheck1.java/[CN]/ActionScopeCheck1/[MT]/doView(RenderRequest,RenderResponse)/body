{
  response.setContentType("text/html");
  PrintWriter writer=response.getWriter();
  PortletURL aurl=response.createActionURL();
  writer.println("Here action-scoped attributes are set after " + "which Events are sent to same portlet.");
  writer.println("<br><a href=\"" + aurl + "\">ActionScopeEventSamePortlet</a>");
  writer.println("<br>Action scope attribute set in PA():" + request.getAttribute(("n1")));
  if (request.getParameter("keya") != null) {
    writer.println("<br>action scope attribute set in" + " handleevent1() of sameportlet:" + request.getAttribute(("n2")));
  }
  if (request.getParameter("keyb") != null) {
    writer.println("<br>action scope attribute set in " + "handleevent2() of sameportlet:" + request.getAttribute(("n3")));
  }
}
