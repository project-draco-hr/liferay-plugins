{
  if (obj == null) {
    return false;
  }
  HRClientClp hrClient=null;
  try {
    hrClient=(HRClientClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=hrClient.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
