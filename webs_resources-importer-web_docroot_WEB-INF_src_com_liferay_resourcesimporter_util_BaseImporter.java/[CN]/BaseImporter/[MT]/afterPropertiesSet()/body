{
  User user=UserLocalServiceUtil.getDefaultUser(companyId);
  userId=user.getUserId();
  LayoutSetPrototype layoutSetPrototype=LayoutSetPrototypeLocalServiceUtil.addLayoutSetPrototype(userId,companyId,layoutSetPrototypeNameMap,StringPool.BLANK,true,true,new ServiceContext());
  Group group=layoutSetPrototype.getGroup();
  groupId=group.getGroupId();
}
