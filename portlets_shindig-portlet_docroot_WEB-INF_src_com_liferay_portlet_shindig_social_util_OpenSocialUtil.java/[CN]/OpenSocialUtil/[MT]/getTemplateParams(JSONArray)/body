{
  if (array == null) {
    return null;
  }
  Map<String,String> map=new LinkedHashMap<String,String>();
  for (int i=0; i < array.length(); i++) {
    try {
      JSONObject jsonItem=array.getJSONObject(i);
      map.put(jsonItem.getString("key"),jsonItem.getString("value"));
    }
 catch (    JSONException je) {
    }
  }
  return map;
}
