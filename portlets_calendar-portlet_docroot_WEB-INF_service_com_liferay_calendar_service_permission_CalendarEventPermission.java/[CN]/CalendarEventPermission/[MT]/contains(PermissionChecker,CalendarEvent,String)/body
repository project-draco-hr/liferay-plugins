{
  if (permissionChecker.hasOwnerPermission(calendarEvent.getCompanyId(),CalendarEvent.class.getName(),calendarEvent.getCalendarEventId(),calendarEvent.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(calendarEvent.getGroupId(),CalendarEvent.class.getName(),calendarEvent.getCalendarEventId(),actionId);
}
