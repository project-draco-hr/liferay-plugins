{
  KaleoTask kaleoTask=(KaleoTask)EntityCacheUtil.getResult(KaleoTaskModelImpl.ENTITY_CACHE_ENABLED,KaleoTaskImpl.class,kaleoTaskId,this);
  if (kaleoTask == null) {
    Session session=null;
    try {
      session=openSession();
      kaleoTask=(KaleoTask)session.get(KaleoTaskImpl.class,new Long(kaleoTaskId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (kaleoTask != null) {
        cacheResult(kaleoTask);
      }
      closeSession(session);
    }
  }
  return kaleoTask;
}
