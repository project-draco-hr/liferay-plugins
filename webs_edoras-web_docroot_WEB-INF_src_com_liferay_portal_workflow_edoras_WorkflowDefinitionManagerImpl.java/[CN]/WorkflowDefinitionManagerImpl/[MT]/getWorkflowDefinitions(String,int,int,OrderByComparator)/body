{
  try {
    List<com.liferay.portal.workflow.edoras.model.WorkflowDefinition> workflowDefinitions=WorkflowDefinitionUtil.findByName(workflowDefinitionName,start,end,orderByComparator);
    return WorkflowManagerUtil.wrapWorkflowDefinitions(workflowDefinitions);
  }
 catch (  Exception e) {
    throw new WorkflowException(e.getMessage(),e);
  }
}
