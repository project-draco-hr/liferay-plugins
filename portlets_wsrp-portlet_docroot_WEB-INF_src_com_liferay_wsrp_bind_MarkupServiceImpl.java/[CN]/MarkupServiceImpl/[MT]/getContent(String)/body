{
  int x=rawContent.indexOf("portlet-content-container");
  if (x == -1) {
    return rawContent;
  }
  x=rawContent.indexOf("<div>",x);
  if (x == -1) {
    return rawContent;
  }
  int y=rawContent.indexOf("</div></div></div></div>",x);
  if (y == -1) {
    return rawContent;
  }
  return rawContent.substring(x + 5,y);
}
