{
  if (obj == null) {
    return false;
  }
  HRTimeOffPolicyClp hrTimeOffPolicy=null;
  try {
    hrTimeOffPolicy=(HRTimeOffPolicyClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
