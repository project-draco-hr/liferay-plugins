{
  if (_service == null) {
    InvokableLocalService invokableLocalService=(InvokableLocalService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),KaleoActionLocalService.class.getName());
    _service=new KaleoActionLocalServiceClp(invokableLocalService);
    ReferenceRegistry.registerReference(KaleoActionLocalServiceUtil.class,"_service");
    MethodCache.remove(KaleoActionLocalService.class);
  }
  return _service;
}
