{
  Session session=null;
  try {
    session=openSession();
    KaleoInstance kaleoInstance=(KaleoInstance)session.get(KaleoInstanceImpl.class,new Long(kaleoInstanceId));
    if (kaleoInstance == null) {
      if (_log.isWarnEnabled()) {
        _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoInstanceId);
      }
      throw new NoSuchInstanceException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + kaleoInstanceId);
    }
    return remove(kaleoInstance);
  }
 catch (  NoSuchInstanceException nsee) {
    throw nsee;
  }
catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
}
