{
  KaleoInstance kaleoInstance=(KaleoInstance)EntityCacheUtil.getResult(KaleoInstanceModelImpl.ENTITY_CACHE_ENABLED,KaleoInstanceImpl.class,kaleoInstanceId);
  if (kaleoInstance == _nullKaleoInstance) {
    return null;
  }
  if (kaleoInstance == null) {
    Session session=null;
    boolean hasException=false;
    try {
      session=openSession();
      kaleoInstance=(KaleoInstance)session.get(KaleoInstanceImpl.class,Long.valueOf(kaleoInstanceId));
    }
 catch (    Exception e) {
      hasException=true;
      throw processException(e);
    }
 finally {
      if (kaleoInstance != null) {
        cacheResult(kaleoInstance);
      }
 else       if (!hasException) {
        EntityCacheUtil.putResult(KaleoInstanceModelImpl.ENTITY_CACHE_ENABLED,KaleoInstanceImpl.class,kaleoInstanceId,_nullKaleoInstance);
      }
      closeSession(session);
    }
  }
  return kaleoInstance;
}
