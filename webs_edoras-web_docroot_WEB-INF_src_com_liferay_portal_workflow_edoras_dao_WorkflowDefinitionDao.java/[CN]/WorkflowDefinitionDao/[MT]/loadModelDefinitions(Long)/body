{
  try {
    List<WorkflowDefinition> workflowDefinitionList=null;
    if (tenantId == null) {
      workflowDefinitionList=WorkflowDefinitionUtil.findAll();
    }
 else {
      workflowDefinitionList=WorkflowDefinitionUtil.findByCompanyId(tenantId.longValue());
    }
    return WorkflowEntityBridgeUtil.wrapWorkflowDefinitionList(workflowDefinitionList);
  }
 catch (  SystemException se) {
    throw new ProcessException("Could not find workflow definitions",se);
  }
}
