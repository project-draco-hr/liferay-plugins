{
  long id=0;
  try {
    id=((WorkflowDefinition)entity).getWorkflowDefinitionId();
    WorkflowDefinitionUtil.remove(id);
  }
 catch (  NoSuchWorkflowDefinitionException nswde) {
    throw new ProcessException("Could not delete workflow definition with id " + id,nswde);
  }
catch (  SystemException se) {
    throw new ProcessException("Could not delete workflow definition with id " + id,se);
  }
}
