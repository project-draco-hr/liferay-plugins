{
  try {
    long companyId=CompanyConstants.SYSTEM;
    if (tenantId != null) {
      companyId=tenantId;
    }
    WorkflowDefinition workflowDefinition=WorkflowDefinitionUtil.findByC_N_V(companyId,modelId,modelVersion);
    return new WorkflowDefinitionBridge(workflowDefinition);
  }
 catch (  NoSuchWorkflowDefinitionException nswde) {
    return null;
  }
catch (  Exception e) {
    throw new ProcessException(e.getMessage(),e);
  }
}
