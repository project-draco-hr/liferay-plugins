{
  long primaryKey=(Long)identity;
  try {
    WorkflowDefinition workflowDefinition=WorkflowDefinitionUtil.findByPrimaryKey(primaryKey);
    return (T)new WorkflowDefinitionBridge(workflowDefinition);
  }
 catch (  NoSuchWorkflowDefinitionException nswde) {
    return null;
  }
catch (  SystemException se) {
    throw new ProcessException("Could not load workflow definition with id " + primaryKey,se);
  }
}
