{
  long id=(Long)identity;
  try {
    return (T)WorkflowDefinitionUtil.findByPrimaryKey(id);
  }
 catch (  NoSuchWorkflowDefinitionException nswde) {
    throw new ProcessException("Could not load workflow definition with id " + id,nswde);
  }
catch (  SystemException se) {
    throw new ProcessException("Could not load workflow definition with id " + id,se);
  }
}
