{
  if (_persistence == null) {
    _persistence=(BlobEntryPersistence)PortletBeanLocatorUtil.locate(com.liferay.testblob.service.ClpSerializer.getServletContextName(),BlobEntryPersistence.class.getName());
    ReferenceRegistry.registerReference(BlobEntryUtil.class,"_persistence");
  }
  return _persistence;
}
