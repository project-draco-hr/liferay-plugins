{
  if (humanPriority <= 0) {
    return null;
  }
  List<Article> articles=ArticleServiceUtil.getSiblingArticles(groupId,parentResourcePrimKey,WorkflowConstants.STATUS_ANY,humanPriority - 1,humanPriority,new ArticlePriorityComparator(true));
  if (articles.isEmpty()) {
    return null;
  }
  return articles.get(0);
}
