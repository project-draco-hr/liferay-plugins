{
  entry=toUnwrappedModel(entry);
  Session session=null;
  try {
    session=openSession();
    if (entry.isCachedModel()) {
      entry=(Entry)session.get(EntryImpl.class,entry.getPrimaryKeyObj());
    }
    session.delete(entry);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  clearCache(entry);
  return entry;
}
