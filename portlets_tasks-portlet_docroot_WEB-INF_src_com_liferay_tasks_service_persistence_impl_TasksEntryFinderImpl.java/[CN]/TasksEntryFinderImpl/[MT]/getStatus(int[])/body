{
  if (statusArray == null) {
    return StringPool.BLANK;
  }
  StringBundler sb=new StringBundler((statusArray.length * 2) + 1);
  sb.append(" (");
  for (int i=0; i < statusArray.length; i++) {
    sb.append("TMS_TasksEntry.status = ? ");
    sb.append("OR ");
  }
  sb.setIndex(sb.index() - 1);
  sb.append(") AND ");
  return sb.toString();
}
