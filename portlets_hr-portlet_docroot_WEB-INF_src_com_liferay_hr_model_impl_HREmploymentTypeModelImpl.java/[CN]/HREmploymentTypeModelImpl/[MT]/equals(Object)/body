{
  if (obj == null) {
    return false;
  }
  HREmploymentType hrEmploymentType=null;
  try {
    hrEmploymentType=(HREmploymentType)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrEmploymentType.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
