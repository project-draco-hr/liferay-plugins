{
  if (obj == null) {
    return false;
  }
  HRUserTaskClp hrUserTask=null;
  try {
    hrUserTask=(HRUserTaskClp)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long pk=hrUserTask.getPrimaryKey();
  if (getPrimaryKey() == pk) {
    return true;
  }
 else {
    return false;
  }
}
