{
  HRTimeSheetHoursPerDay hrTimeSheetHoursPerDay=fetchByPrimaryKey(hrTimeSheetHoursPerDayId);
  if (hrTimeSheetHoursPerDay == null) {
    if (_log.isWarnEnabled()) {
      _log.warn(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeSheetHoursPerDayId);
    }
    throw new NoSuchTimeSheetHoursPerDayException(_NO_SUCH_ENTITY_WITH_PRIMARY_KEY + hrTimeSheetHoursPerDayId);
  }
  return hrTimeSheetHoursPerDay;
}
