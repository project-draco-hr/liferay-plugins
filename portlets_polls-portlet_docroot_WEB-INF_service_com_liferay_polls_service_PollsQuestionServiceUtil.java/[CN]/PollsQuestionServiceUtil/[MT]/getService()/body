{
  if (_service == null) {
    InvokableService invokableService=(InvokableService)PortletBeanLocatorUtil.locate(ClpSerializer.getServletContextName(),PollsQuestionService.class.getName());
    if (invokableService instanceof PollsQuestionService) {
      _service=(PollsQuestionService)invokableService;
    }
 else {
      _service=new PollsQuestionServiceClp(invokableService);
    }
    ReferenceRegistry.registerReference(PollsQuestionServiceUtil.class,"_service");
  }
  return _service;
}
