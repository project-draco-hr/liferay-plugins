{
  if (!calendarBooking.isMasterBooking()) {
    return;
  }
  List<CalendarBooking> childCalendarBookings=calendarBookingPersistence.findByParentCalendarBookingId(calendarBooking.getCalendarBookingId());
  for (  CalendarBooking childCalendarBooking : childCalendarBookings) {
    if (ArrayUtil.contains(childCalendarIds,childCalendarBooking.getCalendarId())) {
      continue;
    }
    deleteCalendarBooking(childCalendarBooking.getCalendarBookingId());
  }
  for (  long calendarId : childCalendarIds) {
    int count=calendarBookingPersistence.countByC_P(calendarId,calendarBooking.getCalendarBookingId());
    if (count > 0) {
      continue;
    }
    addCalendarBooking(calendarBooking.getUserId(),calendarId,calendarBooking.getCalendarBookingId(),new long[0],calendarBooking.getTitleMap(),calendarBooking.getDescriptionMap(),calendarBooking.getLocation(),calendarBooking.getUTCStartDate(),calendarBooking.getUTCEndDate(),calendarBooking.getAllDay(),calendarBooking.getRecurrence(),calendarBooking.getFirstReminder(),calendarBooking.getSecondReminder(),serviceContext);
  }
}
