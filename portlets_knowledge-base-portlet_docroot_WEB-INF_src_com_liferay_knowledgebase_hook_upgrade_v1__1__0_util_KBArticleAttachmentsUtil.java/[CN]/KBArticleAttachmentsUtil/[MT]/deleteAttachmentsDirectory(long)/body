{
  try {
    String[] fileNames=DLLocalServiceUtil.getFileNames(companyId,CompanyConstants.SYSTEM,"knowledgebase/articles");
    if (fileNames.length > 0) {
      if (_log.isWarnEnabled()) {
        _log.warn("Unable to delete knowledgebase/articles");
      }
      return;
    }
    DLLocalServiceUtil.deleteDirectory(companyId,CompanyConstants.SYSTEM_STRING,CompanyConstants.SYSTEM,"knowledgebase/articles");
  }
 catch (  Exception e) {
    _log.error(e.getMessage());
  }
}
