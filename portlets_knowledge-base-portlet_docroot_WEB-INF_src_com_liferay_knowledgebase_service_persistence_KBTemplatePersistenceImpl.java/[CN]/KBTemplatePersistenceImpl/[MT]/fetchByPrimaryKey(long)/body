{
  KBTemplate kbTemplate=(KBTemplate)EntityCacheUtil.getResult(KBTemplateModelImpl.ENTITY_CACHE_ENABLED,KBTemplateImpl.class,kbTemplateId,this);
  if (kbTemplate == null) {
    Session session=null;
    try {
      session=openSession();
      kbTemplate=(KBTemplate)session.get(KBTemplateImpl.class,Long.valueOf(kbTemplateId));
    }
 catch (    Exception e) {
      throw processException(e);
    }
 finally {
      if (kbTemplate != null) {
        cacheResult(kbTemplate);
      }
      closeSession(session);
    }
  }
  return kbTemplate;
}
