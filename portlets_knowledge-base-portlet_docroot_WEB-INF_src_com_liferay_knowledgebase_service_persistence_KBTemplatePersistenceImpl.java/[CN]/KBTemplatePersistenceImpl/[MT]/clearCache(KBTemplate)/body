{
  EntityCacheUtil.removeResult(KBTemplateModelImpl.ENTITY_CACHE_ENABLED,KBTemplateImpl.class,kbTemplate.getPrimaryKey());
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITH_PAGINATION);
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST_WITHOUT_PAGINATION);
  FinderCacheUtil.removeResult(FINDER_PATH_FETCH_BY_UUID_G,new Object[]{kbTemplate.getUuid(),Long.valueOf(kbTemplate.getGroupId())});
}
