{
  Connection con=null;
  PreparedStatement ps=null;
  try {
    con=DataAccess.getConnection();
    ps=con.prepareStatement("update " + tableName + " set workflowContext = ? where "+ uniqueId+ " = ?");
    ps.setString(1,workflowContext);
    ps.setLong(2,Id);
    ps.executeUpdate();
  }
  finally {
    DataAccess.cleanUp(con,ps);
  }
}
