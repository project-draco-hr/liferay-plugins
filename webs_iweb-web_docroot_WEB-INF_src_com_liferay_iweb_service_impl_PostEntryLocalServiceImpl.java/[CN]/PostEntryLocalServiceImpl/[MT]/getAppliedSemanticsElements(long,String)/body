{
  try {
    PostEntry postEntry=postEntryPersistence.fetchById_Type(postEntryId,postEntryType);
    long pk=postEntry.getPrimaryKey();
    Set<SemanticsElement> semanticsElementSet=new TreeSet(postEntryPersistence.getSemanticsElements(pk));
    return semanticsElementSet;
  }
 catch (  SystemException ex) {
    throw new IWebException(ex);
  }
}
