{
  try {
    com.liferay.knowledgebase.model.KBArticle returnValue=KBArticleServiceUtil.updateKBArticle(resourcePrimKey,title,content,description,kbTemplateId,sections,dirName,serviceContext);
    return com.liferay.knowledgebase.model.KBArticleSoap.toSoapModel(returnValue);
  }
 catch (  Exception e) {
    _log.error(e,e);
    throw new RemoteException(e.getMessage());
  }
}
