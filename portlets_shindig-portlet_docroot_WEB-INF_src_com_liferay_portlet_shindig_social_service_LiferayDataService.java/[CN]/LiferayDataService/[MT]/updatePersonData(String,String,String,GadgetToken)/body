{
  if (!ShindigUtil.isValidKey(key)) {
    return new ResponseItem<Object>(ResponseError.BAD_REQUEST,"The person data key had invalid characters",new JSONObject());
  }
  try {
    ExpandoColumn column=ShindigUtil.getColumn(key);
    ExpandoValueLocalServiceUtil.addValue(User.class.getName(),ShindigUtil.OPEN_SOCIAL_DATA,column.getName(),Long.parseLong(userId),value);
  }
 catch (  Exception e) {
  }
  return new ResponseItem<JSONObject>(new JSONObject());
}
