{
  if (permissionChecker.hasOwnerPermission(bbbMeeting.getCompanyId(),BBBMeeting.class.getName(),bbbMeeting.getBbbMeetingId(),bbbMeeting.getUserId(),actionId)) {
    return true;
  }
  return permissionChecker.hasPermission(bbbMeeting.getGroupId(),BBBMeeting.class.getName(),bbbMeeting.getBbbServerId(),actionId);
}
