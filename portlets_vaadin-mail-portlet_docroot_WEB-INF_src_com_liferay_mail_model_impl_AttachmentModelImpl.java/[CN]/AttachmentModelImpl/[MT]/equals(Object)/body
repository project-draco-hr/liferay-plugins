{
  if (obj == null) {
    return false;
  }
  Attachment attachment=null;
  try {
    attachment=(Attachment)obj;
  }
 catch (  ClassCastException cce) {
    return false;
  }
  long primaryKey=attachment.getPrimaryKey();
  if (getPrimaryKey() == primaryKey) {
    return true;
  }
 else {
    return false;
  }
}
