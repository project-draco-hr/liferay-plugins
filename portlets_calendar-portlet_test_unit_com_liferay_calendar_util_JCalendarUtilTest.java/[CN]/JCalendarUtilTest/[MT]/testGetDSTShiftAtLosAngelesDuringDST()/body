{
  Calendar calendar1=JCalendarUtil.getJCalendar(2012,Calendar.MAY,1,12,0,0,0,TimeZoneUtil.GMT);
  Calendar calendar2=JCalendarUtil.getJCalendar(2013,Calendar.JULY,2,12,0,0,0,TimeZoneUtil.GMT);
  int shift=JCalendarUtil.getDSTShift(calendar1,calendar2,_losAngelesTimeZone);
  Assert.assertEquals(0,shift);
}
