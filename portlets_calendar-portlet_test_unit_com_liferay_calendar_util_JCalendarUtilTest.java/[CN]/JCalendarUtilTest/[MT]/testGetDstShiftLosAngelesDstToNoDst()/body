{
  Calendar calendar1=JCalendarUtil.getJCalendar(2013,Calendar.JULY,1,12,0,0,0,TimeZoneUtil.GMT);
  Calendar calendar2=JCalendarUtil.getJCalendar(2013,Calendar.JANUARY,1,12,0,0,0,TimeZoneUtil.GMT);
  int shift=JCalendarUtil.getDstShift(calendar1,calendar2,_losAngelesTimeZone);
  Assert.assertEquals(JCalendarUtil.HOUR,shift);
}
