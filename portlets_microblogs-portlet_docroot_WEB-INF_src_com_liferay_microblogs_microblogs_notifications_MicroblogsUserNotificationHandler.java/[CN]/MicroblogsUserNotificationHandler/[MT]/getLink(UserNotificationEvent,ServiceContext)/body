{
  JSONObject jsonObject=JSONFactoryUtil.createJSONObject(userNotificationEvent.getPayload());
  long microblogsEntryId=jsonObject.getLong("microblogsEntryId");
  AssetRendererFactory assetRendererFactory=AssetRendererFactoryRegistryUtil.getAssetRendererFactoryByClassName(MicroblogsEntry.class.getName());
  AssetRenderer assetRenderer=assetRendererFactory.getAssetRenderer(microblogsEntryId);
  return assetRenderer.getURLViewInContext(serviceContext.getLiferayPortletRequest(),serviceContext.getLiferayPortletResponse(),null);
}
