{
  attachment=toUnwrappedModel(attachment);
  Session session=null;
  try {
    session=openSession();
    BatchSessionUtil.update(session,attachment,merge);
    attachment.setNew(false);
  }
 catch (  Exception e) {
    throw processException(e);
  }
 finally {
    closeSession(session);
  }
  FinderCacheUtil.clearCache(FINDER_CLASS_NAME_LIST);
  EntityCacheUtil.putResult(AttachmentModelImpl.ENTITY_CACHE_ENABLED,AttachmentImpl.class,attachment.getPrimaryKey(),attachment);
  return attachment;
}
