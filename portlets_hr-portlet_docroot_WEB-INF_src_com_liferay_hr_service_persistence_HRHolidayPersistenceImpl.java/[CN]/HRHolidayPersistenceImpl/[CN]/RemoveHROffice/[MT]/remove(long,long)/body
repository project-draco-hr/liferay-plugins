{
  if (_persistenceImpl.containsHROffice.contains(hrHolidayId,hrOfficeId)) {
    ModelListener<com.liferay.hr.model.HROffice>[] hrOfficeListeners=hrOfficePersistence.getListeners();
    for (    ModelListener<HRHoliday> listener : listeners) {
      listener.onBeforeRemoveAssociation(hrHolidayId,com.liferay.hr.model.HROffice.class.getName(),hrOfficeId);
    }
    for (    ModelListener<com.liferay.hr.model.HROffice> listener : hrOfficeListeners) {
      listener.onBeforeRemoveAssociation(hrOfficeId,HRHoliday.class.getName(),hrHolidayId);
    }
    _sqlUpdate.update(new Object[]{new Long(hrHolidayId),new Long(hrOfficeId)});
    for (    ModelListener<HRHoliday> listener : listeners) {
      listener.onAfterRemoveAssociation(hrHolidayId,com.liferay.hr.model.HROffice.class.getName(),hrOfficeId);
    }
    for (    ModelListener<com.liferay.hr.model.HROffice> listener : hrOfficeListeners) {
      listener.onAfterRemoveAssociation(hrOfficeId,HRHoliday.class.getName(),hrHolidayId);
    }
  }
}
