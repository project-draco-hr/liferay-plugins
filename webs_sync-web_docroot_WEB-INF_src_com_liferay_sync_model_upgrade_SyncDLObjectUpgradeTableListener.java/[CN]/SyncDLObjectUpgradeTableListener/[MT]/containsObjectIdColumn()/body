{
  Connection con=null;
  PreparedStatement ps=null;
  ResultSet rs=null;
  try {
    con=DataAccess.getUpgradeOptimizedConnection();
    ps=con.prepareStatement("select * from SyncDLObject where objectId = 0");
    rs=ps.executeQuery();
  }
 catch (  Exception e) {
    return false;
  }
 finally {
    DataAccess.cleanUp(con,ps,rs);
  }
  return true;
}
